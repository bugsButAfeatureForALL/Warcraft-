if(not(IsUnitAliveBJ(udg_Dolcetto_Unit)==true))then
return false
endif
return true
endfunction
function Trig_Evil_Actions takes nothing returns nothing
call ForceRemovePlayerSimple(udg_Roa_Player,udg_Neutral)
call ForceRemovePlayerSimple(udg_Dolcetto_Player,udg_Neutral)
call CreateNUnitsAtLoc(1,'h01N',udg_Greed_Player,GetRectCenter(gg_rct_Soul),bj_UNIT_FACING)
call RemoveUnit(GetTriggerUnit())
set udg_Greed_Evil=true
call DisplayTextToForce(GetPlayersAll(),"Greed has chosen to side with the Homunculi!")
call ForceAddPlayerSimple(udg_Greed_Player,udg_Homunculi)
call ForceAddPlayerSimple(udg_Greed_Player,udg_Evil_Team)
if(Trig_Evil_Func011C())then
call ForceAddPlayerSimple(udg_Roa_Player,udg_Homunculi)
call ForceAddPlayerSimple(udg_Roa_Player,udg_Homunculi)
call UnitRemoveAbilityBJ('A09I',udg_Roa_Unit)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Roa_Player),bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Roa_Player),udg_Evil_Team,bj_ALLIANCE_ALLIED_VISION)
else
endif
if(Trig_Evil_Func012C())then
call ForceAddPlayerSimple(udg_Dolcetto_Player,udg_Homunculi)
call ForceAddPlayerSimple(udg_Dolcetto_Player,udg_Evil_Team)
call UnitRemoveAbilityBJ('A09I',udg_Dolcetto_Unit)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Dolcetto_Player),udg_Evil_Team,bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Dolcetto_Player),bj_ALLIANCE_ALLIED_VISION)
else
endif
call DisableTrigger(gg_trg_Roa_Dies_S)
call EnableTrigger(gg_trg_Roa_Dies_H)
call DisableTrigger(gg_trg_Dolcetto_Dies_S)
call EnableTrigger(gg_trg_Dolcetto_Dies_H)
endfunction
