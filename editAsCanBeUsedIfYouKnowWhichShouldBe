function Trig_Chimera_Timer_Actions takes nothing returns nothing
set udg_Chimera_Limiter=0
set udg_Chimera_Limiter_A=0
call DisplayTextToForce(GetForceOfPlayer(udg_Heinkel_Player),"The time has come for the Chimeras to make their choice. Will you stand for Amestris and side with the |cffff0000Alchemists|r against Father? Or will you rejoin the military and side with the |cffff0000Homunculi|r against the Alchemists?                                                                       \n\n-Alchemists\n\n-Homunculi (Choosing this will grant you an immediate +20 to all stats)\n\nYou have |cffff0000one minute|r to decide!")
call DisplayTextToForce(GetForceOfPlayer(udg_Jerso_Player),"The time has come for the Chimeras to make their choice. Will you stand for Amestris and side with the |cffff0000Alchemists|r against Father? Or will you rejoin the military and side with the |cffff0000Homunculi|r against the Alchemists?                                                                                                                                                                                                                       -Alchemists\n\n-Homunculi (Choosing this will grant you an immediate +20 to all stats)\n\nYou have |cffff0000one minute|r to decide!                                     ")
call DisplayTextToForce(GetForceOfPlayer(udg_Darius_Player),"The time has come for the Chimeras to make their choice. Will you stand for Amestris and side with the |cffff0000Alchemists|r against Father? Or will you rejoin the military and side with the |cffff0000Homunculi|r against the Alchemists?                                                                                                                                                                                                                       -Alchemists\n\n-Homunculi (Choosing this will grant you an immediate +20 to all stats)\n\nYou have |cffff0000one minute|r to decide!                                     ")
call DisplayTextToForce(GetForceOfPlayer(udg_Zampano_Player),"The time has come for the Chimeras to make their choice. Will you stand for Amestris and side with the |cffff0000Alchemists|r against Father? Or will you rejoin the military and side with the |cffff0000Homunculi|r against the Alchemists?                                                                                                                                                                                                                       -Alchemists\n\n-Homunculi (Choosing this will grant you an immediate +20 to all stats)\n\nYou have |cffff0000one minute|r to decide!                                     ")
call EnableTrigger(gg_trg_Zampano_Homunculi_G)
call EnableTrigger(gg_trg_Zampano_Homunculi_LB)
call EnableTrigger(gg_trg_Zampano_Alchemists_G)
call EnableTrigger(gg_trg_Zampano_Alchemists_LB)
call EnableTrigger(gg_trg_Heinkel_Homunculi_G)
call EnableTrigger(gg_trg_Heinkel_Alchemists_G)
call EnableTrigger(gg_trg_Heinkel_Homunculi_LB)
call EnableTrigger(gg_trg_Heinkel_Alchemists_LB)
call EnableTrigger(gg_trg_Jerso_Homunculi_G)
call EnableTrigger(gg_trg_Jerso_Alchemists_G)
call EnableTrigger(gg_trg_Jerso_Homunculi_LB)
call EnableTrigger(gg_trg_Jerso_Alchemists_LB)
call EnableTrigger(gg_trg_Darius_Alchemists_G)
call EnableTrigger(gg_trg_Darius_Alchemists_LB)
call EnableTrigger(gg_trg_Darius_Homunculi_G)
call EnableTrigger(gg_trg_Darius_Homunculi_LB)
call TriggerSleepAction(60.00)
call DisableTrigger(gg_trg_Zampano_Alchemists_G)
call DisableTrigger(gg_trg_Zampano_Alchemists_LB)
call DisableTrigger(gg_trg_Zampano_Homunculi_G)
call DisableTrigger(gg_trg_Zampano_Homunculi_LB)
call DisableTrigger(gg_trg_Heinkel_Alchemists_G)
call DisableTrigger(gg_trg_Heinkel_Alchemists_LB)
call DisableTrigger(gg_trg_Heinkel_Homunculi_G)
call DisableTrigger(gg_trg_Heinkel_Homunculi_LB)
call DisableTrigger(gg_trg_Jerso_Alchemists_G)
call DisableTrigger(gg_trg_Jerso_Alchemists_LB)
call DisableTrigger(gg_trg_Jerso_Homunculi_G)
call DisableTrigger(gg_trg_Jerso_Homunculi_LB)
call DisableTrigger(gg_trg_Darius_Alchemists_G)
call DisableTrigger(gg_trg_Darius_Alchemists_LB)
call DisableTrigger(gg_trg_Darius_Homunculi_G)
call DisableTrigger(gg_trg_Darius_Homunculi_LB)
endfunction
function InitTrig_Chimera_Timer takes nothing returns nothing
set gg_trg_Chimera_Timer=CreateTrigger()
call TriggerRegisterTimerEventSingle(gg_trg_Chimera_Timer,900.00)
call TriggerAddAction(gg_trg_Chimera_Timer,function Trig_Chimera_Timer_Actions)
endfunction
function Trig_Zampano_Homunculi_G_Conditions takes nothing returns boolean
if(not(GetOwningPlayer(udg_Zampano_Unit)==Player(8)))then
return false
endif
if(not(IsUnitAliveBJ(udg_Zampano_Unit)==true))then
return false
endif
return true
endfunction
function Trig_Zampano_Homunculi_G_Func001C takes nothing returns boolean
if(not(udg_Chimera_Limiter==0))then
return false
endif
return true
endfunction
function Trig_Zampano_Homunculi_G_Actions takes nothing returns nothing
if(Trig_Zampano_Homunculi_G_Func001C())then
call SetPlayerAllianceStateBJ(udg_Zampano_Player,udg_Heinkel_Player,bj_ALLIANCE_UNALLIED)
call SetPlayerAllianceStateBJ(udg_Heinkel_Player,udg_Zampano_Player,bj_ALLIANCE_UNALLIED)
call SetPlayerAllianceStateBJ(udg_Darius_Player,udg_Zampano_Player,bj_ALLIANCE_UNALLIED)
call SetPlayerAllianceStateBJ(udg_Zampano_Player,udg_Darius_Player,bj_ALLIANCE_UNALLIED)
call SetPlayerAllianceStateBJ(udg_Jerso_Player,udg_Zampano_Player,bj_ALLIANCE_UNALLIED)
call SetPlayerAllianceStateBJ(udg_Zampano_Player,udg_Jerso_Player,bj_ALLIANCE_UNALLIED)
set udg_Chimera_Limiter=1
call DisableTrigger(GetTriggeringTrigger())
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Zampano|r has decided to return to the military and side with the Homunculi! Father has used his Alchemy to enhance Zampano's strength!")
call ForceAddPlayerSimple(udg_Zampano_Player,udg_Homunculi)
call ForceAddPlayerSimple(udg_Zampano_Player,udg_Evil_Team)
call ForceRemovePlayerSimple(udg_Zampano_Player,udg_Neutral)
call SetForceAllianceStateBJ(udg_Homunculi,GetForceOfPlayer(udg_Zampano_Player),bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Zampano_Player),udg_Homunculi,bj_ALLIANCE_ALLIED_VISION)
call SetPlayerAllianceStateBJ(Player(bj_PLAYER_NEUTRAL_EXTRA),udg_Zampano_Player,bj_ALLIANCE_ALLIED_VISION)
call SetPlayerAllianceStateBJ(udg_Zampano_Player,Player(bj_PLAYER_NEUTRAL_EXTRA),bj_ALLIANCE_ALLIED_VISION)
call DisableTrigger(gg_trg_Zampano_Alchemists_G)
call DisableTrigger(gg_trg_Zampano_Dies_S)
call EnableTrigger(gg_trg_Zampano_Dies_H)
call ModifyHeroStat(bj_HEROSTAT_STR,udg_Zampano_Unit,bj_MODIFYMETHOD_ADD,20)
call ModifyHeroStat(bj_HEROSTAT_AGI,udg_Zampano_Unit,bj_MODIFYMETHOD_ADD,20)
call RemoveUnit(udg_Chimera_Healer)
else
call DisplayTextToForce(GetForceOfPlayer(udg_Zampano_Player),"A Chimera has already joined the Homunculi! You can not join them.")
endif
call DestroyTrigger(GetTriggeringTrigger())
endfunction
function InitTrig_Zampano_Homunculi_G takes nothing returns nothing
set gg_trg_Zampano_Homunculi_G=CreateTrigger()
call DisableTrigger(gg_trg_Zampano_Homunculi_G)
call TriggerRegisterPlayerChatEvent(gg_trg_Zampano_Homunculi_G,Player(8),"-Homunculi",true)
call TriggerAddCondition(gg_trg_Zampano_Homunculi_G,Condition(function Trig_Zampano_Homunculi_G_Conditions))
call TriggerAddAction(gg_trg_Zampano_Homunculi_G,function Trig_Zampano_Homunculi_G_Actions)
endfunction
