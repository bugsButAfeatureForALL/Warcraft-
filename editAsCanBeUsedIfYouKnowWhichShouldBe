function Trig_Flash_Bang_Cast_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A04C'))then
return false
endif
return true
endfunction
function Trig_Flash_Bang_Cast_Actions takes nothing returns nothing
call CreateNUnitsAtLoc(1,'h02O',Player(PLAYER_NEUTRAL_PASSIVE),GetUnitLoc(udg_Ling_Unit),bj_UNIT_FACING)
call UnitApplyTimedLifeBJ(2.00,'BTLF',GetLastCreatedUnit())
call CreateNUnitsAtLoc(1,'h035',GetOwningPlayer(GetSpellAbilityUnit()),GetUnitLoc(udg_Ling_Unit),bj_UNIT_FACING)
call UnitApplyTimedLifeBJ(2.00,'BTLF',GetLastCreatedUnit())
call IssueImmediateOrder(GetLastCreatedUnit(),"stomp")
endfunction
function InitTrig_Flash_Bang_Cast takes nothing returns nothing
set gg_trg_Flash_Bang_Cast=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Flash_Bang_Cast,EVENT_PLAYER_UNIT_SPELL_FINISH)
