function Trig_Giolio_Comanche_Func002C takes nothing returns boolean
if(not(udg_Boss_Random_Number==0))then
return false
endif
return true
endfunction
function Trig_Giolio_Comanche_Actions takes nothing returns nothing
set udg_Boss_Random_Number=GetRandomInt(0,1)
if(Trig_Giolio_Comanche_Func002C())then
set udg_Boss_Random_Number=GetRandomInt(0,56)
else
set udg_Boss_Random_Number=GetRandomInt(69,125)
endif
call CreateNUnitsAtLoc(1,'H040',Player(PLAYER_NEUTRAL_AGGRESSIVE),GetRectCenter(udg_Spawn_Array[udg_Boss_Random_Number]),bj_UNIT_FACING)
call PingMinimapLocForForceEx(GetPlayersAll(),GetUnitLoc(GetLastCreatedUnit()),10.00,bj_MINIMAPPINGSTYLE_SIMPLE,100,0.00,0.00)
call SetHeroLevelBJ(GetLastCreatedUnit(),30,false)
call DisplayTextToForce(GetPlayersAll(),"Giolio Comanche the Silver Alchemist has appeared and challenges all to face his alchemy!")
call DestroyTrigger(GetTriggeringTrigger())
endfunction
