function Trig_Our_Fadder_Purple_Conditions takes nothing returns boolean
if(not(RectContainsUnit(gg_rct_Evil_Spawns,gg_unit_h01P_0053)==true))then
return false
endif
if(not(udg_Father_Is_Picked==false))then
return false
endif
return true
endfunction
function Trig_Our_Fadder_Purple_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
call DisableTrigger(gg_trg_Our_Fadder_Dark_Green)
call DisableTrigger(gg_trg_Our_Fadder_Brown)
set udg_Fadder_Player=Player(3)
call RemoveUnit(gg_unit_h01P_0053)
call ShowUnitHide(gg_unit_H03A_0103)
call PauseUnitBJ(true,gg_unit_H03A_0103)
call CreateNUnitsAtLoc(1,'H04J',udg_Fadder_Player,GetRectCenter(gg_rct_Our_Father),bj_UNIT_FACING)
set udg_Our_Fadder=GetLastCreatedUnit()
call SetPlayerName(udg_Fadder_Player,"Fadder")
call PanCameraToTimedLocForPlayer(udg_Fadder_Player,GetUnitLoc(udg_Our_Fadder),0)
call SelectUnitForPlayerSingle(udg_Our_Fadder,GetOwningPlayer(udg_Our_Fadder))
call SetPlayerStateBJ(udg_Fadder_Player,PLAYER_STATE_RESOURCE_LUMBER,0)
call DestroyTrigger(GetTriggeringTrigger())
endfunction
