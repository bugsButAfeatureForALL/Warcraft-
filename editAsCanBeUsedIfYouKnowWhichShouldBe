function Trig_May_Chang_is_Chosen_Func009C takes nothing returns boolean
if(not(udg_Scar_Is_Not_Picked==false))then
return false
endif
return true
endfunction
function Trig_May_Chang_is_Chosen_Actions takes nothing returns nothing
set udg_May_Chang_Player=GetOwningPlayer(GetTriggerUnit())
set udg_May_Chang_Is_Not_Picked=false
set udg_May_Chang=GetTriggerUnit()
call SetPlayerName(udg_May_Chang_Player,"May Chang")
call SetUnitPositionLoc(GetTriggerUnit(),GetRectCenter(gg_rct_May_Chang_Spawn))
call PanCameraToTimedLocForPlayer(GetOwningPlayer(udg_May_Chang),GetRectCenter(gg_rct_May_Chang_Spawn),0)
call SelectUnitForPlayerSingle(GetTriggerUnit(),GetOwningPlayer(GetTriggerUnit()))
call EnableTrigger(gg_trg_Stop_it_pls)
if(Trig_May_Chang_is_Chosen_Func009C())then
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_May_Chang_Player),bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_May_Chang_Player),udg_Good_Team,bj_ALLIANCE_UNALLIED)
call SetPlayerAllianceStateBJ(udg_May_Chang_Player,udg_Scar_Player,bj_ALLIANCE_ALLIED)
call SetPlayerAllianceStateBJ(udg_Scar_Player,udg_May_Chang_Player,bj_ALLIANCE_ALLIED)
call ForceRemovePlayerSimple(udg_May_Chang_Player,udg_Good_Team)
call DisplayTextToForce(GetForceOfPlayer(udg_May_Chang_Player),"You have met the mysterious Ishvalan |cffff0000Scar|r and joined up with him! But be warned. After 15 minutes you will join the Alchemist team and Scar may not share your new allies!")
else
call CreateNUnitsAtLoc(1,'h01Z',udg_May_Chang_Player,GetRectCenter(gg_rct_Scar_Spawn),bj_UNIT_FACING)
call UnitApplyTimedLifeBJ(300.00,'BTLF',GetLastCreatedUnit())
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[bj_PLAYER_NEUTRAL_VICTIM],udg_Good_Team,bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[7],udg_Good_Team,bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(udg_Good_Team,udg_Evil_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_May_Chang_Player),bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[bj_PLAYER_NEUTRAL_EXTRA],udg_Good_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_May_Chang_Player),bj_FORCE_PLAYER[bj_PLAYER_NEUTRAL_VICTIM],bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_May_Chang_Player),udg_Good_Team,bj_ALLIANCE_ALLIED)
call SetPlayerAllianceStateBJ(Player(7),udg_May_Chang_Player,bj_ALLIANCE_ALLIED_VISION)
call DisplayTextToForce(GetForceOfPlayer(udg_May_Chang_Player),"You are a member of the |cffff0000Alchemists|r team it is your job to stop Fathers plan before he can destroy the entire nation of Amestris for his own goals! You must also protect |cffff0000Winry|r at all costs! If she dies you can no longer revive!")
call ForceAddPlayerSimple(udg_May_Chang_Player,udg_Alchemists)
call ForceAddPlayerSimple(udg_May_Chang_Player,udg_Good_Team)
endif
call DestroyTrigger(GetTriggeringTrigger())
endfunction
function InitTrig_May_Chang_is_Chosen takes nothing returns nothing
set gg_trg_May_Chang_is_Chosen=CreateTrigger()
call TriggerRegisterEnterRectSimple(gg_trg_May_Chang_is_Chosen,gg_rct_Good_Spawns)
call TriggerAddCondition(gg_trg_May_Chang_is_Chosen,Condition(function Trig_May_Chang_is_Chosen_Conditions))
call TriggerAddAction(gg_trg_May_Chang_is_Chosen,function Trig_May_Chang_is_Chosen_Actions)
endfunction
