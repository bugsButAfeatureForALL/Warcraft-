function Trig_Use_Stone_Conditions takes nothing returns boolean
if(not(GetItemTypeId(GetManipulatedItem())=='I002'))then
return false
endif
if(not(GetUnitTypeId(GetTriggerUnit())=='H007'))then
return false
endif
if(not(RectContainsUnit(gg_rct_Rect_198,GetTriggerUnit())==false))then
return false
endif
if(not(RectContainsUnit(gg_rct_Inside_Gluttony,GetTriggerUnit())==false))then
return false
endif
return true
endfunction
function Trig_Use_Stone_Func001Func003C takes nothing returns boolean
if(not(GetPlayerSlotState(udg_Greed_Player)==PLAYER_SLOT_STATE_LEFT))then
return false
endif
return true
endfunction
function Trig_Use_Stone_Func001C takes nothing returns boolean
if(not(GetUnitLevel(udg_Ling_Unit)<20))then
return false
endif
return true
endfunction
function Trig_Use_Stone_Actions takes nothing returns nothing
if(Trig_Use_Stone_Func001C())then
call DisplayTextToForce(GetForceOfPlayer(udg_Ling_Player),"My body isn't strong enough to house this soul...")
else
if(Trig_Use_Stone_Func001Func003C())then
call DisplayTextToForce(GetForceOfPlayer(udg_Ling_Player),"It seems the soul has left this stone...it's useless now.")
else
call SetUnitInvulnerable(udg_Ling_Unit,true)
call DisplayTextToForce(GetPlayersAll(),"Ling Yao: AHHH! This...PAIN...My..body is being...torn apart!")
call RemoveItem(udg_Greeds_Stone)
call PauseUnitBJ(true,udg_Ling_Unit)
call AddSpecialEffectLocBJ(GetUnitLoc(GetTriggerUnit()),"war3mapImported\\SS2LightningRED.mdx")
set udg_GreedLing_Effect=GetLastCreatedEffectBJ()
call DestroyEffect(udg_GreedLing_Effect)
call TriggerSleepAction(5.00)
call SetPlayerState(udg_Greed_Player,PLAYER_STATE_OBSERVER,0)
call CreateNUnitsAtLoc(1,'H01B',udg_Greed_Player,GetUnitLoc(GetTriggerUnit()),bj_UNIT_FACING)
set udg_Greed=GetLastCreatedUnit()
call ShowUnitHide(udg_Ling_Unit)
call SetUnitPositionLoc(udg_Ling_Unit,GetRectCenter(gg_rct_Rect_218))
call SetUnitOwner(udg_Ling_Unit,Player(bj_PLAYER_NEUTRAL_VICTIM),true)
call SetHeroLevelBJ(udg_Greed,GetHeroLevel(udg_Ling_Unit),false)
call EnableTrigger(gg_trg_Greed_Item_Change)
call TriggerSleepAction(1.00)
call SelectUnitForPlayerSingle(udg_Greed,GetOwningPlayer(udg_Greed))
call PanCameraToTimedLocForPlayer(GetOwningPlayer(udg_Greed),GetUnitLoc(udg_Greed),0)
call DisplayTextToForce(GetPlayersAll(),"Greed: It's good to be back!")
call EnableTrigger(gg_trg_Lings_Soul)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Greed_Player),udg_Evil_Team,bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Greed_Player),bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Greed_Player),udg_Good_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Greed_Player),bj_ALLIANCE_UNALLIED)
call ForceRemovePlayerSimple(udg_Ling_Player,udg_Alchemists)
call ForceAddPlayerSimple(udg_Greed_Player,udg_Homunculi)
call DisplayTextToForce(GetForceOfPlayer(udg_Ling_Player),"Your body has been taken over by the Homunculus |cffff0000Greed|r! In three minutes  you will gain the chance to take your body back!")
call SetPlayerAllianceStateBJ(udg_Greed_Player,udg_Ling_Player,bj_ALLIANCE_UNALLIED_VISION)
call UnitRemoveItemFromSlotSwapped(1,udg_Greed)
call UnitAddItemByIdSwapped('I000',udg_Greed)
call SetPlayerAllianceStateBJ(udg_Dolcetto_Player,udg_Greed_Player,bj_ALLIANCE_UNALLIED)
call SetPlayerAllianceStateBJ(udg_Roa_Player,udg_Greed_Player,bj_ALLIANCE_UNALLIED)
call SetPlayerAllianceStateBJ(udg_Greed_Player,udg_Roa_Player,bj_ALLIANCE_UNALLIED)
call SetPlayerAllianceStateBJ(udg_Greed_Player,udg_Dolcetto_Player,bj_ALLIANCE_UNALLIED)
call RemoveUnit(udg_Greed_Healer)
call RemoveUnit(udg_Dolcetto_Healer)
call RemoveUnit(udg_Roa_Healer)
set udg_GreedLing_Is_Active=true
endif
endif
endfunction
function InitTrig_Use_Stone takes nothing returns nothing
