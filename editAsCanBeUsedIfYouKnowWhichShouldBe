endif
call TriggerSleepAction(7.00)
call SetUnitAnimation(gg_unit_h04L_0131,"Spell")
call SetUnitAnimation(gg_unit_H01R_0055,"Stand Alternate")
call TriggerSleepAction(2.90)
call ShowDestructableBJ(true,gg_dest_B005_10050)
call ShowDestructableBJ(true,gg_dest_B004_10049)
call ShowDestructableBJ(true,gg_dest_B004_0572)
call SetUnitPositionLoc(udg_He_Who_Sees_Truth,GetRectCenter(gg_rct_Return))
call PauseUnitBJ(false,udg_He_Who_Sees_Truth)
call SetUnitInvulnerable(udg_He_Who_Sees_Truth,false)
call PanCameraToTimedLocForPlayer(GetOwningPlayer(udg_He_Who_Sees_Truth),GetUnitLoc(udg_He_Who_Sees_Truth),0)
call EnableTrigger(gg_trg_Open_the_Gate)
call EnableTrigger(gg_trg_Unpause_Seer_of_Truth)
call SetUnitAnimation(gg_unit_H01R_0055,"Stand")
call TriggerSleepAction(2.00)
call DisableTrigger(gg_trg_Unpause_Seer_of_Truth)
endfunction
function InitTrig_Knowledge takes nothing returns nothing
set gg_trg_Knowledge=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Knowledge,EVENT_PLAYER_UNIT_SPELL_FINISH)
call TriggerAddCondition(gg_trg_Knowledge,Condition(function Trig_Knowledge_Conditions))
call TriggerAddAction(gg_trg_Knowledge,function Trig_Knowledge_Actions)
endfunction
function Trig_Friends_Revive_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A02O'))then
return false
endif
if(not(GetUnitStateSwap(UNIT_STATE_MAX_MANA,udg_He_Who_Sees_Truth)>=1000.00))then
return false
endif
return true
endfunction
function Trig_Friends_Revive_Func001Func006002002002 takes nothing returns boolean
return(IsUnitType(GetFilterUnit(),UNIT_TYPE_HERO)==true)
endfunction
function Trig_Friends_Revive_Func001Func007A takes nothing returns nothing
call ReviveHeroLoc(GetEnumUnit(),GetRectCenter(gg_rct_Gods_Domain),true)
call GroupRemoveUnitSimple(GetEnumUnit(),udg_Dead_Heroes)
set udg_Revived_By_Truth=GetEnumUnit()
set udg_Revived_By_Truth_Player=GetOwningPlayer(udg_Revived_By_Truth)
call SetUnitManaPercentBJ(udg_Revived_By_Truth,100)
call SetUnitFacingToFaceUnitTimed(udg_Revived_By_Truth,gg_unit_H01R_0055,0)
call PanCameraToTimedLocForPlayer(udg_Revived_By_Truth_Player,GetRectCenter(gg_rct_Rect_006),0)
call SelectUnitForPlayerSingle(udg_Revived_By_Truth,udg_Revived_By_Truth_Player)
call PauseUnitBJ(true,udg_Revived_By_Truth)
call SetPlayerState(udg_Revived_By_Truth_Player,PLAYER_STATE_OBSERVER,0)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Revived_By_Truth_Player),bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Revived_By_Truth_Player),bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Revived_By_Truth_Player),udg_Good_Team,bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[bj_PLAYER_NEUTRAL_VICTIM],GetForceOfPlayer(udg_Revived_By_Truth_Player),bj_ALLIANCE_ALLIED)
call ForceRemovePlayerSimple(udg_Revived_By_Truth_Player,udg_Dead_Good_Guys)
call ForceAddPlayerSimple(udg_Revived_By_Truth_Player,udg_Alchemists)
call DisplayTextToForce(GetForceOfPlayer(udg_He_Who_Sees_Truth_Player),"|cff995500Truth: There. Your lost little friend as good as new. But now little Al-che-mist. It's time I take something from you. What is equivalent exchange for a human life? How about...your ability to use Alchemy?.. |r")
call DisplayTextToForce(GetForceOfPlayer(udg_Revived_By_Truth_Player),"|cff995500Truth: There. Your lost little friend as good as new. But now little Al-che-mist. It's time I take something from you. What is equivalent exchange for a human life? How about...your ability to use Alchemy?.. |r")
endfunction
function Trig_Friends_Revive_Func001Func012002002002 takes nothing returns boolean
return(IsUnitType(GetFilterUnit(),UNIT_TYPE_HERO)==true)
endfunction
function Trig_Friends_Revive_Func001Func013A takes nothing returns nothing
call ReviveHeroLoc(GetEnumUnit(),GetRectCenter(gg_rct_Gods_Domain),true)
call GroupRemoveUnitSimple(GetEnumUnit(),udg_Dead_Heroes)
set udg_Revived_By_Truth=GetEnumUnit()
set udg_Revived_By_Truth_Player=GetOwningPlayer(udg_Revived_By_Truth)
call SetUnitManaPercentBJ(udg_Revived_By_Truth,100)
call SetUnitFacingToFaceUnitTimed(udg_Revived_By_Truth,gg_unit_H01R_0055,0)
call PauseUnitBJ(true,udg_Revived_By_Truth)
call PanCameraToTimedLocForPlayer(udg_Revived_By_Truth_Player,GetRectCenter(gg_rct_Rect_006),0)
call SelectUnitForPlayerSingle(udg_Revived_By_Truth,udg_Revived_By_Truth_Player)
call SetPlayerState(udg_Revived_By_Truth_Player,PLAYER_STATE_OBSERVER,0)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Revived_By_Truth_Player),bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Revived_By_Truth_Player),bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Revived_By_Truth_Player),udg_Good_Team,bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[bj_PLAYER_NEUTRAL_VICTIM],GetForceOfPlayer(udg_Revived_By_Truth_Player),bj_ALLIANCE_ALLIED)
call ForceRemovePlayerSimple(udg_Revived_By_Truth_Player,udg_Dead_Good_Guys)
call ForceAddPlayerSimple(udg_Revived_By_Truth_Player,udg_Alchemists)
call DisplayTextToForce(GetForceOfPlayer(udg_He_Who_Sees_Truth_Player),"|cff995500Truth: There. Your lost little friend as good as new. But now little Al-che-mist. It's time I take something from you. What is equivalent exchange for a human life? How about...that Stone of yours?.. |r")
call DisplayTextToForce(GetForceOfPlayer(udg_Revived_By_Truth_Player),"|cff995500Truth: There. Your lost little friend as good as new. But now little Al-che-mist. It's time I take something from you. What is equivalent exchange for a human life? How about...that Stone of yours?.. |r")
call RemoveItem(GetItemOfTypeFromUnitBJ(udg_He_Who_Sees_Truth,'I00G'))
endfunction
function Trig_Friends_Revive_Func001C takes nothing returns boolean
if(not(UnitHasItemOfTypeBJ(udg_He_Who_Sees_Truth,'I00G')==true))then
return false
endif
return true
endfunction
function Trig_Friends_Revive_Actions takes nothing returns nothing
if(Trig_Friends_Revive_Func001C())then
call SelectUnitForPlayerSingle(udg_He_Who_Sees_Truth,GetOwningPlayer(udg_He_Who_Sees_Truth))
call RemoveUnit(udg_Truth_Choice)
call DisplayTextToForce(GetForceOfPlayer(udg_He_Who_Sees_Truth_Player),"|cff995500Truth: Oh? Friendship is what the little Al-che-mist wants? You wish to play God and revive someone you miss? Very well...but the cost will be high....|r")
call TriggerSleepAction(10.00)
set udg_Dead_Heroes=GetRandomSubGroup(1,GetUnitsOfPlayerMatching(ForcePickRandomPlayer(udg_Dead_Good_Guys),Condition(function Trig_Friends_Revive_Func001Func012002002002)))
call ForGroupBJ(udg_Dead_Heroes,function Trig_Friends_Revive_Func001Func013A)
else
call SelectUnitForPlayerSingle(udg_He_Who_Sees_Truth,GetOwningPlayer(udg_He_Who_Sees_Truth))
call RemoveUnit(udg_Truth_Choice)
call DisplayTextToForce(GetForceOfPlayer(udg_He_Who_Sees_Truth_Player),"|cff995500Truth: Oh? Friendship is what the little Al-che-mist wants? You wish to play God and revive someone you miss? Very well...but the cost will be high....|r")
call TriggerSleepAction(10.00)
call ModifyHeroStat(bj_HEROSTAT_INT,udg_He_Who_Sees_Truth,bj_MODIFYMETHOD_SET,1)
set udg_Dead_Heroes=GetRandomSubGroup(1,GetUnitsOfPlayerMatching(ForcePickRandomPlayer(udg_Dead_Good_Guys),Condition(function Trig_Friends_Revive_Func001Func006002002002)))
call ForGroupBJ(udg_Dead_Heroes,function Trig_Friends_Revive_Func001Func007A)
endif
call TriggerSleepAction(10.00)
call ShowDestructableBJ(true,gg_dest_B005_10050)
call ShowDestructableBJ(true,gg_dest_B004_10049)
call ShowDestructableBJ(true,gg_dest_B004_0572)
call SetUnitPositionLoc(udg_He_Who_Sees_Truth,GetRectCenter(gg_rct_Return))
call SetUnitPositionLoc(udg_Revived_By_Truth,GetRectCenter(gg_rct_Return))
call SetUnitInvulnerable(udg_He_Who_Sees_Truth,false)
call PauseUnitBJ(false,udg_He_Who_Sees_Truth)
call PauseUnitBJ(false,udg_Revived_By_Truth)
call PanCameraToTimedLocForPlayer(GetOwningPlayer(udg_He_Who_Sees_Truth),GetUnitLoc(udg_He_Who_Sees_Truth),0)
call PanCameraToTimedLocForPlayer(GetOwningPlayer(udg_Revived_By_Truth),GetUnitLoc(udg_Revived_By_Truth),0)
call EnableTrigger(gg_trg_Open_the_Gate)
endfunction
function InitTrig_Friends_Revive takes nothing returns nothing
set gg_trg_Friends_Revive=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Friends_Revive,EVENT_PLAYER_UNIT_SPELL_FINISH)
call TriggerAddCondition(gg_trg_Friends_Revive,Condition(function Trig_Friends_Revive_Conditions))
call TriggerAddAction(gg_trg_Friends_Revive,function Trig_Friends_Revive_Actions)
endfunction
