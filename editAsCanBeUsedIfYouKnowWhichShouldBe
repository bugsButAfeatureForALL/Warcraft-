function Trig_Canine_Slash_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A076'))then
return false
endif
return true
endfunction
function Trig_Canine_Slash_Actions takes nothing returns nothing
set bj_forLoopAIndex=0
set bj_forLoopAIndexEnd=9
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set udg_Canine_Point=PolarProjectionBJ(GetUnitLoc(udg_Dolcetto_Unit),150.00,udg_Canine_Angle)
call AddSpecialEffectLocBJ(udg_Canine_Point,"war3mapImported\\mihawkslashhit.mdx")
set udg_Canine_Effect[GetForLoopIndexA()]=GetLastCreatedEffectBJ()
set udg_Canine_Angle=(udg_Canine_Angle+40.00)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set udg_Canine_Angle=0.00
call TriggerSleepAction(2)
set bj_forLoopAIndex=0
set bj_forLoopAIndexEnd=9
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call DestroyEffect(udg_Canine_Effect[GetForLoopIndexA()])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
call RemoveLocation(udg_Canine_Point)
endfunction
function InitTrig_Canine_Slash takes nothing returns nothing
set gg_trg_Canine_Slash=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Canine_Slash,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Canine_Slash,Condition(function Trig_Canine_Slash_Conditions))
call TriggerAddAction(gg_trg_Canine_Slash,function Trig_Canine_Slash_Actions)
endfunction
