return false
endif
return true
endfunction
function Trig_Father_Dies_Func031C takes nothing returns boolean
if(not(CountPlayersInForceBJ(udg_Neutral)==0))then
return false
endif
if(not(CountPlayersInForceBJ(udg_Homunculi)==0))then
return false
endif
return true
endfunction
function Trig_Father_Dies_Actions takes nothing returns nothing
call UnitRemoveAbilityBJ('A0A8',gg_unit_H01V_0080)
call PauseUnitBJ(false,gg_unit_H01V_0080)
call DestroyTimer(udg_Father_Timer)
call ForceClear(udg_Homunculi)
call RemoveUnit(udg_Envy_First_Form)
call RemoveUnit(udg_Envy_Shapeshifted_Form)
call RemoveUnit(udg_Envy_True_Form)
call RemoveUnit(udg_Kimblee_Unit)
call RemoveUnit(udg_Pride_Unit)
call RemoveUnit(udg_Gluttony_Unit)
call RemoveUnit(udg_Sloth_Unit)
call RemoveUnit(udg_Lust_Unit)
call RemoveUnit(udg_Bradley_Unit)
call DisableTrigger(gg_trg_The_Promised_Day)
call DisableTrigger(gg_trg_Promised_Daymage)
call DisableTrigger(gg_trg_The_Promised_Day_Comes)
call DisableTrigger(gg_trg_The_Promised_Day_Comes_Father)
call EnableWeatherEffect(udg_Promised_Day_Effect[0],false)
call EnableWeatherEffect(udg_Promised_Day_Effect[1],false)
call EnableWeatherEffect(udg_Promised_Day_Effect[2],false)
call EnableWeatherEffect(udg_Promised_Day_Effect[3],false)
call EnableWeatherEffect(udg_Promised_Day_Effect[4],false)
call EnableWeatherEffect(udg_Promised_Day_Effect[5],false)
call EnableWeatherEffect(udg_Promised_Day_Effect[6],false)
call EnableWeatherEffect(udg_Promised_Day_Effect[7],false)
call EnableWeatherEffect(udg_Promised_Day_Effect[8],false)
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Father|r has been sent back to The Gate! His plans for freedom have failed. The Homunculi have lost!|r")
call CustomDefeatBJ(Player(3),"Father has died!")
call CustomDefeatBJ(Player(10),"Father has died!")
call CustomDefeatBJ(Player(11),"Father has died!")
if(Trig_Father_Dies_Func031C())then
call DisplayTextToForce(GetPlayersAll(),"The |cffff0000Alchemists and their friends|r defeated all enemies as well as Father and put an end to his plans once and for all! A new era of peace falls over Amestris.")
call TriggerSleepAction(10.00)
call CustomVictoryBJ(Player(0),true,true)
call CustomVictoryBJ(Player(1),true,true)
call CustomVictoryBJ(Player(2),true,true)
call CustomDefeatBJ(Player(3),"You have lost!")
call CustomVictoryBJ(Player(4),true,true)
call CustomVictoryBJ(Player(5),true,true)
call CustomVictoryBJ(Player(6),true,true)
if(Trig_Father_Dies_Func031Func010C())then
call CustomVictoryBJ(udg_Scar_Player,true,true)
else
call CustomDefeatBJ(udg_Scar_Player,"You have lost!")
endif
call CustomDefeatBJ(udg_Greed_Player,"You have lost!")
call CustomDefeatBJ(udg_Barry_Player,"You have lost!")
call CustomDefeatBJ(udg_Slicer_Player,"You have lost!")
call CustomDefeatBJ(Player(10),"You have lost!")
call CustomDefeatBJ(Player(11),"You have lost!")
else
endif
endfunction
