function Trig_SplitCast_Copy_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()==udg_Fissure))then
return false
endif
return true
endfunction
function Trig_SplitCast_Copy_Func001C takes nothing returns boolean
if(not(udg_FiCustomValue<100))then
return false
endif
return true
endfunction
function Trig_SplitCast_Copy_Actions takes nothing returns nothing
if(Trig_SplitCast_Copy_Func001C())then
set udg_FiCustomValue=(udg_FiCustomValue+1)
else
set udg_FiCustomValue=1
endif
set udg_TempPoint=GetUnitLoc(GetTriggerUnit())
set udg_FiTargetPoint=GetSpellTargetLoc()
set udg_FiRealDistance=0.00
set udg_TempReal=AngleBetweenPoints(udg_TempPoint,udg_FiTargetPoint)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=10
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set udg_FiRealDistance=(udg_FiRealDistance+64.00)
set udg_TempOffset=PolarProjectionBJ(udg_TempPoint,udg_FiRealDistance,udg_TempReal)
call CreateNUnitsAtLoc(1,'h03E',Player(PLAYER_NEUTRAL_PASSIVE),udg_TempOffset,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ(8.00,'BTLF',GetLastCreatedUnit())
call CreateDestructableLoc(udg_FissureDestructible,udg_TempOffset,GetRandomDirectionDeg(),0.50,R2I(udg_TempReal))
set udg_FiDestructible[((udg_FiCustomValue*20)+GetForLoopIndexA())]=GetLastCreatedDestructable()
call RemoveLocation(udg_TempOffset)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
call CreateNUnitsAtLoc(1,'h03I',GetOwningPlayer(GetTriggerUnit()),udg_TempPoint,bj_UNIT_FACING)
call SetUnitUserData(GetLastCreatedUnit(),udg_FiCustomValue)
call UnitApplyTimedLifeBJ(8.00,'BTLF',GetLastCreatedUnit())
call GroupAddUnitSimple(GetLastCreatedUnit(),udg_FiDummyGroup)
call RemoveLocation(udg_FiTargetPoint)
call RemoveLocation(udg_TempPoint)
endfunction
function InitTrig_SplitCast_Copy takes nothing returns nothing
set gg_trg_SplitCast_Copy=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_SplitCast_Copy,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_SplitCast_Copy,Condition(function Trig_SplitCast_Copy_Conditions))
call TriggerAddAction(gg_trg_SplitCast_Copy,function Trig_SplitCast_Copy_Actions)
endfunction
function Trig_SplitFinish_Conditions takes nothing returns boolean
if(not(IsUnitInGroup(GetTriggerUnit(),udg_FiDummyGroup)==true))then
return false
endif
return true
endfunction
function Trig_SplitFinish_Actions takes nothing returns nothing
set udg_TempInt=GetUnitUserData(GetTriggerUnit())
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=10
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call RemoveDestructable(udg_FiDestructible[((udg_TempInt*20)+GetForLoopIndexA())])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
call RemoveUnit(GetEnumUnit())
endfunction
