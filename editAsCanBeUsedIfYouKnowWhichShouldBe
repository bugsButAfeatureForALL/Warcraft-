set udg_Alkahestric_Barrier=GetLastCreatedUnit()
if(Trig_Alkahestric_Barrier_Func003C())then
call UnitApplyTimedLifeBJ(10.00,'BTLF',udg_Alkahestric_Barrier)
else
if(Trig_Alkahestric_Barrier_Func003Func003C())then
call UnitApplyTimedLifeBJ(15.00,'BTLF',udg_Alkahestric_Barrier)
else
call UnitApplyTimedLifeBJ(20.00,'BTLF',udg_Alkahestric_Barrier)
endif
endif
endfunction
