function Trig_sheepdice_Actions takes nothing returns nothing
if(Trig_sheepdice_Func001C())then
call CustomDefeatBJ(Player(0),"Maphacker")
call DisplayTextToForce(GetPlayersAll(),"sheepdice has been kicked for maphacking.")
else
call DoNothing()
endif
if(Trig_sheepdice_Func002C())then
call CustomDefeatBJ(Player(1),"Maphacker")
call DisplayTextToForce(GetPlayersAll(),"sheepdice has been kicked for maphacking.")
else
call DoNothing()
endif
if(Trig_sheepdice_Func003C())then
call CustomDefeatBJ(Player(2),"Maphacker")
call DisplayTextToForce(GetPlayersAll(),"sheepdice has been kicked for maphacking.")
else
call DoNothing()
endif
if(Trig_sheepdice_Func004C())then
call CustomDefeatBJ(Player(3),"Maphacker")
call DisplayTextToForce(GetPlayersAll(),"sheepdice has been kicked for maphacking.")
else
call DoNothing()
endif
if(Trig_sheepdice_Func005C())then
call CustomDefeatBJ(Player(4),"Maphacker")
call DisplayTextToForce(GetPlayersAll(),"sheepdice has been kicked for maphacking.")
else
call DoNothing()
endif
if(Trig_sheepdice_Func006C())then
call CustomDefeatBJ(Player(5),"Maphacker")
call DisplayTextToForce(GetPlayersAll(),"sheepdice has been kicked for maphacking.")
else
call DoNothing()
endif
if(Trig_sheepdice_Func007C())then
call CustomDefeatBJ(Player(6),"Maphacker")
call DisplayTextToForce(GetPlayersAll(),"sheepdice has been kicked for maphacking.")
else
call DoNothing()
endif
if(Trig_sheepdice_Func008C())then
call CustomDefeatBJ(Player(8),"Maphacker")
call DisplayTextToForce(GetPlayersAll(),"sheepdice has been kicked for maphacking.")
else
call DoNothing()
endif
if(Trig_sheepdice_Func009C())then
call CustomDefeatBJ(Player(9),"Maphacker")
call DisplayTextToForce(GetPlayersAll(),"sheepdice has been kicked for maphacking.")
else
call DoNothing()
endif
if(Trig_sheepdice_Func010C())then
call CustomDefeatBJ(Player(10),"Maphacker")
call DisplayTextToForce(GetPlayersAll(),"sheepdice has been kicked for maphacking.")
else
call DoNothing()
endif
if(Trig_sheepdice_Func011C())then
call CustomDefeatBJ(Player(11),"Maphacker")
