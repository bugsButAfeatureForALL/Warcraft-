function Trig_Change_Stance_To_Evasion_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
call DisplayTextToForce(GetForceOfPlayer(udg_Slicer_Player),"|cffff0000Evasion|r")
call UnitRemoveAbilityBJ('A03U',udg_Slicer_Unit)
call UnitAddAbilityBJ('A03W',udg_Slicer_Unit)
call SetUnitAbilityLevelSwapped('A03W',udg_Slicer_Unit,GetUnitAbilityLevelSwapped('A03T',udg_Slicer_Unit))
call TriggerSleepAction(2)
call EnableTrigger(gg_trg_Change_Stance_To_Cleave)
endfunction
