function Trig_Fathers_Cast_Actions takes nothing returns nothing
call CreateNUnitsAtLoc(1,'h01U',GetOwningPlayer(GetSpellAbilityUnit()),GetUnitLoc(GetSpellAbilityUnit()),bj_UNIT_FACING)
set udg_Fathers_Power=GetLastCreatedUnit()
set udg_Alchemists_Power[0]=GetHeroStatBJ(bj_HEROSTAT_INT,udg_Alphonse_Unit,false)
set udg_Alchemists_Power[1]=GetHeroStatBJ(bj_HEROSTAT_INT,udg_Armstrong_Unit,false)
set udg_Alchemists_Power[2]=GetHeroStatBJ(bj_HEROSTAT_INT,udg_Edward_Unit,false)
set udg_Alchemists_Power[3]=GetHeroStatBJ(bj_HEROSTAT_INT,udg_Izumi_Unit,false)
set udg_Alchemists_Power[4]=GetHeroStatBJ(bj_HEROSTAT_INT,udg_Mustang_Unit,false)
set udg_Alchemists_Power[5]=GetHeroStatBJ(bj_HEROSTAT_INT,udg_Marcoh_Unit,false)
call TriggerSleepAction(1.00)
call RemoveUnit(udg_Fathers_Power)
if(Trig_Fathers_Cast_Func011C())then
call ModifyHeroStat(bj_HEROSTAT_INT,udg_Marcoh_Unit,bj_MODIFYMETHOD_SET,1)
else
endif
if(Trig_Fathers_Cast_Func012C())then
call ModifyHeroStat(bj_HEROSTAT_INT,udg_Alphonse_Unit,bj_MODIFYMETHOD_SET,1)
else
endif
if(Trig_Fathers_Cast_Func013C())then
call ModifyHeroStat(bj_HEROSTAT_INT,udg_Armstrong_Unit,bj_MODIFYMETHOD_SET,1)
else
endif
if(Trig_Fathers_Cast_Func014C())then
call ModifyHeroStat(bj_HEROSTAT_INT,udg_Edward_Unit,bj_MODIFYMETHOD_SET,1)
else
endif
if(Trig_Fathers_Cast_Func015C())then
call ModifyHeroStat(bj_HEROSTAT_INT,udg_Izumi_Unit,bj_MODIFYMETHOD_SET,1)
else
endif
if(Trig_Fathers_Cast_Func016C())then
call ModifyHeroStat(bj_HEROSTAT_INT,udg_Mustang_Unit,bj_MODIFYMETHOD_SET,1)
else
endif
call TriggerSleepAction(10.00)
call ModifyHeroStat(bj_HEROSTAT_INT,udg_Alphonse_Unit,bj_MODIFYMETHOD_SET,udg_Alchemists_Power[0])
call ModifyHeroStat(bj_HEROSTAT_INT,udg_Armstrong_Unit,bj_MODIFYMETHOD_SET,udg_Alchemists_Power[1])
call ModifyHeroStat(bj_HEROSTAT_INT,udg_Edward_Unit,bj_MODIFYMETHOD_SET,udg_Alchemists_Power[2])
call ModifyHeroStat(bj_HEROSTAT_INT,udg_Izumi_Unit,bj_MODIFYMETHOD_SET,udg_Alchemists_Power[3])
call ModifyHeroStat(bj_HEROSTAT_INT,udg_Mustang_Unit,bj_MODIFYMETHOD_SET,udg_Alchemists_Power[4])
call ModifyHeroStat(bj_HEROSTAT_INT,udg_Marcoh_Unit,bj_MODIFYMETHOD_SET,udg_Alchemists_Power[5])
endfunction
