set udg_Power_Of_Avarice_OffSet=PolarProjectionBJ(udg_Power_Of_Avarice_Position,2.50,udg_Avarice_Knock)
if(Trig_Knock_Back_Copy_Func003C())then
call AddSpecialEffectTargetUnitBJ("chest",udg_Power_Of_Avarice_Target,"Abilities\\Spells\\Human\\FlakCannons\\FlakTarget.mdl")
call SetUnitPositionLoc(udg_Power_Of_Avarice_Target,udg_Power_Of_Avarice_OffSet)
else
endif
call RemoveLocation(udg_Power_Of_Avarice_Position)
call RemoveLocation(udg_Power_Of_Avarice_OffSet)
endfunction
