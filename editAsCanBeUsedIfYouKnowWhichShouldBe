if(not(udg_Rock_Loop_Counter==5))then
return false
endif
return true
endfunction
function Trig_Cast_Rock_Spikes_Actions takes nothing returns nothing
set udg_Rock_Spike_Caster=GetTriggerUnit()
set udg_Change_In_Angle=10.00
set udg_Duration_RockSpikes=1.10
set udg_Rock_Loop_Counter=1
loop
exitwhen udg_Rock_Loop_Counter>5
if(Trig_Cast_Rock_Spikes_Func004Func004C())then
set udg_Rock_Distance=125.00
set udg_Rock_Spikes_Angle=GetUnitFacing(udg_Rock_Spike_Caster)
set udg_Rock_Spike_Location=PolarProjectionBJ(GetUnitLoc(udg_Rock_Spike_Caster),udg_Rock_Distance,udg_Rock_Spikes_Angle)
call CreateNUnitsAtLoc(1,'h00V',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,udg_Rock_Spikes_Angle)
set udg_FirstRock=GetLastCreatedUnit()
call CreateNUnitsAtLoc(1,'h00Z',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ((udg_Duration_RockSpikes/2.00),'BTLF',GetLastCreatedUnit())
call UnitApplyTimedLifeBJ(udg_Duration_RockSpikes,'BTLF',udg_FirstRock)
else
endif
if(Trig_Cast_Rock_Spikes_Func004Func008C())then
set udg_Rock_Distance=(udg_Rock_Distance+125.00)
set udg_Rock_Spikes_Angle=(udg_Rock_Spikes_Angle-(udg_Change_In_Angle*1.20))
set udg_Rock_Spike_Location=PolarProjectionBJ(GetUnitLoc(udg_Rock_Spike_Caster),udg_Rock_Distance,udg_Rock_Spikes_Angle)
call CreateNUnitsAtLoc(1,'h00V',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,udg_Rock_Spikes_Angle)
call UnitApplyTimedLifeBJ(udg_Duration_RockSpikes,'BTLF',GetLastCreatedUnit())
call CreateNUnitsAtLoc(1,'h00Z',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ((udg_Duration_RockSpikes/2.00),'BTLF',GetLastCreatedUnit())
call RemoveLocation(udg_Rock_Spike_Location)
set udg_Rock_Spikes_Angle=(udg_Rock_Spikes_Angle+(udg_Change_In_Angle*2.40))
set udg_Rock_Spike_Location=PolarProjectionBJ(GetUnitLoc(udg_Rock_Spike_Caster),udg_Rock_Distance,udg_Rock_Spikes_Angle)
call CreateNUnitsAtLoc(1,'h00V',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,udg_Rock_Spikes_Angle)
call UnitApplyTimedLifeBJ(udg_Duration_RockSpikes,'BTLF',GetLastCreatedUnit())
call CreateNUnitsAtLoc(1,'h00Z',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ((udg_Duration_RockSpikes/2.00),'BTLF',GetLastCreatedUnit())
call RemoveLocation(udg_Rock_Spike_Location)
set udg_Rock_Spikes_Angle=GetUnitFacing(udg_FirstRock)
else
endif
if(Trig_Cast_Rock_Spikes_Func004Func012C())then
set udg_Rock_Distance=(udg_Rock_Distance+125.00)
set udg_Rock_Spikes_Angle=(udg_Rock_Spikes_Angle-(udg_Change_In_Angle*1.50))
set udg_Rock_Spike_Location=PolarProjectionBJ(GetUnitLoc(udg_Rock_Spike_Caster),udg_Rock_Distance,udg_Rock_Spikes_Angle)
call CreateNUnitsAtLoc(1,'h00V',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,udg_Rock_Spikes_Angle)
call UnitApplyTimedLifeBJ(udg_Duration_RockSpikes,'BTLF',GetLastCreatedUnit())
call CreateNUnitsAtLoc(1,'h00Z',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ((udg_Duration_RockSpikes/2.00),'BTLF',GetLastCreatedUnit())
call RemoveLocation(udg_Rock_Spike_Location)
set udg_Rock_Spikes_Angle=(udg_Rock_Spikes_Angle+(udg_Change_In_Angle*1.50))
set udg_Rock_Spike_Location=PolarProjectionBJ(GetUnitLoc(udg_Rock_Spike_Caster),udg_Rock_Distance,udg_Rock_Spikes_Angle)
call CreateNUnitsAtLoc(1,'h00V',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,udg_Rock_Spikes_Angle)
call UnitApplyTimedLifeBJ(udg_Duration_RockSpikes,'BTLF',GetLastCreatedUnit())
call CreateNUnitsAtLoc(1,'h00Z',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ((udg_Duration_RockSpikes/2.00),'BTLF',GetLastCreatedUnit())
call RemoveLocation(udg_Rock_Spike_Location)
set udg_Rock_Spikes_Angle=(udg_Rock_Spikes_Angle+(udg_Change_In_Angle*1.50))
set udg_Rock_Spike_Location=PolarProjectionBJ(GetUnitLoc(udg_Rock_Spike_Caster),udg_Rock_Distance,udg_Rock_Spikes_Angle)
call CreateNUnitsAtLoc(1,'h00V',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,udg_Rock_Spikes_Angle)
call UnitApplyTimedLifeBJ(udg_Duration_RockSpikes,'BTLF',GetLastCreatedUnit())
call CreateNUnitsAtLoc(1,'h00Z',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ((udg_Duration_RockSpikes/2.00),'BTLF',GetLastCreatedUnit())
call RemoveLocation(udg_Rock_Spike_Location)
set udg_Rock_Spikes_Angle=GetUnitFacing(udg_FirstRock)
else
endif
if(Trig_Cast_Rock_Spikes_Func004Func016C())then
set udg_Rock_Distance=(udg_Rock_Distance+125.00)
set udg_Rock_Spikes_Angle=(udg_Rock_Spikes_Angle-(udg_Change_In_Angle*1.50))
set udg_Rock_Spike_Location=PolarProjectionBJ(GetUnitLoc(udg_Rock_Spike_Caster),udg_Rock_Distance,udg_Rock_Spikes_Angle)
call CreateNUnitsAtLoc(1,'h00V',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,udg_Rock_Spikes_Angle)
call UnitApplyTimedLifeBJ(udg_Duration_RockSpikes,'BTLF',GetLastCreatedUnit())
call CreateNUnitsAtLoc(1,'h00Z',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ((udg_Duration_RockSpikes/2.00),'BTLF',GetLastCreatedUnit())
call RemoveLocation(udg_Rock_Spike_Location)
set udg_Rock_Spikes_Angle=(udg_Rock_Spikes_Angle+(udg_Change_In_Angle*1.00))
set udg_Rock_Spike_Location=PolarProjectionBJ(GetUnitLoc(udg_Rock_Spike_Caster),udg_Rock_Distance,udg_Rock_Spikes_Angle)
call CreateNUnitsAtLoc(1,'h00V',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,udg_Rock_Spikes_Angle)
call UnitApplyTimedLifeBJ(udg_Duration_RockSpikes,'BTLF',GetLastCreatedUnit())
call CreateNUnitsAtLoc(1,'h00Z',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ((udg_Duration_RockSpikes/2.00),'BTLF',GetLastCreatedUnit())
call RemoveLocation(udg_Rock_Spike_Location)
set udg_Rock_Spikes_Angle=(udg_Rock_Spikes_Angle+(udg_Change_In_Angle*1.00))
set udg_Rock_Spike_Location=PolarProjectionBJ(GetUnitLoc(udg_Rock_Spike_Caster),udg_Rock_Distance,udg_Rock_Spikes_Angle)
call CreateNUnitsAtLoc(1,'h00V',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,udg_Rock_Spikes_Angle)
call UnitApplyTimedLifeBJ(udg_Duration_RockSpikes,'BTLF',GetLastCreatedUnit())
call CreateNUnitsAtLoc(1,'h00Z',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ((udg_Duration_RockSpikes/2.00),'BTLF',GetLastCreatedUnit())
call RemoveLocation(udg_Rock_Spike_Location)
set udg_Rock_Spikes_Angle=(udg_Rock_Spikes_Angle+(udg_Change_In_Angle*1.00))
set udg_Rock_Spike_Location=PolarProjectionBJ(GetUnitLoc(udg_Rock_Spike_Caster),udg_Rock_Distance,udg_Rock_Spikes_Angle)
call CreateNUnitsAtLoc(1,'h00V',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,udg_Rock_Spikes_Angle)
call UnitApplyTimedLifeBJ(udg_Duration_RockSpikes,'BTLF',GetLastCreatedUnit())
call CreateNUnitsAtLoc(1,'h00Z',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ((udg_Duration_RockSpikes/2.00),'BTLF',GetLastCreatedUnit())
call RemoveLocation(udg_Rock_Spike_Location)
set udg_Rock_Spikes_Angle=GetUnitFacing(udg_FirstRock)
else
endif
if(Trig_Cast_Rock_Spikes_Func004Func020C())then
set udg_Rock_Distance=(udg_Rock_Distance+125.00)
set udg_Rock_Spikes_Angle=(udg_Rock_Spikes_Angle-(udg_Change_In_Angle*2.00))
set udg_Rock_Spike_Location=PolarProjectionBJ(GetUnitLoc(udg_Rock_Spike_Caster),udg_Rock_Distance,udg_Rock_Spikes_Angle)
call CreateNUnitsAtLoc(1,'h00V',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,udg_Rock_Spikes_Angle)
call UnitApplyTimedLifeBJ(udg_Duration_RockSpikes,'BTLF',GetLastCreatedUnit())
call CreateNUnitsAtLoc(1,'h00Z',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ((udg_Duration_RockSpikes/2.00),'BTLF',GetLastCreatedUnit())
call RemoveLocation(udg_Rock_Spike_Location)
set udg_Rock_Spikes_Angle=(udg_Rock_Spikes_Angle+(udg_Change_In_Angle*1.00))
set udg_Rock_Spike_Location=PolarProjectionBJ(GetUnitLoc(udg_Rock_Spike_Caster),udg_Rock_Distance,udg_Rock_Spikes_Angle)
call CreateNUnitsAtLoc(1,'h00V',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,udg_Rock_Spikes_Angle)
call UnitApplyTimedLifeBJ(udg_Duration_RockSpikes,'BTLF',GetLastCreatedUnit())
call CreateNUnitsAtLoc(1,'h00Z',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ((udg_Duration_RockSpikes/2.00),'BTLF',GetLastCreatedUnit())
call RemoveLocation(udg_Rock_Spike_Location)
set udg_Rock_Spikes_Angle=(udg_Rock_Spikes_Angle+(udg_Change_In_Angle*1.00))
set udg_Rock_Spike_Location=PolarProjectionBJ(GetUnitLoc(udg_Rock_Spike_Caster),udg_Rock_Distance,udg_Rock_Spikes_Angle)
call CreateNUnitsAtLoc(1,'h00V',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,udg_Rock_Spikes_Angle)
call UnitApplyTimedLifeBJ(udg_Duration_RockSpikes,'BTLF',GetLastCreatedUnit())
call CreateNUnitsAtLoc(1,'h00Z',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ((udg_Duration_RockSpikes/2.00),'BTLF',GetLastCreatedUnit())
call RemoveLocation(udg_Rock_Spike_Location)
set udg_Rock_Spikes_Angle=(udg_Rock_Spikes_Angle+(udg_Change_In_Angle*1.00))
set udg_Rock_Spike_Location=PolarProjectionBJ(GetUnitLoc(udg_Rock_Spike_Caster),udg_Rock_Distance,udg_Rock_Spikes_Angle)
call CreateNUnitsAtLoc(1,'h00V',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,udg_Rock_Spikes_Angle)
call UnitApplyTimedLifeBJ(udg_Duration_RockSpikes,'BTLF',GetLastCreatedUnit())
call CreateNUnitsAtLoc(1,'h00Z',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ((udg_Duration_RockSpikes/2.00),'BTLF',GetLastCreatedUnit())
call RemoveLocation(udg_Rock_Spike_Location)
set udg_Rock_Spikes_Angle=(udg_Rock_Spikes_Angle+(udg_Change_In_Angle*1.00))
set udg_Rock_Spike_Location=PolarProjectionBJ(GetUnitLoc(udg_Rock_Spike_Caster),udg_Rock_Distance,udg_Rock_Spikes_Angle)
call CreateNUnitsAtLoc(1,'h00V',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,udg_Rock_Spikes_Angle)
call UnitApplyTimedLifeBJ(udg_Duration_RockSpikes,'BTLF',GetLastCreatedUnit())
call CreateNUnitsAtLoc(1,'h00Z',GetOwningPlayer(udg_Rock_Spike_Caster),udg_Rock_Spike_Location,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ((udg_Duration_RockSpikes/2.00),'BTLF',GetLastCreatedUnit())
call RemoveLocation(udg_Rock_Spike_Location)
set udg_Rock_Spikes_Angle=GetUnitFacing(udg_FirstRock)
else
endif
set udg_Rock_Loop_Counter=udg_Rock_Loop_Counter+1
endloop
endfunction
function InitTrig_Cast_Rock_Spikes takes nothing returns nothing
set gg_trg_Cast_Rock_Spikes=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Cast_Rock_Spikes,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Cast_Rock_Spikes,Condition(function Trig_Cast_Rock_Spikes_Conditions))
call TriggerAddAction(gg_trg_Cast_Rock_Spikes,function Trig_Cast_Rock_Spikes_Actions)
endfunction
