call UnitAddItemByIdSwapped('I006',udg_Cornello_Unit)
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[bj_PLAYER_NEUTRAL_VICTIM],udg_Evil_Team,bj_ALLIANCE_UNALLIED)
call DisplayTextToForce(GetForceOfPlayer(udg_Cornello_Player),"You are a member of the |cffff0000Homunculi|r it is your job to destroy any who oppose Our Father. But be warned. Should Father be defeated you will lose! So protect him at all costs.")
call SetPlayerHandicapXPBJ(udg_Cornello_Player,185.00)
call DestroyTrigger(GetTriggeringTrigger())
endfunction
