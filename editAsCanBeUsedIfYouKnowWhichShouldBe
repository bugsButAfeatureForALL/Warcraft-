call SetHeroLevelBJ(GetTriggerUnit(),(GetHeroLevel(GetDyingUnit())-3),false)
call PanCameraToTimedLocForPlayer(GetOwningPlayer(GetDyingUnit()),GetUnitLoc(GetDyingUnit()),0)
call SelectUnitForPlayerSingle(GetDyingUnit(),udg_Bradley_Player)
else
call SetPlayerState(udg_Bradley_Player,PLAYER_STATE_OBSERVER,1)
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Fuhrer Bradley has finally been defeated once and for all!|r")
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Bradley_Player),bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Bradley_Player),udg_Evil_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Bradley_Player),bj_ALLIANCE_ALLIED_VISION)
call ForceRemovePlayerSimple(udg_Bradley_Player,udg_Homunculi)
