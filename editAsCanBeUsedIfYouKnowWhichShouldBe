set gg_trg_Armstrong_Death=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Armstrong_Death,EVENT_PLAYER_UNIT_DEATH)
call TriggerAddCondition(gg_trg_Armstrong_Death,Condition(function Trig_Armstrong_Death_Conditions))
call TriggerAddAction(gg_trg_Armstrong_Death,function Trig_Armstrong_Death_Actions)
endfunction
