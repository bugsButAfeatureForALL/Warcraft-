function Trig_Alchemists_Wrath_Actions takes nothing returns nothing
set udg_Alchemist_Target=GetSpellTargetUnit()
set udg_Alchemist_Real=45.00
set udg_Alchemist_Target_Position=GetUnitLoc(udg_Alchemist_Target)
set udg_Alchemist_Dummy_Position=PolarProjectionBJ(udg_Alchemist_Target_Position,500.00,udg_Alchemist_Real)
set bj_forLoopAIndex=0
set bj_forLoopAIndexEnd=3
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call CreateNUnitsAtLoc(1,'n004',udg_Izumi_Player,udg_Alchemist_Dummy_Position,AngleBetweenPoints(udg_Alchemist_Dummy_Position,udg_Alchemist_Target_Position))
set udg_Alchemist_Dummy=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(2.40,'BTLF',udg_Alchemist_Dummy)
set udg_Alchemist_Real=(udg_Alchemist_Real+90.00)
set udg_Alchemist_Dummy_Position=PolarProjectionBJ(udg_Alchemist_Target_Position,500.00,udg_Alchemist_Real)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
call RemoveLocation(udg_Alchemist_Dummy_Position)
call RemoveLocation(udg_Alchemist_Target_Position)
endfunction
