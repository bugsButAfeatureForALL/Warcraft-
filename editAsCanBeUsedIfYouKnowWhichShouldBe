function Trig_Godhood_Actions takes nothing returns nothing
call ForceClear(udg_Alchemists)
call ForceClear(udg_Neutral)
call TriggerSleepAction(5.00)
if(Trig_Godhood_Func004C())then
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Father|r has achieved the power of God..nothing can stop him now!")
call ModifyHeroStat(bj_HEROSTAT_STR,udg_Our_Father,bj_MODIFYMETHOD_ADD,1000)
call ModifyHeroStat(bj_HEROSTAT_AGI,udg_Our_Father,bj_MODIFYMETHOD_ADD,1000)
call ModifyHeroStat(bj_HEROSTAT_INT,udg_Our_Father,bj_MODIFYMETHOD_ADD,1000)
call TriggerSleepAction(10.00)
call CustomDefeatBJ(Player(0),"Father has won!")
call CustomDefeatBJ(Player(1),"Father has won!")
call CustomDefeatBJ(Player(2),"Father has won!")
call CustomVictoryBJ(Player(3),true,true)
call CustomDefeatBJ(Player(4),"Father has won!")
call CustomDefeatBJ(Player(5),"Father has won!")
call CustomDefeatBJ(Player(6),"Father has won!")
call CustomDefeatBJ(Player(8),"Father has won!")
call CustomDefeatBJ(Player(9),"Father has won!")
call CustomVictoryBJ(Player(10),true,true)
call CustomVictoryBJ(Player(11),true,true)
else
endif
endfunction
