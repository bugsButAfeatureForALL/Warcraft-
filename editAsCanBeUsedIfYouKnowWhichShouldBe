function Trig_Knock_Back_Copy_2_Func001C takes nothing returns boolean
if(not(IsTerrainPathableBJ(PolarProjectionBJ(GetUnitLoc(udg_Inhuman_Speed_Target),2.50,udg_Inhuman_Angle),PATHING_TYPE_WALKABILITY)==false))then
return false
endif
return true
endfunction
function Trig_Knock_Back_Copy_2_Actions takes nothing returns nothing
if(Trig_Knock_Back_Copy_2_Func001C())then
call AddSpecialEffectTargetUnitBJ("chest",udg_Inhuman_Speed_Target,"Abilities\\Spells\\Human\\FlakCannons\\FlakTarget.mdl")
call SetUnitPositionLoc(udg_Inhuman_Speed_Target,PolarProjectionBJ(GetUnitLoc(udg_Inhuman_Speed_Target),2.50,udg_Inhuman_Angle))
else
endif
endfunction
function InitTrig_Knock_Back_Copy_2 takes nothing returns nothing
set gg_trg_Knock_Back_Copy_2=CreateTrigger()
call DisableTrigger(gg_trg_Knock_Back_Copy_2)
call TriggerRegisterTimerEventPeriodic(gg_trg_Knock_Back_Copy_2,0.01)
call TriggerAddAction(gg_trg_Knock_Back_Copy_2,function Trig_Knock_Back_Copy_2_Actions)
endfunction
function Trig_Knock_Copy_2_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A05E'))then
return false
endif
if(not(udg_Sloth_ON==true))then
return false
endif
return true
endfunction
function Trig_Knock_Copy_2_Func002C takes nothing returns boolean
if(not(IsUnitType(GetAttackedUnitBJ(),UNIT_TYPE_MECHANICAL)==false))then
return false
endif
if(not(IsUnitType(GetAttackedUnitBJ(),UNIT_TYPE_TOWNHALL)==false))then
return false
endif
if(not(IsUnitType(GetAttackedUnitBJ(),UNIT_TYPE_STRUCTURE)==false))then
return false
endif
return true
endfunction
function Trig_Knock_Copy_2_Actions takes nothing returns nothing
if(Trig_Knock_Copy_2_Func002C())then
call AddSpecialEffectTargetUnitBJ("chest",udg_Inhuman_Caster,"war3mapImported\\BlackBlink.mdx")
set udg_Inhuman_Speed_Target=GetSpellTargetUnit()
call TriggerSleepAction(0.10)
call SetUnitPositionLoc(udg_Inhuman_Caster,GetUnitLoc(udg_Inhuman_Speed_Target))
set udg_Inhuman_Location=GetUnitLoc(udg_Inhuman_Caster)
set udg_Inhuman_Angle=AngleBetweenPoints(udg_Inhuman_Location,GetUnitLoc(udg_Inhuman_Speed_Target))
call EnableTrigger(gg_trg_Knock_Back_Copy_2)
call DisableTrigger(GetTriggeringTrigger())
call TriggerSleepAction(1.50)
call DisableTrigger(gg_trg_Knock_Back_Copy_2)
call EnableTrigger(GetTriggeringTrigger())
else
endif
endfunction
function InitTrig_Knock_Copy_2 takes nothing returns nothing
set gg_trg_Knock_Copy_2=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Knock_Copy_2,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Knock_Copy_2,Condition(function Trig_Knock_Copy_2_Conditions))
call TriggerAddAction(gg_trg_Knock_Copy_2,function Trig_Knock_Copy_2_Actions)
