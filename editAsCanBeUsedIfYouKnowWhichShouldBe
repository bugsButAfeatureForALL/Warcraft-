call UnitAddAbilityBJ('A028',udg_Bradley_Unit)
call TriggerSleepAction(15.00)
call UnitRemoveAbilityBJ('A01O',udg_Bradley_Unit)
call UnitRemoveAbilityBJ('A028',udg_Bradley_Unit)
else
endif
if(Trig_Ultimate_Eye_Duration_Func003C())then
call UnitAddAbilityBJ('A01O',udg_Bradley_Unit)
call UnitAddAbilityBJ('A028',udg_Bradley_Unit)
call TriggerSleepAction(20.00)
call UnitRemoveAbilityBJ('A01O',udg_Bradley_Unit)
call UnitRemoveAbilityBJ('A028',udg_Bradley_Unit)
else
endif
endfunction
