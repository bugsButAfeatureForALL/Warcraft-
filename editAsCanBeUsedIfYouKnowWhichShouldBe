function Trig_Normal_to_Van_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
set bj_forLoopAIndex=0
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set udg_Items_For_Van[GetForLoopIndexA()]=UnitItemInSlotBJ(udg_Becoming_Van,(GetForLoopIndexA()+1))
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set bj_forLoopBIndex=0
set bj_forLoopBIndexEnd=5
loop
exitwhen bj_forLoopBIndex>bj_forLoopBIndexEnd
call UnitRemoveItemFromSlotSwapped((GetForLoopIndexB()+1),udg_Becoming_Van)
call UnitAddItemSwapped(udg_Items_For_Van[GetForLoopIndexB()],udg_Hohenheim_Unit)
set bj_forLoopBIndex=bj_forLoopBIndex+1
endloop
endfunction
function InitTrig_Normal_to_Van takes nothing returns nothing
set gg_trg_Normal_to_Van=CreateTrigger()
call DisableTrigger(gg_trg_Normal_to_Van)
call TriggerRegisterTimerEventPeriodic(gg_trg_Normal_to_Van,0.01)
call TriggerAddAction(gg_trg_Normal_to_Van,function Trig_Normal_to_Van_Actions)
endfunction
function Trig_Hohenheim_Selection_Conditions takes nothing returns boolean
if(not(IsUnitAlly(GetTriggerUnit(),Player(7))==true))then
return false
endif
if(not(CountPlayersInForceBJ(udg_Alchemists)==1))then
return false
endif
if(not(CountPlayersInForceBJ(udg_Homunculi)>=3))then
return false
endif
if(not(udg_No_Van_Abuse==true))then
return false
endif
return true
endfunction
function Trig_Hohenheim_Selection_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
set udg_Becoming_Van=GetTriggerUnit()
call ShowUnitHide(udg_Becoming_Van)
call PauseUnitBJ(true,udg_Becoming_Van)
call CreateNUnitsAtLoc(1,'H043',GetOwningPlayer(udg_Becoming_Van),GetRectCenter(gg_rct_Father_Before_the_Grave),bj_UNIT_FACING)
set udg_Hohenheim_Unit=GetLastCreatedUnit()
set udg_Hohenheim_Player=GetOwningPlayer(udg_Becoming_Van)
call SelectUnitForPlayerSingle(udg_Hohenheim_Unit,udg_Hohenheim_Player)
call SetPlayerName(udg_Hohenheim_Player,"Van Hohenheim")
call SetUnitOwner(udg_Becoming_Van,Player(PLAYER_NEUTRAL_PASSIVE),true)
call SetHeroLevelBJ(udg_Hohenheim_Unit,GetHeroLevel(udg_Becoming_Van),false)
call SetHeroXP(udg_Hohenheim_Unit,GetHeroXP(udg_Becoming_Van),false)
call DisplayTextToForce(GetPlayersAll(),"With the defeat of the Alchemist's, |cffff0000Van Hohenheim|r has arrived in Amestris to try and stop Father himself!")
call EnableTrigger(gg_trg_Normal_to_Van)
set udg_Is_Hohenheim_Selected=true
call TriggerSleepAction(2)
call RemoveUnit(udg_Becoming_Van)
call TriggerSleepAction(2)
call DestroyTrigger(GetTriggeringTrigger())
endfunction
function InitTrig_Hohenheim_Selection takes nothing returns nothing
set gg_trg_Hohenheim_Selection=CreateTrigger()
call DisableTrigger(gg_trg_Hohenheim_Selection)
call TriggerRegisterEnterRectSimple(gg_trg_Hohenheim_Selection,gg_rct_Father_Before_the_Grave)
call TriggerAddCondition(gg_trg_Hohenheim_Selection,Condition(function Trig_Hohenheim_Selection_Conditions))
call TriggerAddAction(gg_trg_Hohenheim_Selection,function Trig_Hohenheim_Selection_Actions)
endfunction
function Trig_Vanny_Dies_Conditions takes nothing returns boolean
if(not(GetDyingUnit()==udg_Vanny_Unit))then
return false
endif
return true
endfunction
function Trig_Vanny_Dies_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
call PanCameraToTimedLocForPlayer(udg_Vanny_Player,GetRectCenter(gg_rct_Good_Spawns),0)
call RemoveUnit(udg_Vanny_Unit)
call CreateNUnitsAtLoc(1,'h01P',udg_Vanny_Player,GetRectCenter(gg_rct_Good_Spawns),bj_UNIT_FACING)
call SetPlayerStateBJ(udg_Vanny_Player,PLAYER_STATE_RESOURCE_LUMBER,5)
call TriggerSleepAction(0.20)
call SelectUnitForPlayerSingle(gg_unit_n001_0029,udg_Vanny_Player)
endfunction
