return false
endif
return true
endfunction
function Trig_Slash_Cast_Func010Func002C takes nothing returns boolean
if(not(GetUnitAbilityLevelSwapped('A07E',udg_Caster_Slash)==2))then
return false
endif
return true
endfunction
function Trig_Slash_Cast_Func010C takes nothing returns boolean
if(not(GetUnitAbilityLevelSwapped('A07E',udg_Caster_Slash)==1))then
return false
endif
return true
endfunction
function Trig_Slash_Cast_Func016C takes nothing returns boolean
if(not(udg_Feltet_Slashing_ON==true))then
return false
endif
return true
endfunction
function Trig_Slash_Cast_Actions takes nothing returns nothing
set udg_Slash_Group=CreateGroup()
set udg_Target_Slash=GetSpellTargetUnit()
set udg_Caster_Slash=GetSpellAbilityUnit()
set udg_Slash_Point=GetUnitLoc(udg_Caster_Slash)
call GroupAddUnitSimple(udg_Target_Slash,udg_Slash_Group)
call AddSpecialEffectTargetUnitBJ("chest",udg_Caster_Slash,"war3mapImported\\BlackBlink.mdx")
set udg_Effect_Slash=GetLastCreatedEffectBJ()
call PauseUnitBJ(true,udg_Lan_Fan_Unit)
call SetUnitInvulnerable(udg_Lan_Fan_Unit,true)
if(Trig_Slash_Cast_Func010C())then
set udg_Slash_Damage=35.00
else
if(Trig_Slash_Cast_Func010Func002C())then
set udg_Slash_Damage=55.00
else
if(Trig_Slash_Cast_Func010Func002Func002C())then
set udg_Slash_Damage=65.00
else
endif
endif
endif
set udg_Slash_Point=GetUnitLoc(udg_Caster_Slash)
call EnableTrigger(gg_trg_Slashing)
call SetUnitTimeScalePercent(udg_Caster_Slash,150.00)
set udg_Feltet_Slashing_ON=true
call TriggerSleepAction(3.00)
if(Trig_Slash_Cast_Func016C())then
call SetUnitInvulnerable(udg_Lan_Fan_Unit,false)
call PauseUnitBJ(false,udg_Lan_Fan_Unit)
call SetUnitTimeScalePercent(udg_Caster_Slash,100)
call DestroyEffect(udg_Effect_Slash)
call DisableTrigger(gg_trg_Slashing)
call GroupClear(udg_Slash_Group)
call ResetUnitAnimation(udg_Caster_Slash)
else
endif
call DestroyGroup(udg_Slash_Group)
call RemoveLocation(udg_Slash_Point)
call RemoveLocation(udg_Slash_Position)
endfunction
function InitTrig_Slash_Cast takes nothing returns nothing
set gg_trg_Slash_Cast=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Slash_Cast,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Slash_Cast,Condition(function Trig_Slash_Cast_Conditions))
call TriggerAddAction(gg_trg_Slash_Cast,function Trig_Slash_Cast_Actions)
endfunction
function Trig_Slashing_Func001Func023002003001 takes nothing returns boolean
return(IsUnitAliveBJ(GetFilterUnit())==true)
endfunction
function Trig_Slashing_Func001Func023002003002001 takes nothing returns boolean
return(IsUnitEnemy(GetFilterUnit(),udg_Lan_Fan_Player)==true)
endfunction
function Trig_Slashing_Func001Func023002003002002 takes nothing returns boolean
return(IsUnitType(GetFilterUnit(),UNIT_TYPE_HERO)==true)
endfunction
function Trig_Slashing_Func001Func023002003002 takes nothing returns boolean
return GetBooleanAnd((IsUnitEnemy(GetFilterUnit(),udg_Lan_Fan_Player)==true),(IsUnitType(GetFilterUnit(),UNIT_TYPE_HERO)==true))
endfunction
function Trig_Slashing_Func001Func023002003 takes nothing returns boolean
return GetBooleanAnd((IsUnitAliveBJ(GetFilterUnit())==true),(GetBooleanAnd((IsUnitEnemy(GetFilterUnit(),udg_Lan_Fan_Player)==true),(IsUnitType(GetFilterUnit(),UNIT_TYPE_HERO)==true))))
endfunction
function Trig_Slashing_Func001C takes nothing returns boolean
if(not(CountUnitsInGroup(udg_Slash_Group)>0))then
return false
endif
return true
endfunction
function Trig_Slashing_Actions takes nothing returns nothing
if(Trig_Slashing_Func001C())then
call DestroyGroup(udg_Slash_Group)
set udg_Slash_Position=GetUnitLoc(udg_Target_Slash)
set udg_Slash_Point=GetUnitLoc(udg_Caster_Slash)
call AddSpecialEffectLocBJ(udg_Slash_Point,"war3mapImported\\BlackBlink.mdx")
set udg_Slash_Offset=PolarProjectionBJ(udg_Slash_Position,90.00,GetRandomReal(0,360.00))
call SetUnitPositionLoc(udg_Caster_Slash,udg_Slash_Offset)
call SetUnitFacingToFaceUnitTimed(udg_Caster_Slash,udg_Target_Slash,0.01)
call SetUnitAnimation(udg_Caster_Slash,"spell two")
call UnitDamageTargetBJ(udg_Caster_Slash,udg_Target_Slash,udg_Slash_Damage,ATTACK_TYPE_HERO,DAMAGE_TYPE_UNKNOWN)
call RemoveLocation(udg_Slash_Point)
set udg_Slash_Point=GetUnitLoc(udg_Caster_Slash)
set udg_Slash_Group=GetUnitsInRangeOfLocMatching(500.00,udg_Slash_Point,Condition(function Trig_Slashing_Func001Func023002003))
set udg_Target_Slash=GroupPickRandomUnit(udg_Slash_Group)
call RemoveLocation(udg_Slash_Position)
call RemoveLocation(udg_Slash_Point)
else
set udg_Feltet_Slashing_ON=false
call SetUnitTimeScalePercent(udg_Caster_Slash,100)
call PauseUnitBJ(false,udg_Lan_Fan_Unit)
call SetUnitInvulnerable(udg_Lan_Fan_Unit,false)
call DestroyEffect(udg_Effect_Slash)
call DisableTrigger(GetTriggeringTrigger())
call ResetUnitAnimation(udg_Caster_Slash)
call GroupClear(udg_Slash_Group)
call RemoveLocation(udg_Slash_Position)
call RemoveLocation(udg_Slash_Point)
call DestroyGroup(udg_Slash_Group)
endif
endfunction
function InitTrig_Slashing takes nothing returns nothing
set gg_trg_Slashing=CreateTrigger()
call DisableTrigger(gg_trg_Slashing)
call TriggerRegisterTimerEventPeriodic(gg_trg_Slashing,0.30)
call TriggerAddAction(gg_trg_Slashing,function Trig_Slashing_Actions)
endfunction
