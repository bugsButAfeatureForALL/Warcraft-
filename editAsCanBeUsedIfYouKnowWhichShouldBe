function Trig_Dummy_Spawn_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
set udg_Dummy_Target=gg_unit_n00F_0168
call SetUnitFacingTimed(gg_unit_n00F_0168,GetRandomDirectionDeg(),0)
call SetUnitFacingTimed(gg_unit_n00F_0168,GetRandomDirectionDeg(),0)
call SetUnitFacingTimed(gg_unit_n00F_0168,GetRandomDirectionDeg(),0)
call SetUnitFacingTimed(gg_unit_n00F_0168,GetRandomDirectionDeg(),0)
call SetUnitFacingTimed(gg_unit_n00F_0168,GetRandomDirectionDeg(),0)
call SetUnitFacingTimed(gg_unit_n00F_0168,GetRandomDirectionDeg(),0)
call SetUnitFacingTimed(gg_unit_n00F_0168,GetRandomDirectionDeg(),0)
call TriggerSleepAction(0.50)
set udg_Dummy_Caster=I2S(R2I(GetUnitFacing(gg_unit_n00F_0168)))
endfunction
function InitTrig_Dummy_Spawn takes nothing returns nothing
