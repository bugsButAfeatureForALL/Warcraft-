endfunction
function Trig_Power_of_the_Holy_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A04K'))then
return false
endif
if(not(GetSpellTargetUnit()!=udg_Kimblee_Unit))then
return false
endif
if(not(GetSpellTargetUnit()!=udg_Bradley_Unit))then
return false
endif
if(not(GetSpellTargetUnit()!=udg_Zampano_Unit))then
return false
endif
if(not(GetSpellTargetUnit()!=udg_Heinkel_Unit))then
return false
endif
if(not(GetSpellTargetUnit()!=udg_Darius_Unit))then
return false
endif
if(not(GetSpellTargetUnit()!=udg_Jerso_Unit))then
return false
endif
if(not(GetSpellTargetUnit()!=udg_Cornello_Unit))then
return false
endif
return true
endfunction
function Trig_Power_of_the_Holy_Actions takes nothing returns nothing
set udg_Holy_Target=GetSpellTargetUnit()
call UnitAddAbilityBJ('A093',udg_Holy_Target)
call UnitAddAbilityBJ('A094',udg_Holy_Target)
call TriggerSleepAction(10.00)
call UnitRemoveAbilityBJ('A093',udg_Holy_Target)
call UnitRemoveAbilityBJ('A094',udg_Holy_Target)
endfunction
