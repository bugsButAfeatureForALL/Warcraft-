call SetPlayerAllianceStateBJ(Player(7),udg_Zampano_Player,bj_ALLIANCE_ALLIED_VISION)
call SetPlayerAllianceStateBJ(udg_Zampano_Player,Player(7),bj_ALLIANCE_ALLIED_VISION)
call DisableTrigger(gg_trg_Zampano_Homunculi_G)
call DisableTrigger(gg_trg_Zampano_Dies_S)
