function Trig_no_gate_for_white_kids_Func003001 takes nothing returns boolean
return(GetOwningPlayer(GetManipulatingUnit())==udg_Bradley_Player)
endfunction
function Trig_no_gate_for_white_kids_Func003002001 takes nothing returns boolean
return(GetOwningPlayer(GetManipulatingUnit())==udg_Envy_Player)
endfunction
function Trig_no_gate_for_white_kids_Func003002002001 takes nothing returns boolean
return(GetOwningPlayer(GetManipulatingUnit())==udg_Gluttony_Player)
endfunction
function Trig_no_gate_for_white_kids_Func003002002002001 takes nothing returns boolean
return(GetOwningPlayer(GetManipulatingUnit())==udg_Greed_Player)
endfunction
function Trig_no_gate_for_white_kids_Func003002002002002001 takes nothing returns boolean
return(GetOwningPlayer(GetManipulatingUnit())==udg_Lust_Player)
endfunction
function Trig_no_gate_for_white_kids_Func003002002002002002001 takes nothing returns boolean
return(GetOwningPlayer(GetManipulatingUnit())==udg_Pride_Player)
endfunction
function Trig_no_gate_for_white_kids_Func003002002002002002002 takes nothing returns boolean
return(GetOwningPlayer(GetManipulatingUnit())==udg_Sloth_Player)
endfunction
function Trig_no_gate_for_white_kids_Func003002002002002002 takes nothing returns boolean
return GetBooleanOr((GetOwningPlayer(GetManipulatingUnit())==udg_Pride_Player),(GetOwningPlayer(GetManipulatingUnit())==udg_Sloth_Player))
endfunction
function Trig_no_gate_for_white_kids_Func003002002002002 takes nothing returns boolean
return GetBooleanOr((GetOwningPlayer(GetManipulatingUnit())==udg_Lust_Player),(GetBooleanOr((GetOwningPlayer(GetManipulatingUnit())==udg_Pride_Player),(GetOwningPlayer(GetManipulatingUnit())==udg_Sloth_Player))))
endfunction
function Trig_no_gate_for_white_kids_Func003002002002 takes nothing returns boolean
return GetBooleanOr((GetOwningPlayer(GetManipulatingUnit())==udg_Greed_Player),(GetBooleanOr((GetOwningPlayer(GetManipulatingUnit())==udg_Lust_Player),(GetBooleanOr((GetOwningPlayer(GetManipulatingUnit())==udg_Pride_Player),(GetOwningPlayer(GetManipulatingUnit())==udg_Sloth_Player))))))
endfunction
function Trig_no_gate_for_white_kids_Func003002002 takes nothing returns boolean
return GetBooleanOr((GetOwningPlayer(GetManipulatingUnit())==udg_Gluttony_Player),(GetBooleanOr((GetOwningPlayer(GetManipulatingUnit())==udg_Greed_Player),(GetBooleanOr((GetOwningPlayer(GetManipulatingUnit())==udg_Lust_Player),(GetBooleanOr((GetOwningPlayer(GetManipulatingUnit())==udg_Pride_Player),(GetOwningPlayer(GetManipulatingUnit())==udg_Sloth_Player))))))))
endfunction
function Trig_no_gate_for_white_kids_Func003002 takes nothing returns boolean
return GetBooleanOr((GetOwningPlayer(GetManipulatingUnit())==udg_Envy_Player),(GetBooleanOr((GetOwningPlayer(GetManipulatingUnit())==udg_Gluttony_Player),(GetBooleanOr((GetOwningPlayer(GetManipulatingUnit())==udg_Greed_Player),(GetBooleanOr((GetOwningPlayer(GetManipulatingUnit())==udg_Lust_Player),(GetBooleanOr((GetOwningPlayer(GetManipulatingUnit())==udg_Pride_Player),(GetOwningPlayer(GetManipulatingUnit())==udg_Sloth_Player))))))))))
endfunction
function Trig_no_gate_for_white_kids_Conditions takes nothing returns boolean
if(not(GetItemTypeId(GetManipulatedItem())=='I004'))then
return false
endif
if(not GetBooleanOr((GetOwningPlayer(GetManipulatingUnit())==udg_Bradley_Player),(GetBooleanOr((GetOwningPlayer(GetManipulatingUnit())==udg_Envy_Player),(GetBooleanOr((GetOwningPlayer(GetManipulatingUnit())==udg_Gluttony_Player),(GetBooleanOr((GetOwningPlayer(GetManipulatingUnit())==udg_Greed_Player),(GetBooleanOr((GetOwningPlayer(GetManipulatingUnit())==udg_Lust_Player),(GetBooleanOr((GetOwningPlayer(GetManipulatingUnit())==udg_Pride_Player),(GetOwningPlayer(GetManipulatingUnit())==udg_Sloth_Player)))))))))))))then
return false
endif
return true
endfunction
function Trig_no_gate_for_white_kids_Actions takes nothing returns nothing
call UnitRemoveItemSwapped(GetManipulatedItem(),GetManipulatingUnit())
endfunction
