function Trig_Envy_is_Chosen_Conditions takes nothing returns boolean
if(not(GetUnitTypeId(GetTriggerUnit())=='H014'))then
return false
endif
return true
endfunction
function Trig_Envy_is_Chosen_Actions takes nothing returns nothing
call SetUnitPositionLoc(GetTriggerUnit(),GetRectCenter(gg_rct_Evil_Spawn))
set udg_Envy_First_Form=GetTriggerUnit()
set udg_Envy_Player=GetOwningPlayer(GetTriggerUnit())
set udg_Envy_Original_Color=GetPlayerColor(udg_Envy_Player)
call SetPlayerName(udg_Envy_Player,"Envy")
call ForceAddPlayerSimple(udg_Envy_Player,udg_Evil_Team)
call ForceAddPlayerSimple(udg_Envy_Player,udg_Homunculi)
call UnitAddItemByIdSwapped('I000',udg_Envy_First_Form)
call PanCameraToTimedLocForPlayer(udg_Envy_Player,GetUnitLoc(udg_Envy_First_Form),0)
call SelectUnitForPlayerSingle(GetTriggerUnit(),GetOwningPlayer(GetTriggerUnit()))
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[bj_PLAYER_NEUTRAL_VICTIM],udg_Evil_Team,bj_ALLIANCE_UNALLIED)
call DisplayTextToForce(GetForceOfPlayer(udg_Envy_Player),"You are a member of the |cffff0000Homunculi|r it is your job to destroy any who oppose Our Father. But be warned. Should Father be defeated you will lose! So protect him at all costs.")
call DestroyTrigger(GetTriggeringTrigger())
endfunction
function InitTrig_Envy_is_Chosen takes nothing returns nothing
set gg_trg_Envy_is_Chosen=CreateTrigger()
call TriggerRegisterEnterRectSimple(gg_trg_Envy_is_Chosen,gg_rct_Evil_Spawns)
