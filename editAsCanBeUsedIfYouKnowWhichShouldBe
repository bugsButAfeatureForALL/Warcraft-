function Trig_Stomp_Cast_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A07B'))then
return false
endif
return true
endfunction
function Trig_Stomp_Cast_Func001002003 takes nothing returns boolean
return(IsUnitAlly(GetFilterUnit(),udg_Kimblee_Player)==false)
endfunction
function Trig_Stomp_Cast_Func006Func002002003 takes nothing returns boolean
return(IsUnitAlly(GetFilterUnit(),udg_Kimblee_Player)==false)
endfunction
function Trig_Stomp_Cast_Func006Func006A takes nothing returns nothing
call UnitDamageTargetBJ(udg_Kimblee_Unit,GetEnumUnit(),udg_Stomp_Damage,ATTACK_TYPE_NORMAL,DAMAGE_TYPE_NORMAL)
endfunction
function Trig_Stomp_Cast_Actions takes nothing returns nothing
set udg_Stomp_Group=GetUnitsInRangeOfLocMatching(300.00,GetSpellTargetLoc(),Condition(function Trig_Stomp_Cast_Func001002003))
set udg_Stomp_Damage=(100.00+(100.00*(I2R(GetUnitAbilityLevelSwapped('A07B',udg_Kimblee_Unit))/7.00)))
set udg_Stomp_Offset=0.00
set udg_Stomp_Target_Point=GetSpellTargetLoc()
set udg_Caster_Stomp=GetTriggerUnit()
set bj_forLoopAIndex=0
set bj_forLoopAIndexEnd=6
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set udg_Location_Stomp=PolarProjectionBJ(udg_Stomp_Target_Point,200.00,udg_Stomp_Offset)
set udg_Stomp_Group=GetUnitsInRangeOfLocMatching(300.00,udg_Location_Stomp,Condition(function Trig_Stomp_Cast_Func006Func002002003))
call AddSpecialEffectLocBJ(udg_Location_Stomp,"war3mapImported\\NewGroundEX.mdx")
call DestroyEffect(GetLastCreatedEffectBJ())
set udg_Stomp_Offset=(udg_Stomp_Offset+60.00)
call ForGroupBJ(udg_Stomp_Group,function Trig_Stomp_Cast_Func006Func006A)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
call AddSpecialEffectLocBJ(udg_Stomp_Target_Point,"war3mapImported\\NewGroundEX.mdx")
call RemoveLocation(udg_Stomp_Target_Point)
call DestroyGroup(udg_Stomp_Group)
call RemoveLocation(udg_Location_Stomp)
endfunction
function InitTrig_Stomp_Cast takes nothing returns nothing
set gg_trg_Stomp_Cast=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Stomp_Cast,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Stomp_Cast,Condition(function Trig_Stomp_Cast_Conditions))
call TriggerAddAction(gg_trg_Stomp_Cast,function Trig_Stomp_Cast_Actions)
endfunction
