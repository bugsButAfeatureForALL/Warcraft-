function Trig_Kills_Edward_Conditions takes nothing returns boolean
if(not(GetDyingUnit()==udg_Edward_Unit))then
return false
endif
if(not(GetUnitTypeId(GetKillingUnitBJ())=='H018'))then
return false
endif
if(not(udg_Scar_Is_With_Alchemists==false))then
return false
endif
return true
endfunction
function Trig_Kills_Edward_Func006C takes nothing returns boolean
if(not(udg_Scar_Kills==1))then
return false
endif
return true
endfunction
function Trig_Kills_Edward_Actions takes nothing returns nothing
set udg_Scar_Kills=1
call DisableTrigger(GetTriggeringTrigger())
call ModifyHeroStat(bj_HEROSTAT_STR,GetKillingUnitBJ(),bj_MODIFYMETHOD_ADD,25)
call ModifyHeroStat(bj_HEROSTAT_AGI,GetKillingUnitBJ(),bj_MODIFYMETHOD_ADD,25)
call ModifyHeroStat(bj_HEROSTAT_INT,GetKillingUnitBJ(),bj_MODIFYMETHOD_ADD,25)
if(Trig_Kills_Edward_Func006C())then
call DisableTrigger(gg_trg_Times_Up)
call EnableTrigger(gg_trg_Times_Up_Copy)
else
endif
call DisplayTextToForce(GetPlayersAll(),"Scar has slain a state alchemist!")
call DestroyTrigger(GetTriggeringTrigger())
endfunction
