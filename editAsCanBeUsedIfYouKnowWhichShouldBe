function Trig_Player_2_Leaves_Copy_Actions takes nothing returns nothing
if(Trig_Player_2_Leaves_Copy_Func001C())then
set udg_Player_Left=udg_Marcoh_Unit
call EnableTrigger(gg_trg_Player_Leave_Items)
call TriggerSleepAction(1.00)
call RemoveUnit(udg_Marcoh_Unit)
call ForceRemovePlayerSimple(udg_Marcoh_Player,udg_Alchemists)
call ForceRemovePlayerSimple(udg_Marcoh_Player,udg_Dead_Good_Guys)
call UnitAddItemByIdSwapped('I00R',gg_unit_H01V_0080)
else
endif
if(Trig_Player_2_Leaves_Copy_Func002C())then
set udg_Player_Left=udg_Olivier_Unit
