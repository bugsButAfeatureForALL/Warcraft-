function Trig_Ultimate_Spear_Damage_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
call SetUnitInvulnerable(udg_Ultimate_Spear_Caster,false)
call SetUnitInvulnerable(udg_Ultimate_Spear_Target,false)
call PauseUnitBJ(false,udg_Ultimate_Spear_Caster)
call PauseUnitBJ(false,udg_Ultimate_Spear_Target)
call TriggerSleepAction(0.30)
if(Trig_Ultimate_Spear_Damage_Func007C())then
call UnitDamageTargetBJ(udg_Ultimate_Spear_Caster,udg_Ultimate_Spear_Target,500.00,ATTACK_TYPE_HERO,DAMAGE_TYPE_DEATH)
else
if(Trig_Ultimate_Spear_Damage_Func007Func001C())then
call UnitDamageTargetBJ(udg_Ultimate_Spear_Caster,udg_Ultimate_Spear_Target,650.00,ATTACK_TYPE_HERO,DAMAGE_TYPE_DEATH)
else
if(Trig_Ultimate_Spear_Damage_Func007Func001Func001C())then
call UnitDamageTargetBJ(udg_Ultimate_Spear_Caster,udg_Ultimate_Spear_Target,750.00,ATTACK_TYPE_HERO,DAMAGE_TYPE_DEATH)
else
endif
endif
endif
endfunction
