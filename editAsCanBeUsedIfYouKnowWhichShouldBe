function Trig_Travel_to_Briggs_Conditions takes nothing returns boolean
if(not(GetItemTypeId(GetManipulatedItem())=='I00B'))then
return false
endif
if(not(IsUnitAlly(GetManipulatingUnit(),Player(7))==true))then
return false
endif
if(not(GetManipulatingUnit()!=udg_Envy_Shapeshifted_Form))then
return false
endif
return true
endfunction
function Trig_Travel_to_Briggs_Actions takes nothing returns nothing
call SetUnitMoveSpeed(gg_unit_H01V_0080,285.00)
call EnableTrigger(gg_trg_Traveling_to_Briggs)
call DisableTrigger(gg_trg_Traveling_to_Resembool)
call DisableTrigger(gg_trg_Traveling_to_Dublith)
call DisableTrigger(gg_trg_Traveling_to_Rush_Valley)
call DisableTrigger(gg_trg_Traveling_to_Central)
endfunction
