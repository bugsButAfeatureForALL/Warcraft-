function Trig_Cast_Ultimate_Actions takes nothing returns nothing
if(Trig_Cast_Ultimate_Func001C())then
call EnableTrigger(gg_trg_Clash_of_Philosophy)
else
endif
set udg_Shou_Ultimate_Caster=GetSpellAbilityUnit()
call TriggerSleepAction(0.15)
call SetUnitTimeScalePercent(udg_Shou_Ultimate_Caster,0.00)
call PauseUnitBJ(true,udg_Shou_Ultimate_Caster)
call SetUnitInvulnerable(udg_Shou_Ultimate_Caster,true)
set udg_AnimationSpeed=9.00
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=360
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set udg_POSITIONFUNCTION=PolarProjectionBJ(GetUnitLoc(udg_Shou_Ultimate_Caster),600.00,udg_Ultimate_Degrees_Variable1)
call CreateNUnitsAtLoc(1,'h00P',GetOwningPlayer(udg_Shou_Ultimate_Caster),udg_POSITIONFUNCTION,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ(udg_DurationVariable_ShouUlt,'BTLF',GetLastCreatedUnit())
set udg_Ultimate_Degrees_Variable1=(udg_Ultimate_Degrees_Variable1+1)
call RemoveLocation(udg_POSITIONFUNCTION)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set udg_Ultimate_Degrees_Variable1=20.00
set udg_POSITIONFUNCTION=PolarProjectionBJ(GetUnitLoc(udg_Shou_Ultimate_Caster),600.00,udg_Ultimate_Degrees_Variable1)
call CreateNUnitsAtLoc(1,'h00Q',GetOwningPlayer(udg_Shou_Ultimate_Caster),udg_POSITIONFUNCTION,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ(udg_DurationVariable_ShouUlt,'BTLF',udg_PentagonTopRightUnit)
call SetUnitTimeScalePercent(GetLastCreatedUnit(),udg_AnimationSpeed)
set udg_PentagonTopRightUnit=GetLastCreatedUnit()
call RemoveLocation(udg_POSITIONFUNCTION)
set udg_Ultimate_Degrees_Variable1=160.00
set udg_POSITIONFUNCTION=PolarProjectionBJ(GetUnitLoc(udg_Shou_Ultimate_Caster),600.00,udg_Ultimate_Degrees_Variable1)
call CreateNUnitsAtLocFacingLocBJ(1,'h00Q',GetOwningPlayer(udg_Shou_Ultimate_Caster),udg_POSITIONFUNCTION,GetUnitLoc(udg_PentagonBottomLeftUnit))
call UnitApplyTimedLifeBJ(udg_DurationVariable_ShouUlt,'BTLF',GetLastCreatedUnit())
call SetUnitTimeScalePercent(GetLastCreatedUnit(),udg_AnimationSpeed)
call CreateNUnitsAtLocFacingLocBJ(1,'h00P',GetOwningPlayer(udg_Shou_Ultimate_Caster),udg_POSITIONFUNCTION,GetUnitLoc(udg_PentagonBottomLeftUnit))
set udg_PentagonTopLeftUnit=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(udg_DurationVariable_ShouUlt,'BTLF',udg_PentagonTopLeftUnit)
call SetUnitTimeScalePercent(GetLastCreatedUnit(),udg_AnimationSpeed)
call RemoveLocation(udg_POSITIONFUNCTION)
set udg_Ultimate_Degrees_Variable1=90.00
set udg_POSITIONFUNCTION=PolarProjectionBJ(GetUnitLoc(udg_Shou_Ultimate_Caster),600.00,udg_Ultimate_Degrees_Variable1)
call CreateNUnitsAtLoc(1,'h00Q',GetOwningPlayer(udg_Shou_Ultimate_Caster),udg_POSITIONFUNCTION,AngleBetweenPoints(GetUnitLoc(udg_PentagonCenterTopUnit),GetUnitLoc(udg_PentagonTopLeftUnit)))
set udg_PentagonCenterTopUnit=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(udg_DurationVariable_ShouUlt,'BTLF',udg_PentagonCenterTopUnit)
call SetUnitTimeScalePercent(GetLastCreatedUnit(),udg_AnimationSpeed)
call RemoveLocation(udg_POSITIONFUNCTION)
set udg_Ultimate_Degrees_Variable1=310.00
set udg_POSITIONFUNCTION=PolarProjectionBJ(GetUnitLoc(udg_Shou_Ultimate_Caster),600.00,udg_Ultimate_Degrees_Variable1)
call CreateNUnitsAtLoc(1,'h00Q',GetOwningPlayer(udg_Shou_Ultimate_Caster),udg_POSITIONFUNCTION,bj_UNIT_FACING)
set udg_PentagonBottomRightUnit=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(udg_DurationVariable_ShouUlt,'BTLF',udg_PentagonBottomRightUnit)
call SetUnitTimeScalePercent(GetLastCreatedUnit(),udg_AnimationSpeed)
call RemoveLocation(udg_POSITIONFUNCTION)
set udg_Ultimate_Degrees_Variable1=230.00
set udg_POSITIONFUNCTION=PolarProjectionBJ(GetUnitLoc(udg_Shou_Ultimate_Caster),600.00,udg_Ultimate_Degrees_Variable1)
call CreateNUnitsAtLoc(1,'h00Q',GetOwningPlayer(udg_Shou_Ultimate_Caster),udg_POSITIONFUNCTION,0.00)
set udg_PentagonBottomLeftUnit=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(udg_DurationVariable_ShouUlt,'BTLF',udg_PentagonBottomLeftUnit)
call SetUnitTimeScalePercent(GetLastCreatedUnit(),udg_AnimationSpeed)
call SetUnitFacingToFaceLocTimed(udg_PentagonBottomLeftUnit,GetUnitLoc(udg_PentagonBottomRightUnit),0)
call RemoveLocation(udg_POSITIONFUNCTION)
set udg_OffsetAmount=480.00
set udg_Ultimate_Degrees_Variable1=270.00
set udg_POSITIONFUNCTION=PolarProjectionBJ(GetUnitLoc(udg_Shou_Ultimate_Caster),460.00,udg_Ultimate_Degrees_Variable1)
call CreateNUnitsAtLoc(1,'h00R',GetOwningPlayer(udg_Shou_Ultimate_Caster),udg_POSITIONFUNCTION,270.00)
set udg_InteriorPentCenterBotUnit=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(udg_DurationVariable_ShouUlt,'BTLF',udg_InteriorPentCenterBotUnit)
call SetUnitTimeScalePercent(GetLastCreatedUnit(),udg_AnimationSpeed)
call RemoveLocation(udg_POSITIONFUNCTION)
set udg_Ultimate_Degrees_Variable1=350.00
set udg_POSITIONFUNCTION=PolarProjectionBJ(GetUnitLoc(udg_Shou_Ultimate_Caster),udg_OffsetAmount,udg_Ultimate_Degrees_Variable1)
call CreateNUnitsAtLoc(1,'h00R',GetOwningPlayer(udg_Shou_Ultimate_Caster),udg_POSITIONFUNCTION,270.00)
set udg_InteriorPentBotRightUnit=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(udg_DurationVariable_ShouUlt,'BTLF',udg_InteriorPentBotRightUnit)
call SetUnitTimeScalePercent(GetLastCreatedUnit(),udg_AnimationSpeed)
call RemoveLocation(udg_POSITIONFUNCTION)
set udg_Ultimate_Degrees_Variable1=190.00
set udg_POSITIONFUNCTION=PolarProjectionBJ(GetUnitLoc(udg_Shou_Ultimate_Caster),udg_OffsetAmount,udg_Ultimate_Degrees_Variable1)
call CreateNUnitsAtLoc(1,'h00R',GetOwningPlayer(udg_Shou_Ultimate_Caster),udg_POSITIONFUNCTION,270.00)
set udg_InteriorPentBotLeftUnit=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(udg_DurationVariable_ShouUlt,'BTLF',udg_InteriorPentBotLeftUnit)
call SetUnitTimeScalePercent(GetLastCreatedUnit(),udg_AnimationSpeed)
set udg_Ultimate_Degrees_Variable1=55.00
set udg_POSITIONFUNCTION=PolarProjectionBJ(GetUnitLoc(udg_Shou_Ultimate_Caster),500.00,udg_Ultimate_Degrees_Variable1)
call CreateNUnitsAtLoc(1,'h00R',GetOwningPlayer(udg_Shou_Ultimate_Caster),udg_POSITIONFUNCTION,270.00)
set udg_InteriorPentTopRightUnit=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(udg_DurationVariable_ShouUlt,'BTLF',udg_InteriorPentTopRightUnit)
call SetUnitTimeScalePercent(GetLastCreatedUnit(),udg_AnimationSpeed)
call RemoveLocation(udg_POSITIONFUNCTION)
set udg_Ultimate_Degrees_Variable1=125.00
set udg_POSITIONFUNCTION=PolarProjectionBJ(GetUnitLoc(udg_Shou_Ultimate_Caster),500.00,udg_Ultimate_Degrees_Variable1)
call CreateNUnitsAtLoc(1,'h00R',GetOwningPlayer(udg_Shou_Ultimate_Caster),udg_POSITIONFUNCTION,270.00)
set udg_InteriorPentTopLeftUnit=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(udg_DurationVariable_ShouUlt,'BTLF',udg_InteriorPentTopLeftUnit)
call SetUnitTimeScalePercent(GetLastCreatedUnit(),udg_AnimationSpeed)
call RemoveLocation(udg_POSITIONFUNCTION)
set udg_OffsetAmount=275.00
set udg_Ultimate_Degrees_Variable1=130.00
set udg_POSITIONFUNCTION=PolarProjectionBJ(GetUnitLoc(udg_Shou_Ultimate_Caster),udg_OffsetAmount,udg_Ultimate_Degrees_Variable1)
call CreateNUnitsAtLoc(1,'h00Q',GetOwningPlayer(udg_Shou_Ultimate_Caster),udg_POSITIONFUNCTION,270.00)
set udg_CenterPointTopLeft=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(udg_DurationVariable_ShouUlt,'BTLF',udg_CenterPointTopLeft)
call SetUnitTimeScalePercent(GetLastCreatedUnit(),udg_AnimationSpeed)
call RemoveLocation(udg_POSITIONFUNCTION)
set udg_Ultimate_Degrees_Variable1=70.00
set udg_POSITIONFUNCTION=PolarProjectionBJ(GetUnitLoc(udg_Shou_Ultimate_Caster),udg_OffsetAmount,udg_Ultimate_Degrees_Variable1)
call CreateNUnitsAtLoc(1,'h00Q',GetOwningPlayer(udg_Shou_Ultimate_Caster),udg_POSITIONFUNCTION,270.00)
set udg_CenterPointTopRight=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(udg_DurationVariable_ShouUlt,'BTLF',udg_CenterPointTopRight)
call SetUnitTimeScalePercent(GetLastCreatedUnit(),udg_AnimationSpeed)
call RemoveLocation(udg_POSITIONFUNCTION)
set udg_Ultimate_Degrees_Variable1=0.00
set udg_POSITIONFUNCTION=PolarProjectionBJ(GetUnitLoc(udg_Shou_Ultimate_Caster),udg_OffsetAmount,udg_Ultimate_Degrees_Variable1)
call CreateNUnitsAtLoc(1,'h00Q',GetOwningPlayer(udg_Shou_Ultimate_Caster),udg_POSITIONFUNCTION,270.00)
set udg_CenterPointMidRight=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(udg_DurationVariable_ShouUlt,'BTLF',udg_CenterPointMidRight)
call SetUnitTimeScalePercent(GetLastCreatedUnit(),udg_AnimationSpeed)
call RemoveLocation(udg_POSITIONFUNCTION)
set udg_Ultimate_Degrees_Variable1=290.00
set udg_POSITIONFUNCTION=PolarProjectionBJ(GetUnitLoc(udg_Shou_Ultimate_Caster),udg_OffsetAmount,udg_Ultimate_Degrees_Variable1)
call CreateNUnitsAtLoc(1,'h00Q',GetOwningPlayer(udg_Shou_Ultimate_Caster),udg_POSITIONFUNCTION,270.00)
set udg_CenterPointBotRight=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(udg_DurationVariable_ShouUlt,'BTLF',udg_CenterPointBotRight)
call SetUnitTimeScalePercent(GetLastCreatedUnit(),udg_AnimationSpeed)
call RemoveLocation(udg_POSITIONFUNCTION)
set udg_Ultimate_Degrees_Variable1=205.00
set udg_POSITIONFUNCTION=PolarProjectionBJ(GetUnitLoc(udg_Shou_Ultimate_Caster),udg_OffsetAmount,udg_Ultimate_Degrees_Variable1)
call CreateNUnitsAtLoc(1,'h00Q',GetOwningPlayer(udg_Shou_Ultimate_Caster),udg_POSITIONFUNCTION,270.00)
set udg_CenterPointBotLeft=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(udg_DurationVariable_ShouUlt,'BTLF',udg_CenterPointBotLeft)
call SetUnitTimeScalePercent(GetLastCreatedUnit(),udg_AnimationSpeed)
call RemoveLocation(udg_POSITIONFUNCTION)
call EnableTrigger(gg_trg_Outer_Pentagon)
call TriggerSleepAction(4.50)
call EnableTrigger(gg_trg_Damage_Shou_Ultimate_Copy)
call TriggerSleepAction(5.60)
call DisableTrigger(gg_trg_Damage_Shou_Ultimate_Copy)
call DisableTrigger(gg_trg_Clash_of_Philosophy)
if(Trig_Cast_Ultimate_Func148C())then
call PauseUnitBJ(false,udg_Shou_Ultimate_Caster)
call SetUnitInvulnerable(udg_Shou_Ultimate_Caster,false)
call SetUnitTimeScalePercent(udg_Shou_Ultimate_Caster,100.00)
else
endif
endfunction
