call TriggerAddAction(gg_trg_Cornello_Dies,function Trig_Cornello_Dies_Actions)
endfunction
function Trig_Winry_Items_Conditions takes nothing returns boolean
if(not(GetItemTypeId(GetManipulatedItem())=='I00I'))then
return false
endif
return true
endfunction
function Trig_Winry_Items_Actions takes nothing returns nothing
call UnitRemoveItemSwapped(GetManipulatedItem(),GetManipulatingUnit())
endfunction
