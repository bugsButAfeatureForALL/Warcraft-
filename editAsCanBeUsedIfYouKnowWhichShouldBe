function Trig_Eny_Doll_Dies_Actions takes nothing returns nothing
call SetUnitPositionLoc(udg_Envy_True_Form,GetUnitLoc(GetDyingUnit()))
call ShowUnitShow(udg_Envy_True_Form)
call PauseUnitBJ(false,udg_Envy_True_Form)
call SetUnitInvulnerable(udg_Envy_True_Form,false)
call SetUnitOwner(udg_Envy_True_Form,udg_Envy_Player,true)
call SelectUnitForPlayerSingle(udg_Envy_True_Form,udg_Envy_Player)
set udg_Envy_Is_In_Doll=false
endfunction
