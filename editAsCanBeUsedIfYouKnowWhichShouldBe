function Trig_Dead_for_Good_Func009C takes nothing returns boolean
if(not(CountPlayersInForceBJ(udg_Neutral)==0))then
return false
endif
if(not(CountPlayersInForceBJ(udg_Homunculi)==0))then
return false
endif
return true
endfunction
function Trig_Dead_for_Good_Actions takes nothing returns nothing
call DisableTrigger(gg_trg_True_Form)
call DestroyTimer(udg_Envy_Timer)
call DisplayTextToForce(GetPlayersAll(),"Envy has been destroyed for good!")
call SetUnitPositionLoc(udg_Envy_First_Form,GetUnitLoc(GetDyingUnit()))
call KillUnit(udg_Envy_First_Form)
call SetPlayerState(udg_Envy_Player,PLAYER_STATE_OBSERVER,1)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Envy_Player),bj_ALLIANCE_ALLIED_VISION)
call ForceRemovePlayerSimple(udg_Envy_Player,udg_Homunculi)
if(Trig_Dead_for_Good_Func009C())then
call DisplayTextToForce(GetPlayersAll(),"The |cffff0000Alchemists and their friends|r defeated all enemies as well as Father and put an end to his plans once and for all! A new era of peace falls over Amestris.")
call TriggerSleepAction(10.00)
call CustomVictoryBJ(Player(0),true,true)
call CustomVictoryBJ(Player(1),true,true)
call CustomVictoryBJ(Player(2),true,true)
call CustomDefeatBJ(Player(3),"You have lost!")
call CustomVictoryBJ(Player(4),true,true)
call CustomVictoryBJ(Player(5),true,true)
call CustomVictoryBJ(Player(6),true,true)
if(Trig_Dead_for_Good_Func009Func010C())then
call CustomVictoryBJ(udg_Scar_Player,true,true)
else
call CustomDefeatBJ(udg_Scar_Player,"You have lost!")
endif
call CustomDefeatBJ(udg_Greed_Player,"You have lost!")
call CustomDefeatBJ(udg_Barry_Player,"You have lost!")
call CustomDefeatBJ(udg_Slicer_Player,"You have lost!")
call CustomDefeatBJ(Player(10),"You have lost!")
call CustomDefeatBJ(Player(11),"You have lost!")
else
endif
endfunction
function InitTrig_Dead_for_Good takes nothing returns nothing
set gg_trg_Dead_for_Good=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Dead_for_Good,EVENT_PLAYER_UNIT_DEATH)
call TriggerAddCondition(gg_trg_Dead_for_Good,Condition(function Trig_Dead_for_Good_Conditions))
call TriggerAddAction(gg_trg_Dead_for_Good,function Trig_Dead_for_Good_Actions)
endfunction
function Trig_Envy_Reformed_Func001Func003C takes nothing returns boolean
