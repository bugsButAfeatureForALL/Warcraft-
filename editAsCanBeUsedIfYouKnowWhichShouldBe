function Trig_Isaac_McDougal_Func002C takes nothing returns boolean
if(not(udg_Boss_Random_Number==0))then
return false
endif
return true
endfunction
function Trig_Isaac_McDougal_Actions takes nothing returns nothing
set udg_Boss_Random_Number=GetRandomInt(0,1)
if(Trig_Isaac_McDougal_Func002C())then
set udg_Boss_Random_Number=GetRandomInt(0,56)
else
set udg_Boss_Random_Number=GetRandomInt(69,125)
endif
call CreateNUnitsAtLoc(1,'H02F',Player(PLAYER_NEUTRAL_AGGRESSIVE),GetRectCenter(udg_Spawn_Array[udg_Boss_Random_Number]),bj_UNIT_FACING)
call PingMinimapLocForForceEx(GetPlayersAll(),GetUnitLoc(GetLastCreatedUnit()),10.00,bj_MINIMAPPINGSTYLE_SIMPLE,100,0.00,0.00)
call SetHeroLevelBJ(GetLastCreatedUnit(),5,false)
call DisplayTextToForce(GetPlayersAll(),"Isaac McDouglas the Freezing Alchemist has appeared and intends to freeze anyone who gets in his way!")
call DestroyTrigger(GetTriggeringTrigger())
endfunction
function InitTrig_Isaac_McDougal takes nothing returns nothing
set gg_trg_Isaac_McDougal=CreateTrigger()
call TriggerRegisterTimerEventSingle(gg_trg_Isaac_McDougal,360.00)
call TriggerAddAction(gg_trg_Isaac_McDougal,function Trig_Isaac_McDougal_Actions)
endfunction
function Trig_Isaac_AI_Conditions takes nothing returns boolean
if(not(GetUnitTypeId(GetAttackedUnitBJ())=='H02F'))then
return false
endif
if(not(GetUnitLifePercent(GetAttackedUnitBJ())<=75.00))then
return false
endif
return true
endfunction
function Trig_Isaac_AI_Actions takes nothing returns nothing
call IssueTargetOrder(GetAttackedUnitBJ(),"impale",GetAttacker())
endfunction
function InitTrig_Isaac_AI takes nothing returns nothing
set gg_trg_Isaac_AI=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Isaac_AI,EVENT_PLAYER_UNIT_ATTACKED)
call TriggerAddCondition(gg_trg_Isaac_AI,Condition(function Trig_Isaac_AI_Conditions))
call TriggerAddAction(gg_trg_Isaac_AI,function Trig_Isaac_AI_Actions)
endfunction
