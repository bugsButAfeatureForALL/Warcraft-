function Trig_Normal_to_Father_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
set bj_forLoopAIndex=0
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set udg_Items_For_Father[GetForLoopIndexA()]=UnitItemInSlotBJ(udg_Becoming_Father,(GetForLoopIndexA()+1))
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set bj_forLoopBIndex=0
set bj_forLoopBIndexEnd=5
loop
exitwhen bj_forLoopBIndex>bj_forLoopBIndexEnd
call UnitRemoveItemFromSlotSwapped((GetForLoopIndexB()+1),udg_Becoming_Father)
call UnitAddItemSwapped(udg_Items_For_Father[GetForLoopIndexB()],udg_Our_Father)
set bj_forLoopBIndex=bj_forLoopBIndex+1
endloop
endfunction
function InitTrig_Normal_to_Father takes nothing returns nothing
set gg_trg_Normal_to_Father=CreateTrigger()
call DisableTrigger(gg_trg_Normal_to_Father)
call TriggerRegisterTimerEventPeriodic(gg_trg_Normal_to_Father,0.01)
call TriggerAddAction(gg_trg_Normal_to_Father,function Trig_Normal_to_Father_Actions)
endfunction
function Trig_No_Betraying_Father_Conditions takes nothing returns boolean
if(not(IsUnitAlly(GetAttacker(),Player(bj_PLAYER_NEUTRAL_EXTRA))==true))then
return false
endif
if(not(GetAttackedUnitBJ()==udg_Our_Father))then
return false
endif
return true
endfunction
function Trig_No_Betraying_Father_Func001Func024001 takes nothing returns boolean
return(CountPlayersInForceBJ(udg_Alchemists)>=3)
endfunction
function Trig_No_Betraying_Father_Func001Func024002 takes nothing returns boolean
return(udg_Is_Hohenheim_Selected==true)
endfunction
function Trig_No_Betraying_Father_Func001C takes nothing returns boolean
if(not(RectContainsUnit(gg_rct_Evil_Spawns,gg_unit_h01P_0052)==false))then
return false
endif
if(not(RectContainsUnit(gg_rct_Evil_Spawns,gg_unit_h01P_0053)==false))then
return false
endif
if(not(RectContainsUnit(gg_rct_Evil_Spawns,gg_unit_h01P_0054)==false))then
return false
endif
if(not(CountPlayersInForceBJ(udg_Homunculi)<=2))then
return false
endif
if(not GetBooleanOr((CountPlayersInForceBJ(udg_Alchemists)>=3),(udg_Is_Hohenheim_Selected==true)))then
return false
endif
if(not(udg_No_Father_Abuse==true))then
return false
endif
if(not(udg_Father_Is_Picked==false))then
return false
endif
if(not(udg_Its_Father_Time==true))then
return false
endif
if(not(udg_Souls_Are_Being_Absorbed==false))then
return false
endif
if(not(udg_Our_Father !=gg_unit_H04N_0132))then
return false
endif
return true
endfunction
function Trig_No_Betraying_Father_Actions takes nothing returns nothing
if(Trig_No_Betraying_Father_Func001C())then
set udg_Father_Is_Picked=true
call DisableTrigger(gg_trg_True_Godhood)
call EnableTrigger(gg_trg_Godhood)
set udg_Becoming_Father=GetAttacker()
set udg_Father_Player=GetOwningPlayer(udg_Becoming_Father)
call ShowUnitHide(udg_Becoming_Father)
call PauseUnitBJ(true,udg_Becoming_Father)
call SetUnitOwner(udg_Becoming_Father,Player(PLAYER_NEUTRAL_PASSIVE),true)
call SetUnitOwner(udg_Our_Father,udg_Father_Player,true)
call SelectUnitForPlayerSingle(udg_Our_Father,udg_Father_Player)
call SetPlayerName(udg_Father_Player,"Father")
call EnableTrigger(gg_trg_Normal_to_Father)
call TriggerSleepAction(1.00)
call DisplayTextToForce(GetPlayersAll(),"With the defeat of his Homunculi, |cffff0000Father|r has decided to destroy the alchemists himself!")
call RemoveUnit(udg_Becoming_Father)
call DisableTrigger(gg_trg_Father_Dies_First_Boss)
call EnableTrigger(gg_trg_Father_Dies_First_Player)
call UnitAddAbilityBJ('A09W',udg_Our_Father)
call UnitAddAbilityBJ('A0A5',udg_Our_Father)
else
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Father:|r You dare betray your own Father? Perish, insolent child.")
call KillUnit(GetAttacker())
endif
endfunction
function InitTrig_No_Betraying_Father takes nothing returns nothing
set gg_trg_No_Betraying_Father=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_No_Betraying_Father,EVENT_PLAYER_UNIT_ATTACKED)
call TriggerAddCondition(gg_trg_No_Betraying_Father,Condition(function Trig_No_Betraying_Father_Conditions))
call TriggerAddAction(gg_trg_No_Betraying_Father,function Trig_No_Betraying_Father_Actions)
endfunction
function Trig_Father_Dies_First_Boss_Conditions takes nothing returns boolean
if(not(GetDyingUnit()==gg_unit_H03A_0103))then
return false
endif
return true
endfunction
function Trig_Father_Dies_First_Boss_Func016C takes nothing returns boolean
if(not(IsTriggerEnabled(gg_trg_Gathering_Souls)==true))then
return false
endif
return true
endfunction
function Trig_Father_Dies_First_Boss_Func017C takes nothing returns boolean
if(not(udg_Greeds_Sacrifice==true))then
return false
endif
return true
endfunction
function Trig_Father_Dies_First_Boss_Actions takes nothing returns nothing
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Father|r: You pathetic ants really believe you can defeat a God..?")
call TriggerSleepAction(5.00)
call CreateNUnitsAtLoc(1,'H02R',Player(bj_PLAYER_NEUTRAL_EXTRA),GetRectCenter(gg_rct_Our_Father),bj_UNIT_FACING)
set udg_Our_Father=GetLastCreatedUnit()
call SetHeroLevelBJ(udg_Our_Father,35,false)
call SelectHeroSkill(udg_Our_Father,'A07O')
call SelectHeroSkill(udg_Our_Father,'A07N')
call SelectHeroSkill(udg_Our_Father,'A04P')
call SelectHeroSkill(udg_Our_Father,'A00S')
call TriggerSleepAction(1.00)
call SetUnitAbilityLevelSwapped('A07O',udg_Our_Father,5)
call SetUnitAbilityLevelSwapped('A07N',udg_Our_Father,5)
call SetUnitAbilityLevelSwapped('A04P',udg_Our_Father,3)
call SetUnitAbilityLevelSwapped('A00S',udg_Our_Father,3)
call EnableTrigger(gg_trg_Sinful_Father_AI)
if(Trig_Father_Dies_First_Boss_Func016C())then
call ModifyHeroStat(bj_HEROSTAT_STR,udg_Our_Father,bj_MODIFYMETHOD_ADD,udg_Godhood_Stats)
call ModifyHeroStat(bj_HEROSTAT_AGI,udg_Our_Father,bj_MODIFYMETHOD_ADD,udg_Godhood_Stats)
call ModifyHeroStat(bj_HEROSTAT_INT,udg_Our_Father,bj_MODIFYMETHOD_ADD,udg_Godhood_Stats)
call UnitAddAbilityBJ('A088',udg_Our_Father)
else
endif
if(Trig_Father_Dies_First_Boss_Func017C())then
call UnitRemoveAbilityBJ('A07N',udg_Our_Father)
call UnitAddAbilityBJ('A086',udg_Our_Father)
else
endif
endfunction
function InitTrig_Father_Dies_First_Boss takes nothing returns nothing
set gg_trg_Father_Dies_First_Boss=CreateTrigger()
