function Trig_Charge_start_Copy_2_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A05B'))then
return false
endif
return true
endfunction
function Trig_Charge_start_Copy_2_Actions takes nothing returns nothing
set udg_Blazing_Speed_Target=GetSpellTargetUnit()
call SaveUnitHandleBJ(GetSpellTargetUnit(),StringHashBJ("target"),GetHandleIdBJ(GetTriggerUnit()),udg_Sloth_Table)
call GroupAddUnitSimple(GetTriggerUnit(),udg_Sloth_Group)
call SetUnitPathing(GetTriggerUnit(),false)
call CreateNUnitsAtLoc(1,'h033',udg_Sloth_Player,GetUnitLoc(udg_Blazing_Speed_Target),bj_UNIT_FACING)
set udg_Inhuman_Vision=GetLastCreatedUnit()
call EnableTrigger(gg_trg_Charge_loop_Copy_2)
endfunction
function InitTrig_Charge_start_Copy_2 takes nothing returns nothing
set gg_trg_Charge_start_Copy_2=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Charge_start_Copy_2,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Charge_start_Copy_2,Condition(function Trig_Charge_start_Copy_2_Conditions))
call TriggerAddAction(gg_trg_Charge_start_Copy_2,function Trig_Charge_start_Copy_2_Actions)
endfunction
function Trig_Charge_loop_Copy_2_Func001Func001Func008Func001A takes nothing returns nothing
call SetUnitTimeScalePercent(GetEnumUnit(),100.00)
call SetUnitAnimation(GetEnumUnit(),"Walk")
endfunction
function Trig_Charge_loop_Copy_2_Func001Func001Func008Func006C takes nothing returns boolean
if(not(GetUnitAbilityLevelSwapped('A05B',GetEnumUnit())==1))then
return false
endif
return true
endfunction
function Trig_Charge_loop_Copy_2_Func001Func001Func008Func007C takes nothing returns boolean
if(not(GetUnitAbilityLevelSwapped('A05B',GetEnumUnit())==2))then
return false
endif
return true
endfunction
function Trig_Charge_loop_Copy_2_Func001Func001Func008Func008C takes nothing returns boolean
if(not(GetUnitAbilityLevelSwapped('A05B',GetEnumUnit())==3))then
return false
endif
return true
endfunction
function Trig_Charge_loop_Copy_2_Func001Func001Func008C takes nothing returns boolean
if(not(DistanceBetweenPoints(udg_Sloth_Points[1],udg_Sloth_Points[2])<=50.00))then
return false
endif
return true
endfunction
function Trig_Charge_loop_Copy_2_Func001Func001A takes nothing returns nothing
call ShowUnitHide(GetEnumUnit())
set udg_Sloth_Points[1]=GetUnitLoc(GetEnumUnit())
set udg_Sloth_Points[2]=GetUnitLoc(LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_Sloth_Table))
set udg_Sloth_Points[3]=PolarProjectionBJ(udg_Sloth_Points[1],15.00,AngleBetweenPoints(udg_Sloth_Points[1],udg_Sloth_Points[2]))
call SetUnitPositionLocFacingLocBJ(GetEnumUnit(),udg_Sloth_Points[3],udg_Sloth_Points[2])
call AddSpecialEffectLocBJ(GetUnitLoc(GetEnumUnit()),"war3mapImported\\BlackBlink.mdx")
call DestroyEffect(GetLastCreatedEffectBJ())
if(Trig_Charge_loop_Copy_2_Func001Func001Func008C())then
call GroupRemoveUnitSimple(GetEnumUnit(),udg_Sloth_Group)
call SetUnitPathing(GetEnumUnit(),true)
call SetUnitTimeScalePercent(GetEnumUnit(),100.00)
call SetUnitAnimation(GetEnumUnit(),"Walk")
if(Trig_Charge_loop_Copy_2_Func001Func001Func008Func006C())then
call UnitDamageTargetBJ(GetEnumUnit(),LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_Sloth_Table),550.00,ATTACK_TYPE_HERO,DAMAGE_TYPE_NORMAL)
else
endif
if(Trig_Charge_loop_Copy_2_Func001Func001Func008Func007C())then
call UnitDamageTargetBJ(GetEnumUnit(),LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_Sloth_Table),650.00,ATTACK_TYPE_HERO,DAMAGE_TYPE_NORMAL)
else
endif
if(Trig_Charge_loop_Copy_2_Func001Func001Func008Func008C())then
call UnitDamageTargetBJ(GetEnumUnit(),LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_Sloth_Table),750.00,ATTACK_TYPE_HERO,DAMAGE_TYPE_NORMAL)
else
endif
call ShowUnitShow(GetEnumUnit())
call SelectUnitForPlayerSingle(GetEnumUnit(),udg_Sloth_Player)
call RemoveUnit(udg_Inhuman_Vision)
call RemoveLocation(udg_Sloth_Points[1])
call RemoveLocation(udg_Sloth_Points[2])
call RemoveLocation(udg_Sloth_Points[3])
call FlushChildHashtableBJ(GetHandleIdBJ(GetEnumUnit()),udg_Sloth_Table)
else
call ForGroupBJ(udg_Sloth_Group,function Trig_Charge_loop_Copy_2_Func001Func001Func008Func001A)
endif
endfunction
function Trig_Charge_loop_Copy_2_Func001C takes nothing returns boolean
if(not(IsUnitGroupEmptyBJ(udg_Sloth_Group)==true))then
return false
endif
return true
endfunction
function Trig_Charge_loop_Copy_2_Actions takes nothing returns nothing
if(Trig_Charge_loop_Copy_2_Func001C())then
call DisableTrigger(GetTriggeringTrigger())
else
call ForGroupBJ(udg_Sloth_Group,function Trig_Charge_loop_Copy_2_Func001Func001A)
endif
endfunction
function InitTrig_Charge_loop_Copy_2 takes nothing returns nothing
set gg_trg_Charge_loop_Copy_2=CreateTrigger()
call DisableTrigger(gg_trg_Charge_loop_Copy_2)
call TriggerRegisterTimerEventPeriodic(gg_trg_Charge_loop_Copy_2,0.03)
call TriggerAddAction(gg_trg_Charge_loop_Copy_2,function Trig_Charge_loop_Copy_2_Actions)
endfunction
function Trig_Learning_Defending_Shadows_Conditions takes nothing returns boolean
if(not(GetLearnedSkillBJ()=='A06G'))then
return false
