if(Trig_Player_2_Leaves_Copy_Func010C())then
set udg_Ling_Yao_Is_Here=false
set udg_Player_Left=udg_Ling_Unit
call EnableTrigger(gg_trg_Player_Leave_Items)
call TriggerSleepAction(1.00)
