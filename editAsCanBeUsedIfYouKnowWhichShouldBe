function Trig_Fu_is_Chosen_Conditions takes nothing returns boolean
if(not(GetUnitTypeId(GetTriggerUnit())=='H00M'))then
return false
endif
return true
endfunction
function Trig_Fu_is_Chosen_Func018C takes nothing returns boolean
if(not(IsUnitAlly(udg_May_Chang,udg_Scar_Player)==true))then
return false
endif
return true
endfunction
function Trig_Fu_is_Chosen_Actions takes nothing returns nothing
set udg_Fu_Unit=GetTriggerUnit()
set udg_Fu_Player=GetOwningPlayer(GetTriggerUnit())
set udg_Fu_Color=GetPlayerColor(GetOwningPlayer(GetTriggerUnit()))
call ForceAddPlayerSimple(udg_Fu_Player,udg_Good_Team)
call ForceAddPlayerSimple(udg_Fu_Player,udg_Alchemists)
call SetPlayerName(udg_Fu_Player,"Fu")
call SetUnitPositionLoc(GetTriggerUnit(),GetRectCenter(gg_rct_Fu_Spawn))
call PanCameraToTimedLocForPlayer(GetOwningPlayer(GetTriggerUnit()),GetRectCenter(gg_rct_Fu_Spawn),0)
call SelectUnitForPlayerSingle(GetTriggerUnit(),GetOwningPlayer(GetTriggerUnit()))
call DisplayTextToForce(GetForceOfPlayer(udg_Fu_Player),"You are a member of the |cffff0000Alchemists|r team it is your job to stop Fathers plan before he can destroy the entire nation of Amestris for his own goals! You must also protect |cffff0000Winry|r at all costs! If she dies you can no longer revive!")
call SetForceAllianceStateBJ(udg_Good_Team,udg_Evil_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,bj_FORCE_PLAYER[bj_PLAYER_NEUTRAL_VICTIM],bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,bj_FORCE_PLAYER[7],bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[bj_PLAYER_NEUTRAL_EXTRA],udg_Good_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[7],udg_Good_Team,bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[bj_PLAYER_NEUTRAL_VICTIM],udg_Good_Team,bj_ALLIANCE_ALLIED)
call UnitAddItemToSlotById(udg_Fu_Unit,'I00O',0)
if(Trig_Fu_is_Chosen_Func018C())then
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_May_Chang_Player),bj_ALLIANCE_UNALLIED)
else
endif
call DestroyTrigger(GetTriggeringTrigger())
endfunction
function InitTrig_Fu_is_Chosen takes nothing returns nothing
set gg_trg_Fu_is_Chosen=CreateTrigger()
call TriggerRegisterEnterRectSimple(gg_trg_Fu_is_Chosen,gg_rct_Good_Spawns)
call TriggerAddCondition(gg_trg_Fu_is_Chosen,Condition(function Trig_Fu_is_Chosen_Conditions))
call TriggerAddAction(gg_trg_Fu_is_Chosen,function Trig_Fu_is_Chosen_Actions)
endfunction
