function Trig_Knock_Copy_Func003C takes nothing returns boolean
if(not(IsUnitType(GetAttackedUnitBJ(),UNIT_TYPE_MECHANICAL)==false))then
return false
endif
if(not(GetSpellTargetUnit()!=gg_unit_H04N_0132))then
return false
endif
if(not(IsUnitType(GetAttackedUnitBJ(),UNIT_TYPE_TOWNHALL)==false))then
return false
endif
if(not(IsUnitType(GetAttackedUnitBJ(),UNIT_TYPE_STRUCTURE)==false))then
return false
endif
return true
endfunction
function Trig_Knock_Copy_Actions takes nothing returns nothing
set udg_Power_Of_Avarice_Caster=GetSpellAbilityUnit()
if(Trig_Knock_Copy_Func003C())then
set udg_Power_Of_Avarice_Target=GetSpellTargetUnit()
set udg_Power_Of_Avarice_Position=GetUnitLoc(udg_Power_Of_Avarice_Target)
set udg_Power_Of_Avarice_Location=GetUnitLoc(udg_Power_Of_Avarice_Caster)
set udg_Avarice_Knock=AngleBetweenPoints(udg_Power_Of_Avarice_Location,udg_Power_Of_Avarice_Position)
call RemoveLocation(udg_Power_Of_Avarice_Position)
call EnableTrigger(gg_trg_Knock_Back_Copy)
call DisableTrigger(GetTriggeringTrigger())
call TriggerSleepAction(1.50)
call DisableTrigger(gg_trg_Knock_Back_Copy)
call EnableTrigger(GetTriggeringTrigger())
call RemoveLocation(udg_Power_Of_Avarice_Location)
else
endif
endfunction
