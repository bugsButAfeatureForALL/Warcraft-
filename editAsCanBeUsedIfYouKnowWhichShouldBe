function Trig_Envy_Dominates_a_Doll_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A099'))then
return false
endif
if(not GetBooleanOr((GetUnitTypeId(GetSpellTargetUnit())=='n007'),(GetUnitTypeId(GetSpellTargetUnit())=='h00I')))then
return false
endif
return true
endfunction
function Trig_Envy_Dominates_a_Doll_Actions takes nothing returns nothing
set udg_Envy_Doll_Body=GetSpellTargetUnit()
call SetUnitInvulnerable(udg_Envy_True_Form,true)
call PauseUnitBJ(true,udg_Envy_True_Form)
call ShowUnitHide(udg_Envy_True_Form)
call SetUnitOwner(udg_Envy_True_Form,Player(PLAYER_NEUTRAL_PASSIVE),true)
call SelectUnitForPlayerSingle(udg_Envy_Doll_Body,udg_Envy_Player)
call SetUnitOwner(udg_Envy_Doll_Body,udg_Envy_Player,true)
call UnitAddAbilityBJ('A03F',udg_Envy_Doll_Body)
call UnitAddAbilityBJ('A093',udg_Envy_Doll_Body)
call UnitAddAbilityBJ('A00W',udg_Envy_Doll_Body)
call UnitAddAbilityBJ('A09B',udg_Envy_Doll_Body)
set udg_Envy_Is_In_Doll=true
endfunction
