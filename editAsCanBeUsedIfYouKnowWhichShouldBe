call RemoveUnit(udg_Killer_Healer)
set udg_Slicer_Unit=GetTriggerUnit()
set udg_Slicer_Player=GetOwningPlayer(GetTriggerUnit())
call SetPlayerName(udg_Slicer_Player,"Slicer")
if(Trig_Slicer_is_Chosen_Func005C())then
call SetPlayerAllianceStateBJ(udg_Barry_Player,udg_Slicer_Player,bj_ALLIANCE_ALLIED_VISION)
call SetPlayerAllianceStateBJ(udg_Slicer_Player,udg_Barry_Player,bj_ALLIANCE_ALLIED_VISION)
else
endif
call SetUnitPositionLoc(GetTriggerUnit(),GetRectCenter(gg_rct_Slicer_Spawn))
call PanCameraToTimedLocForPlayer(udg_Slicer_Player,GetRectCenter(gg_rct_Slicer_Spawn),0)
call SelectUnitForPlayerSingle(GetTriggerUnit(),GetOwningPlayer(GetTriggerUnit()))
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Slicer_Player),udg_Evil_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Slicer_Player),udg_Good_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Slicer_Player),bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Slicer_Player),bj_ALLIANCE_UNALLIED)
call ForceAddPlayerSimple(udg_Slicer_Player,udg_Neutral)
call DisplayTextToForce(GetForceOfPlayer(udg_Slicer_Player),"You are the deranged serial killer |cffff0000Slicer|r! You gain bonus stats during the Day so it's best to wait until then before venturing out too far. However if Barry has been chosen than the two of you can work together to kill all before you! If things get too tough you can return to your camp to heal.\n\nBe warned though, if you die once you are |cffff0000gone|r!")
call CreateNUnitsAtLoc(1,'h01Z',udg_Slicer_Player,GetRectCenter(gg_rct_Slicer_Spawn),bj_UNIT_FACING)
set udg_Killer_Healer=GetLastCreatedUnit()
call SetPlayerHandicapXPBJ(udg_Slicer_Player,150.00)
call DestroyTrigger(GetTriggeringTrigger())
endfunction
function InitTrig_Slicer_is_Chosen takes nothing returns nothing
set gg_trg_Slicer_is_Chosen=CreateTrigger()
call TriggerRegisterEnterRectSimple(gg_trg_Slicer_is_Chosen,gg_rct_Neutral_Spawns)
call TriggerAddCondition(gg_trg_Slicer_is_Chosen,Condition(function Trig_Slicer_is_Chosen_Conditions))
call TriggerAddAction(gg_trg_Slicer_is_Chosen,function Trig_Slicer_is_Chosen_Actions)
endfunction
function Trig_Our_Fadder_Dies_Conditions takes nothing returns boolean
if(not(GetDyingUnit()==udg_Our_Fadder))then
return false
endif
return true
endfunction
function Trig_Our_Fadder_Dies_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
call PanCameraToTimedLocForPlayer(udg_Fadder_Player,GetRectCenter(gg_rct_Evil_Spawns),0)
call RemoveUnit(udg_Our_Fadder)
call CreateNUnitsAtLoc(1,'h01P',udg_Fadder_Player,GetRectCenter(gg_rct_Evil_Spawns),bj_UNIT_FACING)
call TriggerSleepAction(0.20)
call SelectUnitForPlayerSingle(gg_unit_n003_0031,udg_Fadder_Player)
call SetPlayerStateBJ(udg_Fadder_Player,PLAYER_STATE_RESOURCE_LUMBER,5)
call ShowUnitShow(gg_unit_H03A_0103)
call PauseUnitBJ(false,gg_unit_H03A_0103)
endfunction
function InitTrig_Our_Fadder_Dies takes nothing returns nothing
set gg_trg_Our_Fadder_Dies=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Our_Fadder_Dies,EVENT_PLAYER_UNIT_DEATH)
call TriggerAddCondition(gg_trg_Our_Fadder_Dies,Condition(function Trig_Our_Fadder_Dies_Conditions))
call TriggerAddAction(gg_trg_Our_Fadder_Dies,function Trig_Our_Fadder_Dies_Actions)
endfunction
