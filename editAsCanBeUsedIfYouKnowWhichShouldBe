function Trig_Aimed_Cast_Actions takes nothing returns nothing
set udg_Aimed_Number=0.00
set udg_Aimed_Target=GetSpellTargetUnit()
set udg_Aimed_Caster=GetSpellAbilityUnit()
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=10
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set udg_Aimed_Targ_Pos=GetUnitLoc(GetSpellTargetUnit())
set udg_Aimed_Targ_Off=PolarProjectionBJ(udg_Aimed_Targ_Pos,450.00,(udg_Aimed_Number+GetRandomReal(0,26.00)))
call CreateNUnitsAtLocFacingLocBJ(1,'h02T',GetOwningPlayer(udg_Aimed_Caster),udg_Aimed_Targ_Off,udg_Aimed_Targ_Pos)
set udg_Aimed_Dummies[GetForLoopIndexA()]=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(2.00,'BTLF',GetLastCreatedUnit())
set udg_Aimed_Number=(udg_Aimed_Number+36.00)
call RemoveLocation(udg_Aimed_Targ_Pos)
call RemoveLocation(udg_Aimed_Targ_Off)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
call EnableTrigger(gg_trg_Aimed_Fast)
call EnableTrigger(gg_trg_Aimed_Check)
endfunction
function InitTrig_Aimed_Cast takes nothing returns nothing
set gg_trg_Aimed_Cast=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Aimed_Cast,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Aimed_Cast,Condition(function Trig_Aimed_Cast_Conditions))
call TriggerAddAction(gg_trg_Aimed_Cast,function Trig_Aimed_Cast_Actions)
endfunction
function Trig_Aimed_Fast_Func002Func003C takes nothing returns boolean
if(not(DistanceBetweenPoints(udg_Aimed_Targ_Pos,udg_Aimed_Dummy_Pos)<=50.00))then
return false
endif
if(not(IsUnitAliveBJ(udg_Aimed_Dummies[GetForLoopIndexA()])==true))then
return false
endif
return true
endfunction
function Trig_Aimed_Fast_Actions takes nothing returns nothing
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=10
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set udg_Aimed_Dummy_Pos=GetUnitLoc(udg_Aimed_Dummies[GetForLoopIndexA()])
set udg_Aimed_Targ_Pos=GetUnitLoc(udg_Aimed_Target)
set udg_Aimed_Dummy_Off=PolarProjectionBJ(udg_Aimed_Dummy_Pos,8.00,AngleBetweenPoints(udg_Aimed_Dummy_Pos,udg_Aimed_Targ_Pos))
call SetUnitPositionLoc(udg_Aimed_Dummies[GetForLoopIndexA()],udg_Aimed_Dummy_Off)
call RemoveLocation(udg_Aimed_Dummy_Pos)
call RemoveLocation(udg_Aimed_Dummy_Off)
call RemoveLocation(udg_Aimed_Targ_Pos)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=10
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set udg_Aimed_Dummy_Pos=GetUnitLoc(udg_Aimed_Dummies[GetForLoopIndexA()])
set udg_Aimed_Targ_Pos=GetUnitLoc(udg_Aimed_Target)
if(Trig_Aimed_Fast_Func002Func003C())then
call RemoveUnit(udg_Aimed_Dummies[GetForLoopIndexA()])
else
call SetUnitFlyHeightBJ(udg_Aimed_Dummies[GetForLoopIndexA()],DistanceBetweenPoints(udg_Aimed_Dummy_Pos,udg_Aimed_Targ_Pos),2000.00)
endif
call RemoveLocation(udg_Aimed_Targ_Pos)
call RemoveLocation(udg_Aimed_Dummy_Pos)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endfunction
function InitTrig_Aimed_Fast takes nothing returns nothing
set gg_trg_Aimed_Fast=CreateTrigger()
call DisableTrigger(gg_trg_Aimed_Fast)
call TriggerRegisterTimerEventPeriodic(gg_trg_Aimed_Fast,0.01)
call TriggerAddAction(gg_trg_Aimed_Fast,function Trig_Aimed_Fast_Actions)
endfunction
function Trig_Aimed_Check_Func001C takes nothing returns boolean
if(not(IsUnitAliveBJ(udg_Aimed_Dummies[1])==false))then
return false
endif
if(not(IsUnitAliveBJ(udg_Aimed_Dummies[2])==false))then
return false
endif
if(not(IsUnitAliveBJ(udg_Aimed_Dummies[3])==false))then
return false
endif
if(not(IsUnitAliveBJ(udg_Aimed_Dummies[4])==false))then
return false
endif
if(not(IsUnitAliveBJ(udg_Aimed_Dummies[5])==false))then
return false
endif
if(not(IsUnitAliveBJ(udg_Aimed_Dummies[6])==false))then
return false
endif
if(not(IsUnitAliveBJ(udg_Aimed_Dummies[7])==false))then
return false
endif
if(not(IsUnitAliveBJ(udg_Aimed_Dummies[8])==false))then
return false
endif
if(not(IsUnitAliveBJ(udg_Aimed_Dummies[9])==false))then
return false
endif
if(not(IsUnitAliveBJ(udg_Aimed_Dummies[10])==false))then
return false
endif
return true
endfunction
