function Trig_Surrounding_Darkness_Cast_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A05L'))then
return false
endif
return true
endfunction
function Trig_Surrounding_Darkness_Cast_Actions takes nothing returns nothing
set udg_SDarkness_Target=GetSpellTargetUnit()
set udg_SDarkness_Real=45.00
set udg_SDarkness_Target_Position=GetUnitLoc(udg_SDarkness_Target)
set udg_SDarkness_Dummy_Position=PolarProjectionBJ(udg_SDarkness_Target_Position,300.00,udg_SDarkness_Real)
set bj_forLoopAIndex=0
set bj_forLoopAIndexEnd=3
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call CreateNUnitsAtLoc(1,'h037',udg_Pride_Player,udg_SDarkness_Dummy_Position,AngleBetweenPoints(udg_SDarkness_Dummy_Position,udg_SDarkness_Target_Position))
set udg_SDarkness_Dummy=GetLastCreatedUnit()
call IssuePointOrderLoc(udg_SDarkness_Dummy,"impale",udg_SDarkness_Target_Position)
call UnitApplyTimedLifeBJ(2.00,'BTLF',udg_SDarkness_Dummy)
set udg_SDarkness_Real=(udg_SDarkness_Real+90.00)
set udg_SDarkness_Dummy_Position=PolarProjectionBJ(GetUnitLoc(udg_SDarkness_Target),300.00,udg_SDarkness_Real)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
call RemoveLocation(udg_SDarkness_Dummy_Position)
call RemoveLocation(udg_SDarkness_Target_Position)
endfunction
function InitTrig_Surrounding_Darkness_Cast takes nothing returns nothing
set gg_trg_Surrounding_Darkness_Cast=CreateTrigger()
