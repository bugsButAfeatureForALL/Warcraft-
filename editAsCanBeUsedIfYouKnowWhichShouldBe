function Trig_True_Form_Func009001 takes nothing returns boolean
return(GetDyingUnit()==udg_Envy_First_Form)
endfunction
function Trig_True_Form_Func009002 takes nothing returns boolean
return(GetDyingUnit()==udg_Envy_Shapeshifted_Form)
endfunction
function Trig_True_Form_Conditions takes nothing returns boolean
if(not GetBooleanOr((GetDyingUnit()==udg_Envy_First_Form),(GetDyingUnit()==udg_Envy_Shapeshifted_Form)))then
return false
endif
return true
endfunction
function Trig_True_Form_Func001C takes nothing returns boolean
if(not(GetDyingUnit()==udg_Envy_First_Form))then
return false
endif
return true
endfunction
function Trig_True_Form_Actions takes nothing returns nothing
if(Trig_True_Form_Func001C())then
call SetPlayerName(GetOwningPlayer(udg_Envy_First_Form),"Envy")
else
call SetPlayerName(GetOwningPlayer(udg_Envy_Shapeshifted_Form),"Envy")
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Envy_Player),bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Envy_Player),udg_Good_Team,bj_ALLIANCE_UNALLIED)
endif
call SetPlayerColorBJ(udg_Envy_Player,udg_Envy_Original_Color,true)
call CreateNUnitsAtLoc(1,'H01K',GetOwningPlayer(GetDyingUnit()),GetUnitLoc(GetDyingUnit()),bj_UNIT_FACING)
set udg_Envy_True_Form=GetLastCreatedUnit()
call SelectUnitForPlayerSingle(GetLastCreatedUnit(),GetOwningPlayer(GetDyingUnit()))
call DisplayTextToForce(GetForceOfPlayer(udg_Envy_Player),"If you can survive for four minutes Envy will reform!")
call TriggerSleepAction(5.00)
call StartTimerBJ(udg_Envy_Timer,false,240.00)
endfunction
