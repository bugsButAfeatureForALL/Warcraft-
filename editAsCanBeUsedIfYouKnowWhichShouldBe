function Trig_Captures_Winry_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
call DisplayTimedTextToForce(GetPlayersAll(),5.00,"|cff995500Father:|r You....can't...win...this....")
call TriggerSleepAction(2.00)
if(Trig_Captures_Winry_Func004C())then
call CreateNUnitsAtLoc(1,'h03D',Player(PLAYER_NEUTRAL_PASSIVE),GetUnitLoc(gg_unit_H01V_0080),bj_UNIT_FACING)
call UnitApplyTimedLifeBJ(2.00,'BTLF',GetLastCreatedUnit())
call TriggerSleepAction(2.00)
call SetUnitPositionLocFacingLocBJ(gg_unit_H01V_0080,GetRectCenter(gg_rct_Winry_Captured),GetRectCenter(gg_rct_Evil_Spawn))
call SetUnitInvulnerable(gg_unit_H01V_0080,true)
call UnitAddAbilityBJ('A0A8',gg_unit_H01V_0080)
call PauseUnitBJ(true,gg_unit_H01V_0080)
call UnitRemoveAbilityBJ('ANre',gg_unit_H01V_0080)
call UnitRemoveAbilityBJ('ACnr',gg_unit_H01V_0080)
set udg_Winry_Is_Dead=true
call DisplayTimedTextToForce(GetPlayersAll(),5.00,"|cffff0000Father has captured Winry! She is no longer able to heal or revive you!|r")
else
endif
endfunction
