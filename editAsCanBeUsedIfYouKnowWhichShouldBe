if(not(CountPlayersInForceBJ(udg_Neutral)==0))then
return false
endif
if(not(CountPlayersInForceBJ(udg_Homunculi)==0))then
return false
endif
return true
endfunction
function Trig_Dolcetto_Dies_H_Func002C takes nothing returns boolean
if(not(udg_Doctor_Is_Dead==false))then
return false
endif
return true
endfunction
function Trig_Dolcetto_Dies_H_Actions takes nothing returns nothing
call TriggerSleepAction(10.00)
if(Trig_Dolcetto_Dies_H_Func002C())then
call ReviveHeroLoc(udg_Dolcetto_Unit,GetUnitLoc(gg_unit_H02L_0157),false)
call DisplayTextToForce(GetPlayersAll(),"|cffff0000The Gold Toothed Doctor|r has used his medical knowledge to revive Dolcetto!")
call SetHeroLevelBJ(GetTriggerUnit(),(GetHeroLevel(GetDyingUnit())-3),false)
call PanCameraToTimedLocForPlayer(GetOwningPlayer(GetDyingUnit()),GetUnitLoc(GetDyingUnit()),0)
call SelectUnitForPlayerSingle(GetDyingUnit(),udg_Dolcetto_Player)
else
call SetPlayerState(udg_Dolcetto_Player,PLAYER_STATE_OBSERVER,1)
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Dolcetto has been killed! His dog days are over.|r")
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Dolcetto_Player),bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Dolcetto_Player),udg_Evil_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Dolcetto_Player),bj_ALLIANCE_ALLIED_VISION)
call ForceRemovePlayerSimple(udg_Dolcetto_Player,udg_Homunculi)
if(Trig_Dolcetto_Dies_H_Func002Func013C())then
call DisplayTextToForce(GetPlayersAll(),"The |cffff0000Alchemists and their friends|r defeated all enemies as well as Father and put an end to his plans once and for all! A new era of peace falls over Amestris.")
call TriggerSleepAction(10.00)
call CustomVictoryBJ(Player(0),true,true)
call CustomVictoryBJ(Player(1),true,true)
call CustomVictoryBJ(Player(2),true,true)
call CustomDefeatBJ(Player(3),"You have lost!")
call CustomVictoryBJ(Player(4),true,true)
call CustomVictoryBJ(Player(5),true,true)
call CustomVictoryBJ(Player(6),true,true)
if(Trig_Dolcetto_Dies_H_Func002Func013Func010C())then
call CustomVictoryBJ(udg_Scar_Player,true,true)
else
call CustomDefeatBJ(udg_Scar_Player,"You have lost!")
endif
call CustomDefeatBJ(udg_Greed_Player,"You have lost!")
call CustomDefeatBJ(udg_Barry_Player,"You have lost!")
call CustomDefeatBJ(udg_Slicer_Player,"You have lost!")
call CustomDefeatBJ(Player(10),"You have lost!")
call CustomDefeatBJ(Player(11),"You have lost!")
else
endif
endif
endfunction
