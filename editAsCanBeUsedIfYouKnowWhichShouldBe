function Trig_RANDOM_ASS_Conditions takes nothing returns boolean
if(not(GetUnitTypeId(GetSoldUnit())=='h04I'))then
return false
endif
return true
endfunction
function Trig_RANDOM_ASS_Func004Func003Func001C takes nothing returns boolean
if(not(udg_Random_Good_Roll==udg_List[GetForLoopIndexA()]))then
return false
endif
return true
endfunction
function Trig_RANDOM_ASS_Func004Func004C takes nothing returns boolean
if(not(udg_Good_Hero_Is_Available==true))then
return false
endif
return true
endfunction
function Trig_RANDOM_ASS_Actions takes nothing returns nothing
set udg_Buying_Good_Player=GetOwningPlayer(GetBuyingUnit())
call UnitApplyTimedLifeBJ(2.00,'BTLF',GetSoldUnit())
set bj_forLoopAIndex=0
set bj_forLoopAIndexEnd=1000
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set udg_Good_Hero_Is_Available=true
set udg_Random_Good_Roll=GetRandomInt(1,11)
set bj_forLoopAIndex=0
set bj_forLoopAIndexEnd=(udg_List_Size-1)
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
if(Trig_RANDOM_ASS_Func004Func003Func001C())then
set udg_Good_Hero_Is_Available=false
else
endif
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
if(Trig_RANDOM_ASS_Func004Func004C())then
call IssueNeutralImmediateOrderById(udg_Buying_Good_Player,gg_unit_n001_0029,udg_Random_Good_Pool[udg_Random_Good_Roll])
return
else
endif
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endfunction
