call ForceRemovePlayerSimple(udg_Scar_Player,udg_Alchemists)
else
call ForceRemovePlayerSimple(udg_Scar_Player,udg_Neutral)
endif
else
endif
if(Trig_Player_9_Leaves_Func009C())then
set udg_Player_Left=udg_Slicer_Unit
call EnableTrigger(gg_trg_Player_Leave_Items)
call TriggerSleepAction(1.00)
call RemoveUnit(udg_Slicer_Unit)
call ForceRemovePlayerSimple(udg_Slicer_Player,udg_Neutral)
else
endif
if(Trig_Player_9_Leaves_Func010C())then
if(Trig_Player_9_Leaves_Func010Func001C())then
call TriggerExecute(gg_trg_Ling_Takes_Control)
call ForceRemovePlayerSimple(udg_Greed_Player,udg_Neutral)
call ForceRemovePlayerSimple(udg_Greed_Player,udg_Homunculi)
else
set udg_Player_Left=udg_Greed
call EnableTrigger(gg_trg_Player_Leave_Items)
call TriggerSleepAction(1.00)
call RemoveUnit(udg_Greed)
call ForceRemovePlayerSimple(udg_Greed_Player,udg_Neutral)
endif
else
endif
endfunction
