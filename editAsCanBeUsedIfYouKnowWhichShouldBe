function Trig_Kills_a_Homunculus_Func005001 takes nothing returns boolean
return(GetUnitTypeId(GetDyingUnit())=='H01K')
endfunction
function Trig_Kills_a_Homunculus_Func005002001 takes nothing returns boolean
return(GetUnitTypeId(GetDyingUnit())=='H003')
endfunction
function Trig_Kills_a_Homunculus_Func005002002001 takes nothing returns boolean
return(GetUnitTypeId(GetDyingUnit())=='H004')
endfunction
function Trig_Kills_a_Homunculus_Func005002002002001 takes nothing returns boolean
return(GetUnitTypeId(GetDyingUnit())=='H032')
endfunction
function Trig_Kills_a_Homunculus_Func005002002002002 takes nothing returns boolean
return(GetUnitTypeId(GetDyingUnit())=='H031')
endfunction
function Trig_Kills_a_Homunculus_Func005002002002 takes nothing returns boolean
return GetBooleanOr((GetUnitTypeId(GetDyingUnit())=='H032'),(GetUnitTypeId(GetDyingUnit())=='H031'))
endfunction
function Trig_Kills_a_Homunculus_Func005002002 takes nothing returns boolean
return GetBooleanOr((GetUnitTypeId(GetDyingUnit())=='H004'),(GetBooleanOr((GetUnitTypeId(GetDyingUnit())=='H032'),(GetUnitTypeId(GetDyingUnit())=='H031'))))
endfunction
function Trig_Kills_a_Homunculus_Func005002 takes nothing returns boolean
return GetBooleanOr((GetUnitTypeId(GetDyingUnit())=='H003'),(GetBooleanOr((GetUnitTypeId(GetDyingUnit())=='H004'),(GetBooleanOr((GetUnitTypeId(GetDyingUnit())=='H032'),(GetUnitTypeId(GetDyingUnit())=='H031'))))))
endfunction
function Trig_Kills_a_Homunculus_Conditions takes nothing returns boolean
if(not GetBooleanOr((GetUnitTypeId(GetDyingUnit())=='H01K'),(GetBooleanOr((GetUnitTypeId(GetDyingUnit())=='H003'),(GetBooleanOr((GetUnitTypeId(GetDyingUnit())=='H004'),(GetBooleanOr((GetUnitTypeId(GetDyingUnit())=='H032'),(GetUnitTypeId(GetDyingUnit())=='H031')))))))))then
return false
endif
if(not(GetUnitTypeId(GetKillingUnitBJ())=='H018'))then
return false
endif
return true
endfunction
function Trig_Kills_a_Homunculus_Actions takes nothing returns nothing
call ModifyHeroStat(bj_HEROSTAT_STR,GetKillingUnitBJ(),bj_MODIFYMETHOD_ADD,25)
call ModifyHeroStat(bj_HEROSTAT_AGI,GetKillingUnitBJ(),bj_MODIFYMETHOD_ADD,25)
call ModifyHeroStat(bj_HEROSTAT_INT,GetKillingUnitBJ(),bj_MODIFYMETHOD_ADD,25)
call DisplayTextToForce(GetPlayersAll(),"Scar has slain a Homunculus!")
endfunction
function InitTrig_Kills_a_Homunculus takes nothing returns nothing
set gg_trg_Kills_a_Homunculus=CreateTrigger()
call DisableTrigger(gg_trg_Kills_a_Homunculus)
call TriggerRegisterAnyUnitEventBJ(gg_trg_Kills_a_Homunculus,EVENT_PLAYER_UNIT_DEATH)
call TriggerAddCondition(gg_trg_Kills_a_Homunculus,Condition(function Trig_Kills_a_Homunculus_Conditions))
call TriggerAddAction(gg_trg_Kills_a_Homunculus,function Trig_Kills_a_Homunculus_Actions)
endfunction
