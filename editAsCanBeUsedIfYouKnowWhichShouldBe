function Trig_True_Godhood_Actions takes nothing returns nothing
call DisableTrigger(gg_trg_Open_the_Gate)
call DisableTrigger(gg_trg_Gathering_Souls)
call DestroyTimerDialog(udg_Father_Window)
set udg_Becoming_Father=udg_Our_Father
set udg_God_Father_Position=GetUnitLoc(udg_Our_Father)
call PauseAllUnitsBJ(true)
call CreateNUnitsAtLoc(1,'h00T',Player(7),GetRectCenter(gg_rct_Our_Father),bj_UNIT_FACING)
call UnitApplyTimedLifeBJ(14.00,'BTLF',GetLastCreatedUnit())
call TerrainDeformationRippleBJ(10.00,false,GetUnitLoc(udg_Our_Father),1024,1024,64,1,512)
call DisplayTimedTextToForce(GetPlayersAll(),6.00,"|cffff0000Father:|r God!! Respond to my soul! Come! That's right, come! I shall no longer be chained down by you! I shall drag you down to earth and make you part of me! ")
call TriggerSleepAction(7.00)
call CreateNUnitsAtLoc(1,'h04F',Player(PLAYER_NEUTRAL_PASSIVE),udg_God_Father_Position,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ(2.00,'BTLF',GetLastCreatedUnit())
set udg_Our_Father=gg_unit_H04N_0132
call EnableTrigger(gg_trg_Normal_to_Father)
call TriggerSleepAction(2.00)
call RemoveUnit(udg_Becoming_Father)
call ShowUnitShow(gg_unit_H04N_0132)
call DisplayTextToForce(GetPlayersAll(),"|cff995500Father:|r A job well done, my dear sacrifices.")
call TriggerSleepAction(2.00)
call PauseAllUnitsBJ(false)
call SetUnitInvulnerable(gg_unit_H04N_0132,false)
call UnitAddAbilityBJ('A021',gg_unit_H04N_0132)
if(Trig_True_Godhood_Func024C())then
call UnitAddAbilityBJ('A086',udg_Our_Father)
else
endif
call TriggerSleepAction(80.00)
if(Trig_True_Godhood_Func026C())then
call CreateNUnitsAtLoc(1,'H043',Player(7),GetRectCenter(gg_rct_Rect_166),bj_UNIT_FACING)
set udg_Hohenheim_Unit=GetLastCreatedUnit()
call DisplayTextToForce(GetPlayersAll(),"With |cffff0000Father's|r ascent to Godhood |cffff0000Van Hohenheim|r has arrived to try and stop him!")
call EnableTrigger(gg_trg_Hohenheims_Pathing)
call DisableTrigger(gg_trg_Hohenheim_Selection)
call SetUnitInvulnerable(udg_Hohenheim_Unit,true)
call SetHeroLevelBJ(udg_Hohenheim_Unit,35,false)
else
endif
call RemoveLocation(udg_God_Father_Position)
call DestroyTrigger(GetTriggeringTrigger())
endfunction
