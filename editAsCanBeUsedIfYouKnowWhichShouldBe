else
if(Trig_May_Chang_Dies_Func002Func001C())then
call ForceAddPlayerSimple(udg_May_Chang_Player,udg_Good_Team)
call ForceAddPlayerSimple(udg_May_Chang_Player,udg_Alchemists)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_May_Chang_Player),udg_Good_Team,bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_May_Chang_Player),bj_ALLIANCE_ALLIED)
call SetPlayerAllianceStateBJ(udg_May_Chang_Player,udg_Scar_Player,bj_ALLIANCE_UNALLIED)
call SetPlayerAllianceStateBJ(udg_Scar_Player,udg_May_Chang_Player,bj_ALLIANCE_UNALLIED)
call TriggerSleepAction(10.00)
call SetPlayerAllianceStateBJ(Player(7),udg_May_Chang_Player,bj_ALLIANCE_ALLIED_VISION)
call ReviveHeroLoc(udg_May_Chang,GetUnitLoc(gg_unit_H01V_0080),false)
call DisplayTextToForce(GetPlayersAll(),"|cffff0000May Chang|r has been wounded and found by the Alchemists! She thanks them for saving her life by joining their cause!")
call PanCameraToTimedLocForPlayer(GetOwningPlayer(GetDyingUnit()),GetUnitLoc(GetDyingUnit()),0)
call SelectUnitForPlayerSingle(GetDyingUnit(),udg_May_Chang_Player)
call ModifyHeroStat(bj_HEROSTAT_STR,udg_May_Chang,bj_MODIFYMETHOD_SUB,10)
call ModifyHeroStat(bj_HEROSTAT_AGI,udg_May_Chang,bj_MODIFYMETHOD_SUB,10)
call ModifyHeroStat(bj_HEROSTAT_INT,udg_May_Chang,bj_MODIFYMETHOD_SUB,10)
else
call DisableTrigger(gg_trg_Stop_it_pls)
call DisplayTextToForce(GetPlayersAll(),"|cffff0000May Chang has been killed, she can no longer heal her friends!|r")
call SetPlayerState(udg_May_Chang_Player,PLAYER_STATE_OBSERVER,1)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_May_Chang_Player),bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_May_Chang_Player),udg_Good_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_May_Chang_Player),bj_ALLIANCE_ALLIED_VISION)
if(Trig_May_Chang_Dies_Func002Func001Func023C())then
call ForceRemovePlayerSimple(udg_May_Chang_Player,udg_Alchemists)
call ForceAddPlayerSimple(udg_May_Chang_Player,udg_Dead_Good_Guys)
call GroupAddUnitSimple(udg_May_Chang,udg_Dead_Heroes)
if(Trig_May_Chang_Dies_Func002Func001Func023Func004C())then
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Father|r has eradicated all those who stood before his plans. The Promised Day arrives and a dark cloud falls over Amestris...")
call TriggerSleepAction(10.00)
call CustomDefeatBJ(Player(0),"Father has won!")
call CustomDefeatBJ(Player(1),"Father has won!")
call CustomDefeatBJ(Player(2),"Father has won!")
call CustomVictoryBJ(Player(3),true,true)
call CustomDefeatBJ(Player(4),"Father has won!")
call CustomDefeatBJ(Player(5),"Father has won!")
call CustomDefeatBJ(Player(6),"Father has won!")
call CustomDefeatBJ(Player(8),"Father has won!")
call CustomDefeatBJ(Player(9),"Father has won!")
call CustomVictoryBJ(Player(10),true,true)
call CustomVictoryBJ(Player(11),true,true)
else
endif
if(Trig_May_Chang_Dies_Func002Func001Func023Func005C())then
call CustomDefeatBJ(Player(0),"You have lost!")
call CustomDefeatBJ(Player(1),"You have lost!")
call CustomDefeatBJ(Player(2),"You have lost!")
