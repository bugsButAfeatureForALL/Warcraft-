function Trig_Yok_Island_Spawns_Actions takes nothing returns nothing
call CreateNUnitsAtLoc(1,'h039',Player(PLAYER_NEUTRAL_AGGRESSIVE),GetRectCenter(gg_rct_Yok_Spawn_1),bj_UNIT_FACING)
set udg_Yok_Wolves[0]=GetLastCreatedUnit()
call IssuePointOrderLoc(udg_Yok_Wolves[0],"attack",GetRectCenter(gg_rct_Enter_Yok_Island))
call TriggerSleepAction(0.20)
call CreateNUnitsAtLoc(1,'h039',Player(PLAYER_NEUTRAL_AGGRESSIVE),GetRectCenter(gg_rct_Yok_Spawn_2),bj_UNIT_FACING)
set udg_Yok_Wolves[1]=GetLastCreatedUnit()
call IssuePointOrderLoc(udg_Yok_Wolves[1],"attack",GetRectCenter(gg_rct_Enter_Yok_Island))
call TriggerSleepAction(0.20)
call CreateNUnitsAtLoc(1,'h039',Player(PLAYER_NEUTRAL_AGGRESSIVE),GetRectCenter(gg_rct_Yok_Spawn_3),bj_UNIT_FACING)
set udg_Yok_Wolves[2]=GetLastCreatedUnit()
call IssuePointOrderLoc(udg_Yok_Wolves[2],"attack",GetRectCenter(gg_rct_Enter_Yok_Island))
call TriggerSleepAction(0.20)
call CreateNUnitsAtLoc(1,'h039',Player(PLAYER_NEUTRAL_AGGRESSIVE),GetRectCenter(gg_rct_Yok_Spawn_4),bj_UNIT_FACING)
set udg_Yok_Wolves[3]=GetLastCreatedUnit()
call IssuePointOrderLoc(udg_Yok_Wolves[3],"attack",GetRectCenter(gg_rct_Enter_Yok_Island))
call TriggerSleepAction(0.20)
call CreateNUnitsAtLoc(1,'h039',Player(PLAYER_NEUTRAL_AGGRESSIVE),GetRectCenter(gg_rct_Yok_Spawn_5),bj_UNIT_FACING)
set udg_Yok_Wolves[4]=GetLastCreatedUnit()
call TriggerSleepAction(0.20)
call IssuePointOrderLoc(udg_Yok_Wolves[4],"attack",GetRectCenter(gg_rct_Enter_Yok_Island))
call UnitApplyTimedLifeBJ(60,'BTLF',udg_Yok_Wolves[0])
call UnitApplyTimedLifeBJ(60,'BTLF',udg_Yok_Wolves[1])
call UnitApplyTimedLifeBJ(60,'BTLF',udg_Yok_Wolves[2])
call UnitApplyTimedLifeBJ(60,'BTLF',udg_Yok_Wolves[3])
call UnitApplyTimedLifeBJ(60,'BTLF',udg_Yok_Wolves[4])
endfunction
