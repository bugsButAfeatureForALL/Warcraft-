endif
return true
endfunction
function Trig_Escape_from_the_Belly_Actions takes nothing returns nothing
set udg_Gluttony_Location=GetUnitLoc(udg_Gluttony_Unit)
if(Trig_Escape_from_the_Belly_Func002C())then
call CreateNUnitsAtLoc(1,'h04U',udg_Gluttony_Player,udg_Gluttony_Location,bj_UNIT_FACING)
set udg_Gluttony_Stun_Dummy=GetLastCreatedUnit()
call IssueTargetOrder(udg_Gluttony_Stun_Dummy,"firebolt",udg_Gluttony_Unit)
call UnitApplyTimedLifeBJ(2.00,'BTLF',udg_Gluttony_Stun_Dummy)
else
endif
call EnumItemsInRectBJ(gg_rct_Inside_Gluttony,function Trig_Escape_from_the_Belly_Func004002)
if(Trig_Escape_from_the_Belly_Func005C())then
call ForGroupBJ(GetUnitsInRectAll(gg_rct_Inside_Gluttony),function Trig_Escape_from_the_Belly_Func005Func002A)
else
call ForGroupBJ(GetUnitsInRectAll(gg_rct_Inside_Gluttony),function Trig_Escape_from_the_Belly_Func005Func001A)
endif
call RemoveLocation(udg_Gluttony_Location)
endfunction
