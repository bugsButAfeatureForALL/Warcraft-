endfunction
function Trig_Leave_Lair_Father_Func001Func003001 takes nothing returns boolean
return(GetUnitTypeId(GetTriggerUnit())=='H03B')
endfunction
function Trig_Leave_Lair_Father_Func001Func003002 takes nothing returns boolean
return(GetUnitTypeId(GetTriggerUnit())=='H01T')
endfunction
function Trig_Leave_Lair_Father_Func001C takes nothing returns boolean
if(not GetBooleanOr((GetUnitTypeId(GetTriggerUnit())=='H03B'),(GetUnitTypeId(GetTriggerUnit())=='H01T')))then
return false
endif
return true
endfunction
function Trig_Leave_Lair_Father_Actions takes nothing returns nothing
if(Trig_Leave_Lair_Father_Func001C())then
call DisplayTextToForce(GetForceOfPlayer(udg_Father_Player),"I can not leave! I must stay in the center of the transmutation circle!")
call SetUnitPositionLoc(GetTriggerUnit(),GetRectCenter(gg_rct_Enter_Lair_2))
else
call SetUnitPositionLoc(GetTriggerUnit(),GetRectCenter(gg_rct_Leave_Lair_2))
call PanCameraToTimedLocForPlayer(GetOwningPlayer(GetTriggerUnit()),GetUnitLoc(GetTriggerUnit()),0)
endif
endfunction
