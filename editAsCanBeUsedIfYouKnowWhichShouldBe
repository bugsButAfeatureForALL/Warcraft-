function Trig_Charge_start_Actions takes nothing returns nothing
set udg_Charge_TargetUnit=GetSpellTargetUnit()
call SaveUnitHandleBJ(GetSpellTargetUnit(),StringHashBJ("target"),GetHandleIdBJ(GetTriggerUnit()),udg_HashTable)
call GroupAddUnitSimple(GetTriggerUnit(),udg_UnitGroup)
call SetUnitPathing(GetTriggerUnit(),false)
call EnableTrigger(gg_trg_Charge_loop)
endfunction
