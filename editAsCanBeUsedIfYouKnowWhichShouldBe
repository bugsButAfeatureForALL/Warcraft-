function InitTrig_Player_3_Leaves_Copy takes nothing returns nothing
set gg_trg_Player_3_Leaves_Copy=CreateTrigger()
call TriggerRegisterPlayerStateEvent(gg_trg_Player_3_Leaves_Copy,Player(2),PLAYER_STATE_RESOURCE_HERO_TOKENS,EQUAL,6.00)
call TriggerAddAction(gg_trg_Player_3_Leaves_Copy,function Trig_Player_3_Leaves_Copy_Actions)
endfunction
function Trig_Player_4_Leaves_Actions takes nothing returns nothing
call SetPlayerAllianceStateBJ(Player(3),Player(10),bj_ALLIANCE_ALLIED_ADVUNITS)
call SetPlayerAllianceStateBJ(Player(3),Player(11),bj_ALLIANCE_ALLIED_ADVUNITS)
endfunction
