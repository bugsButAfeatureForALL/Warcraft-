function Trig_Flame_Wave2_Actions takes nothing returns nothing
if(Trig_Flame_Wave2_Func001C())then
set udg_Flame_Distance=(udg_Flame_Distance+80.00)
set udg_Count_Fire=(udg_Count_Fire+1)
set udg_FCaster_Position=GetUnitLoc(udg_Flame_Caster)
set udg_Fire_Point=PolarProjectionBJ(udg_FCaster_Position,udg_Flame_Distance,GetUnitFacing(udg_Flame_Caster))
call RemoveLocation(udg_FCaster_Position)
call CreateNUnitsAtLoc(1,'h00E',GetOwningPlayer(udg_Flame_Caster),udg_Fire_Point,bj_UNIT_FACING)
set udg_Flame_Group[udg_Count_Fire]=udg_Fire_Point
call UnitApplyTimedLifeBJ(10.00,'BTLF',GetLastCreatedUnit())
call RemoveLocation(udg_Fire_Point)
else
call DisableTrigger(GetTriggeringTrigger())
set udg_Count_Fire=0
set udg_Flame_Distance=0.00
endif
endfunction
