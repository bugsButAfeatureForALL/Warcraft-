function Trig_Point_Blank_Actions takes nothing returns nothing
set udg_Jean_Location=GetUnitLoc(udg_Jean_Unit)
set udg_Point_Blank_Target_Location=GetUnitLoc(GetAttackedUnitBJ())
set udg_Point_Blank_Damage=(20.00*I2R(GetUnitAbilityLevelSwapped('A09T',udg_Jean_Unit)))
call DisableTrigger(GetTriggeringTrigger())
if(Trig_Point_Blank_Func005C())then
call UnitDamageTargetBJ(udg_Jean_Unit,GetAttackedUnitBJ(),udg_Point_Blank_Damage,ATTACK_TYPE_HERO,DAMAGE_TYPE_NORMAL)
else
endif
call RemoveLocation(udg_Jean_Location)
call RemoveLocation(udg_Point_Blank_Target_Location)
call TriggerSleepAction(1.00)
call EnableTrigger(GetTriggeringTrigger())
endfunction
