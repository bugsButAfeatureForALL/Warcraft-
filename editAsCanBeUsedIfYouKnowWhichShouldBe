function Trig_Philosophers_Transmutation_Cast_Func001C takes nothing returns boolean
if(not(udg_Father_Is_Picked==true))then
return false
endif
return true
endfunction
function Trig_Philosophers_Transmutation_Cast_Func015C takes nothing returns boolean
if(not(udg_Philosophers_Clash==false))then
return false
endif
return true
endfunction
function Trig_Philosophers_Transmutation_Cast_Actions takes nothing returns nothing
if(Trig_Philosophers_Transmutation_Cast_Func001C())then
call EnableTrigger(gg_trg_Clash_of_Philosophy)
else
endif
set udg_Philosopher_Effect_Real=45.00
set udg_Philosopher_Trans_Position=PolarProjectionBJ(GetUnitLoc(udg_Hohenheim_Unit),85.00,udg_Philosopher_Effect_Real)
call CreateNUnitsAtLoc(1,'h044',udg_Hohenheim_Player,GetUnitLoc(udg_Hohenheim_Unit),bj_UNIT_FACING)
set udg_Philosophers_Transmutation_Dum[0]=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(10.20,'BTLF',udg_Philosophers_Transmutation_Dum[0])
call SetUnitInvulnerable(udg_Hohenheim_Unit,true)
call PauseUnitBJ(true,udg_Hohenheim_Unit)
set bj_forLoopAIndex=0
set bj_forLoopAIndexEnd=3
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call CreateNUnitsAtLoc(1,'h046',udg_Hohenheim_Player,udg_Philosopher_Trans_Position,AngleBetweenPoints(udg_Philosopher_Trans_Position,GetUnitLoc(udg_Hohenheim_Unit)))
set udg_Philosophers_Transmutation_Dum[1]=GetLastCreatedUnit()
call SetUnitTimeScalePercent(udg_Philosophers_Transmutation_Dum[1],9.00)
call UnitApplyTimedLifeBJ(10.20,'BTLF',udg_Philosophers_Transmutation_Dum[1])
set udg_Philosopher_Effect_Real=(udg_Philosopher_Effect_Real+90.00)
set udg_Philosopher_Trans_Position=PolarProjectionBJ(GetUnitLoc(udg_Hohenheim_Unit),85.00,udg_Philosopher_Effect_Real)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
call TriggerSleepAction(4.60)
call EnableTrigger(gg_trg_Philosophers_Damage)
call TriggerSleepAction(5.60)
call DisableTrigger(gg_trg_Philosophers_Damage)
call DisableTrigger(gg_trg_Clash_of_Philosophy)
if(Trig_Philosophers_Transmutation_Cast_Func015C())then
call SetUnitInvulnerable(udg_Hohenheim_Unit,false)
call PauseUnitBJ(false,udg_Hohenheim_Unit)
else
endif
endfunction
function InitTrig_Philosophers_Transmutation_Cast takes nothing returns nothing
set gg_trg_Philosophers_Transmutation_Cast=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Philosophers_Transmutation_Cast,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Philosophers_Transmutation_Cast,Condition(function Trig_Philosophers_Transmutation_Cast_Conditions))
