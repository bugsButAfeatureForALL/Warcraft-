function Trig_Roa_Stun_Chance_Actions takes nothing returns nothing
set udg_Roa_Stun_Chance=GetRandomInt(1,20)
if(Trig_Roa_Stun_Chance_Func002C())then
call CreateNUnitsAtLoc(1,'h03P',udg_Roa_Player,GetUnitLoc(GetAttackedUnitBJ()),bj_UNIT_FACING)
set udg_Roa_Stomp=GetLastCreatedUnit()
call SetUnitAbilityLevelSwapped('A071',udg_Roa_Stomp,GetUnitAbilityLevelSwapped('A070',udg_Roa_Unit))
call UnitApplyTimedLifeBJ(2.00,'BTLF',udg_Roa_Stomp)
call IssueImmediateOrder(udg_Roa_Stomp,"stomp")
else
endif
endfunction
