function Trig_Olivier_Death_Actions takes nothing returns nothing
call TriggerSleepAction(10.00)
if(Trig_Olivier_Death_Func002C())then
call DisableTrigger(gg_trg_Icey)
call ReviveHeroLoc(udg_Olivier_Unit,GetUnitLoc(gg_unit_H01V_0080),false)
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Winry|r has used her medical knowledge to revive a member of the Alchemist team!")
call SetHeroLevelBJ(GetTriggerUnit(),(GetHeroLevel(GetDyingUnit())-3),false)
call PanCameraToTimedLocForPlayer(GetOwningPlayer(GetDyingUnit()),GetUnitLoc(GetDyingUnit()),0)
call SelectUnitForPlayerSingle(GetDyingUnit(),udg_Olivier_Player)
else
call GroupAddUnitSimple(udg_Olivier_Unit,udg_Dead_Heroes)
call ForceRemovePlayerSimple(udg_Olivier_Player,udg_Alchemists)
call SetPlayerState(udg_Olivier_Player,PLAYER_STATE_OBSERVER,1)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Olivier_Player),bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Olivier_Player),udg_Good_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Olivier_Player),bj_ALLIANCE_ALLIED_VISION)
call ForceAddPlayerSimple(udg_Olivier_Player,udg_Dead_Good_Guys)
endif
