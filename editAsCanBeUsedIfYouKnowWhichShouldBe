function Trig_Unleashed_is_preformed_Actions takes nothing returns nothing
call EnableTrigger(gg_trg_Unleashed_Envy_Dies)
if(Trig_Unleashed_is_preformed_Func002C())then
call StartTimerBJ(udg_Unleashed_Envy_Timer,false,33.00)
else
if(Trig_Unleashed_is_preformed_Func002Func003C())then
call StartTimerBJ(udg_Unleashed_Envy_Timer,false,47.00)
else
call StartTimerBJ(udg_Unleashed_Envy_Timer,false,63.00)
endif
endif
endfunction
