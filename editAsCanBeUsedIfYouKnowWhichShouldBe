function Trig_Gods_Wrath_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
call TriggerSleepAction(0.40)
call IssueImmediateOrder(udg_Our_Father,"thunderclap")
call TriggerSleepAction(0.40)
call SetUnitInvulnerable(udg_Our_Father,true)
call PauseUnitBJ(true,udg_Our_Father)
call UnitAddAbilityBJ('A09G',udg_Our_Father)
call DisplayTimedTextToForce(GetPlayersAll(),5.00,"|cff995500Father:|r Do you fools really not comprehend the power I have?")
call TriggerSleepAction(5.00)
call PauseUnitBJ(false,udg_Our_Father)
call UnitRemoveAbilityBJ('A09G',udg_Our_Father)
call SetUnitInvulnerable(udg_Our_Father,false)
endfunction
