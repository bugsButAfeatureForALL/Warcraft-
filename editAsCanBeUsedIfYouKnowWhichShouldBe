function Trig_Strength_Cast_Actions takes nothing returns nothing
set udg_Strength_Point=GetUnitLoc(GetSpellAbilityUnit())
set udg_Position_To_Cast=PolarProjectionBJ(udg_Strength_Point,25.00,(GetUnitFacing(GetSpellAbilityUnit())-45.00))
call CreateNUnitsAtLoc(1,'h04A',udg_Darius_Player,PolarProjectionBJ(GetUnitLoc(udg_Darius_Unit),0.00,0),GetUnitFacing(udg_Darius_Unit))
set udg_Gorrila_Strength_Dummy[1]=GetLastCreatedUnit()
call SetUnitAbilityLevelSwapped('A08R',udg_Gorrila_Strength_Dummy[1],GetUnitAbilityLevelSwapped('A08Q',udg_Darius_Unit))
call CreateNUnitsAtLoc(1,'h04A',udg_Darius_Player,PolarProjectionBJ(GetUnitLoc(udg_Darius_Unit),0.00,0),GetUnitFacing(udg_Darius_Unit))
set udg_Gorrila_Strength_Dummy[2]=GetLastCreatedUnit()
call SetUnitAbilityLevelSwapped('A08R',udg_Gorrila_Strength_Dummy[2],GetUnitAbilityLevelSwapped('A08Q',udg_Darius_Unit))
call UnitApplyTimedLifeBJ(5.00,'BTLF',udg_Gorrila_Strength_Dummy[1])
call UnitApplyTimedLifeBJ(5.00,'BTLF',udg_Gorrila_Strength_Dummy[2])
set udg_Position_To_Cast=PolarProjectionBJ(GetUnitLoc(GetSpellAbilityUnit()),200.00,(GetUnitFacing(GetSpellAbilityUnit())-45.00))
call IssuePointOrderLoc(udg_Gorrila_Strength_Dummy[1],"impale",udg_Position_To_Cast)
set udg_Position_To_Cast=PolarProjectionBJ(GetUnitLoc(GetSpellAbilityUnit()),200.00,(GetUnitFacing(GetSpellAbilityUnit())+45.00))
call IssuePointOrderLoc(udg_Gorrila_Strength_Dummy[2],"impale",udg_Position_To_Cast)
endfunction
