function Trig_Leave_Lair_Actions takes nothing returns nothing
if(Trig_Leave_Lair_Func001C())then
call SetUnitPositionLoc(GetTriggerUnit(),GetRectCenter(gg_rct_Our_Father))
else
call SetUnitPositionLoc(GetTriggerUnit(),GetRectCenter(gg_rct_Leave_Lair_2))
call PanCameraToTimedLocForPlayer(GetOwningPlayer(GetTriggerUnit()),GetUnitLoc(GetTriggerUnit()),0)
endif
endfunction
