if(not(udg_Greed_Good==true))then
return false
endif
return true
endfunction
function Trig_GreedLing_Dies_Actions takes nothing returns nothing
call DisplayTextToForce(GetPlayersAll(),"Ling Yao's body has been destroyed! Both him and Greed are gone for good!")
call SetPlayerState(udg_Ling_Player,PLAYER_STATE_OBSERVER,1)
call SetPlayerState(udg_Greed_Player,PLAYER_STATE_OBSERVER,1)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Ling_Player),bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Ling_Player),bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Greed_Player),bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Greed_Player),bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Ling_Player),GetPlayersAll(),bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Greed_Player),GetPlayersAll(),bj_ALLIANCE_UNALLIED)
if(Trig_GreedLing_Dies_Func010C())then
call ForceRemovePlayerSimple(udg_Greed_Player,udg_Alchemists)
call ForceRemovePlayerSimple(udg_Ling_Player,udg_Alchemists)
if(Trig_GreedLing_Dies_Func010Func003C())then
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Father|r has eradicated all those who stood before his plans. The Promised Day arrives and a dark cloud falls over Amestris...")
call TriggerSleepAction(10.00)
call CustomDefeatBJ(Player(0),"Father has won!")
call CustomDefeatBJ(Player(1),"Father has won!")
call CustomDefeatBJ(Player(2),"Father has won!")
call CustomVictoryBJ(Player(3),true,true)
call CustomDefeatBJ(Player(4),"Father has won!")
call CustomDefeatBJ(Player(5),"Father has won!")
call CustomDefeatBJ(Player(6),"Father has won!")
call CustomDefeatBJ(Player(8),"Father has won!")
call CustomDefeatBJ(Player(9),"Father has won!")
call CustomVictoryBJ(Player(10),true,true)
call CustomVictoryBJ(Player(11),true,true)
else
endif
else
endif
endfunction
function InitTrig_GreedLing_Dies takes nothing returns nothing
set gg_trg_GreedLing_Dies=CreateTrigger()
