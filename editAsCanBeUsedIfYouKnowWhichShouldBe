function Trig_Shard_Ping_Actions takes nothing returns nothing
if(Trig_Shard_Ping_Func001C())then
call PingMinimapLocForForceEx(GetPlayersAll(),GetItemLoc(udg_First_Shard),5.00,bj_MINIMAPPINGSTYLE_SIMPLE,100,35.00,35.00)
else
endif
if(Trig_Shard_Ping_Func002C())then
call PingMinimapLocForForceEx(GetPlayersAll(),GetItemLoc(udg_Second_Shard),5.00,bj_MINIMAPPINGSTYLE_SIMPLE,100,35.00,35.00)
else
endif
if(Trig_Shard_Ping_Func003C())then
call PingMinimapLocForForceEx(GetPlayersAll(),GetItemLoc(udg_Third_Shard),5.00,bj_MINIMAPPINGSTYLE_SIMPLE,100,35.00,35.00)
else
endif
if(Trig_Shard_Ping_Func004C())then
call PingMinimapLocForForceEx(GetPlayersAll(),GetItemLoc(udg_Fourth_Shard),5.00,bj_MINIMAPPINGSTYLE_SIMPLE,100,35.00,35.00)
else
endif
endfunction
