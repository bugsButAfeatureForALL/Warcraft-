function Trig_Charge_loop_Func001Func001Func011C takes nothing returns boolean
if(not(DistanceBetweenPoints(udg_Points[1],udg_Points[2])<=50.00))then
return false
endif
return true
endfunction
function Trig_Charge_loop_Func001Func001A takes nothing returns nothing
set udg_Points[1]=GetUnitLoc(GetEnumUnit())
set udg_Points[2]=GetUnitLoc(LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_HashTable))
set udg_Points[3]=PolarProjectionBJ(udg_Points[1],15.00,AngleBetweenPoints(udg_Points[1],udg_Points[2]))
call SetUnitPositionLocFacingLocBJ(GetEnumUnit(),udg_Points[3],udg_Points[2])
set udg_Charge_Picked_Location=GetUnitLoc(GetEnumUnit())
call AddSpecialEffectLocBJ(udg_Charge_Picked_Location,"Abilities\\Spells\\Human\\Thunderclap\\ThunderClapCaster.mdl")
call RemoveLocation(udg_Charge_Picked_Location)
call DestroyEffect(GetLastCreatedEffectBJ())
call AddSpecialEffectTargetUnitBJ("chest",GetEnumUnit(),"Objects\\Spawnmodels\\Undead\\ImpaleTargetDust\\ImpaleTargetDust.mdl")
call DestroyEffect(GetLastCreatedEffectBJ())
if(Trig_Charge_loop_Func001Func001Func011C())then
call GroupRemoveUnitSimple(GetEnumUnit(),udg_UnitGroup)
call SetUnitPathing(GetEnumUnit(),true)
call SetUnitTimeScalePercent(GetEnumUnit(),100.00)
call SetUnitAnimation(GetEnumUnit(),"Walk")
if(Trig_Charge_loop_Func001Func001Func011Func006C())then
call UnitDamageTargetBJ(GetEnumUnit(),LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_HashTable),300.00,ATTACK_TYPE_MAGIC,DAMAGE_TYPE_NORMAL)
else
endif
if(Trig_Charge_loop_Func001Func001Func011Func007C())then
call UnitDamageTargetBJ(GetEnumUnit(),LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_HashTable),350.00,ATTACK_TYPE_MAGIC,DAMAGE_TYPE_NORMAL)
else
endif
if(Trig_Charge_loop_Func001Func001Func011Func008C())then
call UnitDamageTargetBJ(GetEnumUnit(),LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_HashTable),400.00,ATTACK_TYPE_MAGIC,DAMAGE_TYPE_NORMAL)
else
endif
if(Trig_Charge_loop_Func001Func001Func011Func009C())then
call UnitDamageTargetBJ(GetEnumUnit(),LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_HashTable),450.00,ATTACK_TYPE_MAGIC,DAMAGE_TYPE_NORMAL)
else
endif
if(Trig_Charge_loop_Func001Func001Func011Func010C())then
call UnitDamageTargetBJ(GetEnumUnit(),LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_HashTable),500.00,ATTACK_TYPE_MAGIC,DAMAGE_TYPE_NORMAL)
else
endif
call AddSpecialEffectTargetUnitBJ("chest",LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_HashTable),"Objects\\Spawnmodels\\Human\\HumanBlood\\BloodElfSpellThiefBlood.mdl")
call DestroyEffect(GetLastCreatedEffectBJ())
call RemoveLocation(udg_Points[1])
call RemoveLocation(udg_Points[2])
call RemoveLocation(udg_Points[3])
call FlushChildHashtableBJ(GetHandleIdBJ(GetEnumUnit()),udg_HashTable)
else
call ForGroupBJ(udg_UnitGroup,function Trig_Charge_loop_Func001Func001Func011Func001A)
endif
endfunction
function Trig_Charge_loop_Func001C takes nothing returns boolean
if(not(IsUnitGroupEmptyBJ(udg_UnitGroup)==true))then
return false
endif
return true
endfunction
function Trig_Charge_loop_Actions takes nothing returns nothing
if(Trig_Charge_loop_Func001C())then
call DisableTrigger(GetTriggeringTrigger())
else
call ForGroupBJ(udg_UnitGroup,function Trig_Charge_loop_Func001Func001A)
endif
endfunction
function InitTrig_Charge_loop takes nothing returns nothing
set gg_trg_Charge_loop=CreateTrigger()
call DisableTrigger(gg_trg_Charge_loop)
call TriggerRegisterTimerEventPeriodic(gg_trg_Charge_loop,0.03)
call TriggerAddAction(gg_trg_Charge_loop,function Trig_Charge_loop_Actions)
endfunction
function Trig_Alchemic_Shatter_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A00B'))then
return false
endif
return true
endfunction
function Trig_Alchemic_Shatter_Func004C takes nothing returns boolean
return true
endfunction
function Trig_Alchemic_Shatter_Actions takes nothing returns nothing
set udg_Alchemic_Shatter_Point1=GetUnitLoc(udg_Armstrong_Unit)
set udg_Rock_Degrees=0.00
if((true))then
set udg_Counter_AlchemicArray=0
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=25
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set udg_Position_Spells[4]=PolarProjectionBJ(udg_Alchemic_Shatter_Point1,400.00,udg_Rock_Degrees)
call CreateNUnitsAtLoc(1,'h00B',udg_Armstrong_Player,udg_Position_Spells[4],bj_UNIT_FACING)
set udg_AlchemicShatter_RockArray[udg_Counter_AlchemicArray]=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(5.00,'BTLF',udg_AlchemicShatter_RockArray[udg_Counter_AlchemicArray])
call CreateNUnitsAtLoc(1,'h011',Player(PLAYER_NEUTRAL_PASSIVE),udg_Position_Spells[4],bj_UNIT_FACING)
call UnitApplyTimedLifeBJ(1.00,'BTLF',GetLastCreatedUnit())
call SetUnitPathing(udg_AlchemicShatter_RockArray[udg_Counter_AlchemicArray],false)
set udg_Counter_AlchemicArray=(udg_Counter_AlchemicArray+1)
call RemoveLocation(udg_Position_Spells[4])
set udg_Position_Spells[4]=PolarProjectionBJ(udg_Alchemic_Shatter_Point1,600.00,udg_Rock_Degrees)
call CreateNUnitsAtLoc(1,'h00B',Player(PLAYER_NEUTRAL_PASSIVE),udg_Position_Spells[4],bj_UNIT_FACING)
set udg_AlchemicShatter_RockArray[udg_Counter_AlchemicArray]=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(5.00,'BTLF',udg_AlchemicShatter_RockArray[udg_Counter_AlchemicArray])
call CreateNUnitsAtLoc(1,'h011',Player(PLAYER_NEUTRAL_PASSIVE),udg_Position_Spells[4],bj_UNIT_FACING)
call UnitApplyTimedLifeBJ(1.00,'BTLF',GetLastCreatedUnit())
call SetUnitPathing(udg_AlchemicShatter_RockArray[udg_Counter_AlchemicArray],false)
set udg_Counter_AlchemicArray=(udg_Counter_AlchemicArray+1)
call RemoveLocation(udg_Position_Spells[4])
set udg_Position_Spells[4]=PolarProjectionBJ(udg_Alchemic_Shatter_Point1,800.00,udg_Rock_Degrees)
call CreateNUnitsAtLoc(1,'h00B',udg_Armstrong_Player,udg_Position_Spells[4],bj_UNIT_FACING)
set udg_AlchemicShatter_RockArray[udg_Counter_AlchemicArray]=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(5.00,'BTLF',udg_AlchemicShatter_RockArray[udg_Counter_AlchemicArray])
call CreateNUnitsAtLoc(1,'h011',Player(PLAYER_NEUTRAL_PASSIVE),udg_Position_Spells[4],bj_UNIT_FACING)
call UnitApplyTimedLifeBJ(1.00,'BTLF',GetLastCreatedUnit())
call SetUnitPathing(udg_AlchemicShatter_RockArray[udg_Counter_AlchemicArray],false)
set udg_Counter_AlchemicArray=(udg_Counter_AlchemicArray+1)
set udg_Rock_Degrees=(udg_Rock_Degrees+14.40)
call RemoveLocation(udg_Position_Spells[4])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set udg_Counter_AlchemicArray=0
set udg_Rock_Degrees=0.00
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=25
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call CreateNUnitsAtLoc(1,'n000',GetOwningPlayer(GetTriggerUnit()),udg_Alchemic_Shatter_Point1,(I2R(GetForLoopIndexA())*18.00))
set udg_Position_Spells[4]=GetUnitLoc(GetLastCreatedUnit())
call UnitApplyTimedLifeBJ(5.00,'BTLF',GetLastCreatedUnit())
call UnitAddAbilityBJ('A01M',GetLastCreatedUnit())
call SetUnitAbilityLevelSwapped('A01M',GetLastCreatedUnit(),GetUnitAbilityLevelSwapped('A00B',GetTriggerUnit()))
call IssuePointOrderLoc(GetLastCreatedUnit(),"impale",udg_Position_Spells[4])
set udg_Alchemic_Shatter_Point2=PolarProjectionBJ(udg_Position_Spells[4],400.00,udg_Rock_Degrees)
call SetUnitPositionLoc(udg_AlchemicShatter_RockArray[udg_Counter_AlchemicArray],udg_Alchemic_Shatter_Point2)
call SetUnitPathing(udg_AlchemicShatter_RockArray[udg_Counter_AlchemicArray],true)
set udg_Counter_AlchemicArray=(udg_Counter_AlchemicArray+1)
call RemoveLocation(udg_Alchemic_Shatter_Point2)
set udg_Alchemic_Shatter_Point2=PolarProjectionBJ(udg_Position_Spells[4],600.00,udg_Rock_Degrees)
call SetUnitPositionLoc(udg_AlchemicShatter_RockArray[udg_Counter_AlchemicArray],udg_Alchemic_Shatter_Point2)
call SetUnitPathing(udg_AlchemicShatter_RockArray[udg_Counter_AlchemicArray],true)
set udg_Counter_AlchemicArray=(udg_Counter_AlchemicArray+1)
call RemoveLocation(udg_Alchemic_Shatter_Point2)
set udg_Alchemic_Shatter_Point2=PolarProjectionBJ(udg_Position_Spells[4],800.00,udg_Rock_Degrees)
call SetUnitPositionLoc(udg_AlchemicShatter_RockArray[udg_Counter_AlchemicArray],udg_Alchemic_Shatter_Point2)
call SetUnitPathing(udg_AlchemicShatter_RockArray[udg_Counter_AlchemicArray],true)
set udg_Counter_AlchemicArray=(udg_Counter_AlchemicArray+1)
set udg_Rock_Degrees=(udg_Rock_Degrees+14.40)
call RemoveLocation(udg_Alchemic_Shatter_Point2)
call RemoveLocation(udg_Position_Spells[4])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set udg_Counter_AlchemicArray=0
else
endif
call EnableTrigger(gg_trg_Alchemic_Shatter_Rock_Raise)
call RemoveLocation(udg_Position_Spells[4])
call RemoveLocation(udg_Alchemic_Shatter_Point1)
endfunction
function InitTrig_Alchemic_Shatter takes nothing returns nothing
set gg_trg_Alchemic_Shatter=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Alchemic_Shatter,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Alchemic_Shatter,Condition(function Trig_Alchemic_Shatter_Conditions))
call TriggerAddAction(gg_trg_Alchemic_Shatter,function Trig_Alchemic_Shatter_Actions)
endfunction
function Trig_Alchemic_Shatter_Rock_Raise_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
call TriggerSleepAction(0.50)
set udg_Counter_AlchemicArray=0
loop
exitwhen udg_Counter_AlchemicArray>76
call SetUnitTimeScalePercent(udg_AlchemicShatter_RockArray[udg_Counter_AlchemicArray],0.00)
set udg_Counter_AlchemicArray=udg_Counter_AlchemicArray+1
endloop
endfunction
