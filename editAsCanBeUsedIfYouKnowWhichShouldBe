function Trig_Ultimate_Spear_Cast_Actions takes nothing returns nothing
set udg_Ultimate_Spear_Target=GetSpellTargetUnit()
set udg_Ultimate_Spear_Caster=GetSpellAbilityUnit()
call TriggerSleepAction(0.10)
call PauseUnitBJ(true,udg_Ultimate_Spear_Caster)
call PauseUnitBJ(true,udg_Ultimate_Spear_Target)
call SetUnitInvulnerable(udg_Ultimate_Spear_Caster,true)
call SetUnitInvulnerable(udg_Ultimate_Spear_Target,true)
call SetUnitPositionLoc(udg_Ultimate_Spear_Caster,GetUnitLoc(udg_Ultimate_Spear_Target))
call SetUnitFacingToFaceUnitTimed(udg_Ultimate_Spear_Caster,udg_Ultimate_Spear_Target,0)
call EnableTrigger(gg_trg_Ultimate_Spear_Effect)
call TriggerSleepAction(4.00)
call DisableTrigger(gg_trg_Ultimate_Spear_Effect)
call SetUnitAnimation(udg_Ultimate_Spear_Caster,"stand")
call EnableTrigger(gg_trg_Ultimate_Spear_Damage)
endfunction
function InitTrig_Ultimate_Spear_Cast takes nothing returns nothing
set gg_trg_Ultimate_Spear_Cast=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Ultimate_Spear_Cast,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Ultimate_Spear_Cast,Condition(function Trig_Ultimate_Spear_Cast_Conditions))
call TriggerAddAction(gg_trg_Ultimate_Spear_Cast,function Trig_Ultimate_Spear_Cast_Actions)
endfunction
function Trig_Ultimate_Spear_Effect_Actions takes nothing returns nothing
call SetUnitAnimation(udg_Ultimate_Spear_Caster,"spell two")
call AddSpecialEffectLocBJ(GetUnitLoc(udg_Ultimate_Spear_Target),"Objects\\Spawnmodels\\Human\\HumanBlood\\BloodElfSpellThiefBlood.mdl")
endfunction
