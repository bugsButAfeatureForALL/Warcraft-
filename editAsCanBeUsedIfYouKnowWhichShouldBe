function Trig_Sinful_Father_AI_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
call TriggerSleepAction(2)
set udg_Father_Attacks=GetRandomInt(1,3)
if(Trig_Sinful_Father_AI_Func005C())then
call IssuePointOrderLoc(udg_Our_Father,"clusterrockets",GetUnitLoc(GetAttacker()))
call TriggerSleepAction(1.45)
call IssueImmediateOrder(udg_Our_Father,"stop")
call EnableTrigger(GetTriggeringTrigger())
else
if(Trig_Sinful_Father_AI_Func005Func006C())then
call IssueTargetOrder(udg_Our_Father,"forkedlightning",GetAttacker())
call TriggerSleepAction(1.00)
call EnableTrigger(GetTriggeringTrigger())
else
if(Trig_Sinful_Father_AI_Func005Func006Func005C())then
call IssueImmediateOrder(udg_Our_Father,"thunderclap")
call TriggerSleepAction(1.00)
call EnableTrigger(GetTriggeringTrigger())
else
endif
endif
endif
endfunction
