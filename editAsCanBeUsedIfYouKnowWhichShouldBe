endfunction
function Trig_Good_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A025'))then
return false
endif
return true
endfunction
function Trig_Good_Func010C takes nothing returns boolean
if(not(IsUnitAliveBJ(udg_Roa_Unit)==true))then
return false
endif
return true
endfunction
function Trig_Good_Func011C takes nothing returns boolean
if(not(IsUnitAliveBJ(udg_Dolcetto_Unit)==true))then
return false
endif
return true
endfunction
function Trig_Good_Actions takes nothing returns nothing
call ForceRemovePlayerSimple(udg_Roa_Player,udg_Neutral)
call ForceRemovePlayerSimple(udg_Dolcetto_Player,udg_Neutral)
call CreateNUnitsAtLoc(1,'h01N',udg_Greed_Player,GetRectCenter(gg_rct_Soul),bj_UNIT_FACING)
set udg_Greed_Soul=GetLastCreatedUnit()
call RemoveUnit(GetTriggerUnit())
set udg_Greed_Good=true
call DisplayTextToForce(GetPlayersAll(),"Greed has sided with Ling Yao's friends and will help destroy the Homunculi!")
call ForceAddPlayerSimple(udg_Greed_Player,udg_Alchemists)
call ForceAddPlayerSimple(udg_Greed_Player,udg_Good_Team)
if(Trig_Good_Func010C())then
call ForceAddPlayerSimple(udg_Roa_Player,udg_Alchemists)
call ForceAddPlayerSimple(udg_Roa_Player,udg_Good_Team)
call UnitRemoveAbilityBJ('A09I',udg_Roa_Unit)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Roa_Player),udg_Good_Team,bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Roa_Player),bj_ALLIANCE_ALLIED)
else
endif
if(Trig_Good_Func011C())then
call ForceAddPlayerSimple(udg_Dolcetto_Player,udg_Good_Team)
call ForceAddPlayerSimple(udg_Dolcetto_Player,udg_Alchemists)
call UnitRemoveAbilityBJ('A09I',udg_Dolcetto_Unit)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Dolcetto_Player),udg_Good_Team,bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Dolcetto_Player),bj_ALLIANCE_ALLIED)
else
endif
call SetPlayerAllianceStateBJ(Player(7),udg_Dolcetto_Player,bj_ALLIANCE_ALLIED_VISION)
call SetPlayerAllianceStateBJ(Player(7),udg_Roa_Player,bj_ALLIANCE_ALLIED_VISION)
call SetPlayerAllianceStateBJ(Player(7),udg_Greed_Player,bj_ALLIANCE_ALLIED_VISION)
call DisableTrigger(gg_trg_Roa_Dies_S)
call EnableTrigger(gg_trg_Roa_Dies_A)
call DisableTrigger(gg_trg_Dolcetto_Dies_S)
call EnableTrigger(gg_trg_Dolcetto_Dies_A)
endfunction
