function Trig_Kings_Resolve_Func005Func002C takes nothing returns boolean
if(not(GetUnitAbilityLevelSwapped('A02H',udg_Bradley_Unit)==2))then
return false
endif
return true
endfunction
function Trig_Kings_Resolve_Func005C takes nothing returns boolean
if(not(GetUnitAbilityLevelSwapped('A02H',udg_Bradley_Unit)==1))then
return false
endif
return true
endfunction
function Trig_Kings_Resolve_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
if(Trig_Kings_Resolve_Func005C())then
call EnableTrigger(gg_trg_Kings_Removal)
call UnitAddAbilityBJ('A02G',udg_Bradley_Unit)
call SetUnitAbilityLevelSwapped('A02G',udg_Bradley_Unit,GetUnitAbilityLevelSwapped('A02H',udg_Bradley_Unit))
else
if(Trig_Kings_Resolve_Func005Func002C())then
call EnableTrigger(gg_trg_Kings_Removal)
call UnitAddAbilityBJ('A02G',udg_Bradley_Unit)
call SetUnitAbilityLevelSwapped('A02G',udg_Bradley_Unit,GetUnitAbilityLevelSwapped('A02H',udg_Bradley_Unit))
else
if(Trig_Kings_Resolve_Func005Func002Func002C())then
call EnableTrigger(gg_trg_Kings_Removal)
call UnitAddAbilityBJ('A02G',udg_Bradley_Unit)
call SetUnitAbilityLevelSwapped('A02G',udg_Bradley_Unit,GetUnitAbilityLevelSwapped('A02H',udg_Bradley_Unit))
else
if(Trig_Kings_Resolve_Func005Func002Func002Func002C())then
call EnableTrigger(gg_trg_Kings_Removal)
call UnitAddAbilityBJ('A02G',udg_Bradley_Unit)
call SetUnitAbilityLevelSwapped('A02G',udg_Bradley_Unit,GetUnitAbilityLevelSwapped('A02H',udg_Bradley_Unit))
else
if(Trig_Kings_Resolve_Func005Func002Func002Func002Func002C())then
call EnableTrigger(gg_trg_Kings_Removal)
call UnitAddAbilityBJ('A02G',udg_Bradley_Unit)
call SetUnitAbilityLevelSwapped('A02G',udg_Bradley_Unit,GetUnitAbilityLevelSwapped('A02H',udg_Bradley_Unit))
else
endif
endif
endif
endif
endif
endfunction
