call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Darius_Player),bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(udg_Homunculi,GetForceOfPlayer(udg_Darius_Player),bj_ALLIANCE_ALLIED_VISION)
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Darius has been killed, his brute strength wasn't enough!|r")
if(Trig_Darius_Dies_S_Func006C())then
call DisplayTextToForce(GetPlayersAll(),"The |cffff0000Alchemists and their friends|r defeated all enemies as well as Father and put an end to his plans once and for all! A new era of peace falls over Amestris.")
