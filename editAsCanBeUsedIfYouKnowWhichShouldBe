function Trig_Greed_Dies_Actions takes nothing returns nothing
call UnitRemoveAbilityBJ('A020',udg_Dolcetto_Unit)
call UnitRemoveAbilityBJ('A020',udg_Roa_Unit)
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Greed the Avaricious has been destroyed...!|r")
call PingMinimapLocForForce(GetForceOfPlayer(udg_Ling_Player),GetUnitLoc(GetDyingUnit()),10.00)
call SetPlayerState(udg_Greed_Player,PLAYER_STATE_OBSERVER,1)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Greed_Player),bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Greed_Player),bj_ALLIANCE_ALLIED_VISION)
call ForceRemovePlayerSimple(udg_Greed_Player,udg_Neutral)
call DisplayTextToForce(GetForceOfPlayer(udg_Ling_Player),"Ling Yao: I sense...something...a Philosopher's Stone?")
call RemoveUnit(GetDyingUnit())
if(Trig_Greed_Dies_Func011C())then
call CreateItemLoc('I002',GetUnitLoc(GetDyingUnit()))
set udg_Greeds_Stone=GetLastCreatedItem()
call EnableTrigger(gg_trg_Ping_Stone_for_Ling)
else
endif
if(Trig_Greed_Dies_Func012C())then
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Father|r has eradicated all those who stood before his plans. The Promised Day arrives and a dark cloud falls over Amestris...")
call TriggerSleepAction(10.00)
call CustomDefeatBJ(Player(0),"Father has won!")
call CustomDefeatBJ(Player(1),"Father has won!")
call CustomDefeatBJ(Player(2),"Father has won!")
call CustomVictoryBJ(Player(3),true,true)
call CustomDefeatBJ(Player(4),"Father has won!")
call CustomDefeatBJ(Player(5),"Father has won!")
call CustomDefeatBJ(Player(6),"Father has won!")
call CustomDefeatBJ(Player(8),"Father has won!")
call CustomDefeatBJ(Player(9),"Father has won!")
call CustomVictoryBJ(Player(10),true,true)
call CustomVictoryBJ(Player(11),true,true)
else
endif
if(Trig_Greed_Dies_Func013C())then
call DisplayTextToForce(GetPlayersAll(),"The |cffff0000Alchemists and their friends|r defeated all enemies as well as Father and put an end to his plans once and for all! A new era of peace falls over Amestris.")
call TriggerSleepAction(10.00)
call CustomVictoryBJ(Player(0),true,true)
call CustomVictoryBJ(Player(1),true,true)
call CustomVictoryBJ(Player(2),true,true)
call CustomDefeatBJ(Player(3),"You have lost!")
call CustomVictoryBJ(Player(4),true,true)
call CustomVictoryBJ(Player(5),true,true)
call CustomVictoryBJ(Player(6),true,true)
if(Trig_Greed_Dies_Func013Func010C())then
call CustomVictoryBJ(udg_Scar_Player,true,true)
else
call CustomDefeatBJ(udg_Scar_Player,"You have lost!")
endif
call CustomDefeatBJ(udg_Greed_Player,"You have lost!")
call CustomDefeatBJ(udg_Barry_Player,"You have lost!")
call CustomDefeatBJ(udg_Slicer_Player,"You have lost!")
call CustomDefeatBJ(Player(10),"You have lost!")
call CustomDefeatBJ(Player(11),"You have lost!")
else
endif
endfunction
