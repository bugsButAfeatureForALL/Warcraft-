endfunction
function Trig_Lings_Soul_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
call TriggerSleepAction(30.00)
call CreateNUnitsAtLoc(1,'h01M',udg_Ling_Player,GetRectCenter(gg_rct_Soul),bj_UNIT_FACING)
set udg_Lings_Soul=GetLastCreatedUnit()
call DisplayTextToForce(GetForceOfPlayer(udg_Ling_Player),"You have regained your consciousness amongest the souls within Greed! You can now retake control of your body by using your spell. But be warned, Just like you can take Greed over at any point after 3 minutes. He can do the same to you and he may not share your Alligences.")
endfunction
function InitTrig_Lings_Soul takes nothing returns nothing
set gg_trg_Lings_Soul=CreateTrigger()
call DisableTrigger(gg_trg_Lings_Soul)
call TriggerRegisterTimerEventPeriodic(gg_trg_Lings_Soul,0.10)
call TriggerAddAction(gg_trg_Lings_Soul,function Trig_Lings_Soul_Actions)
endfunction
function Trig_Greeds_Alligence_Conditions takes nothing returns boolean
if(not(GetUnitTypeId(GetTriggerUnit())=='h01O'))then
return false
endif
return true
endfunction
function Trig_Greeds_Alligence_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
call DisplayTextToForce(GetForceOfPlayer(udg_Greed_Player),"Ling Yao has retaken control over his body from you. In three minutes you can take it back if you choose. But first you must decide who you are going to ally with. The Alchemist? Your Homunculi brethren? Or are you going to show them all your Avarice and go it alone? This choice will affect who you ally with when you are in control.")
call ForceRemovePlayerSimple(udg_Greed_Player,udg_Homunculi)
endfunction
function InitTrig_Greeds_Alligence takes nothing returns nothing
set gg_trg_Greeds_Alligence=CreateTrigger()
call TriggerRegisterEnterRectSimple(gg_trg_Greeds_Alligence,gg_rct_Soul)
