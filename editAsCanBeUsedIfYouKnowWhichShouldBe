function Trig_DD_Cast_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A00K'))then
return false
endif
return true
endfunction
function Trig_DD_Cast_Actions takes nothing returns nothing
set udg_DD_Caster=GetSpellAbilityUnit()
set udg_DD_Location=GetUnitLoc(GetSpellAbilityUnit())
set udg_DD_Number=0.00
call PauseUnitBJ(true,GetSpellAbilityUnit())
call TriggerSleepAction(0.10)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=18
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call CreateNUnitsAtLoc(1,'h00K',GetOwningPlayer(udg_DD_Caster),udg_DD_Location,udg_DD_Number)
set udg_DD_Dummies[GetForLoopIndexA()]=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(1.20,'BTLF',udg_DD_Dummies[GetForLoopIndexA()])
call SetUnitFlyHeightBJ(udg_DD_Dummies[GetForLoopIndexA()],10.00,750.00)
set udg_DD_Number=(udg_DD_Number+20.00)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
call TriggerSleepAction(0.10)
set udg_DD_Number=0.00
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=18
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set udg_DD_Point=PolarProjectionBJ(udg_DD_Location,500.00,udg_DD_Number)
call IssuePointOrderLoc(udg_DD_Dummies[GetForLoopIndexA()],"move",udg_DD_Point)
set udg_DD_Number=(udg_DD_Number+20.00)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
call TriggerSleepAction(0.20)
call PauseUnitBJ(false,GetSpellAbilityUnit())
set udg_DD_Number=250.00
call EnableTrigger(gg_trg_DD_Effect)
call TriggerSleepAction(0.90)
call DisableTrigger(gg_trg_DD_Effect)
call RemoveLocation(udg_DD_Location)
call RemoveLocation(udg_DD_Point)
endfunction
function InitTrig_DD_Cast takes nothing returns nothing
set gg_trg_DD_Cast=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_DD_Cast,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_DD_Cast,Condition(function Trig_DD_Cast_Conditions))
call TriggerAddAction(gg_trg_DD_Cast,function Trig_DD_Cast_Actions)
endfunction
function Trig_DD_Effect_Func002Func003C takes nothing returns boolean
if(not(IsUnitEnemy(GetEnumUnit(),GetOwningPlayer(udg_DD_Caster))==true))then
return false
endif
return true
endfunction
function Trig_DD_Effect_Func002Func006C takes nothing returns boolean
if(not(IsUnitAliveBJ(GetEnumUnit())==true))then
return false
endif
if(not(IsTerrainPathableBJ(udg_DD_TempPoint2,PATHING_TYPE_WALKABILITY)==false))then
return false
endif
if(not(IsUnitEnemy(GetEnumUnit(),GetOwningPlayer(udg_DD_Caster))==true))then
return false
endif
if(not(GetEnumUnit()!=gg_unit_H04N_0132))then
return false
endif
if(not(GetUnitTypeId(GetEnumUnit())!='hpea'))then
return false
endif
if(not(GetUnitTypeId(GetEnumUnit())!='h04Q'))then
return false
endif
if(not(GetUnitTypeId(GetEnumUnit())!='h00P'))then
return false
endif
if(not(GetUnitTypeId(GetEnumUnit())!='h00Q'))then
return false
endif
if(not(GetUnitTypeId(GetEnumUnit())!='h00R'))then
return false
endif
return true
endfunction
function Trig_DD_Effect_Func002A takes nothing returns nothing
set udg_DD_TempPoint=GetUnitLoc(GetEnumUnit())
set udg_DD_TempPoint2=PolarProjectionBJ(udg_DD_TempPoint,6.00,AngleBetweenPoints(udg_DD_Location,udg_DD_TempPoint))
if(Trig_DD_Effect_Func002Func003C())then
call UnitDamageTargetBJ(udg_DD_Caster,GetEnumUnit(),10.00,ATTACK_TYPE_HERO,DAMAGE_TYPE_DEATH)
else
endif
call AddSpecialEffectLocBJ(udg_DD_TempPoint2,"Abilities\\Weapons\\RedDragonBreath\\RedDragonMissile.mdl")
call DestroyEffect(GetLastCreatedEffectBJ())
if(Trig_DD_Effect_Func002Func006C())then
call SetUnitPositionLoc(GetEnumUnit(),udg_DD_TempPoint2)
else
endif
call RemoveLocation(udg_DD_TempPoint)
call RemoveLocation(udg_DD_TempPoint2)
endfunction
function Trig_DD_Effect_Actions takes nothing returns nothing
set udg_DD_TempUnitGroup=GetUnitsInRangeOfLocAll(udg_DD_Number,udg_DD_Location)
call ForGroupBJ(udg_DD_TempUnitGroup,function Trig_DD_Effect_Func002A)
call DestroyGroup(udg_DD_TempUnitGroup)
set udg_DD_Number=(udg_DD_Number+10.00)
endfunction
