function Trig_Hohenheim_Reaches_Central_Conditions takes nothing returns boolean
if(not(GetTriggerUnit()==udg_Hohenheim_Unit))then
return false
endif
if(not(GetOwningPlayer(udg_Hohenheim_Unit)==Player(7)))then
return false
endif
return true
endfunction
function Trig_Hohenheim_Reaches_Central_Func026C takes nothing returns boolean
if(not(udg_Greeds_Sacrifice==true))then
return false
endif
return true
endfunction
function Trig_Hohenheim_Reaches_Central_Actions takes nothing returns nothing
call IssuePointOrderLoc(udg_Hohenheim_Unit,"move",GetRectCenter(gg_rct_Evil_Spawn))
call DisableTrigger(gg_trg_Hohenheims_Pathing)
call DisableTrigger(GetTriggeringTrigger())
call TriggerSleepAction(3.00)
call SetUnitLifePercentBJ(udg_Hohenheim_Unit,100)
call PauseUnitBJ(true,udg_Hohenheim_Unit)
call SetUnitInvulnerable(udg_Hohenheim_Unit,true)
call SetUnitFacingToFaceUnitTimed(udg_Hohenheim_Unit,gg_unit_H04N_0132,0)
call SetUnitInvulnerable(gg_unit_H04N_0132,true)
call PauseUnitBJ(true,gg_unit_H04N_0132)
call DisplayTimedTextToForce(GetPlayersAll(),5.00,"|cffff0000Van Hohenheim:|r I knew you'd try to contain God! I've spent years coming up with a counter measure!")
call TriggerSleepAction(5.00)
call DisplayTimedTextToForce(GetPlayersAll(),5.00,"|cff995500Father:|r You just continue to get in my way, don't you, Hohenheim?!")
call TriggerSleepAction(4.00)
call SetUnitAnimation(gg_unit_H04N_0132,"Morph Alternate")
call DisplayTimedTextToForce(GetPlayersAll(),5.00,"|cffff0000Van Hohenheim:|r It's why I came, Dwarf in the flask!")
call TriggerSleepAction(1.00)
call AddUnitAnimationPropertiesBJ(false,"Alternate",gg_unit_H04N_0132)
call TriggerSleepAction(4.00)
call ReplaceUnitBJ(gg_unit_H04N_0132,'H04O',bj_UNIT_STATE_METHOD_RELATIVE)
set udg_Our_Father=GetLastReplacedUnitBJ()
call TriggerSleepAction(0.20)
call EnableTrigger(gg_trg_Captures_Winry)
call EnableTrigger(gg_trg_God_Father_AI)
call UnitAddAbilityBJ('A044',udg_Our_Father)
if(Trig_Hohenheim_Reaches_Central_Func026C())then
call UnitAddAbilityBJ('A086',udg_Our_Father)
call UnitRemoveAbilityBJ('A0A4',udg_Our_Father)
else
endif
call UnitAddAbilityBJ('A09E',udg_Our_Father)
call UnitAddAbilityBJ('A0A9',udg_Our_Father)
call DisplayTimedTextToForce(GetPlayersAll(),5.00,"|cffff0000Van Hohenheim|r has weakened Father! Now may be the only chance for victory!")
call PauseUnitBJ(false,udg_Hohenheim_Unit)
call SetUnitInvulnerable(udg_Hohenheim_Unit,false)
call SetUnitOwner(gg_unit_H04N_0132,Player(bj_PLAYER_NEUTRAL_EXTRA),true)
endfunction
function InitTrig_Hohenheim_Reaches_Central takes nothing returns nothing
set gg_trg_Hohenheim_Reaches_Central=CreateTrigger()
call TriggerRegisterEnterRectSimple(gg_trg_Hohenheim_Reaches_Central,gg_rct_Enter_Lair_2)
call TriggerAddCondition(gg_trg_Hohenheim_Reaches_Central,Condition(function Trig_Hohenheim_Reaches_Central_Conditions))
call TriggerAddAction(gg_trg_Hohenheim_Reaches_Central,function Trig_Hohenheim_Reaches_Central_Actions)
endfunction
function Trig_Hohenheim_Player_Confronts_God_Conditions takes nothing returns boolean
if(not(udg_Our_Father==gg_unit_H04N_0132))then
return false
endif
if(not(GetTriggerUnit()==udg_Hohenheim_Unit))then
return false
endif
if(not(udg_Is_Hohenheim_Selected==true))then
return false
endif
if(not(GetOwningPlayer(udg_Hohenheim_Unit)!=Player(7)))then
return false
endif
return true
endfunction
function Trig_Hohenheim_Player_Confronts_God_Func026C takes nothing returns boolean
if(not(udg_Greeds_Sacrifice==true))then
return false
endif
return true
endfunction
function Trig_Hohenheim_Player_Confronts_God_Actions takes nothing returns nothing
call PauseUnitBJ(true,udg_Hohenheim_Unit)
call SetUnitInvulnerable(udg_Hohenheim_Unit,true)
call SetUnitFacingToFaceUnitTimed(udg_Hohenheim_Unit,gg_unit_H04N_0132,0)
call SetUnitInvulnerable(gg_unit_H04N_0132,true)
call PauseUnitBJ(true,gg_unit_H04N_0132)
call DisplayTimedTextToForce(GetPlayersAll(),5.00,"|cffff0000Van Hohenheim:|r I knew you'd try to contain God! I've spent years coming up with a counter measure!")
call TriggerSleepAction(5.00)
call DisplayTimedTextToForce(GetPlayersAll(),5.00,"|cff995500Father:|r You just continue to get in my way, don't you, Hohenheim?!")
call TriggerSleepAction(4.00)
call SetUnitAnimation(gg_unit_H04N_0132,"Morph Alternate")
call DisplayTimedTextToForce(GetPlayersAll(),5.00,"|cffff0000Van Hohenheim:|r It's why I came, Dwarf in the flask!")
call TriggerSleepAction(1.00)
call AddUnitAnimationPropertiesBJ(false,"Alternate",gg_unit_H04N_0132)
call TriggerSleepAction(4.00)
call ReplaceUnitBJ(gg_unit_H04N_0132,'H04O',bj_UNIT_STATE_METHOD_RELATIVE)
set udg_Our_Father=GetLastReplacedUnitBJ()
call TriggerSleepAction(0.20)
call EnableTrigger(gg_trg_Captures_Winry)
call EnableTrigger(gg_trg_God_Father_AI)
call UnitAddAbilityBJ('A044',udg_Our_Father)
if(Trig_Hohenheim_Player_Confronts_God_Func026C())then
call UnitAddAbilityBJ('A086',udg_Our_Father)
call UnitRemoveAbilityBJ('A0A4',udg_Our_Father)
else
endif
call UnitAddAbilityBJ('A09E',udg_Our_Father)
call UnitAddAbilityBJ('A0A9',udg_Our_Father)
call DisplayTimedTextToForce(GetPlayersAll(),5.00,"|cffff0000Van Hohenheim|r has weakened Father! Now may be the only chance for victory!")
call PauseUnitBJ(false,udg_Hohenheim_Unit)
call SetUnitInvulnerable(udg_Hohenheim_Unit,false)
call SetUnitOwner(gg_unit_H04N_0132,Player(bj_PLAYER_NEUTRAL_EXTRA),true)
endfunction
