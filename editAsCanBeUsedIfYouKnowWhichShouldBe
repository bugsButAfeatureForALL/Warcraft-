function Trig_Shapeshifted_to_Normal_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
set bj_forLoopAIndex=0
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set udg_Current_Envy_Items[GetForLoopIndexA()]=UnitItemInSlotBJ(udg_Envy_Shapeshifted_Form,(GetForLoopIndexA()+1))
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set bj_forLoopBIndex=0
set bj_forLoopBIndexEnd=5
loop
exitwhen bj_forLoopBIndex>bj_forLoopBIndexEnd
call UnitRemoveItemFromSlotSwapped((GetForLoopIndexB()+1),udg_Envy_Shapeshifted_Form)
call UnitAddItemSwapped(udg_Current_Envy_Items[GetForLoopIndexB()],udg_Envy_First_Form)
set bj_forLoopBIndex=bj_forLoopBIndex+1
endloop
endfunction
function InitTrig_Shapeshifted_to_Normal takes nothing returns nothing
set gg_trg_Shapeshifted_to_Normal=CreateTrigger()
call DisableTrigger(gg_trg_Shapeshifted_to_Normal)
call TriggerRegisterTimerEventPeriodic(gg_trg_Shapeshifted_to_Normal,0.01)
call TriggerAddAction(gg_trg_Shapeshifted_to_Normal,function Trig_Shapeshifted_to_Normal_Actions)
endfunction
function Trig_Normal_to_Shapeshifted_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
set bj_forLoopAIndex=0
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set udg_Current_Envy_Items[GetForLoopIndexA()]=UnitItemInSlotBJ(udg_Envy_First_Form,(GetForLoopIndexA()+1))
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set bj_forLoopBIndex=0
set bj_forLoopBIndexEnd=5
loop
exitwhen bj_forLoopBIndex>bj_forLoopBIndexEnd
call UnitRemoveItemFromSlotSwapped((GetForLoopIndexB()+1),udg_Envy_First_Form)
call UnitAddItemSwapped(udg_Current_Envy_Items[GetForLoopIndexB()],udg_Envy_Shapeshifted_Form)
set bj_forLoopBIndex=bj_forLoopBIndex+1
endloop
endfunction
function InitTrig_Normal_to_Shapeshifted takes nothing returns nothing
set gg_trg_Normal_to_Shapeshifted=CreateTrigger()
call DisableTrigger(gg_trg_Normal_to_Shapeshifted)
call TriggerRegisterTimerEventPeriodic(gg_trg_Normal_to_Shapeshifted,0.01)
call TriggerAddAction(gg_trg_Normal_to_Shapeshifted,function Trig_Normal_to_Shapeshifted_Actions)
endfunction
function Trig_Learning_Shapeshift_Conditions takes nothing returns boolean
if(not(GetLearnedSkillBJ()=='A03B'))then
return false
endif
return true
endfunction
function Trig_Learning_Shapeshift_Func002Func004Func005Func005Func005C takes nothing returns boolean
if(not(GetUnitAbilityLevelSwapped('A03B',udg_Envy_First_Form)==5))then
return false
endif
return true
endfunction
function Trig_Learning_Shapeshift_Func002Func004Func005Func005C takes nothing returns boolean
if(not(GetUnitAbilityLevelSwapped('A03B',udg_Envy_First_Form)==4))then
return false
endif
return true
endfunction
function Trig_Learning_Shapeshift_Func002Func004Func005C takes nothing returns boolean
if(not(GetUnitAbilityLevelSwapped('A03B',udg_Envy_First_Form)==3))then
return false
endif
return true
endfunction
function Trig_Learning_Shapeshift_Func002Func004C takes nothing returns boolean
if(not(GetUnitAbilityLevelSwapped('A03B',udg_Envy_First_Form)==2))then
return false
endif
return true
endfunction
function Trig_Learning_Shapeshift_Func002C takes nothing returns boolean
if(not(GetUnitAbilityLevelSwapped('A03B',udg_Envy_First_Form)==1))then
return false
endif
return true
endfunction
function Trig_Learning_Shapeshift_Actions takes nothing returns nothing
if(Trig_Learning_Shapeshift_Func002C())then
call UnitAddAbilityBJ('A036',udg_Envy_First_Form)
set udg_Current_Shapeshift='A036'
else
if(Trig_Learning_Shapeshift_Func002Func004C())then
else
call UnitRemoveAbilityBJ(udg_Current_Shapeshift,udg_Envy_First_Form)
call UnitAddAbilityBJ('A037',udg_Envy_First_Form)
set udg_Current_Shapeshift='A037'
if(Trig_Learning_Shapeshift_Func002Func004Func005C())then
call UnitRemoveAbilityBJ(udg_Current_Shapeshift,udg_Envy_First_Form)
call UnitAddAbilityBJ('A038',udg_Envy_First_Form)
set udg_Current_Shapeshift='A038'
else
if(Trig_Learning_Shapeshift_Func002Func004Func005Func005C())then
call UnitRemoveAbilityBJ(udg_Current_Shapeshift,udg_Envy_First_Form)
call UnitAddAbilityBJ('A039',udg_Envy_First_Form)
set udg_Current_Shapeshift='A039'
else
if(Trig_Learning_Shapeshift_Func002Func004Func005Func005Func005C())then
call UnitRemoveAbilityBJ(udg_Current_Shapeshift,udg_Envy_First_Form)
call UnitAddAbilityBJ('A03A',udg_Envy_First_Form)
set udg_Current_Shapeshift='A03A'
else
endif
endif
endif
endif
endif
endfunction
function InitTrig_Learning_Shapeshift takes nothing returns nothing
set gg_trg_Learning_Shapeshift=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Learning_Shapeshift,EVENT_PLAYER_HERO_SKILL)
call TriggerAddCondition(gg_trg_Learning_Shapeshift,Condition(function Trig_Learning_Shapeshift_Conditions))
call TriggerAddAction(gg_trg_Learning_Shapeshift,function Trig_Learning_Shapeshift_Actions)
endfunction
function Trig_Revert_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A01S'))then
return false
endif
return true
endfunction
function Trig_Revert_Actions takes nothing returns nothing
call EnableTrigger(gg_trg_Shapeshifted_to_Normal)
call SetHeroLevelBJ(udg_Envy_First_Form,udg_Envy_Current_Level,false)
call ShowUnitHide(udg_Envy_Shapeshifted_Form)
call SetUnitOwner(udg_Envy_Shapeshifted_Form,Player(PLAYER_NEUTRAL_PASSIVE),true)
call SetPlayerColorBJ(udg_Envy_Player,udg_Envy_Original_Color,true)
call SetUnitPositionLoc(udg_Envy_First_Form,GetUnitLoc(udg_Envy_Shapeshifted_Form))
call PauseUnitBJ(false,udg_Envy_First_Form)
call SetUnitInvulnerable(udg_Envy_First_Form,false)
call ShowUnitShow(udg_Envy_First_Form)
call SetUnitOwner(udg_Envy_First_Form,udg_Envy_Player,true)
call SelectUnitForPlayerSingle(udg_Envy_First_Form,GetOwningPlayer(udg_Envy_First_Form))
call SetPlayerName(GetOwningPlayer(udg_Envy_First_Form),"Envy")
call SetPlayerHandicapXPBJ(udg_Envy_Player,175.00)
call TriggerSleepAction(0.25)
call SetUnitLifePercentBJ(udg_Envy_First_Form,GetUnitLifePercent(udg_Envy_Shapeshifted_Form))
call TriggerSleepAction(0.25)
call RemoveUnit(udg_Envy_Shapeshifted_Form)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Envy_Player),bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Envy_Player),udg_Good_Team,bj_ALLIANCE_UNALLIED)
endfunction
function InitTrig_Revert takes nothing returns nothing
set gg_trg_Revert=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Revert,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Revert,Condition(function Trig_Revert_Conditions))
call TriggerAddAction(gg_trg_Revert,function Trig_Revert_Actions)
endfunction
function Trig_Edward_Conditions takes nothing returns boolean
if(not(GetOwningPlayer(udg_Envy_First_Form)==udg_Envy_Player))then
return false
endif
if(not(GetSpellAbilityId()=='A01R'))then
return false
endif
if(not(GetUnitTypeId(GetSpellAbilityUnit())!='H015'))then
return false
endif
return true
endfunction
function Trig_Edward_Actions takes nothing returns nothing
set udg_Envy_Position=GetUnitLoc(udg_Envy_First_Form)
set udg_Envy_First_Form=GetSpellAbilityUnit()
set udg_Envy_Current_Level=GetHeroLevel(udg_Envy_First_Form)
call SetUnitInvulnerable(udg_Envy_First_Form,true)
call ShowUnitHide(udg_Envy_First_Form)
call CreateNUnitsAtLoc(1,'H01D',GetOwningPlayer(udg_Envy_First_Form),udg_Envy_Position,bj_UNIT_FACING)
set udg_Envy_Shapeshifted_Form=GetLastCreatedUnit()
call SetUnitOwner(udg_Envy_First_Form,Player(PLAYER_NEUTRAL_PASSIVE),true)
call SetUnitLifePercentBJ(udg_Envy_Shapeshifted_Form,GetUnitLifePercent(udg_Envy_First_Form))
call SetPlayerName(GetOwningPlayer(udg_Envy_Shapeshifted_Form),"Edward Elric")
call SetPlayerColorBJ(udg_Envy_Player,udg_Edward_Color,true)
call SetHeroLevelBJ(udg_Envy_Shapeshifted_Form,GetHeroLevel(udg_Edward_Unit),false)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Envy_Player),bj_ALLIANCE_NEUTRAL)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Envy_Player),udg_Good_Team,bj_ALLIANCE_NEUTRAL)
call EnableTrigger(gg_trg_Normal_to_Shapeshifted)
call SetPlayerHandicapXPBJ(udg_Envy_Player,0.00)
call RemoveLocation(udg_Envy_Position)
endfunction
function InitTrig_Edward takes nothing returns nothing
set gg_trg_Edward=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Edward,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Edward,Condition(function Trig_Edward_Conditions))
call TriggerAddAction(gg_trg_Edward,function Trig_Edward_Actions)
endfunction
