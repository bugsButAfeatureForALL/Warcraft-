function Trig_Dolcetto_is_Chosen_Func013C takes nothing returns boolean
if(not(IsUnitAliveBJ(udg_Greed)==true))then
return false
endif
return true
endfunction
function Trig_Dolcetto_is_Chosen_Actions takes nothing returns nothing
set udg_Dolcetto_Unit=GetTriggerUnit()
set udg_Dolcetto_Player=GetOwningPlayer(GetTriggerUnit())
call SetPlayerName(udg_Dolcetto_Player,"Dolcetto")
call SetUnitPositionLoc(GetTriggerUnit(),GetRectCenter(gg_rct_Dolcetto_Spawn))
call PanCameraToTimedLocForPlayer(GetOwningPlayer(udg_Dolcetto_Unit),GetRectCenter(gg_rct_Dolcetto_Spawn),0)
call SelectUnitForPlayerSingle(GetTriggerUnit(),GetOwningPlayer(GetTriggerUnit()))
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Dolcetto_Player),udg_Evil_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Dolcetto_Player),udg_Good_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Dolcetto_Player),bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Dolcetto_Player),bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[bj_PLAYER_NEUTRAL_VICTIM],GetForceOfPlayer(udg_Dolcetto_Player),bj_ALLIANCE_UNALLIED)
call ForceAddPlayerSimple(udg_Dolcetto_Player,udg_Neutral)
if(Trig_Dolcetto_is_Chosen_Func013C())then
call ModifyHeroStat(bj_HEROSTAT_STR,udg_Dolcetto_Unit,bj_MODIFYMETHOD_ADD,15)
call ModifyHeroStat(bj_HEROSTAT_AGI,udg_Dolcetto_Unit,bj_MODIFYMETHOD_ADD,15)
call SetPlayerAllianceStateBJ(udg_Dolcetto_Player,udg_Greed_Player,bj_ALLIANCE_ALLIED_VISION)
call SetPlayerAllianceStateBJ(udg_Greed_Player,udg_Dolcetto_Player,bj_ALLIANCE_ALLIED_VISION)
else
if(Trig_Dolcetto_is_Chosen_Func013Func003C())then
call SetPlayerAllianceStateBJ(udg_Dolcetto_Player,udg_Roa_Player,bj_ALLIANCE_ALLIED_VISION)
call SetPlayerAllianceStateBJ(udg_Roa_Player,udg_Dolcetto_Player,bj_ALLIANCE_ALLIED_VISION)
else
endif
endif
call DisplayTextToForce(GetForceOfPlayer(udg_Dolcetto_Player),"You are the Chimera Dolcetto! If |cffff0000Greed|r or |cffff0000Roa|r  have been selected they will be your only ally against everyone else. You may heal at Devil's Nest to restore your health, but be warned, should you die once you're |cffff0000gone|r!")
call CreateNUnitsAtLoc(1,'h01Z',udg_Dolcetto_Player,GetRectCenter(gg_rct_Dolcetto_Spawn),bj_UNIT_FACING)
set udg_Dolcetto_Healer=GetLastCreatedUnit()
call RemoveUnit(udg_Roa_Healer)
call RemoveUnit(udg_Greed_Healer)
call DestroyTrigger(GetTriggeringTrigger())
endfunction
function InitTrig_Dolcetto_is_Chosen takes nothing returns nothing
set gg_trg_Dolcetto_is_Chosen=CreateTrigger()
call TriggerRegisterEnterRectSimple(gg_trg_Dolcetto_is_Chosen,gg_rct_Neutral_Spawns)
call TriggerAddCondition(gg_trg_Dolcetto_is_Chosen,Condition(function Trig_Dolcetto_is_Chosen_Conditions))
call TriggerAddAction(gg_trg_Dolcetto_is_Chosen,function Trig_Dolcetto_is_Chosen_Actions)
endfunction
