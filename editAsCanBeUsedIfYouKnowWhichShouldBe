function Trig_Shard_Ping_Func001C takes nothing returns boolean
if(not(RectContainsItem(udg_First_Shard,GetPlayableMapRect())==true))then
return false
endif
if(not(IsItemOwned(udg_First_Shard)==false))then
return false
endif
if(not(udg_First_Shard !=gg_item_stpg_0232))then
return false
endif
return true
endfunction
function Trig_Shard_Ping_Func002C takes nothing returns boolean
if(not(RectContainsItem(udg_Second_Shard,GetPlayableMapRect())==true))then
return false
endif
