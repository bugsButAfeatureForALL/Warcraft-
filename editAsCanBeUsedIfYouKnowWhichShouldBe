return false
endif
return true
endfunction
function Trig_Leap_Func016C takes nothing returns boolean
if(not(udg_Time>=40.00))then
return false
endif
return true
endfunction
function Trig_Leap_Actions takes nothing returns nothing
set udg_Time=(udg_Time+1)
call SetUnitFlyHeightBJ(udg_Jump_Caster,udg_hieght,10000.00)
set udg_TempPoint1=GetUnitLoc(udg_Jump_Caster)
set udg_TempPoint2=PolarProjectionBJ(udg_TempPoint1,udg_DistanceBetweenPoints,GetUnitFacing(udg_Jump_Caster))
call SetUnitPositionLoc(udg_Jump_Caster,udg_TempPoint2)
call RemoveLocation(udg_TempPoint1)
call RemoveLocation(udg_TempPoint2)
if(Trig_Leap_Func013C())then
set udg_hieght=(udg_hieght+15.00)
if(Trig_Leap_Func013Func003C())then
set udg_hieght=(udg_hieght+5.00)
else
endif
else
endif
if(Trig_Leap_Func016C())then
set udg_hieght=(udg_hieght-5.00)
if(Trig_Leap_Func016Func003C())then
set udg_hieght=(udg_hieght-10.00)
if(Trig_Leap_Func016Func003Func003C())then
call DisableTrigger(gg_trg_Leap)
call SetUnitPathing(udg_Jump_Caster,true)
call PauseUnitBJ(false,udg_Jump_Caster)
call SetUnitPositionLoc(udg_Jump_Caster,PolarProjectionBJ(GetUnitLoc(udg_Jump_Caster),udg_DistanceBetweenPoints,GetUnitFacing(udg_Jump_Caster)))
call UnitRemoveAbilityBJ('Arav',udg_Jump_Caster)
set udg_EffectPoint=GetUnitLoc(udg_Jump_Caster)
call AddSpecialEffectLocBJ(udg_EffectPoint,"Abilities\\Spells\\Orc\\WarStomp\\WarStompCaster.mdl")
call DestroyEffect(GetLastCreatedEffectBJ())
call CreateNUnitsAtLoc(1,'h04U',udg_Gluttony_Player,udg_EffectPoint,bj_UNIT_FACING)
set udg_Leap_Damage_Dummy=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(2.00,'BTLF',udg_Leap_Damage_Dummy)
call SetUnitAbilityLevelSwapped('A09K',udg_Leap_Damage_Dummy,GetUnitAbilityLevelSwapped('A09J',udg_Gluttony_Unit))
call IssueImmediateOrder(udg_Leap_Damage_Dummy,"thunderclap")
call RemoveLocation(udg_EffectPoint)
else
endif
else
endif
else
endif
endfunction
function InitTrig_Leap takes nothing returns nothing
set gg_trg_Leap=CreateTrigger()
call DisableTrigger(gg_trg_Leap)
call TriggerRegisterTimerEventPeriodic(gg_trg_Leap,0.01)
call TriggerAddAction(gg_trg_Leap,function Trig_Leap_Actions)
endfunction
