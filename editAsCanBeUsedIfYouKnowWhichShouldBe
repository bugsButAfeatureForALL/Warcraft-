function Trig_Destructions_Chance_Conditions takes nothing returns boolean
if(not(GetAttackedUnitBJ()!=udg_Our_Father))then
return false
endif
if(not(GetAttackedUnitBJ()!=gg_unit_H02L_0157))then
return false
endif
if(not(GetAttackedUnitBJ()!=gg_unit_H01V_0080))then
return false
endif
if(not(GetAttacker()==udg_Scar_Unit))then
return false
endif
return true
endfunction
function Trig_Destructions_Chance_Func005C takes nothing returns boolean
if(not(udg_Destruction_Chance<=3))then
return false
endif
return true
endfunction
function Trig_Destructions_Chance_Func006C takes nothing returns boolean
if(not(GetUnitAbilityLevelSwapped('A011',udg_Scar_Unit)==2))then
return false
endif
if(not(udg_Destruction_Chance<=4))then
return false
endif
return true
endfunction
function Trig_Destructions_Chance_Func007C takes nothing returns boolean
if(not(GetUnitAbilityLevelSwapped('A011',udg_Scar_Unit)==3))then
return false
endif
if(not(udg_Destruction_Chance<=5))then
return false
endif
return true
endfunction
function Trig_Destructions_Chance_Actions takes nothing returns nothing
set udg_Destruction_Chance=GetRandomInt(1,100)
if(Trig_Destructions_Chance_Func005C())then
call DisableTrigger(GetTriggeringTrigger())
call UnitDamageTargetBJ(udg_Scar_Unit,GetAttackedUnitBJ(),(I2R(GetHeroStatBJ(bj_HEROSTAT_INT,udg_Scar_Unit,false))*999.00),ATTACK_TYPE_NORMAL,DAMAGE_TYPE_NORMAL)
call CreateTextTagUnitBJ((I2S((GetHeroStatBJ(bj_HEROSTAT_STR,udg_Scar_Unit,false)*9999))+"!"),GetAttackedUnitBJ(),0,10,100,0.00,0.00,0)
set udg_Destruction_Damage=GetLastCreatedTextTag()
call TriggerSleepAction(5.00)
call DestroyTextTag(udg_Destruction_Damage)
else
endif
if(Trig_Destructions_Chance_Func006C())then
call DisableTrigger(GetTriggeringTrigger())
call UnitDamageTargetBJ(udg_Scar_Unit,GetAttackedUnitBJ(),(I2R(GetHeroStatBJ(bj_HEROSTAT_INT,udg_Scar_Unit,false))*999.00),ATTACK_TYPE_NORMAL,DAMAGE_TYPE_NORMAL)
call CreateTextTagUnitBJ((I2S((GetHeroStatBJ(bj_HEROSTAT_STR,udg_Scar_Unit,false)*9999))+"!"),GetAttackedUnitBJ(),0,10,100,0.00,0.00,0)
set udg_Destruction_Damage=GetLastCreatedTextTag()
call TriggerSleepAction(5.00)
call DestroyTextTag(udg_Destruction_Damage)
else
endif
if(Trig_Destructions_Chance_Func007C())then
call DisableTrigger(GetTriggeringTrigger())
call UnitDamageTargetBJ(udg_Scar_Unit,GetAttackedUnitBJ(),(I2R(GetHeroStatBJ(bj_HEROSTAT_INT,udg_Scar_Unit,false))*999.00),ATTACK_TYPE_NORMAL,DAMAGE_TYPE_NORMAL)
call CreateTextTagUnitBJ((I2S((GetHeroStatBJ(bj_HEROSTAT_STR,udg_Scar_Unit,false)*9999))+"!"),GetAttackedUnitBJ(),0,10,100,0.00,0.00,0)
set udg_Destruction_Damage=GetLastCreatedTextTag()
call TriggerSleepAction(5.00)
call DestroyTextTag(udg_Destruction_Damage)
else
endif
endfunction
function InitTrig_Destructions_Chance takes nothing returns nothing
set gg_trg_Destructions_Chance=CreateTrigger()
call DisableTrigger(gg_trg_Destructions_Chance)
call TriggerRegisterAnyUnitEventBJ(gg_trg_Destructions_Chance,EVENT_PLAYER_UNIT_ATTACKED)
