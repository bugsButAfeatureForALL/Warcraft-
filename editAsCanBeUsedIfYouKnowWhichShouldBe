function Trig_Clash_of_Philosophy_Func002001 takes nothing returns boolean
return(GetUnitTypeId(GetTriggerUnit())=='h044')
endfunction
function Trig_Clash_of_Philosophy_Func002002 takes nothing returns boolean
return(GetUnitTypeId(GetTriggerUnit())=='h00Q')
endfunction
function Trig_Clash_of_Philosophy_Conditions takes nothing returns boolean
if(not GetBooleanOr((GetUnitTypeId(GetTriggerUnit())=='h044'),(GetUnitTypeId(GetTriggerUnit())=='h00Q')))then
return false
endif
return true
endfunction
function Trig_Clash_of_Philosophy_Func001Func013002003 takes nothing returns boolean
return(IsUnitAliveBJ(GetFilterUnit())==true)
endfunction
function Trig_Clash_of_Philosophy_Func001Func022A takes nothing returns nothing
call SetUnitLifeBJ(GetEnumUnit(),(GetUnitStateSwap(UNIT_STATE_LIFE,GetEnumUnit())-3000.00))
endfunction
function Trig_Clash_of_Philosophy_Func001C takes nothing returns boolean
if(not(DistanceBetweenPoints(GetUnitLoc(udg_Philosophers_Transmutation_Dum[0]),GetUnitLoc(udg_CenterPointMidRight))<=600.00))then
return false
endif
return true
endfunction
function Trig_Clash_of_Philosophy_Actions takes nothing returns nothing
if(Trig_Clash_of_Philosophy_Func001C())then
set udg_Between_Philosophy=(DistanceBetweenPoints(GetUnitLoc(udg_Hohenheim_Unit),GetUnitLoc(udg_Our_Father))/2.00)
set udg_Philosophers_Clash=true
call DisableTrigger(GetTriggeringTrigger())
call DisableTrigger(gg_trg_Open_the_Gate)
call TriggerSleepAction(2.00)
call CreateNUnitsAtLoc(1,'h03D',Player(PLAYER_NEUTRAL_PASSIVE),PolarProjectionBJ(GetUnitLoc(udg_Our_Father),(udg_Between_Philosophy/2.00),AngleBetweenPoints(GetUnitLoc(udg_Our_Father),GetUnitLoc(udg_Hohenheim_Unit))),bj_UNIT_FACING)
set udg_Truths_Eye=GetLastCreatedUnit()
call CreateNUnitsAtLoc(1,'h03C',Player(PLAYER_NEUTRAL_PASSIVE),PolarProjectionBJ(GetUnitLoc(udg_Our_Father),(udg_Between_Philosophy/2.00),AngleBetweenPoints(GetUnitLoc(udg_Our_Father),GetUnitLoc(udg_Hohenheim_Unit))),bj_UNIT_FACING)
set udg_Truths_Reaction=GetLastCreatedUnit()
call CreateNUnitsAtLoc(1,'h04E',Player(PLAYER_NEUTRAL_PASSIVE),PolarProjectionBJ(GetUnitLoc(udg_Our_Father),(udg_Between_Philosophy/2.00),AngleBetweenPoints(GetUnitLoc(udg_Our_Father),GetUnitLoc(udg_Hohenheim_Unit))),bj_UNIT_FACING)
set udg_Truth=GetLastCreatedUnit()
set udg_Position_Of_Truth=GetUnitLoc(udg_Truth)
set udg_Truth_Damage_Group=GetUnitsInRangeOfLocMatching(600.00,GetUnitLoc(udg_Truth),Condition(function Trig_Clash_of_Philosophy_Func001Func013002003))
call UnitApplyTimedLifeBJ(5.00,'BTLF',udg_Truths_Eye)
call SetUnitFacingToFaceUnitTimed(udg_Hohenheim_Unit,udg_Truth,0)
call SetUnitFacingToFaceUnitTimed(udg_Our_Father,udg_Truth,0)
call DisplayTextToForce(GetPlayersAll(),"|cff995500Truth: Hehehe...|r")
call TriggerSleepAction(3.00)
call CreateNUnitsAtLoc(1,'h04F',Player(PLAYER_NEUTRAL_PASSIVE),GetUnitLoc(udg_Truth),bj_UNIT_FACING)
call UnitApplyTimedLifeBJ(3.00,'BTLF',GetLastCreatedUnit())
call TriggerSleepAction(3.00)
call ForGroupBJ(udg_Truth_Damage_Group,function Trig_Clash_of_Philosophy_Func001Func022A)
call RemoveLocation(udg_Position_Of_Truth)
call DestroyGroup(udg_Truth_Damage_Group)
call TriggerSleepAction(2.20)
call RemoveUnit(udg_Truth)
call RemoveUnit(udg_Truths_Reaction)
call PauseUnitBJ(false,udg_Our_Father)
call SetUnitInvulnerable(udg_Our_Father,false)
call SetUnitInvulnerable(udg_Hohenheim_Unit,false)
call PauseUnitBJ(false,udg_Hohenheim_Unit)
set udg_Philosophers_Clash=false
else
endif
endfunction
function InitTrig_Clash_of_Philosophy takes nothing returns nothing
set gg_trg_Clash_of_Philosophy=CreateTrigger()
call DisableTrigger(gg_trg_Clash_of_Philosophy)
call TriggerRegisterEnterRectSimple(gg_trg_Clash_of_Philosophy,GetPlayableMapRect())
call TriggerAddCondition(gg_trg_Clash_of_Philosophy,Condition(function Trig_Clash_of_Philosophy_Conditions))
call TriggerAddAction(gg_trg_Clash_of_Philosophy,function Trig_Clash_of_Philosophy_Actions)
endfunction
function Trig_Sinful_Alchemy_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A07O'))then
return false
endif
return true
endfunction
function Trig_Sinful_Alchemy_Actions takes nothing returns nothing
call MoveRectToLoc(gg_rct_Sinful_Alchemy,GetSpellTargetLoc())
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=7
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call CreateNUnitsAtLoc(1,'hfoo',udg_Father_Player,GetRandomLocInRect(gg_rct_Sinful_Alchemy),GetRandomDirectionDeg())
call IssueImmediateOrder(GetLastCreatedUnit(),"stomp")
call UnitApplyTimedLifeBJ(3.00,'BTLF',GetLastCreatedUnit())
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endfunction
