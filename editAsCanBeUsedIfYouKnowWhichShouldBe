function Trig_Dublith_Mountain_Spawns_Actions takes nothing returns nothing
local location SL_local
set udg_Dublith_Mountains_Integer=0
loop
exitwhen udg_Dublith_Mountains_Integer>16
set udg_Dublith_Mountain_Boolean=IsUnitAliveBJ(udg_Dublith_Mountain_Unit_Array[udg_Dublith_Mountains_Integer])
if(Trig_Dublith_Mountain_Spawns_Func002Func002C())then
set SL_local=GetRectCenter(udg_Spawn_Array[udg_Dublith_Mountains_Integer])
set udg_Dublith_Spawn_Location=SL_local
call CreateNUnitsAtLoc(1,'h029',Player(PLAYER_NEUTRAL_AGGRESSIVE),udg_Dublith_Spawn_Location,bj_UNIT_FACING)
set udg_Dublith_Spawn_Location=SL_local
call CreateNUnitsAtLoc(1,'h029',Player(PLAYER_NEUTRAL_AGGRESSIVE),udg_Dublith_Spawn_Location,bj_UNIT_FACING)
set udg_Dublith_Spawn_Location=SL_local
call CreateNUnitsAtLoc(1,'h029',Player(PLAYER_NEUTRAL_AGGRESSIVE),udg_Dublith_Spawn_Location,bj_UNIT_FACING)
set udg_Dublith_Spawn_Location=SL_local
call CreateNUnitsAtLoc(1,'h029',Player(PLAYER_NEUTRAL_AGGRESSIVE),udg_Dublith_Spawn_Location,bj_UNIT_FACING)
set udg_Dublith_Spawn_Location=SL_local
set udg_Dublith_Mountain_Unit_Array[udg_Dublith_Mountains_Integer]=GetLastCreatedUnit()
set udg_Dublith_Mountain_Boolean=IsUnitAliveBJ(udg_Dublith_Mountain_Unit_Array[udg_Dublith_Mountains_Integer])
call RemoveLocation(SL_local)
else
call DoNothing()
endif
set udg_Dublith_Mountains_Integer=udg_Dublith_Mountains_Integer+1
endloop
set udg_Dublith_Iteration=1
endfunction
