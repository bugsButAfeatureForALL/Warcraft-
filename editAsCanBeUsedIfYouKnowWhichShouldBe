return false
endif
return true
endfunction
function Trig_Charge_loop_Copy_Func001Func001A takes nothing returns nothing
set udg_Princes_Charge_Points[1]=GetUnitLoc(GetEnumUnit())
set udg_Princes_Charge_Points[2]=GetUnitLoc(LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_Princes_Charge_Hashtable))
set udg_Princes_Charge_Points[3]=PolarProjectionBJ(udg_Princes_Charge_Points[1],15.00,AngleBetweenPoints(udg_Princes_Charge_Points[1],udg_Princes_Charge_Points[2]))
call SetUnitPositionLocFacingLocBJ(GetEnumUnit(),udg_Princes_Charge_Points[3],udg_Princes_Charge_Points[2])
call AddSpecialEffectLocBJ(GetUnitLoc(GetEnumUnit()),"Abilities\\Spells\\Human\\Thunderclap\\ThunderClapCaster.mdl")
call DestroyEffect(GetLastCreatedEffectBJ())
call AddSpecialEffectTargetUnitBJ("chest",GetEnumUnit(),"Objects\\Spawnmodels\\Undead\\ImpaleTargetDust\\ImpaleTargetDust.mdl")
call DestroyEffect(GetLastCreatedEffectBJ())
if(Trig_Charge_loop_Copy_Func001Func001Func009C())then
call GroupRemoveUnitSimple(GetEnumUnit(),udg_Princes_Charge_Group)
call SetUnitPathing(GetEnumUnit(),true)
call SetUnitTimeScalePercent(GetEnumUnit(),100.00)
call SetUnitAnimation(GetEnumUnit(),"Walk")
if(Trig_Charge_loop_Copy_Func001Func001Func009Func006C())then
call UnitDamageTargetBJ(GetEnumUnit(),LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_Princes_Charge_Hashtable),300.00,ATTACK_TYPE_MAGIC,DAMAGE_TYPE_NORMAL)
else
endif
if(Trig_Charge_loop_Copy_Func001Func001Func009Func007C())then
call UnitDamageTargetBJ(GetEnumUnit(),LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_Princes_Charge_Hashtable),350.00,ATTACK_TYPE_MAGIC,DAMAGE_TYPE_NORMAL)
else
endif
if(Trig_Charge_loop_Copy_Func001Func001Func009Func008C())then
call UnitDamageTargetBJ(GetEnumUnit(),LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_Princes_Charge_Hashtable),400.00,ATTACK_TYPE_MAGIC,DAMAGE_TYPE_NORMAL)
else
endif
if(Trig_Charge_loop_Copy_Func001Func001Func009Func009C())then
call UnitDamageTargetBJ(GetEnumUnit(),LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_Princes_Charge_Hashtable),450.00,ATTACK_TYPE_MAGIC,DAMAGE_TYPE_NORMAL)
else
endif
if(Trig_Charge_loop_Copy_Func001Func001Func009Func010C())then
call UnitDamageTargetBJ(GetEnumUnit(),LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_Princes_Charge_Hashtable),500.00,ATTACK_TYPE_MAGIC,DAMAGE_TYPE_NORMAL)
else
endif
call AddSpecialEffectTargetUnitBJ("chest",LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_Princes_Charge_Hashtable),"Objects\\Spawnmodels\\Human\\HumanBlood\\BloodElfSpellThiefBlood.mdl")
call DestroyEffect(GetLastCreatedEffectBJ())
call RemoveLocation(udg_Princes_Charge_Points[1])
call RemoveLocation(udg_Princes_Charge_Points[2])
call RemoveLocation(udg_Princes_Charge_Points[3])
call FlushChildHashtableBJ(GetHandleIdBJ(GetEnumUnit()),udg_Princes_Charge_Hashtable)
else
call ForGroupBJ(udg_Princes_Charge_Group,function Trig_Charge_loop_Copy_Func001Func001Func009Func001A)
endif
endfunction
function Trig_Charge_loop_Copy_Func001C takes nothing returns boolean
if(not(IsUnitGroupEmptyBJ(udg_Princes_Charge_Group)==true))then
return false
endif
return true
endfunction
