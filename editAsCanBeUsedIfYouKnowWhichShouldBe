endfunction
function Trig_God_Father_AI_Conditions takes nothing returns boolean
if(not(GetUnitLifePercent(udg_Our_Father)<=95.00))then
return false
endif
if(not(GetUnitTypeId(GetAttackedUnitBJ())=='H04O'))then
return false
endif
return true
endfunction
function Trig_God_Father_AI_Func004Func007Func007C takes nothing returns boolean
if(not(udg_Father_Attacks==3))then
return false
endif
return true
endfunction
function Trig_God_Father_AI_Func004Func007C takes nothing returns boolean
if(not(udg_Father_Attacks==2))then
return false
endif
return true
