endfunction
function Trig_Father_Dies_First_Player_Actions takes nothing returns nothing
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Father|r: You pathetic ants really believe you can defeat a God..?")
call TriggerSleepAction(5.00)
call CreateNUnitsAtLoc(1,'H02R',udg_Father_Player,GetUnitLoc(GetDyingUnit()),bj_UNIT_FACING)
call SetHeroLevelBJ(GetLastCreatedUnit(),35,false)
call RemoveUnit(GetDyingUnit())
set udg_Our_Father=GetLastCreatedUnit()
call SelectUnitForPlayerSingle(udg_Our_Father,udg_Father_Player)
call SelectHeroSkill(udg_Our_Father,'A07O')
call SelectHeroSkill(udg_Our_Father,'A07N')
call SelectHeroSkill(udg_Our_Father,'A04P')
call SelectHeroSkill(udg_Our_Father,'A00S')
call UnitAddAbilityBJ('A09W',udg_Our_Father)
if(Trig_Father_Dies_First_Player_Func013C())then
call ModifyHeroStat(bj_HEROSTAT_STR,udg_Our_Father,bj_MODIFYMETHOD_ADD,udg_Godhood_Stats)
call ModifyHeroStat(bj_HEROSTAT_AGI,udg_Our_Father,bj_MODIFYMETHOD_ADD,udg_Godhood_Stats)
call ModifyHeroStat(bj_HEROSTAT_INT,udg_Our_Father,bj_MODIFYMETHOD_ADD,udg_Godhood_Stats)
call UnitAddAbilityBJ('A088',udg_Our_Father)
else
endif
endfunction
