if(Trig_Charge_loop_Copy_Func001C())then
call DisableTrigger(GetTriggeringTrigger())
else
call ForGroupBJ(udg_Princes_Charge_Group,function Trig_Charge_loop_Copy_Func001Func001A)
endif
endfunction
