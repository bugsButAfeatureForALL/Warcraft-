function Trig_Blade_Rush_Loop_Func001Func001Func005C takes nothing returns boolean
if(not(DistanceBetweenPoints(udg_Blade_Points[1],udg_Blade_Points[2])<=50.00))then
return false
endif
return true
endfunction
function Trig_Blade_Rush_Loop_Func001Func001A takes nothing returns nothing
set udg_Blade_Points[1]=GetUnitLoc(GetEnumUnit())
set udg_Blade_Points[2]=GetUnitLoc(LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_Blade_Table))
set udg_Blade_Points[3]=PolarProjectionBJ(udg_Blade_Points[1],15.00,AngleBetweenPoints(udg_Blade_Points[1],udg_Blade_Points[2]))
call SetUnitPositionLocFacingLocBJ(GetEnumUnit(),udg_Blade_Points[3],udg_Blade_Points[2])
if(Trig_Blade_Rush_Loop_Func001Func001Func005C())then
call GroupRemoveUnitSimple(GetEnumUnit(),udg_Blade_Group)
call SetUnitPathing(GetEnumUnit(),true)
call SetUnitTimeScalePercent(GetEnumUnit(),100.00)
call SetUnitAnimation(GetEnumUnit(),"Walk")
if(Trig_Blade_Rush_Loop_Func001Func001Func005Func006C())then
call SetUnitAnimation(GetEnumUnit(),"Spell")
call UnitDamageTargetBJ(GetEnumUnit(),LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_Blade_Table),300.00,ATTACK_TYPE_MAGIC,DAMAGE_TYPE_NORMAL)
call CreateNUnitsAtLoc(1,'h03Y',udg_Slicer_Player,GetUnitLoc(udg_Blade_Rush_Target),bj_UNIT_FACING)
set udg_Blade_Rush_Bonus=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(1.00,'BTLF',udg_Blade_Rush_Bonus)
call IssuePointOrderLoc(udg_Blade_Rush_Bonus,"silence",GetUnitLoc(udg_Blade_Rush_Target))
else
endif
if(Trig_Blade_Rush_Loop_Func001Func001Func005Func007C())then
call SetUnitAnimation(GetEnumUnit(),"Spell")
call UnitDamageTargetBJ(GetEnumUnit(),LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_Blade_Table),400.00,ATTACK_TYPE_MAGIC,DAMAGE_TYPE_NORMAL)
call CreateNUnitsAtLoc(1,'h03Y',udg_Slicer_Player,GetUnitLoc(udg_Blade_Rush_Target),bj_UNIT_FACING)
set udg_Blade_Rush_Bonus=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(1.00,'BTLF',udg_Blade_Rush_Bonus)
call IssuePointOrderLoc(udg_Blade_Rush_Bonus,"silence",GetUnitLoc(udg_Blade_Rush_Target))
else
endif
if(Trig_Blade_Rush_Loop_Func001Func001Func005Func008C())then
call SetUnitAnimation(GetEnumUnit(),"Spell")
call UnitDamageTargetBJ(GetEnumUnit(),LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_Blade_Table),500.00,ATTACK_TYPE_MAGIC,DAMAGE_TYPE_NORMAL)
call CreateNUnitsAtLoc(1,'h03Y',udg_Slicer_Player,GetUnitLoc(udg_Blade_Rush_Target),bj_UNIT_FACING)
set udg_Blade_Rush_Bonus=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(1.00,'BTLF',udg_Blade_Rush_Bonus)
call IssuePointOrderLoc(udg_Blade_Rush_Bonus,"silence",GetUnitLoc(udg_Blade_Rush_Target))
else
endif
if(Trig_Blade_Rush_Loop_Func001Func001Func005Func009C())then
call SetUnitAnimation(GetEnumUnit(),"Spell")
call UnitDamageTargetBJ(GetEnumUnit(),LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_Blade_Table),600.00,ATTACK_TYPE_MAGIC,DAMAGE_TYPE_NORMAL)
call CreateNUnitsAtLoc(1,'h03Y',udg_Slicer_Player,GetUnitLoc(udg_Blade_Rush_Target),bj_UNIT_FACING)
set udg_Blade_Rush_Bonus=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(1.00,'BTLF',udg_Blade_Rush_Bonus)
call IssuePointOrderLoc(udg_Blade_Rush_Bonus,"silence",GetUnitLoc(udg_Blade_Rush_Target))
else
endif
if(Trig_Blade_Rush_Loop_Func001Func001Func005Func010C())then
call SetUnitAnimation(GetEnumUnit(),"Spell")
call UnitDamageTargetBJ(GetEnumUnit(),LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_Blade_Table),700.00,ATTACK_TYPE_MAGIC,DAMAGE_TYPE_NORMAL)
call CreateNUnitsAtLoc(1,'h03Y',udg_Slicer_Player,GetUnitLoc(udg_Blade_Rush_Target),bj_UNIT_FACING)
set udg_Blade_Rush_Bonus=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(1.00,'BTLF',udg_Blade_Rush_Bonus)
call IssuePointOrderLoc(udg_Blade_Rush_Bonus,"silence",GetUnitLoc(udg_Blade_Rush_Target))
else
endif
call AddSpecialEffectTargetUnitBJ("chest",LoadUnitHandleBJ(StringHashBJ("target"),GetHandleIdBJ(GetEnumUnit()),udg_Blade_Table),"Objects\\Spawnmodels\\Human\\HumanBlood\\BloodElfSpellThiefBlood.mdl")
call DestroyEffect(GetLastCreatedEffectBJ())
call RemoveLocation(udg_Blade_Points[1])
call RemoveLocation(udg_Blade_Points[2])
call RemoveLocation(udg_Blade_Points[3])
call FlushChildHashtableBJ(GetHandleIdBJ(GetEnumUnit()),udg_Blade_Table)
else
call ForGroupBJ(udg_Blade_Group,function Trig_Blade_Rush_Loop_Func001Func001Func005Func001A)
endif
endfunction
function Trig_Blade_Rush_Loop_Func001C takes nothing returns boolean
if(not(IsUnitGroupEmptyBJ(udg_Blade_Group)==true))then
return false
endif
return true
endfunction
function Trig_Blade_Rush_Loop_Actions takes nothing returns nothing
if(Trig_Blade_Rush_Loop_Func001C())then
call DisableTrigger(GetTriggeringTrigger())
else
call ForGroupBJ(udg_Blade_Group,function Trig_Blade_Rush_Loop_Func001Func001A)
endif
endfunction
function InitTrig_Blade_Rush_Loop takes nothing returns nothing
set gg_trg_Blade_Rush_Loop=CreateTrigger()
call DisableTrigger(gg_trg_Blade_Rush_Loop)
call TriggerRegisterTimerEventPeriodic(gg_trg_Blade_Rush_Loop,0.03)
call TriggerAddAction(gg_trg_Blade_Rush_Loop,function Trig_Blade_Rush_Loop_Actions)
endfunction
