if(not(IsUnitEnemy(GetEnumUnit(),udg_Pride_Player)==true))then
return false
endif
return true
endfunction
function Trig_Creeping_Cast_Func005A takes nothing returns nothing
if(Trig_Creeping_Cast_Func005Func001C())then
set udg_Creeping_Shadows_Point=GetUnitLoc(GetEnumUnit())
call CreateNUnitsAtLoc(1,'h034',udg_Pride_Player,udg_Creeping_Shadows_Point,bj_UNIT_FACING)
set udg_Creeping_Dummy=GetLastCreatedUnit()
call SetUnitAbilityLevelSwapped('A05G',udg_Creeping_Dummy,GetUnitAbilityLevelSwapped('A05F',udg_Pride_Unit))
call IssueTargetOrder(udg_Creeping_Dummy,"entanglingroots",GetEnumUnit())
call UnitApplyTimedLifeBJ(2.00,'BTLF',udg_Creeping_Dummy)
call RemoveLocation(udg_Creeping_Shadows_Point)
else
endif
endfunction
function Trig_Creeping_Cast_Actions takes nothing returns nothing
set udg_Creeping_Shadows_Point=GetUnitLoc(udg_Pride_Unit)
set udg_Creeping_Shadows_Radius=(350.00+(100.00*I2R(GetUnitAbilityLevelSwapped('A05F',udg_Pride_Unit))))
set udg_Creeping_Group=GetUnitsInRangeOfLocAll(udg_Creeping_Shadows_Radius,udg_Creeping_Shadows_Point)
call RemoveLocation(udg_Creeping_Shadows_Point)
call ForGroupBJ(udg_Creeping_Group,function Trig_Creeping_Cast_Func005A)
call DestroyGroup(udg_Creeping_Group)
endfunction
