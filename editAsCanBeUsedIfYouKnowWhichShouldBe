function Trig_Ninja_Cast_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A00R'))then
return false
endif
return true
endfunction
function Trig_Ninja_Cast_Func020C takes nothing returns boolean
if(not(udg_Ninja_Number==101.00))then
return false
endif
return true
endfunction
function Trig_Ninja_Cast_Actions takes nothing returns nothing
set udg_Ninja_Caster=GetSpellAbilityUnit()
set udg_Ninja_Target=GetSpellTargetUnit()
set udg_Ninja_Number=100.00
call TriggerSleepAction(0.05)
call PauseUnitBJ(true,GetTriggerUnit())
call SetUnitInvulnerable(udg_Ninja_Caster,true)
call TriggerSleepAction(0.05)
call SetUnitAnimation(udg_Ninja_Caster,"attack")
call EnableTrigger(gg_trg_Ninja_Fading)
call TriggerSleepAction(0.30)
call SetUnitTimeScalePercent(udg_Ninja_Caster,40.00)
call SetUnitFlyHeightBJ(udg_Ninja_Caster,300.00,800.00)
call TriggerSleepAction(0.65)
call SetUnitTimeScalePercent(udg_Ninja_Caster,110.00)
call SetUnitAnimation(udg_Ninja_Caster,"attack")
call EnableTrigger(gg_trg_Ninja_Rotate)
call TriggerSleepAction(3.00)
call DisableTrigger(gg_trg_Ninja_Rotate)
call TriggerSleepAction(0.10)
if(Trig_Ninja_Cast_Func020C())then
call DoNothing()
else
call SetUnitVertexColorBJ(udg_Ninja_Caster,100.00,100.00,100.00,0)
call SetUnitPositionLocFacingLocBJ(udg_Ninja_Caster,PolarProjectionBJ(GetUnitLoc(udg_Ninja_Target),100.00,(GetUnitFacing(udg_Ninja_Caster)-GetRandomReal(290.00,70.00))),GetUnitLoc(udg_Ninja_Target))
call SetUnitFlyHeightBJ(udg_Ninja_Caster,0.00,9999.00)
call PauseUnitBJ(false,GetTriggerUnit())
call SetUnitInvulnerable(udg_Ninja_Caster,false)
call ResetUnitAnimation(udg_Ninja_Caster)
endif
endfunction
function InitTrig_Ninja_Cast takes nothing returns nothing
set gg_trg_Ninja_Cast=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Ninja_Cast,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Ninja_Cast,Condition(function Trig_Ninja_Cast_Conditions))
call TriggerAddAction(gg_trg_Ninja_Cast,function Trig_Ninja_Cast_Actions)
endfunction
function Trig_Ninja_Rotate_Func001C takes nothing returns boolean
if(not(IsUnitAliveBJ(udg_Ninja_Target)==true))then
return false
endif
return true
endfunction
function Trig_Ninja_Rotate_Actions takes nothing returns nothing
if(Trig_Ninja_Rotate_Func001C())then
call CreateNUnitsAtLocFacingLocBJ(1,'h00O',GetOwningPlayer(udg_Ninja_Caster),GetUnitLoc(udg_Ninja_Caster),GetUnitLoc(udg_Ninja_Target))
call IssueTargetOrder(GetLastCreatedUnit(),"attack",udg_Ninja_Target)
call UnitApplyTimedLifeBJ(0.20,'BTLF',GetLastCreatedUnit())
call SetUnitPositionLocFacingLocBJ(udg_Ninja_Caster,PolarProjectionBJ(GetUnitLoc(udg_Ninja_Target),200.00,(GetUnitFacing(udg_Ninja_Caster)-GetRandomReal(290.00,70.00))),GetUnitLoc(udg_Ninja_Target))
call SetUnitAnimation(udg_Ninja_Caster,"spell")
call CreateNUnitsAtLocFacingLocBJ(1,'h00O',GetOwningPlayer(udg_Ninja_Caster),GetUnitLoc(udg_Ninja_Caster),GetUnitLoc(udg_Ninja_Target))
call IssueTargetOrder(GetLastCreatedUnit(),"attack",udg_Ninja_Target)
call UnitApplyTimedLifeBJ(1.00,'BTLF',GetLastCreatedUnit())
else
call DisableTrigger(GetTriggeringTrigger())
set udg_Ninja_Number=101.00
call SetUnitFlyHeightBJ(udg_Ninja_Caster,0.00,9999.00)
call SetUnitVertexColorBJ(udg_Ninja_Caster,100.00,100.00,100.00,0)
call PauseUnitBJ(false,udg_Ninja_Caster)
call ResetUnitAnimation(udg_Ninja_Caster)
call SetUnitInvulnerable(udg_Ninja_Caster,false)
endif
endfunction
function InitTrig_Ninja_Rotate takes nothing returns nothing
set gg_trg_Ninja_Rotate=CreateTrigger()
call DisableTrigger(gg_trg_Ninja_Rotate)
call TriggerRegisterTimerEventPeriodic(gg_trg_Ninja_Rotate,0.50)
call TriggerAddAction(gg_trg_Ninja_Rotate,function Trig_Ninja_Rotate_Actions)
endfunction
