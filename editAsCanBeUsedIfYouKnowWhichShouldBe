else
if(Trig_Knowledge_Func008Func002Func004Func004Func001C())then
call ModifyHeroStat(bj_HEROSTAT_STR,udg_He_Who_Sees_Truth,bj_MODIFYMETHOD_SET,(udg_Truth_Strength+75))
call ModifyHeroStat(bj_HEROSTAT_INT,udg_He_Who_Sees_Truth,bj_MODIFYMETHOD_SET,(udg_Truth_Alchemy-45))
call DisplayTextToForce(GetForceOfPlayer(udg_He_Who_Sees_Truth_Player),"|cff995500Truth: There little Al-che-mist. I have increased one of your attributes. But in exchange I have lowered another. That is the price of Truth.|r")
else
if(Trig_Knowledge_Func008Func002Func004Func004Func001Func001C())then
call ModifyHeroStat(bj_HEROSTAT_STR,udg_He_Who_Sees_Truth,bj_MODIFYMETHOD_SET,(udg_Truth_Strength+75))
call ModifyHeroStat(bj_HEROSTAT_AGI,udg_He_Who_Sees_Truth,bj_MODIFYMETHOD_SET,(udg_Truth_Agility-45))
call DisplayTextToForce(GetForceOfPlayer(udg_He_Who_Sees_Truth_Player),"|cff995500Truth: There little Al-che-mist. I have increased one of your attributes. But in exchange I have lowered another. That is the price of Truth.|r")
else
if(Trig_Knowledge_Func008Func002Func004Func004Func001Func001Func001C())then
call ModifyHeroStat(bj_HEROSTAT_INT,udg_He_Who_Sees_Truth,bj_MODIFYMETHOD_SET,(udg_Truth_Alchemy+75))
call ModifyHeroStat(bj_HEROSTAT_AGI,udg_He_Who_Sees_Truth,bj_MODIFYMETHOD_SET,(udg_Truth_Agility-45))
call DisplayTextToForce(GetForceOfPlayer(udg_He_Who_Sees_Truth_Player),"|cff995500Truth: There little Al-che-mist. I have increased one of your attributes. But in exchange I have lowered another. That is the price of Truth.|r")
else
if(Trig_Knowledge_Func008Func002Func004Func004Func001Func001Func001Func001C())then
call ModifyHeroStat(bj_HEROSTAT_INT,udg_He_Who_Sees_Truth,bj_MODIFYMETHOD_SET,(udg_Truth_Alchemy+75))
call SetUnitMoveSpeed(udg_He_Who_Sees_Truth,(udg_Truth_Movement-75.00))
call DisplayTextToForce(GetForceOfPlayer(udg_He_Who_Sees_Truth_Player),"|cff995500Truth: There little Al-che-mist. I have increased one of your attributes. But in exchange I have lowered another. That is the price of Truth.|r")
