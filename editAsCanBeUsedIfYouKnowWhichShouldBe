function Trig_Player_Leave_Items_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
set bj_forLoopAIndex=0
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set udg_Leaver_Items[GetForLoopIndexA()]=UnitItemInSlotBJ(udg_Player_Left,(GetForLoopIndexA()+1))
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set bj_forLoopBIndex=0
set bj_forLoopBIndexEnd=5
loop
exitwhen bj_forLoopBIndex>bj_forLoopBIndexEnd
call UnitRemoveItemFromSlotSwapped((GetForLoopIndexB()+1),udg_Player_Left)
set bj_forLoopBIndex=bj_forLoopBIndex+1
endloop
endfunction
function InitTrig_Player_Leave_Items takes nothing returns nothing
set gg_trg_Player_Leave_Items=CreateTrigger()
call DisableTrigger(gg_trg_Player_Leave_Items)
call TriggerRegisterTimerEventPeriodic(gg_trg_Player_Leave_Items,0.01)
call TriggerAddAction(gg_trg_Player_Leave_Items,function Trig_Player_Leave_Items_Actions)
endfunction
function Trig_Player_1_Leaves_Func001C takes nothing returns boolean
if(not(GetOwningPlayer(udg_Marcoh_Unit)==Player(0)))then
return false
endif
return true
endfunction
