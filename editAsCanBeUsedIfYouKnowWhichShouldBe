if(not(udg_Father_Attacks==1))then
return false
endif
return true
endfunction
function Trig_God_Father_AI_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
call TriggerSleepAction(2.00)
set udg_Father_Attacks=GetRandomInt(1,3)
if(Trig_God_Father_AI_Func004C())then
call EnableTrigger(gg_trg_Gods_Blast)
call TriggerSleepAction(2.00)
call DisableTrigger(gg_trg_Gods_Blast)
call TriggerSleepAction(15.00)
call EnableTrigger(GetTriggeringTrigger())
else
if(Trig_God_Father_AI_Func004Func007C())then
call EnableTrigger(gg_trg_Gods_Wrath)
call TriggerSleepAction(2.00)
call DisableTrigger(gg_trg_Gods_Wrath)
call TriggerSleepAction(15.00)
call EnableTrigger(GetTriggeringTrigger())
else
if(Trig_God_Father_AI_Func004Func007Func007C())then
call TriggerExecute(gg_trg_Gods_Destruction)
call TriggerSleepAction(15.00)
call EnableTrigger(GetTriggeringTrigger())
else
endif
endif
endif
endfunction
function InitTrig_God_Father_AI takes nothing returns nothing
set gg_trg_God_Father_AI=CreateTrigger()
call DisableTrigger(gg_trg_God_Father_AI)
call TriggerRegisterAnyUnitEventBJ(gg_trg_God_Father_AI,EVENT_PLAYER_UNIT_ATTACKED)
call TriggerAddCondition(gg_trg_God_Father_AI,Condition(function Trig_God_Father_AI_Conditions))
call TriggerAddAction(gg_trg_God_Father_AI,function Trig_God_Father_AI_Actions)
endfunction
function Trig_Gods_Blast_Conditions takes nothing returns boolean
if(not(GetUnitTypeId(GetAttackedUnitBJ())=='H04O'))then
return false
endif
return true
endfunction
function Trig_Gods_Blast_Actions takes nothing returns nothing
set udg_Power_Of_God_Target=GetUnitLoc(GetAttacker())
call DisableTrigger(GetTriggeringTrigger())
call SetUnitInvulnerable(udg_Our_Father,true)
call PauseUnitBJ(true,udg_Our_Father)
call UnitAddAbilityBJ('A09G',udg_Our_Father)
call DisplayTimedTextToForce(GetPlayersAll(),5.00,"|cff995500Father:|r Enough of this! You can't hope to defeat a God!")
call TriggerSleepAction(5.00)
call PauseUnitBJ(false,udg_Our_Father)
call IssuePointOrderLoc(udg_Our_Father,"shockwave",udg_Power_Of_God_Target)
call UnitRemoveAbilityBJ('A09G',udg_Our_Father)
call SetUnitInvulnerable(udg_Our_Father,false)
call RemoveLocation(udg_Power_Of_God_Target)
endfunction
