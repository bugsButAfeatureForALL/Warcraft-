return false
endif
if(not GetBooleanOr((GetUnitTypeId(GetManipulatingUnit())=='H014'),(GetBooleanOr((GetBooleanOr((GetUnitTypeId(GetManipulatingUnit())=='H003'),(GetUnitTypeId(GetManipulatingUnit())=='H016'))),(GetBooleanOr((GetUnitTypeId(GetManipulatingUnit())=='H004'),(GetBooleanOr((GetUnitTypeId(GetManipulatingUnit())=='H017'),(GetBooleanOr((GetUnitTypeId(GetManipulatingUnit())=='H01B'),(GetBooleanOr((GetUnitTypeId(GetManipulatingUnit())=='H031'),(GetBooleanOr((GetUnitTypeId(GetManipulatingUnit())=='H032'),(GetBooleanOr((GetUnitTypeId(GetManipulatingUnit())=='H015'),(GetBooleanOr((GetUnitTypeId(GetManipulatingUnit())=='H019'),(GetBooleanOr((GetUnitTypeId(GetManipulatingUnit())=='H02Q'),(GetUnitTypeId(GetManipulatingUnit())=='H01C')))))))))))))))))))))then
return false
endif
return true
endfunction
function Trig_Getting_the_Stone_Func003C takes nothing returns boolean
if(not(UnitHasItemOfTypeBJ(udg_Hero,'I000')==true))then
return false
endif
return true
endfunction
function Trig_Getting_the_Stone_Actions takes nothing returns nothing
call RemoveItem(GetManipulatedItem())
set udg_Hero=GetManipulatingUnit()
if(Trig_Getting_the_Stone_Func003C())then
call SetItemCharges(GetItemOfTypeFromUnitBJ(udg_Hero,'I000'),(GetItemCharges(GetItemOfTypeFromUnitBJ(udg_Hero,'I000'))+1))
else
call UnitAddItemByIdSwapped('I000',udg_Hero)
call SetItemCharges(GetLastCreatedItem(),1)
endif
endfunction
function InitTrig_Getting_the_Stone takes nothing returns nothing
set gg_trg_Getting_the_Stone=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Getting_the_Stone,EVENT_PLAYER_UNIT_PICKUP_ITEM)
call TriggerAddCondition(gg_trg_Getting_the_Stone,Condition(function Trig_Getting_the_Stone_Conditions))
call TriggerAddAction(gg_trg_Getting_the_Stone,function Trig_Getting_the_Stone_Actions)
endfunction
