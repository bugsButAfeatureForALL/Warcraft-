function Trig_Flash_Bang_Func003002003 takes nothing returns boolean
return(IsUnitEnemy(GetFilterUnit(),udg_Lan_Fan_Player)==true)
endfunction
function Trig_Flash_Bang_Func004A takes nothing returns nothing
call CreateNUnitsAtLoc(1,'h03S',udg_Lan_Fan_Player,GetUnitLoc(GetEnumUnit()),bj_UNIT_FACING)
set udg_Lan_Fan_Flash_Dummy=GetLastCreatedUnit()
call SetUnitAbilityLevelSwapped('A07D',udg_Lan_Fan_Flash_Dummy,GetUnitAbilityLevelSwapped('A00J',udg_Lan_Fan_Unit))
call IssueTargetOrder(udg_Lan_Fan_Flash_Dummy,"curse",GetEnumUnit())
call UnitApplyTimedLifeBJ(2.00,'BTLF',udg_Lan_Fan_Flash_Dummy)
endfunction
function Trig_Flash_Bang_Actions takes nothing returns nothing
call CreateNUnitsAtLoc(1,'h03T',udg_Lan_Fan_Player,GetUnitLoc(udg_Lan_Fan_Unit),bj_UNIT_FACING)
call UnitApplyTimedLifeBJ(1.00,'BTLF',GetLastCreatedUnit())
set udg_Flash_Group=GetUnitsInRangeOfLocMatching(500.00,GetUnitLoc(udg_Lan_Fan_Unit),Condition(function Trig_Flash_Bang_Func003002003))
call ForGroupBJ(udg_Flash_Group,function Trig_Flash_Bang_Func004A)
call DestroyGroup(udg_Flash_Group)
endfunction
function InitTrig_Flash_Bang takes nothing returns nothing
set gg_trg_Flash_Bang=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Flash_Bang,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Flash_Bang,Condition(function Trig_Flash_Bang_Conditions))
call TriggerAddAction(gg_trg_Flash_Bang,function Trig_Flash_Bang_Actions)
endfunction
function Trig_Leap_Cast_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A09J'))then
return false
endif
return true
endfunction
function Trig_Leap_Cast_Actions takes nothing returns nothing
set udg_Jump_Caster=GetSpellAbilityUnit()
set udg_CasterPoint=GetUnitLoc(udg_Jump_Caster)
set udg_Casting_point=GetSpellTargetLoc()
set udg_EnemyToDamage=GetSpellTargetUnit()
set udg_DistanceBetweenPoints=(DistanceBetweenPoints(udg_Casting_point,udg_CasterPoint)/80.00)
call SetUnitAnimation(udg_Jump_Caster,"Stand")
call RemoveLocation(udg_CasterPoint)
call RemoveLocation(udg_Casting_point)
set udg_Time=0.00
set udg_hieght=0.00
call PauseUnitBJ(true,udg_Jump_Caster)
call SetUnitAnimation(udg_Jump_Caster,"Spell Slam")
call UnitAddAbilityBJ('Arav',udg_Jump_Caster)
call UnitRemoveAbilityBJ('Arav',udg_Jump_Caster)
call SetUnitPathing(udg_Jump_Caster,false)
call EnableTrigger(gg_trg_Leap)
endfunction
function InitTrig_Leap_Cast takes nothing returns nothing
set gg_trg_Leap_Cast=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Leap_Cast,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Leap_Cast,Condition(function Trig_Leap_Cast_Conditions))
call TriggerAddAction(gg_trg_Leap_Cast,function Trig_Leap_Cast_Actions)
endfunction
function Trig_Leap_Func013Func003C takes nothing returns boolean
if(not(udg_Time<40.00))then
return false
endif
return true
endfunction
function Trig_Leap_Func013C takes nothing returns boolean
if(not(udg_Time<20.00))then
return false
endif
return true
