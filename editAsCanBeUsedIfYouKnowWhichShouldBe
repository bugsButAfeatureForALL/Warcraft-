function Trig_Weaken_Father_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A029'))then
return false
endif
if(not(DistanceBetweenPoints(GetUnitLoc(udg_Greed),GetUnitLoc(udg_Our_Father))<=300.00))then
return false
endif
return true
endfunction
function Trig_Weaken_Father_Actions takes nothing returns nothing
call PanCameraToTimedLocForPlayer(udg_Ling_Player,GetUnitLoc(udg_Greed),0)
call RemoveUnit(udg_Lings_Soul)
call RemoveUnit(udg_Greed_Soul)
call PauseTimerBJ(true,udg_Father_Timer)
call PauseUnitBJ(true,udg_Our_Father)
call PauseUnitBJ(true,udg_Greed)
call SetUnitInvulnerable(udg_Our_Father,true)
call SetUnitInvulnerable(udg_Greed,true)
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Father|r: Greed! Return to me my son!")
call TriggerSleepAction(5.00)
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Greed|r: Sure thing pops! But you should remember, I was never the best son!")
call AddSpecialEffectTargetUnitBJ("origin",udg_Greed,"war3mapImported\\SS2LightningRED.mdx")
call TriggerSleepAction(3.00)
call EnableTrigger(gg_trg_Unpause_Ling)
call ShowUnitHide(udg_Greed)
call SetPlayerState(udg_Greed_Player,PLAYER_STATE_OBSERVER,1)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Greed_Player),bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Greed_Player),bj_ALLIANCE_ALLIED_VISION)
call SetHeroLevelBJ(udg_Ling_Unit,GetHeroLevel(udg_Greed),false)
call SetUnitPositionLoc(udg_Ling_Unit,GetUnitLoc(udg_Greed))
call SetUnitOwner(udg_Ling_Unit,udg_Ling_Player,true)
call PauseUnitBJ(false,udg_Ling_Unit)
call PanCameraToTimedLocForPlayer(udg_Ling_Player,GetUnitLoc(udg_Ling_Unit),0)
call SelectUnitForPlayerSingle(udg_Ling_Unit,udg_Ling_Player)
call ShowUnitShow(udg_Ling_Unit)
call SetUnitInvulnerable(udg_Ling_Unit,false)
call RemoveUnit(udg_Greed)
call TriggerSleepAction(1.00)
call DisableTrigger(gg_trg_Unpause_Ling)
call TriggerSleepAction(3.00)
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Father|r: What have..you..done to me?!")
call TriggerSleepAction(3.00)
call ForceAddPlayerSimple(udg_Ling_Player,udg_Alchemists)
call ForceRemovePlayerSimple(udg_Greed_Player,udg_Alchemists)
call ForceRemovePlayerSimple(udg_Greed_Player,udg_Good_Team)
call UnitAddAbilityBJ('A086',udg_Our_Father)
set udg_Greeds_Sacrifice=true
call SetUnitInvulnerable(udg_Our_Father,false)
call PauseUnitBJ(false,udg_Our_Father)
call PauseTimerBJ(false,udg_Father_Timer)
call DisplayTextToForce(udg_Good_Team,"|cffff0000Greed|r has sacrificed his life in order to weaken Father! Due to Greeds abilities Father now recieves 50% increased physical damage!")
endfunction
function InitTrig_Weaken_Father takes nothing returns nothing
set gg_trg_Weaken_Father=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Weaken_Father,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Weaken_Father,Condition(function Trig_Weaken_Father_Conditions))
call TriggerAddAction(gg_trg_Weaken_Father,function Trig_Weaken_Father_Actions)
endfunction
function Trig_Greed_Item_Change_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
set bj_forLoopAIndex=0
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set udg_Current_Greed_Items[GetForLoopIndexA()]=UnitItemInSlotBJ(udg_Ling_Unit,(GetForLoopIndexA()+1))
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set bj_forLoopBIndex=0
set bj_forLoopBIndexEnd=5
loop
exitwhen bj_forLoopBIndex>bj_forLoopBIndexEnd
call UnitRemoveItemFromSlotSwapped((GetForLoopIndexB()+1),udg_Ling_Unit)
call UnitAddItemSwapped(udg_Current_Greed_Items[GetForLoopIndexB()],udg_Greed)
set bj_forLoopBIndex=bj_forLoopBIndex+1
endloop
endfunction
function InitTrig_Greed_Item_Change takes nothing returns nothing
set gg_trg_Greed_Item_Change=CreateTrigger()
call DisableTrigger(gg_trg_Greed_Item_Change)
call TriggerRegisterTimerEventPeriodic(gg_trg_Greed_Item_Change,0.01)
call TriggerAddAction(gg_trg_Greed_Item_Change,function Trig_Greed_Item_Change_Actions)
endfunction
