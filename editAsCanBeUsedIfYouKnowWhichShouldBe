function Trig_Drop_Homunc_Items_Func002Func001C takes nothing returns boolean
if(not(GetItemType(UnitItemInSlotBJ(udg_Dead_Homunc,GetForLoopIndexA()))!=ITEM_TYPE_ARTIFACT))then
return false
endif
if(not(GetItemType(UnitItemInSlotBJ(udg_Dead_Homunc,GetForLoopIndexA()))!=ITEM_TYPE_CHARGED))then
return false
endif
return true
endfunction
function Trig_Drop_Homunc_Items_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
set bj_forLoopAIndex=0
set bj_forLoopAIndexEnd=6
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
if(Trig_Drop_Homunc_Items_Func002Func001C())then
call UnitRemoveItemFromSlotSwapped(GetForLoopIndexA(),udg_Dead_Homunc)
else
endif
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endfunction
function InitTrig_Drop_Homunc_Items takes nothing returns nothing
set gg_trg_Drop_Homunc_Items=CreateTrigger()
call DisableTrigger(gg_trg_Drop_Homunc_Items)
call TriggerRegisterTimerEventPeriodic(gg_trg_Drop_Homunc_Items,0.01)
call TriggerAddAction(gg_trg_Drop_Homunc_Items,function Trig_Drop_Homunc_Items_Actions)
endfunction
function Trig_Homunculus_Resurrection_Cast_Func011001 takes nothing returns boolean
return(GetItemTypeId(GetManipulatedItem())=='I000')
endfunction
function Trig_Homunculus_Resurrection_Cast_Func011002 takes nothing returns boolean
return(GetItemTypeId(GetManipulatedItem())=='I005')
endfunction
function Trig_Homunculus_Resurrection_Cast_Conditions takes nothing returns boolean
if(not GetBooleanOr((GetItemTypeId(GetManipulatedItem())=='I000'),(GetItemTypeId(GetManipulatedItem())=='I005')))then
return false
endif
return true
endfunction
function Trig_Homunculus_Resurrection_Cast_Func002C takes nothing returns boolean
if(not(udg_Homunc_Drop_Chance==1))then
return false
endif
return true
endfunction
function Trig_Homunculus_Resurrection_Cast_Actions takes nothing returns nothing
set udg_Homunc_Drop_Chance=GetRandomInt(1,2)
if(Trig_Homunculus_Resurrection_Cast_Func002C())then
call EnableTrigger(gg_trg_Drop_Homunc_Items)
else
endif
set udg_Dead_Homunc=GetManipulatingUnit()
set udg_Timer_Lightning_Effect=0.00
set udg_Caster_Regeneration=GetTriggerUnit()
set udg_LightningRegen_PositionEnd=GetUnitLoc(udg_Caster_Regeneration)
set udg_HomunculusRegPositionBuffer=PolarProjectionBJ(udg_LightningRegen_PositionEnd,100.00,GetUnitFacing(udg_Caster_Regeneration))
call RemoveLocation(udg_LightningRegen_PositionEnd)
call EnableTrigger(gg_trg_Lightning_Effect)
call EnableTrigger(gg_trg_Timer_Lightning_Effect)
endfunction
function InitTrig_Homunculus_Resurrection_Cast takes nothing returns nothing
set gg_trg_Homunculus_Resurrection_Cast=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Homunculus_Resurrection_Cast,EVENT_PLAYER_UNIT_USE_ITEM)
call TriggerAddCondition(gg_trg_Homunculus_Resurrection_Cast,Condition(function Trig_Homunculus_Resurrection_Cast_Conditions))
call TriggerAddAction(gg_trg_Homunculus_Resurrection_Cast,function Trig_Homunculus_Resurrection_Cast_Actions)
endfunction
function Trig_Lightning_Effect_Func006C takes nothing returns boolean
if(not(udg_Timer_Lightning_Effect<10.00))then
return false
endif
return true
endfunction
function Trig_Lightning_Effect_Actions takes nothing returns nothing
local integer lightning_loc=0
call DisableTrigger(GetTriggeringTrigger())
set udg_Lightning_Counter=1
loop
exitwhen udg_Lightning_Counter>4
set udg_LightningRegen_PositionEnd=PolarProjectionBJ(udg_HomunculusRegPositionBuffer,75.00,GetRandomDirectionDeg())
set udg_LightningRegen_PositionStart=PolarProjectionBJ(udg_HomunculusRegPositionBuffer,75.00,GetRandomDirectionDeg())
call AddLightningLoc("AFOD",udg_LightningRegen_PositionStart,udg_LightningRegen_PositionEnd)
set udg_Lightning_FingerOD[udg_Lightning_Counter]=GetLastCreatedLightningBJ()
call RemoveLocation(udg_LightningRegen_PositionStart)
call RemoveLocation(udg_LightningRegen_PositionEnd)
set udg_Lightning_Counter=udg_Lightning_Counter+1
endloop
call TriggerSleepAction(0.25)
set bj_forLoopBIndex=0
set bj_forLoopBIndexEnd=6
loop
exitwhen bj_forLoopBIndex>bj_forLoopBIndexEnd
set udg_Lightning_D_Counter=lightning_loc
call DestroyLightning(udg_Lightning_FingerOD[udg_Lightning_D_Counter])
set lightning_loc=(lightning_loc+1)
set bj_forLoopBIndex=bj_forLoopBIndex+1
endloop
if(Trig_Lightning_Effect_Func006C())then
call EnableTrigger(GetTriggeringTrigger())
else
call DisableTrigger(gg_trg_Timer_Lightning_Effect)
set udg_Timer_Lightning_Effect=0.00
call RemoveLocation(udg_HomunculusRegPositionBuffer)
endif
endfunction
function InitTrig_Lightning_Effect takes nothing returns nothing
set gg_trg_Lightning_Effect=CreateTrigger()
call DisableTrigger(gg_trg_Lightning_Effect)
call TriggerRegisterTimerEventPeriodic(gg_trg_Lightning_Effect,0.40)
call TriggerAddAction(gg_trg_Lightning_Effect,function Trig_Lightning_Effect_Actions)
endfunction
