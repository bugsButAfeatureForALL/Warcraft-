function Trig_Barry_is_Chosen_Actions takes nothing returns nothing
call RemoveUnit(udg_Killer_Healer)
set udg_Barry_Unit=GetTriggerUnit()
set udg_Barry_Player=GetOwningPlayer(GetTriggerUnit())
call SetPlayerName(udg_Barry_Player,"Barry the Chopper")
if(Trig_Barry_is_Chosen_Func005C())then
call SetPlayerAllianceStateBJ(udg_Barry_Player,udg_Slicer_Player,bj_ALLIANCE_ALLIED_VISION)
call SetPlayerAllianceStateBJ(udg_Slicer_Player,udg_Barry_Player,bj_ALLIANCE_ALLIED_VISION)
else
endif
call SetUnitPositionLoc(GetTriggerUnit(),GetRectCenter(gg_rct_Barry_Spawn))
call PanCameraToTimedLocForPlayer(udg_Barry_Player,GetRectCenter(gg_rct_Barry_Spawn),0)
call SelectUnitForPlayerSingle(GetTriggerUnit(),GetOwningPlayer(GetTriggerUnit()))
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Barry_Player),udg_Evil_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Barry_Player),udg_Good_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Barry_Player),bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Barry_Player),bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[bj_PLAYER_NEUTRAL_VICTIM],GetForceOfPlayer(udg_Barry_Player),bj_ALLIANCE_UNALLIED)
call ForceAddPlayerSimple(udg_Barry_Player,udg_Neutral)
call DisplayTextToForce(GetForceOfPlayer(udg_Barry_Player),"You are the deranged serial killer |cffff0000Barry|r! You gain bonus stats during the Night so it's best to wait until then before venturing out too far. However if Slicer has been chosen than the two of you can work together to kill all before you! If things get too tough you can return to your camp to heal.\n\nBe warned though, if you die once you are |cffff0000gone|r!")
call CreateNUnitsAtLoc(1,'h01Z',udg_Barry_Player,GetRectCenter(gg_rct_Barry_Spawn),bj_UNIT_FACING)
set udg_Killer_Healer=GetLastCreatedUnit()
call DestroyTrigger(GetTriggeringTrigger())
endfunction
