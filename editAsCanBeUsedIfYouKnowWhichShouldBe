function Trig_Learning_Defending_Shadows_Actions takes nothing returns nothing
if(Trig_Learning_Defending_Shadows_Func002C())then
call UnitAddAbilityBJ('A05P',udg_Pride_Unit)
else
if(Trig_Learning_Defending_Shadows_Func002Func003C())then
call SetUnitAbilityLevelSwapped('A05P',udg_Pride_Unit,GetUnitAbilityLevelSwapped('A06G',udg_Pride_Unit))
else
if(Trig_Learning_Defending_Shadows_Func002Func003Func003C())then
call SetUnitAbilityLevelSwapped('A05P',udg_Pride_Unit,GetUnitAbilityLevelSwapped('A06G',udg_Pride_Unit))
else
if(Trig_Learning_Defending_Shadows_Func002Func003Func003Func003C())then
call SetUnitAbilityLevelSwapped('A05P',udg_Pride_Unit,GetUnitAbilityLevelSwapped('A06G',udg_Pride_Unit))
else
if(Trig_Learning_Defending_Shadows_Func002Func003Func003Func003Func003C())then
call SetUnitAbilityLevelSwapped('A05P',udg_Pride_Unit,GetUnitAbilityLevelSwapped('A06G',udg_Pride_Unit))
else
endif
endif
endif
endif
endif
endfunction
