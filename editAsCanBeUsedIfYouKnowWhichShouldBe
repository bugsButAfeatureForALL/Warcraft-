function Trig_Truth_Spawn_Conditions takes nothing returns boolean
if(not(udg_Easter_Egg_Activated==false))then
return false
endif
return true
endfunction
function Trig_Truth_Spawn_Func003Func001C takes nothing returns boolean
if(not(GetOwningPlayer(GetEnumUnit())==GetTriggerPlayer()))then
return false
endif
return true
endfunction
function Trig_Truth_Spawn_Func003A takes nothing returns nothing
if(Trig_Truth_Spawn_Func003Func001C())then
set udg_Interval_Ticker=true
else
endif
endfunction
function Trig_Truth_Spawn_Func006Func010Func001001 takes nothing returns boolean
return(S2I(udg_NPC_Counter)>(S2I(udg_Dummy_Caster)-7))
endfunction
function Trig_Truth_Spawn_Func006Func010Func001002 takes nothing returns boolean
return(S2I(udg_NPC_Counter)<(S2I(udg_Dummy_Caster)+7))
endfunction
function Trig_Truth_Spawn_Func006Func010C takes nothing returns boolean
if(not GetBooleanAnd((S2I(udg_NPC_Counter)>(S2I(udg_Dummy_Caster)-7)),(S2I(udg_NPC_Counter)<(S2I(udg_Dummy_Caster)+7))))then
return false
endif
if(not(udg_Interval_Ticker==true))then
return false
endif
return true
endfunction
function Trig_Truth_Spawn_Func006C takes nothing returns boolean
if(not Trig_Truth_Spawn_Func006Func010C())then
return false
endif
return true
endfunction
function Trig_Truth_Spawn_Actions takes nothing returns nothing
set udg_Interval_Ticker=false
set udg_Damage_Counter=GetUnitsInRangeOfLocAll(400.00,GetUnitLoc(gg_unit_n00F_0168))
call ForGroupBJ(udg_Damage_Counter,function Trig_Truth_Spawn_Func003A)
set udg_NPC_Counter=GetEventPlayerChatString()
if(Trig_Truth_Spawn_Func006C())then
set udg_Easter_Egg_Activated=true
call DisplayTextToForce(GetPlayersAll(),"|cff32cd32E|r|cffff0000A|r|cff995500S|r|cffffcc00T|r|cff7777aaE|r|cff32cd32R|r |cffffcc00E|r|cff32cd32G|r|cffff0000G|r")
call KillUnit(gg_unit_n00F_0168)
call DisableTrigger(GetTriggeringTrigger())
call SetUnitAnimation(gg_unit_h00I_0155,"stand alternate")
call SetUnitAnimation(gg_unit_h00I_0156,"stand alternate")
call TriggerSleepAction(15.00)
call SetUnitAnimation(gg_unit_h00I_0155,"stand")
call SetUnitAnimation(gg_unit_h00I_0156,"stand")
else
endif
endfunction
function InitTrig_Truth_Spawn takes nothing returns nothing
set gg_trg_Truth_Spawn=CreateTrigger()
call TriggerRegisterPlayerChatEvent(gg_trg_Truth_Spawn,Player(0),"",false)
call TriggerRegisterPlayerChatEvent(gg_trg_Truth_Spawn,Player(1),"",false)
call TriggerRegisterPlayerChatEvent(gg_trg_Truth_Spawn,Player(2),"",false)
call TriggerRegisterPlayerChatEvent(gg_trg_Truth_Spawn,Player(3),"",false)
call TriggerRegisterPlayerChatEvent(gg_trg_Truth_Spawn,Player(4),"",false)
call TriggerRegisterPlayerChatEvent(gg_trg_Truth_Spawn,Player(5),"",false)
call TriggerRegisterPlayerChatEvent(gg_trg_Truth_Spawn,Player(6),"",false)
call TriggerRegisterPlayerChatEvent(gg_trg_Truth_Spawn,Player(8),"",false)
call TriggerRegisterPlayerChatEvent(gg_trg_Truth_Spawn,Player(9),"",false)
call TriggerRegisterPlayerChatEvent(gg_trg_Truth_Spawn,Player(10),"",false)
call TriggerRegisterPlayerChatEvent(gg_trg_Truth_Spawn,Player(11),"",false)
call TriggerAddCondition(gg_trg_Truth_Spawn,Condition(function Trig_Truth_Spawn_Conditions))
call TriggerAddAction(gg_trg_Truth_Spawn,function Trig_Truth_Spawn_Actions)
endfunction
