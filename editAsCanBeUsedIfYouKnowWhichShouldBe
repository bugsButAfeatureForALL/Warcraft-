if(Trig_I_want_it_all_Func002C())then
set bj_forLoopAIndex=0
set bj_forLoopAIndexEnd=6
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
if(Trig_I_want_it_all_Func002Func002Func001C())then
call UnitRemoveItemFromSlotSwapped(GetForLoopIndexA(),GetAttackedUnitBJ())
else
endif
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
else
endif
endfunction
