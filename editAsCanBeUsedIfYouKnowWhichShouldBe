function Trig_Queens_Resolve_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
if(Trig_Queens_Resolve_Func005C())then
call EnableTrigger(gg_trg_Queens_Removal)
call UnitAddAbilityBJ('A06A',udg_Olivier_Unit)
call SetUnitAbilityLevelSwapped('A06A',udg_Olivier_Unit,GetUnitAbilityLevelSwapped('A069',udg_Olivier_Unit))
else
if(Trig_Queens_Resolve_Func005Func002C())then
call DisableTrigger(GetTriggeringTrigger())
call EnableTrigger(gg_trg_Queens_Removal)
call UnitAddAbilityBJ('A06A',udg_Olivier_Unit)
call SetUnitAbilityLevelSwapped('A06A',udg_Olivier_Unit,GetUnitAbilityLevelSwapped('A069',udg_Olivier_Unit))
else
if(Trig_Queens_Resolve_Func005Func002Func004C())then
call EnableTrigger(gg_trg_Queens_Removal)
call UnitAddAbilityBJ('A06A',udg_Olivier_Unit)
call SetUnitAbilityLevelSwapped('A06A',udg_Olivier_Unit,GetUnitAbilityLevelSwapped('A069',udg_Olivier_Unit))
else
if(Trig_Queens_Resolve_Func005Func002Func004Func002C())then
call EnableTrigger(gg_trg_Queens_Removal)
call UnitAddAbilityBJ('A06A',udg_Olivier_Unit)
call SetUnitAbilityLevelSwapped('A06A',udg_Olivier_Unit,GetUnitAbilityLevelSwapped('A069',udg_Olivier_Unit))
else
if(Trig_Queens_Resolve_Func005Func002Func004Func002Func002C())then
call EnableTrigger(gg_trg_Queens_Removal)
call UnitAddAbilityBJ('A06A',udg_Olivier_Unit)
call SetUnitAbilityLevelSwapped('A06A',udg_Olivier_Unit,GetUnitAbilityLevelSwapped('A069',udg_Olivier_Unit))
else
endif
endif
endif
endif
endif
endfunction
