function Trig_The_Promised_Day_Func035Func004001 takes nothing returns boolean
return(GetOwningPlayer(udg_Greed)==Player(8))
endfunction
function Trig_The_Promised_Day_Func035Func004002 takes nothing returns boolean
return(GetOwningPlayer(udg_Greed)==Player(9))
endfunction
function Trig_The_Promised_Day_Func035C takes nothing returns boolean
if(not(udg_GreedLing_Is_Active==true))then
return false
endif
if(not GetBooleanOr((GetOwningPlayer(udg_Greed)==Player(8)),(GetOwningPlayer(udg_Greed)==Player(9))))then
return false
endif
if(not(IsUnitAlly(udg_Greed,Player(7))==true))then
return false
endif
return true
endfunction
function Trig_The_Promised_Day_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
call UnitAddItemByIdSwapped('I003',gg_unit_H01V_0080)
call DisableTrigger(gg_trg_Mannequin_Soldiers_Spawns)
call DisableTrigger(gg_trg_Mannequin_Soldiers_Spawns_Part_2)
call DisableTrigger(gg_trg_Briggs_Fortress_Spawns)
call DisplayTextToForce(GetPlayersAll(),"The |cffff0000Promised Day|r has arrived! Father has activated the Nation Wide Transmutation Circle! All players will take damage unless they make it to the |cffff0000Center|r of the circle!")
call EnableTrigger(gg_trg_Promised_Daymage)
call EnableTrigger(gg_trg_Promised_Day_Effect)
call EnableWeatherEffect(udg_Promised_Day_Effect[0],true)
call EnableWeatherEffect(udg_Promised_Day_Effect[1],true)
call EnableWeatherEffect(udg_Promised_Day_Effect[2],true)
call EnableWeatherEffect(udg_Promised_Day_Effect[3],true)
call EnableWeatherEffect(udg_Promised_Day_Effect[4],true)
call EnableWeatherEffect(udg_Promised_Day_Effect[5],true)
call EnableWeatherEffect(udg_Promised_Day_Effect[6],true)
call EnableWeatherEffect(udg_Promised_Day_Effect[7],true)
call EnableWeatherEffect(udg_Promised_Day_Effect[8],true)
call RemoveUnit(udg_Scar_Fountain)
call RemoveUnit(udg_Chimera_Healer)
call RemoveUnit(udg_Greed_Healer)
call RemoveUnit(udg_Dolcetto_Healer)
call RemoveUnit(udg_Roa_Healer)
call RemoveUnit(udg_Killer_Healer)
call UnitAddAbilityBJ('A07Z',gg_unit_H01V_0080)
call TriggerSleepAction(120.00)
call EnableTrigger(gg_trg_Gathering_Souls)
call UnitAddAbilityBJ('A088',udg_Our_Father)
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Father|r has begun absorbing all the souls of the Amestrian people! If he isn't stopped soon he will become too powerful to ever beat!")
call StartTimerBJ(udg_Father_Timer,false,420.00)
call CreateTimerDialogBJ(GetLastCreatedTimerBJ(),"Ascension")
set udg_Father_Window=GetLastCreatedTimerDialogBJ()
set udg_Souls_Are_Being_Absorbed=true
call TriggerSleepAction(5.00)
if(Trig_The_Promised_Day_Func035C())then
call UnitAddAbilityBJ('A029',udg_Greed)
call DisplayTextToForce(GetForceOfPlayer(udg_Greed_Player),"|cffff0000Father|r is growing in power rapidly! This is bad, if you don't do something soon he could destroy everything, including your new friends! There might be one way to stop him...")
else
endif
call DestroyTrigger(GetTriggeringTrigger())
endfunction
