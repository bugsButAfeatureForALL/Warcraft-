function Trig_Knock_Func002C takes nothing returns boolean
if(not(IsUnitType(GetAttackedUnitBJ(),UNIT_TYPE_MECHANICAL)==false))then
return false
endif
if(not(GetSpellTargetUnit()!=gg_unit_H04N_0132))then
return false
endif
