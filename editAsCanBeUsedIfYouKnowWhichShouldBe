call ForceAddPlayerSimple(udg_Scar_Player,udg_Good_Team)
call ForceAddPlayerSimple(udg_May_Chang_Player,udg_Good_Team)
if(Trig_Alchemists_Copy_Func003C())then
call DisplayTextToForce(GetPlayersAll(),"Realizing that the Homunculi are the true enemies Scar has given up his quest for revenge and joined the Alchemists!")
else
call DisplayTextToForce(GetPlayersAll(),"Realizing that the Homunculi are the true enemies Scar has given up his quest for revenge and alongside May Chang has joined the Alchemists!")
endif
call DisableTrigger(GetTriggeringTrigger())
call DisableTrigger(gg_trg_Solo_Copy)
set udg_Scar_Is_With_Alchemists=true
set udg_Chimera_Limiter_A=1
call EnableTrigger(gg_trg_Kills_a_Homunculus)
call EnableTrigger(gg_trg_Kills_Kimblee)
call EnableTrigger(gg_trg_Kills_Bradley)
call SetForceAllianceStateBJ(udg_Good_Team,bj_FORCE_PLAYER[9],bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[9],udg_Good_Team,bj_ALLIANCE_ALLIED)
call ForceAddPlayerSimple(udg_May_Chang_Player,udg_Good_Team)
call ForceAddPlayerSimple(udg_Scar_Player,udg_Good_Team)
call ForceAddPlayerSimple(udg_Scar_Player,udg_Alchemists)
call ForceAddPlayerSimple(udg_May_Chang_Player,udg_Alchemists)
call ForceRemovePlayerSimple(udg_Scar_Player,udg_Neutral)
call UnitRemoveAbilityBJ('A06H',udg_Scar_Unit)
call UnitRemoveAbilityBJ('A09N',udg_Scar_Unit)
call UnitAddAbilityBJ('A02S',udg_Scar_Unit)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_May_Chang_Player),udg_Good_Team,bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_May_Chang_Player),bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[bj_PLAYER_NEUTRAL_VICTIM],udg_Good_Team,bj_ALLIANCE_ALLIED)
call RemoveUnit(udg_Scar_Fountain)
call TriggerSleepAction(1.00)
call SetPlayerAllianceStateBJ(Player(7),udg_Scar_Player,bj_ALLIANCE_ALLIED_VISION)
call SetPlayerAllianceStateBJ(Player(7),udg_May_Chang_Player,bj_ALLIANCE_ALLIED_VISION)
call DestroyTrigger(GetTriggeringTrigger())
endfunction
function InitTrig_Alchemists_Copy takes nothing returns nothing
set gg_trg_Alchemists_Copy=CreateTrigger()
call DisableTrigger(gg_trg_Alchemists_Copy)
call TriggerRegisterPlayerChatEvent(gg_trg_Alchemists_Copy,Player(9),"-Alchemists",true)
call TriggerAddCondition(gg_trg_Alchemists_Copy,Condition(function Trig_Alchemists_Copy_Conditions))
call TriggerAddAction(gg_trg_Alchemists_Copy,function Trig_Alchemists_Copy_Actions)
endfunction
function Trig_Solo_Conditions takes nothing returns boolean
if(not(IsUnitAliveBJ(udg_Scar_Unit)==true))then
return false
endif
return true
endfunction
function Trig_Solo_Actions takes nothing returns nothing
call DisplayTextToForce(GetPlayersAll(),"Scar has decided to continue his quest of vengeance against anyone in his path!")
call DisableTrigger(GetTriggeringTrigger())
call DisableTrigger(gg_trg_Alchemists)
call EnableTrigger(gg_trg_Kills_a_Homunculus)
call EnableTrigger(gg_trg_Kills_Kimblee)
call EnableTrigger(gg_trg_Kills_Bradley)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_May_Chang_Player),udg_Good_Team,bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_May_Chang_Player),bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[bj_PLAYER_NEUTRAL_VICTIM],GetForceOfPlayer(udg_May_Chang_Player),bj_ALLIANCE_ALLIED)
call SetPlayerAllianceStateBJ(udg_May_Chang_Player,udg_Scar_Player,bj_ALLIANCE_UNALLIED)
call SetPlayerAllianceStateBJ(udg_Scar_Player,udg_May_Chang_Player,bj_ALLIANCE_UNALLIED)
call SetPlayerAllianceStateBJ(Player(7),udg_May_Chang_Player,bj_ALLIANCE_ALLIED_VISION)
call UnitRemoveAbilityBJ('A06H',udg_Scar_Unit)
call TriggerSleepAction(0.20)
call UnitAddAbilityBJ('A00G',udg_Scar_Unit)
call DestroyTrigger(GetTriggeringTrigger())
endfunction
