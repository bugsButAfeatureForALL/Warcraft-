function Trig_Outer_Pentagon_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
set udg_DistancePentagon=0.00
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=80
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set udg_POSITIONFUNCTION=PolarProjectionBJ(GetUnitLoc(udg_PentagonBottomLeftUnit),udg_DistancePentagon,GetUnitFacing(udg_PentagonBottomLeftUnit))
call CreateNUnitsAtLoc(1,'h00P',GetOwningPlayer(udg_Shou_Ultimate_Caster),udg_POSITIONFUNCTION,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ(udg_DurationVariable_ShouUlt,'BTLF',GetLastCreatedUnit())
set udg_DistancePentagon=(udg_DistancePentagon+10.00)
call RemoveLocation(udg_POSITIONFUNCTION)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set udg_DistancePentagon=0.00
set bj_forLoopBIndex=1
set bj_forLoopBIndexEnd=70
loop
exitwhen bj_forLoopBIndex>bj_forLoopBIndexEnd
set udg_POSITIONFUNCTION=PolarProjectionBJ(GetUnitLoc(udg_PentagonTopLeftUnit),udg_DistancePentagon,285.00)
call CreateNUnitsAtLoc(1,'h00P',GetOwningPlayer(udg_Shou_Ultimate_Caster),udg_POSITIONFUNCTION,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ(udg_DurationVariable_ShouUlt,'BTLF',GetLastCreatedUnit())
set udg_DistancePentagon=(udg_DistancePentagon+10.00)
call RemoveLocation(udg_POSITIONFUNCTION)
set bj_forLoopBIndex=bj_forLoopBIndex+1
endloop
set udg_DistancePentagon=0.00
set bj_forLoopBIndex=1
set bj_forLoopBIndexEnd=70
loop
exitwhen bj_forLoopBIndex>bj_forLoopBIndexEnd
set udg_POSITIONFUNCTION=PolarProjectionBJ(GetUnitLoc(udg_PentagonCenterTopUnit),udg_DistancePentagon,215.00)
call CreateNUnitsAtLoc(1,'h00P',GetOwningPlayer(udg_Shou_Ultimate_Caster),udg_POSITIONFUNCTION,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ(udg_DurationVariable_ShouUlt,'BTLF',GetLastCreatedUnit())
set udg_DistancePentagon=(udg_DistancePentagon+10.00)
call RemoveLocation(udg_POSITIONFUNCTION)
set bj_forLoopBIndex=bj_forLoopBIndex+1
endloop
set udg_DistancePentagon=0.00
set bj_forLoopBIndex=1
set bj_forLoopBIndexEnd=70
loop
exitwhen bj_forLoopBIndex>bj_forLoopBIndexEnd
set udg_POSITIONFUNCTION=PolarProjectionBJ(GetUnitLoc(udg_PentagonTopRightUnit),udg_DistancePentagon,145.00)
call CreateNUnitsAtLoc(1,'h00P',GetOwningPlayer(udg_Shou_Ultimate_Caster),udg_POSITIONFUNCTION,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ(udg_DurationVariable_ShouUlt,'BTLF',GetLastCreatedUnit())
set udg_DistancePentagon=(udg_DistancePentagon+10.00)
call RemoveLocation(udg_POSITIONFUNCTION)
set bj_forLoopBIndex=bj_forLoopBIndex+1
endloop
set udg_DistancePentagon=0.00
set bj_forLoopBIndex=1
set bj_forLoopBIndexEnd=70
loop
exitwhen bj_forLoopBIndex>bj_forLoopBIndexEnd
set udg_POSITIONFUNCTION=PolarProjectionBJ(GetUnitLoc(udg_PentagonTopRightUnit),udg_DistancePentagon,255.00)
call CreateNUnitsAtLoc(1,'h00P',GetOwningPlayer(udg_Shou_Ultimate_Caster),udg_POSITIONFUNCTION,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ(udg_DurationVariable_ShouUlt,'BTLF',GetLastCreatedUnit())
set udg_DistancePentagon=(udg_DistancePentagon+10.00)
call RemoveLocation(udg_POSITIONFUNCTION)
set bj_forLoopBIndex=bj_forLoopBIndex+1
endloop
set udg_DistancePentagon=0.00
call EnableTrigger(gg_trg_Inner_Pentagon_Copy)
endfunction
