call CreateNUnitsAtLoc(1,'nass',Player(PLAYER_NEUTRAL_AGGRESSIVE),GetRectCenter(udg_Spawn_Array[udg_Amestris_Integer]),bj_UNIT_FACING)
set udg_Amestris_Spawn_Location=SL_local
call CreateNUnitsAtLoc(1,'nass',Player(PLAYER_NEUTRAL_AGGRESSIVE),GetRectCenter(udg_Spawn_Array[udg_Amestris_Integer]),bj_UNIT_FACING)
set udg_Amestris_Spawn_Location=SL_local
set udg_Amestris_Unit_Array[udg_Amestris_Integer]=GetLastCreatedUnit()
set udg_Amestris_Boolean=IsUnitAliveBJ(udg_Amestris_Unit_Array[udg_Amestris_Integer])
call RemoveLocation(SL_local)
else
call DoNothing()
endif
set udg_Amestris_Integer=udg_Amestris_Integer+1
endloop
set udg_Amestris_Iteration=1
