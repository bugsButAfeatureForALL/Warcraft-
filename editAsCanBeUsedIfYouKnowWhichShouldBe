function Trig_Player_11_Leaves_Actions takes nothing returns nothing
call SetPlayerAllianceStateBJ(Player(10),Player(3),bj_ALLIANCE_ALLIED_ADVUNITS)
call SetPlayerAllianceStateBJ(Player(10),Player(11),bj_ALLIANCE_ALLIED_ADVUNITS)
endfunction
