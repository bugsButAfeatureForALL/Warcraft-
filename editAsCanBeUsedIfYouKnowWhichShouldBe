function Trig_Heinkel_is_Chosen_Actions takes nothing returns nothing
set udg_Heinkel_Sided_With_Alchs=false
set udg_Heinkel_Sided_With_Homunc=false
set udg_Heinkel_Unit=GetTriggerUnit()
set udg_Heinkel_Player=GetOwningPlayer(GetTriggerUnit())
call SetPlayerHandicapXPBJ(udg_Heinkel_Player,130.00)
call SetPlayerName(udg_Heinkel_Player,"Heinkel")
call SetUnitPositionLoc(GetTriggerUnit(),GetRectCenter(gg_rct_Heinkel_Spawn))
call PanCameraToTimedLocForPlayer(GetOwningPlayer(udg_Heinkel_Unit),GetRectCenter(gg_rct_Heinkel_Spawn),0)
call SelectUnitForPlayerSingle(GetTriggerUnit(),GetOwningPlayer(GetTriggerUnit()))
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Heinkel_Player),udg_Evil_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Heinkel_Player),udg_Good_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Heinkel_Player),bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Heinkel_Player),bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[bj_PLAYER_NEUTRAL_VICTIM],GetForceOfPlayer(udg_Heinkel_Player),bj_ALLIANCE_UNALLIED)
call ForceAddPlayerSimple(udg_Heinkel_Player,udg_Neutral)
call DisplayTextToForce(GetForceOfPlayer(udg_Heinkel_Player),"You are the Chimera Heinkel! If any of the other Chimeras from your squad  have been selected they will be your only ally against everyone else. You may heal at your camp to restore your health, but be warned, should you die once you're |cffff0000gone|r!")
call RemoveUnit(udg_Chimera_Healer)
call CreateNUnitsAtLoc(1,'h01Z',udg_Heinkel_Player,GetRectCenter(gg_rct_Chimera_Healing),bj_UNIT_FACING)
set udg_Chimera_Healer=GetLastCreatedUnit()
if(Trig_Heinkel_is_Chosen_Func020C())then
call SetPlayerAllianceStateBJ(udg_Zampano_Player,udg_Heinkel_Player,bj_ALLIANCE_ALLIED_VISION)
call SetPlayerAllianceStateBJ(udg_Heinkel_Player,udg_Zampano_Player,bj_ALLIANCE_ALLIED_VISION)
else
endif
if(Trig_Heinkel_is_Chosen_Func021C())then
call SetPlayerAllianceStateBJ(udg_Darius_Player,udg_Heinkel_Player,bj_ALLIANCE_ALLIED_VISION)
call SetPlayerAllianceStateBJ(udg_Heinkel_Player,udg_Darius_Player,bj_ALLIANCE_ALLIED_VISION)
else
endif
if(Trig_Heinkel_is_Chosen_Func022C())then
call SetPlayerAllianceStateBJ(udg_Jerso_Player,udg_Heinkel_Player,bj_ALLIANCE_ALLIED_VISION)
call SetPlayerAllianceStateBJ(udg_Heinkel_Player,udg_Jerso_Player,bj_ALLIANCE_ALLIED_VISION)
else
endif
call DestroyTrigger(GetTriggeringTrigger())
endfunction
