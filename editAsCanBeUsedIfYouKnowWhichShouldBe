function Trig_False_Gate_Cast_Real_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A0AA'))then
return false
endif
return true
endfunction
function Trig_False_Gate_Cast_Real_Func013Func003C takes nothing returns boolean
if(not(GetOwningPlayer(GetEnumUnit())!=Player(7)))then
return false
endif
if(not(IsUnitIdType(GetUnitTypeId(GetEnumUnit()),ConvertUnitType(20))==false))then
return false
endif
if(not(IsUnitAlly(GetEnumUnit(),udg_Gluttony_Player)==false))then
return false
endif
if(not(IsUnitPausedBJ(GetEnumUnit())==false))then
return false
endif
return true
endfunction
function Trig_False_Gate_Cast_Real_Func013A takes nothing returns nothing
set udg_Gluttony_Victim_Location=GetUnitLoc(GetEnumUnit())
if(Trig_False_Gate_Cast_Real_Func013Func003C())then
call AddSpecialEffectLocBJ(udg_Gluttony_Victim_Location,"war3mapImported\\BlackBlink.mdx")
call SetUnitPositionLoc(GetEnumUnit(),udg_Inside_Gluttony)
call DestroyEffect(GetLastCreatedEffectBJ())
call DisplayTextToForce(GetForceOfPlayer(GetOwningPlayer(GetEnumUnit())),"You have been trapped by |cffff0000Gluttony's|r False Gate! You are trapped inside of him for |cffff000060 seconds|r, after which you will automatically escape and appear at Gluttony's location.")
else
endif
call RemoveLocation(udg_Gluttony_Victim_Location)
endfunction
function Trig_False_Gate_Cast_Real_Func019C takes nothing returns boolean
if(not(CountUnitsInGroup(GetUnitsInRectAll(gg_rct_Inside_Gluttony))>=1))then
return false
endif
return true
endfunction
function Trig_False_Gate_Cast_Real_Actions takes nothing returns nothing
set udg_Gluttony_Location=GetUnitLoc(udg_Gluttony_Unit)
set udg_Inside_Gluttony=GetRectCenter(gg_rct_Inside_Gluttony)
set udg_Caster_Position_Offset=PolarProjectionBJ(udg_Gluttony_Location,300.00,GetUnitFacing(udg_Gluttony_Unit))
set udg_False_Gate_Unit_Group=GetUnitsInRangeOfLocAll(300.00,udg_Caster_Position_Offset)
call CreateNUnitsAtLoc(1,'h04X',udg_Gluttony_Player,udg_Gluttony_Location,GetUnitFacing(udg_Gluttony_Unit))
set udg_Gluttony_Wave_Dummy=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(0.40,'BTLF',udg_Gluttony_Wave_Dummy)
call SetUnitAnimation(udg_Gluttony_Wave_Dummy,"Death")
call ForGroupBJ(udg_False_Gate_Unit_Group,function Trig_False_Gate_Cast_Real_Func013A)
call GroupClear(udg_False_Gate_Unit_Group)
call RemoveLocation(udg_Gluttony_Location)
call RemoveLocation(udg_Caster_Position_Offset)
call RemoveLocation(udg_Inside_Gluttony)
call DestroyGroup(udg_False_Gate_Unit_Group)
if(Trig_False_Gate_Cast_Real_Func019C())then
call StartTimerBJ(udg_Belly_Timer,false,59.00)
else
endif
endfunction
function InitTrig_False_Gate_Cast_Real takes nothing returns nothing
set gg_trg_False_Gate_Cast_Real=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_False_Gate_Cast_Real,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_False_Gate_Cast_Real,Condition(function Trig_False_Gate_Cast_Real_Conditions))
call TriggerAddAction(gg_trg_False_Gate_Cast_Real,function Trig_False_Gate_Cast_Real_Actions)
