call EnableTrigger(gg_trg_Vanny_is_Selected_Red)
call EnableTrigger(gg_trg_Vanny_is_Selected_Blue)
call EnableTrigger(gg_trg_Vanny_is_Selected_Teal)
call EnableTrigger(gg_trg_Vanny_is_Selected_Yellow)
call EnableTrigger(gg_trg_Vanny_is_Selected_Orange)
call EnableTrigger(gg_trg_Vanny_is_Selected_Green)
call DestroyTrigger(GetTriggeringTrigger())
endfunction
