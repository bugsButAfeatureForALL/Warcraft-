function Trig_Blade_Rush_Start_Actions takes nothing returns nothing
set udg_Blade_Rush_Target=GetSpellTargetUnit()
call SaveUnitHandleBJ(GetSpellTargetUnit(),StringHashBJ("target"),GetHandleIdBJ(GetTriggerUnit()),udg_Blade_Table)
call GroupAddUnitSimple(GetTriggerUnit(),udg_Blade_Group)
call SetUnitPathing(GetTriggerUnit(),false)
call EnableTrigger(gg_trg_Blade_Rush_Loop)
endfunction
