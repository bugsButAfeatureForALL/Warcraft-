function Trig_Scar_Dies_Actions takes nothing returns nothing
if(Trig_Scar_Dies_Func001C())then
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Scar's brutal quest for revenge has finally been ended!|r")
call SetPlayerState(udg_Scar_Player,PLAYER_STATE_OBSERVER,1)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Scar_Player),bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Scar_Player),udg_Good_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Scar_Player),bj_ALLIANCE_ALLIED_VISION)
call ForceRemovePlayerSimple(udg_Scar_Player,udg_Neutral)
else
call TriggerSleepAction(10.00)
if(Trig_Scar_Dies_Func001Func009C())then
call ReviveHeroLoc(udg_Scar_Unit,GetUnitLoc(gg_unit_H01V_0080),false)
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Winry|r has used her medical knowledge to revive a member of the Alchemist team!")
call SetHeroLevelBJ(GetTriggerUnit(),(GetHeroLevel(GetDyingUnit())-3),false)
call PanCameraToTimedLocForPlayer(GetOwningPlayer(GetDyingUnit()),GetUnitLoc(GetDyingUnit()),0)
call SelectUnitForPlayerSingle(GetDyingUnit(),udg_Scar_Player)
else
if(Trig_Scar_Dies_Func001Func009Func007C())then
call ForceAddPlayerSimple(udg_Scar_Player,udg_Dead_Good_Guys)
call ForceRemovePlayerSimple(udg_Scar_Player,udg_Neutral)
call ForceRemovePlayerSimple(udg_Scar_Player,udg_Alchemists)
call GroupAddUnitSimple(udg_Scar_Unit,udg_Dead_Heroes)
if(Trig_Scar_Dies_Func001Func009Func007Func005C())then
call CustomDefeatBJ(Player(0),"You have lost!")
call CustomDefeatBJ(Player(1),"You have lost!")
call CustomDefeatBJ(Player(2),"You have lost!")
call CustomDefeatBJ(Player(3),"You have lost!")
call CustomDefeatBJ(Player(4),"You have lost!")
call CustomDefeatBJ(Player(5),"You have lost!")
call CustomDefeatBJ(Player(6),"You have lost!")
call CustomDefeatBJ(Player(10),"You have lost!")
call CustomDefeatBJ(Player(11),"You have lost!")
else
endif
if(Trig_Scar_Dies_Func001Func009Func007Func006C())then
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Father|r has eradicated all those who stood before his plans. The Promised Day arrives and a dark cloud falls over Amestris...")
call TriggerSleepAction(10.00)
call CustomDefeatBJ(Player(0),"Father has won!")
call CustomDefeatBJ(Player(1),"Father has won!")
call CustomDefeatBJ(Player(2),"Father has won!")
call CustomVictoryBJ(Player(3),true,true)
call CustomDefeatBJ(Player(4),"Father has won!")
call CustomDefeatBJ(Player(5),"Father has won!")
call CustomDefeatBJ(Player(6),"Father has won!")
call CustomDefeatBJ(Player(8),"Father has won!")
call CustomDefeatBJ(Player(9),"Father has won!")
call CustomVictoryBJ(Player(10),true,true)
call CustomVictoryBJ(Player(11),true,true)
else
endif
else
endif
endif
endif
