function Trig_Ling_Takes_Control_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A023'))then
return false
endif
return true
endfunction
function Trig_Ling_Takes_Control_Func001Func006001 takes nothing returns boolean
return(udg_Greed_Neutral==false)
endfunction
function Trig_Ling_Takes_Control_Func001Func006002001 takes nothing returns boolean
return(udg_Greed_Good==false)
endfunction
function Trig_Ling_Takes_Control_Func001Func006002002 takes nothing returns boolean
return(udg_Greed_Evil==false)
endfunction
function Trig_Ling_Takes_Control_Func001Func006002 takes nothing returns boolean
return GetBooleanAnd((udg_Greed_Good==false),(udg_Greed_Evil==false))
endfunction
function Trig_Ling_Takes_Control_Func001C takes nothing returns boolean
if(not GetBooleanAnd((udg_Greed_Neutral==false),(GetBooleanAnd((udg_Greed_Good==false),(udg_Greed_Evil==false)))))then
return false
endif
return true
endfunction
function Trig_Ling_Takes_Control_Func013Func001C takes nothing returns boolean
if(not(udg_Greed_Evil==true))then
return false
endif
return true
endfunction
function Trig_Ling_Takes_Control_Func013C takes nothing returns boolean
if(not(udg_Greed_Good==true))then
return false
endif
return true
endfunction
function Trig_Ling_Takes_Control_Actions takes nothing returns nothing
if(Trig_Ling_Takes_Control_Func001C())then
call TriggerSleepAction(1.00)
call CreateNUnitsAtLoc(1,'h01O',udg_Greed_Player,GetRectCenter(gg_rct_Soul),bj_UNIT_FACING)
set udg_Greed_Soul=GetLastCreatedUnit()
call SelectUnitForPlayerSingle(udg_Greed_Soul,udg_Greed_Player)
call PanCameraToTimedLocForPlayer(udg_Greed_Player,GetUnitLoc(udg_Greed_Soul),0)
else
call CreateNUnitsAtLoc(1,'h01N',udg_Greed_Player,GetRectCenter(gg_rct_Soul),bj_UNIT_FACING)
set udg_Greed_Soul=GetLastCreatedUnit()
call TriggerSleepAction(1.00)
call SelectUnitForPlayerSingle(udg_Greed_Soul,udg_Greed_Player)
call PanCameraToTimedLocForPlayer(GetOwningPlayer(udg_Greed),GetUnitLoc(udg_Greed_Soul),0)
endif
call RemoveUnit(GetTriggerUnit())
call SetUnitOwner(udg_Greed,GetOwningPlayer(GetTriggerUnit()),true)
call SelectUnitForPlayerSingle(udg_Greed,udg_Ling_Player)
call PanCameraToTimedLocForPlayer(GetOwningPlayer(udg_Greed),GetUnitLoc(udg_Greed),0)
call UnitAddAbilityBJ('A04J',udg_Greed)
call DisplayTextToForce(GetPlayersAll(),"Ling Yao: This is my body and I'll be taking it back!")
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Greed_Player),bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Greed_Player),udg_Evil_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Ling_Player),udg_Good_Team,bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Ling_Player),bj_ALLIANCE_ALLIED)
call UnitRemoveAbilityBJ('A029',udg_Greed)
if(Trig_Ling_Takes_Control_Func013C())then
call SetPlayerAllianceStateBJ(udg_Ling_Player,udg_Greed_Player,bj_ALLIANCE_ALLIED_VISION)
call SetPlayerAllianceStateBJ(udg_Greed_Player,udg_Ling_Player,bj_ALLIANCE_ALLIED)
else
if(Trig_Ling_Takes_Control_Func013Func001C())then
call SetPlayerAllianceStateBJ(udg_Ling_Player,udg_Greed_Player,bj_ALLIANCE_ALLIED_VISION)
call ForceRemovePlayerSimple(udg_Greed_Player,udg_Homunculi)
call ForceAddPlayerSimple(udg_Ling_Player,udg_Alchemists)
call SetPlayerAllianceStateBJ(udg_Greed_Player,udg_Ling_Player,bj_ALLIANCE_ALLIED)
else
call SetPlayerAllianceStateBJ(udg_Ling_Player,udg_Greed_Player,bj_ALLIANCE_UNALLIED_VISION)
endif
endif
endfunction
function InitTrig_Ling_Takes_Control takes nothing returns nothing
set gg_trg_Ling_Takes_Control=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Ling_Takes_Control,EVENT_PLAYER_UNIT_SPELL_FINISH)
call TriggerAddCondition(gg_trg_Ling_Takes_Control,Condition(function Trig_Ling_Takes_Control_Conditions))
call TriggerAddAction(gg_trg_Ling_Takes_Control,function Trig_Ling_Takes_Control_Actions)
endfunction
function Trig_Greed_Takes_Control_First_Time_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A024'))then
return false
endif
return true
endfunction
function Trig_Greed_Takes_Control_First_Time_Func001Func008Func005C takes nothing returns boolean
if(not(udg_Greed_Evil==true))then
return false
endif
return true
endfunction
function Trig_Greed_Takes_Control_First_Time_Func001Func008C takes nothing returns boolean
if(not(udg_Greed_Neutral==true))then
return false
endif
return true
endfunction
function Trig_Greed_Takes_Control_First_Time_Func001C takes nothing returns boolean
if(not(udg_Greed_Good==true))then
return false
endif
return true
endfunction
function Trig_Greed_Takes_Control_First_Time_Func013Func006001 takes nothing returns boolean
return(GetOwningPlayer(udg_Greed)==Player(8))
endfunction
function Trig_Greed_Takes_Control_First_Time_Func013Func006002 takes nothing returns boolean
return(GetOwningPlayer(udg_Greed)==Player(9))
endfunction
function Trig_Greed_Takes_Control_First_Time_Func013C takes nothing returns boolean
if(not(IsTriggerEnabled(gg_trg_Gathering_Souls)==true))then
return false
endif
if(not GetBooleanOr((GetOwningPlayer(udg_Greed)==Player(8)),(GetOwningPlayer(udg_Greed)==Player(9))))then
return false
endif
if(not(udg_Greed_Good==true))then
return false
endif
return true
endfunction
function Trig_Greed_Takes_Control_First_Time_Actions takes nothing returns nothing
if(Trig_Greed_Takes_Control_First_Time_Func001C())then
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Greed_Player),udg_Evil_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Greed_Player),bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Greed_Player),bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Greed_Player),udg_Good_Team,bj_ALLIANCE_ALLIED)
call SetPlayerAllianceStateBJ(udg_Greed_Player,udg_Ling_Player,bj_ALLIANCE_ALLIED_VISION)
call SetPlayerAllianceStateBJ(udg_Ling_Player,udg_Greed_Player,bj_ALLIANCE_ALLIED)
else
if(Trig_Greed_Takes_Control_First_Time_Func001Func008C())then
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Greed_Player),GetPlayersAll(),bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(GetPlayersAll(),GetForceOfPlayer(udg_Greed_Player),bj_ALLIANCE_UNALLIED)
call SetPlayerAllianceStateBJ(udg_Greed_Player,udg_Ling_Player,bj_ALLIANCE_UNALLIED_VISION)
else
if(Trig_Greed_Takes_Control_First_Time_Func001Func008Func005C())then
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Greed_Player),udg_Evil_Team,bj_ALLIANCE_ALLIED_VISION)
call ForceRemovePlayerSimple(udg_Ling_Player,udg_Alchemists)
call ForceAddPlayerSimple(udg_Greed_Player,udg_Homunculi)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Greed_Player),bj_ALLIANCE_ALLIED_VISION)
call SetPlayerAllianceStateBJ(udg_Greed_Player,udg_Ling_Player,bj_ALLIANCE_UNALLIED_VISION)
else
endif
endif
endif
call RemoveUnit(GetTriggerUnit())
call CreateNUnitsAtLoc(1,'h01M',udg_Ling_Player,GetRectCenter(gg_rct_Soul),bj_UNIT_FACING)
set udg_Lings_Soul=GetLastCreatedUnit()
call PanCameraToTimedLocForPlayer(udg_Ling_Player,GetUnitLoc(GetLastCreatedUnit()),0)
call SelectUnitForPlayerSingle(GetLastCreatedUnit(),udg_Ling_Player)
call SetUnitOwner(udg_Greed,GetOwningPlayer(GetTriggerUnit()),true)
call UnitRemoveAbilityBJ('A04J',udg_Greed)
call PanCameraToTimedLocForPlayer(GetOwningPlayer(udg_Greed),GetUnitLoc(udg_Greed),0)
call SelectUnitForPlayerSingle(udg_Greed,udg_Greed_Player)
call TriggerSleepAction(1.00)
call DisplayTextToForce(GetPlayersAll(),"Greed: Sorry, but the names Greed now.")
if(Trig_Greed_Takes_Control_First_Time_Func013C())then
call DisableTrigger(GetTriggeringTrigger())
call EnableTrigger(gg_trg_Greed_Takes_Control_Second_Time)
call UnitAddAbilityBJ('A029',udg_Greed)
call DisplayTextToForce(GetForceOfPlayer(udg_Greed_Player),"|cffff0000Father|r is growing in power rapidly! This is bad, if you don't do something soon he could destroy everything, including your new friends! There might be one way to stop him...")
else
endif
endfunction
function InitTrig_Greed_Takes_Control_First_Time takes nothing returns nothing
set gg_trg_Greed_Takes_Control_First_Time=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Greed_Takes_Control_First_Time,EVENT_PLAYER_UNIT_SPELL_FINISH)
call TriggerAddCondition(gg_trg_Greed_Takes_Control_First_Time,Condition(function Trig_Greed_Takes_Control_First_Time_Conditions))
call TriggerAddAction(gg_trg_Greed_Takes_Control_First_Time,function Trig_Greed_Takes_Control_First_Time_Actions)
endfunction
