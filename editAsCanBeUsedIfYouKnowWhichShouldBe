if(not(GetUnitTypeId(GetTriggerUnit())=='H018'))then
return false
endif
return true
endfunction
function Trig_Scar_is_Chosen_Func019C takes nothing returns boolean
if(not(udg_May_Chang_Is_Not_Picked==false))then
return false
endif
return true
endfunction
function Trig_Scar_is_Chosen_Actions takes nothing returns nothing
set udg_Scar_Is_Not_Picked=false
set udg_Scar_Unit=GetTriggerUnit()
set udg_Scar_Player=GetOwningPlayer(GetTriggerUnit())
call SetPlayerName(udg_Scar_Player,"Scar")
call SetUnitPositionLoc(GetTriggerUnit(),GetRectCenter(gg_rct_Scar_Spawn))
call PanCameraToTimedLocForPlayer(GetOwningPlayer(GetTriggerUnit()),GetRectCenter(gg_rct_Scar_Spawn),0)
call SelectUnitForPlayerSingle(GetTriggerUnit(),GetOwningPlayer(GetTriggerUnit()))
call StartTimerBJ(udg_Scar_Timer,false,840.00)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Scar_Player),udg_Evil_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Scar_Player),udg_Good_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Scar_Player),bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Scar_Player),bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[bj_PLAYER_NEUTRAL_VICTIM],GetForceOfPlayer(udg_Scar_Player),bj_ALLIANCE_UNALLIED)
call ForceAddPlayerSimple(udg_Scar_Player,udg_Neutral)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_May_Chang_Player),udg_Good_Team,bj_ALLIANCE_UNALLIED)
call CreateNUnitsAtLoc(1,'h01Z',udg_Scar_Player,GetRectCenter(gg_rct_Scar_Spawn),bj_UNIT_FACING)
set udg_Scar_Fountain=GetLastCreatedUnit()
call DisplayTextToForce(GetForceOfPlayer(udg_Scar_Player),"You are |cffff0000Scar|r one of the survivors of the Ishval Massaacre! You may start out alone so be careful. If things get too tough you can always return to the ruins of Ishval to heal. Beware Mustang though. His high damaging spells will make quick work of you. But if you get the drop on him you should be able to finish him quickly.\n\nRemember, if you die you're |cffff0000gone|r.")
if(Trig_Scar_is_Chosen_Func019C())then
call ForceRemovePlayerSimple(udg_May_Chang_Player,udg_Good_Team)
call SetPlayerAllianceStateBJ(udg_Scar_Player,udg_May_Chang_Player,bj_ALLIANCE_ALLIED)
call SetPlayerAllianceStateBJ(udg_May_Chang_Player,udg_Scar_Player,bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,bj_FORCE_PLAYER[bj_PLAYER_NEUTRAL_VICTIM],bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[7],udg_Good_Team,bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_May_Chang_Player),bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_May_Chang_Player),udg_Good_Team,bj_ALLIANCE_UNALLIED)
else
endif
call DestroyTrigger(GetTriggeringTrigger())
endfunction
function InitTrig_Scar_is_Chosen takes nothing returns nothing
set gg_trg_Scar_is_Chosen=CreateTrigger()
call TriggerRegisterEnterRectSimple(gg_trg_Scar_is_Chosen,gg_rct_Neutral_Spawns)
call TriggerAddCondition(gg_trg_Scar_is_Chosen,Condition(function Trig_Scar_is_Chosen_Conditions))
call TriggerAddAction(gg_trg_Scar_is_Chosen,function Trig_Scar_is_Chosen_Actions)
endfunction
function Trig_may_pls_Func005001 takes nothing returns boolean
return(IsUnitAlly(udg_May_Chang,Player(8))==true)
endfunction
function Trig_may_pls_Func005002 takes nothing returns boolean
return(IsUnitAlly(udg_May_Chang,Player(9))==true)
endfunction
function Trig_may_pls_Conditions takes nothing returns boolean
if(not GetBooleanOr((IsUnitAlly(udg_May_Chang,Player(8))==true),(IsUnitAlly(udg_May_Chang,Player(9))==true)))then
return false
endif
return true
endfunction
function Trig_may_pls_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_May_Chang_Player),udg_Good_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_May_Chang_Player),bj_ALLIANCE_UNALLIED)
call DestroyTrigger(GetTriggeringTrigger())
endfunction
