call PingMinimapLocForForceEx(GetForceOfPlayer(udg_Ling_Player),udg_Greed_Stone_Location,3.00,bj_MINIMAPPINGSTYLE_SIMPLE,100,25.00,25.00)
call RemoveLocation(udg_Greed_Stone_Location)
endfunction
