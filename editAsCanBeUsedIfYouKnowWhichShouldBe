return true
endfunction
function Trig_Endless_Dark_Cast_Actions takes nothing returns nothing
set udg_Endless_Dark_Position[2]=GetUnitLoc(GetTriggerUnit())
if((true))then
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=20
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call CreateNUnitsAtLoc(1,'h036',udg_Pride_Player,udg_Endless_Dark_Position[2],(I2R(GetForLoopIndexA())*18.00))
set udg_Endless_Dark_Dummy=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(2.00,'BTLF',udg_Endless_Dark_Dummy)
call SetUnitAbilityLevelSwapped('A05N',GetLastCreatedUnit(),GetUnitAbilityLevelSwapped('A05M',udg_Pride_Unit))
set udg_Endless_Dark_Position[4]=PolarProjectionBJ(GetUnitLoc(GetLastCreatedUnit()),0.00,(I2R(GetForLoopIndexA())*18.00))
call IssuePointOrderLoc(GetLastCreatedUnit(),"impale",udg_Endless_Dark_Position[4])
call RemoveLocation(udg_Endless_Dark_Position[4])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
else
endif
call RemoveLocation(udg_Endless_Dark_Position[2])
endfunction
function InitTrig_Endless_Dark_Cast takes nothing returns nothing
set gg_trg_Endless_Dark_Cast=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Endless_Dark_Cast,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Endless_Dark_Cast,Condition(function Trig_Endless_Dark_Cast_Conditions))
call TriggerAddAction(gg_trg_Endless_Dark_Cast,function Trig_Endless_Dark_Cast_Actions)
endfunction
