call SetUnitAbilityLevelSwapped('A05Q',GetLastCreatedUnit(),GetUnitAbilityLevelSwapped('A09U',udg_Jean_Unit))
call UnitApplyTimedLifeBJ(2.00,'BTLF',GetLastCreatedUnit())
call IssueImmediateOrder(GetLastCreatedUnit(),"stomp")
endfunction
