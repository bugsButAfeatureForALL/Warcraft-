function Trig_Lan_Fan_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A01X'))then
return false
endif
if(not(GetUnitTypeId(GetSpellAbilityUnit())!='H015'))then
return false
endif
return true
endfunction
function Trig_Lan_Fan_Actions takes nothing returns nothing
set udg_Envy_First_Form=GetSpellAbilityUnit()
set udg_Envy_Current_Level=GetHeroLevel(udg_Envy_First_Form)
call SetUnitInvulnerable(udg_Envy_First_Form,true)
call ShowUnitHide(udg_Envy_First_Form)
call CreateNUnitsAtLoc(1,'H01I',GetOwningPlayer(udg_Envy_First_Form),GetUnitLoc(udg_Envy_First_Form),bj_UNIT_FACING)
set udg_Envy_Shapeshifted_Form=GetLastCreatedUnit()
call SetUnitOwner(udg_Envy_First_Form,Player(PLAYER_NEUTRAL_PASSIVE),true)
call SetUnitLifePercentBJ(udg_Envy_Shapeshifted_Form,GetUnitLifePercent(udg_Envy_First_Form))
call SetPlayerName(GetOwningPlayer(udg_Envy_Shapeshifted_Form),"Lan Fan")
call SetPlayerColorBJ(udg_Envy_Player,udg_Lan_Fan_Color,true)
call SetHeroLevelBJ(udg_Envy_Shapeshifted_Form,GetHeroLevel(udg_Lan_Fan_Unit),false)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Envy_Player),bj_ALLIANCE_NEUTRAL)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Envy_Player),udg_Good_Team,bj_ALLIANCE_NEUTRAL)
call SelectUnitForPlayerSingle(udg_Envy_Shapeshifted_Form,udg_Envy_Player)
call EnableTrigger(gg_trg_Normal_to_Shapeshifted)
call SetPlayerHandicapXPBJ(udg_Envy_Player,0.00)
endfunction
function InitTrig_Lan_Fan takes nothing returns nothing
set gg_trg_Lan_Fan=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Lan_Fan,EVENT_PLAYER_UNIT_SPELL_FINISH)
call TriggerAddCondition(gg_trg_Lan_Fan,Condition(function Trig_Lan_Fan_Conditions))
call TriggerAddAction(gg_trg_Lan_Fan,function Trig_Lan_Fan_Actions)
endfunction
