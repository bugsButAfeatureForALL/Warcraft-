function Trig_Fadder_Timer_Actions takes nothing returns nothing
call EnableTrigger(gg_trg_Our_Fadder_Purple)
call EnableTrigger(gg_trg_Our_Fadder_Dark_Green)
call EnableTrigger(gg_trg_Our_Fadder_Brown)
call DestroyTrigger(GetTriggeringTrigger())
endfunction
