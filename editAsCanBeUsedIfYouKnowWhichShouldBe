function Trig_Fury_Damage_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
call AddSpecialEffectLocBJ(GetUnitLoc(GetAttackedUnitBJ()),"Objects\\Spawnmodels\\Human\\HumanBlood\\HumanBloodFootman.mdl")
call UnitDamageTargetBJ(udg_Heinkel_Unit,GetAttackedUnitBJ(),(3.00*I2R(udg_Heinkel_Agility)),ATTACK_TYPE_NORMAL,DAMAGE_TYPE_NORMAL)
call TriggerSleepAction(0.45)
call EnableTrigger(GetTriggeringTrigger())
endfunction
