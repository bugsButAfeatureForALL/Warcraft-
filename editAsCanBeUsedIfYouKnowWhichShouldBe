function Trig_Alphonse_is_Chosen_Actions takes nothing returns nothing
set udg_Alphonse_Unit=GetTriggerUnit()
set udg_Alphonse_Player=GetOwningPlayer(GetTriggerUnit())
set udg_Alphonse_Color=GetPlayerColor(GetOwningPlayer(GetTriggerUnit()))
call ForceAddPlayerSimple(udg_Alphonse_Player,udg_Good_Team)
call ForceAddPlayerSimple(udg_Alphonse_Player,udg_Alchemists)
call SetPlayerName(udg_Alphonse_Player,"Alphonse Elric")
call SetUnitPositionLoc(GetTriggerUnit(),GetRectCenter(gg_rct_Alphonse_Spawn))
call PanCameraToTimedLocForPlayer(GetOwningPlayer(udg_Alphonse_Unit),GetRectCenter(gg_rct_Alphonse_Spawn),0)
call SelectUnitForPlayerSingle(GetTriggerUnit(),GetOwningPlayer(GetTriggerUnit()))
call DisplayTextToForce(GetForceOfPlayer(udg_Alphonse_Player),"You are a member of the |cffff0000Alchemists|r team it is your job to stop Fathers plan before he can destroy the entire nation of Amestris for his own goals! You must also protect |cffff0000Winry|r at all costs! If she dies you can no longer revive!")
call SetForceAllianceStateBJ(udg_Good_Team,udg_Evil_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,bj_FORCE_PLAYER[bj_PLAYER_NEUTRAL_VICTIM],bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,bj_FORCE_PLAYER[7],bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[bj_PLAYER_NEUTRAL_EXTRA],udg_Good_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[7],udg_Good_Team,bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[bj_PLAYER_NEUTRAL_VICTIM],udg_Good_Team,bj_ALLIANCE_ALLIED)
if(Trig_Alphonse_is_Chosen_Func017C())then
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_May_Chang_Player),bj_ALLIANCE_UNALLIED)
else
endif
call DestroyTrigger(GetTriggeringTrigger())
endfunction
