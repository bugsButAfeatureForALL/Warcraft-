call TriggerRegisterTimerEventSingle(gg_trg_Yok_Message,30.00)
call TriggerAddAction(gg_trg_Yok_Message,function Trig_Yok_Message_Actions)
endfunction
function Trig_To_Yok_Island_Conditions takes nothing returns boolean
if(not(IsUnitAlly(GetTriggerUnit(),Player(7))==true))then
return false
endif
if(not(IsPlayerInForce(GetOwningPlayer(GetTriggerUnit()),udg_Has_Trained_On_Yok)==false))then
return false
endif
if(not(GetHeroLevel(GetTriggerUnit())>=10))then
return false
endif
if(not(GetTriggerUnit()!=udg_Greed))then
return false
endif
if(not(GetTriggerUnit()!=udg_Hohenheim_Unit))then
return false
endif
if(not(GetTriggerUnit()!=udg_Envy_First_Form))then
return false
endif
if(not(GetTriggerUnit()!=udg_Envy_Shapeshifted_Form))then
return false
endif
if(not(GetTriggerUnit()!=gg_unit_H01V_0080))then
return false
endif
if(not(GetTriggerUnit()!=udg_Scar_Unit))then
return false
endif
if(not(GetTriggerUnit()!=udg_Vanny_Unit))then
return false
endif
return true
endfunction
function Trig_To_Yok_Island_Actions takes nothing returns nothing
call ShowUnitHide(gg_unit_hbot_0001)
call DisableTrigger(GetTriggeringTrigger())
call EnableTrigger(gg_trg_Yok_Island_Spawns)
set udg_Yok_Trainer=GetTriggerUnit()
set udg_Yok_Player=GetOwningPlayer(GetTriggerUnit())
call SetUnitPositionLoc(udg_Yok_Trainer,GetRectCenter(gg_rct_Enter_Yok_Island))
call PanCameraToTimedLocForPlayer(GetOwningPlayer(udg_Yok_Trainer),GetRectCenter(gg_rct_Enter_Yok_Island),0)
call CreateNUnitsAtLoc(1,'h01Z',GetOwningPlayer(udg_Yok_Trainer),GetRectCenter(gg_rct_Enter_Yok_Island),bj_UNIT_FACING)
set udg_Yok_Healer=GetLastCreatedUnit()
call StartTimerBJ(udg_Yok_Timer,false,180.00)
set udg_Yok_Training_Timer=GetLastCreatedTimerBJ()
call DisplayTextToForce(GetForceOfPlayer(udg_Yok_Player),"Welcome to Yok Island! You may train here for |cffff00003 minutes|r, should you survive until the end you will be given +20 to all stats.\n\nYou may check how much longer you will be on Yok Island with the command -Time.")
endfunction
function InitTrig_To_Yok_Island takes nothing returns nothing
set gg_trg_To_Yok_Island=CreateTrigger()
call TriggerRegisterEnterRectSimple(gg_trg_To_Yok_Island,gg_rct_To_Yok_Island)
call TriggerAddCondition(gg_trg_To_Yok_Island,Condition(function Trig_To_Yok_Island_Conditions))
call TriggerAddAction(gg_trg_To_Yok_Island,function Trig_To_Yok_Island_Actions)
endfunction
function Trig_Yok_Island_Finish_Func001002 takes nothing returns nothing
call SetItemPositionLoc(GetEnumItem(),GetRectCenter(gg_rct_Return_from_Yok))
endfunction
