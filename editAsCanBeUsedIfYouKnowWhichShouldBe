function Trig_Kings_Wrath_Actions takes nothing returns nothing
set udg_Wrath_Target=GetSpellTargetUnit()
set udg_Wrath_Target_Location=GetUnitLoc(udg_Wrath_Target)
call CreateNUnitsAtLoc(1,'hsor',udg_Bradley_Player,udg_Wrath_Target_Location,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ(3.00,'BTLF',GetLastCreatedUnit())
call IssueTargetOrder(GetLastCreatedUnit(),"slow",udg_Wrath_Target)
call RemoveLocation(udg_Wrath_Target_Location)
endfunction
