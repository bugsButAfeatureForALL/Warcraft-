function Trig_Philosophers_Cast_Actions takes nothing returns nothing
set udg_Philosopher_Target=GetSpellTargetUnit()
set udg_Philosopher_Real=45.00
set udg_Philosopher_Target_Position=GetUnitLoc(udg_Philosopher_Target)
set udg_Philosopher_Dummy_Position=PolarProjectionBJ(udg_Philosopher_Target_Position,85.00,udg_Philosopher_Real)
set bj_forLoopAIndex=0
set bj_forLoopAIndexEnd=3
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call CreateNUnitsAtLoc(1,'hfoo',udg_Father_Player,udg_Philosopher_Dummy_Position,AngleBetweenPoints(udg_Philosopher_Dummy_Position,udg_Philosopher_Target_Position))
set udg_Philosopher_Dummy=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(2.40,'BTLF',udg_Philosopher_Dummy)
set udg_Philosopher_Real=(udg_Philosopher_Real+90.00)
set udg_Philosopher_Dummy_Position=PolarProjectionBJ(udg_Philosopher_Target_Position,85.00,udg_Philosopher_Real)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
call RemoveLocation(udg_Philosopher_Dummy_Position)
call RemoveLocation(udg_Philosopher_Target_Position)
endfunction
