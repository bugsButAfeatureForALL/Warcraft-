function Trig_Rock_Trap_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A092'))then
return false
endif
return true
endfunction
function Trig_Rock_Trap_Func003Func001C takes nothing returns boolean
if(not(GetUnitAbilityLevelSwapped('A092',GetSpellAbilityUnit())==2))then
return false
endif
return true
endfunction
function Trig_Rock_Trap_Func003Func002C takes nothing returns boolean
if(not(GetUnitAbilityLevelSwapped('A092',GetSpellAbilityUnit())==3))then
return false
endif
return true
endfunction
function Trig_Rock_Trap_Func003C takes nothing returns boolean
if(not(GetUnitAbilityLevelSwapped('A092',GetSpellAbilityUnit())==1))then
return false
endif
return true
endfunction
function Trig_Rock_Trap_Actions takes nothing returns nothing
set udg_Degrees_Trap=0.00
set udg_Counter_RockTrap=0
if(Trig_Rock_Trap_Func003C())then
set udg_Trap_Duration=5.00
else
if(Trig_Rock_Trap_Func003Func001C())then
set udg_Trap_Duration=10.00
else
endif
if(Trig_Rock_Trap_Func003Func002C())then
set udg_Trap_Duration=15.00
else
endif
endif
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=25
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set udg_Position_Of_Trap=PolarProjectionBJ(GetSpellTargetLoc(),400.00,udg_Degrees_Trap)
call CreateNUnitsAtLoc(1,'h00Y',udg_Edward_Player,udg_Position_Of_Trap,bj_UNIT_FACING)
set udg_RockTrap_RockArray[udg_Counter_RockTrap]=GetLastCreatedUnit()
call CreateNUnitsAtLoc(1,'h012',udg_Edward_Player,udg_Position_Of_Trap,bj_UNIT_FACING)
call UnitApplyTimedLifeBJ(1.00,'BTLF',GetLastCreatedUnit())
call SetUnitPathing(udg_RockTrap_RockArray[udg_Counter_RockTrap],false)
call UnitApplyTimedLifeBJ(udg_Trap_Duration,'BTLF',udg_RockTrap_RockArray[udg_Counter_RockTrap])
set udg_Degrees_Trap=(udg_Degrees_Trap+14.40)
set udg_Counter_RockTrap=(udg_Counter_RockTrap+1)
call RemoveLocation(udg_Position_Of_Trap)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set udg_Degrees_Trap=0.00
set udg_Counter_RockTrap=0
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=25
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set udg_Position_Of_Trap=PolarProjectionBJ(GetSpellTargetLoc(),400.00,udg_Degrees_Trap)
call SetUnitPositionLoc(udg_RockTrap_RockArray[udg_Counter_RockTrap],udg_Position_Of_Trap)
call SetUnitPathing(udg_RockTrap_RockArray[udg_Counter_RockTrap],true)
set udg_Degrees_Trap=(udg_Degrees_Trap+14.40)
set udg_Counter_RockTrap=(udg_Counter_RockTrap+1)
call RemoveLocation(udg_Position_Of_Trap)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set udg_Counter_RockTrap=0
call EnableTrigger(gg_trg_Rock_Trap_Rock_Raise)
endfunction
function InitTrig_Rock_Trap takes nothing returns nothing
set gg_trg_Rock_Trap=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Rock_Trap,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Rock_Trap,Condition(function Trig_Rock_Trap_Conditions))
call TriggerAddAction(gg_trg_Rock_Trap,function Trig_Rock_Trap_Actions)
endfunction
function Trig_Rock_Trap_Rock_Raise_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
call TriggerSleepAction(0.50)
set udg_Counter_RockTrap=0
loop
exitwhen udg_Counter_RockTrap>26
call SetUnitTimeScalePercent(udg_RockTrap_RockArray[udg_Counter_RockTrap],0.00)
set udg_Counter_RockTrap=udg_Counter_RockTrap+1
endloop
set udg_Counter_RockTrap=0
endfunction
function InitTrig_Rock_Trap_Rock_Raise takes nothing returns nothing
set gg_trg_Rock_Trap_Rock_Raise=CreateTrigger()
call DisableTrigger(gg_trg_Rock_Trap_Rock_Raise)
call TriggerRegisterTimerEventPeriodic(gg_trg_Rock_Trap_Rock_Raise,0.05)
call TriggerAddAction(gg_trg_Rock_Trap_Rock_Raise,function Trig_Rock_Trap_Rock_Raise_Actions)
endfunction
function Trig_Rock_Trap_Rock_Lower_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
set udg_Counter_RockTrap=0
loop
exitwhen udg_Counter_RockTrap>26
call SetUnitTimeScalePercent(udg_RockTrap_RockArray[udg_Counter_RockTrap],100.00)
set udg_Counter_RockTrap=udg_Counter_RockTrap+1
endloop
call TriggerSleepAction(0.25)
set udg_Counter_RockTrap=0
loop
exitwhen udg_Counter_RockTrap>26
call RemoveUnit(udg_RockTrap_RockArray[udg_Counter_RockTrap])
set udg_Counter_RockTrap=udg_Counter_RockTrap+1
endloop
set udg_Counter_RockTrap=0
endfunction
function InitTrig_Rock_Trap_Rock_Lower takes nothing returns nothing
set gg_trg_Rock_Trap_Rock_Lower=CreateTrigger()
call DisableTrigger(gg_trg_Rock_Trap_Rock_Lower)
call TriggerRegisterTimerEventPeriodic(gg_trg_Rock_Trap_Rock_Lower,0.05)
call TriggerAddAction(gg_trg_Rock_Trap_Rock_Lower,function Trig_Rock_Trap_Rock_Lower_Actions)
endfunction
