function Trig_Rock_Trap_Copy_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A08N'))then
return false
endif
return true
endfunction
function Trig_Rock_Trap_Copy_Actions takes nothing returns nothing
set udg_Brute_Trap_Duration=10.00
set udg_Brute_Trap_Degrees=0.00
set udg_Brute_Counter=0
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=25
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set udg_Brute_Position=PolarProjectionBJ(GetUnitLoc(udg_Darius_Unit),400.00,udg_Brute_Trap_Degrees)
call CreateNUnitsAtLoc(1,'h00Y',Player(PLAYER_NEUTRAL_PASSIVE),udg_Brute_Position,bj_UNIT_FACING)
set udg_Brute_Strength_RockArray[udg_Brute_Counter]=GetLastCreatedUnit()
call SetUnitPathing(udg_Brute_Strength_RockArray[udg_Brute_Counter],false)
call UnitApplyTimedLifeBJ(udg_Brute_Trap_Duration,'BTLF',udg_Brute_Strength_RockArray[udg_Brute_Counter])
set udg_Brute_Trap_Degrees=(udg_Brute_Trap_Degrees+14.40)
set udg_Brute_Counter=(udg_Brute_Counter+1)
call RemoveLocation(udg_Brute_Position)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set udg_Brute_Trap_Degrees=0.00
set udg_Brute_Counter=0
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=25
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set udg_Brute_Position=PolarProjectionBJ(GetUnitLoc(udg_Darius_Unit),400.00,udg_Brute_Trap_Degrees)
call SetUnitPositionLoc(udg_Brute_Strength_RockArray[udg_Brute_Counter],udg_Position_Of_Trap)
call SetUnitPathing(udg_Brute_Strength_RockArray[udg_Brute_Counter],true)
set udg_Brute_Trap_Degrees=(udg_Brute_Trap_Degrees+14.40)
set udg_Brute_Counter=(udg_Brute_Counter+1)
call RemoveLocation(udg_Position_Of_Trap)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set udg_Brute_Counter=0
call EnableTrigger(gg_trg_Rock_Trap_Rock_Raise_Copy)
endfunction
function InitTrig_Rock_Trap_Copy takes nothing returns nothing
set gg_trg_Rock_Trap_Copy=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Rock_Trap_Copy,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Rock_Trap_Copy,Condition(function Trig_Rock_Trap_Copy_Conditions))
call TriggerAddAction(gg_trg_Rock_Trap_Copy,function Trig_Rock_Trap_Copy_Actions)
endfunction
function Trig_Rock_Trap_Rock_Raise_Copy_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
call TriggerSleepAction(0.50)
set udg_Brute_Counter=0
loop
exitwhen udg_Brute_Counter>26
call SetUnitTimeScalePercent(udg_Brute_Strength_RockArray[udg_Brute_Counter],0.00)
set udg_Brute_Counter=udg_Brute_Counter+1
endloop
set udg_Brute_Counter=0
endfunction
function InitTrig_Rock_Trap_Rock_Raise_Copy takes nothing returns nothing
set gg_trg_Rock_Trap_Rock_Raise_Copy=CreateTrigger()
call DisableTrigger(gg_trg_Rock_Trap_Rock_Raise_Copy)
call TriggerRegisterTimerEventPeriodic(gg_trg_Rock_Trap_Rock_Raise_Copy,0.05)
call TriggerAddAction(gg_trg_Rock_Trap_Rock_Raise_Copy,function Trig_Rock_Trap_Rock_Raise_Copy_Actions)
endfunction
function Trig_Rock_Trap_Rock_Lower_Copy_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
set udg_Brute_Counter=0
loop
exitwhen udg_Brute_Counter>26
call SetUnitTimeScalePercent(udg_Brute_Strength_RockArray[udg_Brute_Counter],100.00)
set udg_Brute_Counter=udg_Brute_Counter+1
endloop
call TriggerSleepAction(0.25)
set udg_Brute_Counter=0
loop
exitwhen udg_Brute_Counter>26
call RemoveUnit(udg_Brute_Strength_RockArray[udg_Brute_Counter])
set udg_Brute_Counter=udg_Brute_Counter+1
endloop
set udg_Brute_Counter=0
endfunction
function InitTrig_Rock_Trap_Rock_Lower_Copy takes nothing returns nothing
set gg_trg_Rock_Trap_Rock_Lower_Copy=CreateTrigger()
call DisableTrigger(gg_trg_Rock_Trap_Rock_Lower_Copy)
call TriggerRegisterTimerEventPeriodic(gg_trg_Rock_Trap_Rock_Lower_Copy,0.05)
call TriggerAddAction(gg_trg_Rock_Trap_Rock_Lower_Copy,function Trig_Rock_Trap_Rock_Lower_Copy_Actions)
endfunction
