endfunction
function Trig_Rebound_CD_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
call EnableTrigger(gg_trg_Rebound_Limiter)
call DisableTrigger(gg_trg_Rebound_Limit)
call TriggerSleepAction(120.00)
call EnableTrigger(gg_trg_Rebound_Roll_5)
call DisableTrigger(gg_trg_Rebound_Roll_10)
call DisableTrigger(gg_trg_Rebound_Roll_15)
call DisableTrigger(gg_trg_Rebound_Roll_20)
endfunction
function InitTrig_Rebound_CD takes nothing returns nothing
set gg_trg_Rebound_CD=CreateTrigger()
call DisableTrigger(gg_trg_Rebound_CD)
call TriggerRegisterTimerExpireEventBJ(gg_trg_Rebound_CD,udg_Rebound_Duration)
call TriggerAddAction(gg_trg_Rebound_CD,function Trig_Rebound_CD_Actions)
endfunction
function Trig_Rebound_Roll_5_Conditions takes nothing returns boolean
if(not(GetSpellAbilityUnit()==udg_Cornello_Unit))then
return false
endif
if(not(GetSpellAbilityId()!='A03J'))then
return false
endif
if(not(GetSpellAbilityId()!='A095'))then
return false
endif
if(not(GetHeroLevel(udg_Cornello_Unit)>=10))then
return false
endif
return true
endfunction
function Trig_Rebound_Roll_5_Func002C takes nothing returns boolean
if(not(IsTriggerEnabled(gg_trg_Rebound_CD)==false))then
return false
endif
if(not(udg_Rebound_Chance>=95))then
return false
endif
return true
endfunction
function Trig_Rebound_Roll_5_Actions takes nothing returns nothing
set udg_Rebound_Chance=GetRandomInt(1,100)
if(Trig_Rebound_Roll_5_Func002C())then
call DisableTrigger(GetTriggeringTrigger())
call DisableTrigger(gg_trg_Rebound_Limiter)
call SetUnitInvulnerable(udg_Cornello_Unit,true)
call SetUnitOwner(udg_Cornello_Unit,Player(bj_PLAYER_NEUTRAL_EXTRA),true)
call UnitResetCooldown(udg_Cornello_Unit)
call TriggerSleepAction(0.10)
call IssueImmediateOrder(udg_Cornello_Unit,"metamorphosis")
call TriggerSleepAction(1.00)
call SetUnitInvulnerable(udg_Cornello_Unit,false)
call SetUnitOwner(udg_Cornello_Unit,udg_Cornello_Player,true)
call SelectUnitForPlayerSingle(udg_Cornello_Unit,udg_Cornello_Player)
call EnableTrigger(gg_trg_Rebound_Limit)
call StartTimerBJ(udg_Rebound_Duration,false,62.00)
call EnableTrigger(gg_trg_Rebound_CD)
else
call DisableTrigger(GetTriggeringTrigger())
call TriggerExecute(gg_trg_Rebound_Reset)
call EnableTrigger(gg_trg_Rebound_Roll_10)
endif
endfunction
function InitTrig_Rebound_Roll_5 takes nothing returns nothing
set gg_trg_Rebound_Roll_5=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Rebound_Roll_5,EVENT_PLAYER_UNIT_SPELL_FINISH)
call TriggerAddCondition(gg_trg_Rebound_Roll_5,Condition(function Trig_Rebound_Roll_5_Conditions))
call TriggerAddAction(gg_trg_Rebound_Roll_5,function Trig_Rebound_Roll_5_Actions)
endfunction
