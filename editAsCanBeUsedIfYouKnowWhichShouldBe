function Trig_Harder_Spawns_Actions takes nothing returns nothing
call DisplayTextToForce(GetPlayersAll(),"Harder Creeps will now begin spawning.")
call DisableTrigger(GetTriggeringTrigger())
call DisableTrigger(gg_trg_Dublith_Mountain_Spawns)
call EnableTrigger(gg_trg_Dublith_Mountain_Harder_Spawns)
call DisableTrigger(gg_trg_Mountain_Briggs_Spawns)
call DisableTrigger(gg_trg_Cave_Spawns)
call EnableTrigger(gg_trg_Mountain_Briggs_Harder_Spawns)
call DisableTrigger(gg_trg_Resembool_Spawns)
call EnableTrigger(gg_trg_Resembool_Harder_Spawns)
call DisableTrigger(gg_trg_Rush_Valley_Spawns)
call EnableTrigger(gg_trg_Rush_Valley_Harder_Spawns)
call DisableTrigger(gg_trg_I_Fucked_Up_Spawns)
call EnableTrigger(gg_trg_I_Fucked_Up_Harder_Spawn)
call DisableTrigger(gg_trg_Kanama_Spawns)
call EnableTrigger(gg_trg_Kanama_Harder_Spawns)
call DisableTrigger(gg_trg_Amestris_Spawns)
call EnableTrigger(gg_trg_Amestris_Harder_Spawns)
call DisableTrigger(gg_trg_Ishval_Spawns)
call EnableTrigger(gg_trg_Ishval_Harder_Spawns)
call EnableTrigger(gg_trg_Cave_Harder_Spawns)
endfunction
