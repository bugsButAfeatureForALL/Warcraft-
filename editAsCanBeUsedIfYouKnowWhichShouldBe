set gg_trg_DD_Effect=CreateTrigger()
call DisableTrigger(gg_trg_DD_Effect)
call TriggerRegisterTimerEventPeriodic(gg_trg_DD_Effect,0.02)
call TriggerAddAction(gg_trg_DD_Effect,function Trig_DD_Effect_Actions)
endfunction
