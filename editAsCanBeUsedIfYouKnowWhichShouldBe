function Trig_Philosophers_Damage_Actions takes nothing returns nothing
set udg_Philosophers_Damage_Group=GetUnitsInRangeOfLocMatching(600.00,GetUnitLoc(udg_Hohenheim_Unit),Condition(function Trig_Philosophers_Damage_Func002002003))
if(Trig_Philosophers_Damage_Func004C())then
call ForGroupBJ(udg_Philosophers_Damage_Group,function Trig_Philosophers_Damage_Func004Func003A)
call DestroyGroup(udg_Philosophers_Damage_Group)
else
if(Trig_Philosophers_Damage_Func004Func001C())then
call ForGroupBJ(udg_Philosophers_Damage_Group,function Trig_Philosophers_Damage_Func004Func001Func003A)
call DestroyGroup(udg_Philosophers_Damage_Group)
else
if(Trig_Philosophers_Damage_Func004Func001Func001C())then
call ForGroupBJ(udg_Philosophers_Damage_Group,function Trig_Philosophers_Damage_Func004Func001Func001Func002A)
call DestroyGroup(udg_Philosophers_Damage_Group)
else
endif
endif
endif
endfunction
