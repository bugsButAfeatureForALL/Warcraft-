function Trig_Knowledge_Actions takes nothing returns nothing
call SelectUnitForPlayerSingle(udg_He_Who_Sees_Truth,GetOwningPlayer(udg_He_Who_Sees_Truth))
call RemoveUnit(udg_Truth_Choice)
call DisplayTextToForce(GetForceOfPlayer(udg_He_Who_Sees_Truth_Player),"|cff995500Truth: Knowledge? You seek the secrets of this world to gain an advantage over your enemies? Hehe. Very well little Al-che-mist. I will grant you your knowledge...|r")
set udg_Truths_Price=GetRandomInt(1,9)
call TriggerSleepAction(7.00)
if(Trig_Knowledge_Func008C())then
if(Trig_Knowledge_Func008Func001C())then
call ModifyHeroStat(bj_HEROSTAT_AGI,udg_He_Who_Sees_Truth,bj_MODIFYMETHOD_SET,(udg_Truth_Agility+125))
call DisplayTextToForce(GetForceOfPlayer(udg_He_Who_Sees_Truth_Player),"|cff995500Truth: A Philosopher's Stone? You are full of surprises. I'll take that as toll instead then...|r")
call RemoveItem(GetItemOfTypeFromUnitBJ(udg_He_Who_Sees_Truth,'I00G'))
else
if(Trig_Knowledge_Func008Func001Func004C())then
call ModifyHeroStat(bj_HEROSTAT_INT,udg_He_Who_Sees_Truth,bj_MODIFYMETHOD_SET,(udg_Truth_Alchemy+125))
call DisplayTextToForce(GetForceOfPlayer(udg_He_Who_Sees_Truth_Player),"|cff995500Truth: A Philosopher's Stone? You are full of surprises. I'll take that as toll instead then...|r")
call RemoveItem(GetItemOfTypeFromUnitBJ(udg_He_Who_Sees_Truth,'I00G'))
else
if(Trig_Knowledge_Func008Func001Func004Func004C())then
call ModifyHeroStat(bj_HEROSTAT_AGI,udg_He_Who_Sees_Truth,bj_MODIFYMETHOD_SET,(udg_Truth_Agility+125))
call DisplayTextToForce(GetForceOfPlayer(udg_He_Who_Sees_Truth_Player),"|cff995500Truth: A Philosopher's Stone? You are full of surprises. I'll take that as toll instead then...|r")
call RemoveItem(GetItemOfTypeFromUnitBJ(udg_He_Who_Sees_Truth,'I00G'))
