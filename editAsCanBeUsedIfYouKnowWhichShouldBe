function Trig_Fire_Spiral_Init_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A00C'))then
return false
endif
return true
endfunction
function Trig_Fire_Spiral_Init_Func003C takes nothing returns boolean
if(not(IsUnitGroupEmptyBJ(udg_FS_UnitGroup)==true))then
return false
endif
return true
endfunction
function Trig_Fire_Spiral_Init_Actions takes nothing returns nothing
if(Trig_Fire_Spiral_Init_Func003C())then
call EnableTrigger(gg_trg_Fire_Spiral_Loop)
else
endif
call GroupAddUnitSimple(GetTriggerUnit(),udg_FS_UnitGroup)
call SaveUnitHandleBJ(GetTriggerUnit(),0,GetHandleIdBJ(GetTriggerUnit()),udg_FS_Hsahtable)
call SaveLocationHandleBJ(GetUnitLoc(GetTriggerUnit()),1,GetHandleIdBJ(GetTriggerUnit()),udg_FS_Hsahtable)
call SaveRealBJ(0.00,2,GetHandleIdBJ(GetTriggerUnit()),udg_FS_Hsahtable)
call SaveRealBJ((15.00+(7.50*I2R(GetUnitAbilityLevelSwapped('A00C',GetTriggerUnit())))),3,GetHandleIdBJ(GetTriggerUnit()),udg_FS_Hsahtable)
call SaveRealBJ(75.00,4,GetHandleIdBJ(GetTriggerUnit()),udg_FS_Hsahtable)
call SaveGroupHandleBJ(udg_FS_DummyGroup,5,GetHandleIdBJ(GetTriggerUnit()),udg_FS_Hsahtable)
call SaveRealBJ((2.00+(I2R(GetUnitAbilityLevelSwapped('A00C',GetTriggerUnit()))*0.00)),6,GetHandleIdBJ(GetTriggerUnit()),udg_FS_Hsahtable)
call SaveRealBJ(0.00,7,GetHandleIdBJ(GetTriggerUnit()),udg_FS_Hsahtable)
endfunction
function InitTrig_Fire_Spiral_Init takes nothing returns nothing
set gg_trg_Fire_Spiral_Init=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Fire_Spiral_Init,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Fire_Spiral_Init,Condition(function Trig_Fire_Spiral_Init_Conditions))
call TriggerAddAction(gg_trg_Fire_Spiral_Init,function Trig_Fire_Spiral_Init_Actions)
endfunction
function Trig_Fire_Spiral_Loop_Func001Func009Func001A takes nothing returns nothing
call SetUnitFlyHeightBJ(GetEnumUnit(),0.00,250.00)
call UnitApplyTimedLifeBJ(0.50,'BTLF',GetEnumUnit())
endfunction
function Trig_Fire_Spiral_Loop_Func001Func009Func015001003001 takes nothing returns boolean
return(IsUnitType(GetFilterUnit(),UNIT_TYPE_STRUCTURE)!=true)
endfunction
function Trig_Fire_Spiral_Loop_Func001Func009Func015001003002001 takes nothing returns boolean
return(IsUnitType(GetFilterUnit(),UNIT_TYPE_MAGIC_IMMUNE)!=true)
endfunction
function Trig_Fire_Spiral_Loop_Func001Func009Func015001003002002 takes nothing returns boolean
return(IsUnitEnemy(GetFilterUnit(),GetOwningPlayer(udg_FS_Caster))==true)
endfunction
function Trig_Fire_Spiral_Loop_Func001Func009Func015001003002 takes nothing returns boolean
return GetBooleanAnd((IsUnitType(GetFilterUnit(),UNIT_TYPE_MAGIC_IMMUNE)!=true),(IsUnitEnemy(GetFilterUnit(),GetOwningPlayer(udg_FS_Caster))==true))
endfunction
function Trig_Fire_Spiral_Loop_Func001Func009Func015001003 takes nothing returns boolean
return GetBooleanAnd((IsUnitType(GetFilterUnit(),UNIT_TYPE_STRUCTURE)!=true),(GetBooleanAnd((IsUnitType(GetFilterUnit(),UNIT_TYPE_MAGIC_IMMUNE)!=true),(IsUnitEnemy(GetFilterUnit(),GetOwningPlayer(udg_FS_Caster))==true))))
endfunction
function Trig_Fire_Spiral_Loop_Func001Func009Func015A takes nothing returns nothing
call UnitDamageTargetBJ(udg_FS_Caster,GetEnumUnit(),udg_FS_Damage,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_NORMAL)
endfunction
function Trig_Fire_Spiral_Loop_Func001Func009Func023A takes nothing returns nothing
set udg_FS_TempPoint=GetUnitLoc(GetEnumUnit())
set udg_FS_TempPoint2=PolarProjectionBJ(udg_FS_TempPoint,300.00,(AngleBetweenPoints(udg_FS_TempPoint,udg_FS_CasterPoint)+5.00))
call SetUnitPositionLoc(GetEnumUnit(),udg_FS_TempPoint2)
call RemoveLocation(udg_FS_TempPoint)
call RemoveLocation(udg_FS_TempPoint2)
endfunction
function Trig_Fire_Spiral_Loop_Func001Func009C takes nothing returns boolean
if(not(udg_FS_Time<udg_FS_MaxTime))then
return false
endif
if(not(UnitHasBuffBJ(udg_FS_Caster,'BPSE')!=true))then
return false
endif
if(not(UnitHasBuffBJ(udg_FS_Caster,'BUsp')!=true))then
return false
endif
if(not(UnitHasBuffBJ(udg_FS_Caster,'BUst')!=true))then
return false
endif
return true
endfunction
function Trig_Fire_Spiral_Loop_Func001A takes nothing returns nothing
set udg_FS_Caster=LoadUnitHandleBJ(0,GetHandleIdBJ(GetEnumUnit()),udg_FS_Hsahtable)
set udg_FS_CasterPoint=LoadLocationHandleBJ(1,GetHandleIdBJ(GetEnumUnit()),udg_FS_Hsahtable)
set udg_FS_Time=LoadRealBJ(2,GetHandleIdBJ(GetEnumUnit()),udg_FS_Hsahtable)
set udg_FS_Damage=LoadRealBJ(3,GetHandleIdBJ(GetEnumUnit()),udg_FS_Hsahtable)
set udg_FS_Height=LoadRealBJ(4,GetHandleIdBJ(GetEnumUnit()),udg_FS_Hsahtable)
set udg_FS_DummyGroup=LoadGroupHandleBJ(5,GetHandleIdBJ(GetEnumUnit()),udg_FS_Hsahtable)
set udg_FS_MaxTime=LoadRealBJ(6,GetHandleIdBJ(GetEnumUnit()),udg_FS_Hsahtable)
set udg_FS_Angle=LoadRealBJ(7,GetHandleIdBJ(GetEnumUnit()),udg_FS_Hsahtable)
if(Trig_Fire_Spiral_Loop_Func001Func009C())then
set udg_FS_Height=(udg_FS_Height+2.50)
set udg_FS_TempPoint=PolarProjectionBJ(udg_FS_CasterPoint,300.00,udg_FS_Angle)
call CreateNUnitsAtLoc(1,'h00D',GetOwningPlayer(udg_FS_Caster),udg_FS_TempPoint,(270.00+udg_FS_Angle))
call GroupAddUnitSimple(GetLastCreatedUnit(),udg_FS_DummyGroup)
call SetUnitFlyHeightBJ(GetLastCreatedUnit(),udg_FS_Height,9999999.00)
set udg_FS_Time=(udg_FS_Time+0.03)
set udg_FS_Angle=(udg_FS_Angle+5.00)
set bj_wantDestroyGroup=true
call ForGroupBJ(GetUnitsInRangeOfLocMatching(300.00,udg_FS_CasterPoint,Condition(function Trig_Fire_Spiral_Loop_Func001Func009Func015001003)),function Trig_Fire_Spiral_Loop_Func001Func009Func015A)
call RemoveLocation(udg_FS_TempPoint)
call SaveRealBJ(udg_FS_Time,2,GetHandleIdBJ(GetEnumUnit()),udg_FS_Hsahtable)
call SaveRealBJ(udg_FS_Height,4,GetHandleIdBJ(GetEnumUnit()),udg_FS_Hsahtable)
call SaveGroupHandleBJ(udg_FS_DummyGroup,5,GetHandleIdBJ(GetEnumUnit()),udg_FS_Hsahtable)
call SaveRealBJ(udg_FS_Angle,7,GetHandleIdBJ(GetEnumUnit()),udg_FS_Hsahtable)
call ForGroupBJ(udg_FS_DummyGroup,function Trig_Fire_Spiral_Loop_Func001Func009Func023A)
else
call ForGroupBJ(udg_FS_DummyGroup,function Trig_Fire_Spiral_Loop_Func001Func009Func001A)
call FlushChildHashtableBJ(GetHandleIdBJ(GetEnumUnit()),udg_FS_Hsahtable)
call GroupRemoveUnitSimple(GetEnumUnit(),udg_FS_UnitGroup)
endif
endfunction
function Trig_Fire_Spiral_Loop_Func002C takes nothing returns boolean
if(not(IsUnitGroupEmptyBJ(udg_FS_UnitGroup)==true))then
return false
endif
return true
endfunction
function Trig_Fire_Spiral_Loop_Actions takes nothing returns nothing
call ForGroupBJ(udg_FS_UnitGroup,function Trig_Fire_Spiral_Loop_Func001A)
if(Trig_Fire_Spiral_Loop_Func002C())then
call DisableTrigger(GetTriggeringTrigger())
else
endif
endfunction
function InitTrig_Fire_Spiral_Loop takes nothing returns nothing
set gg_trg_Fire_Spiral_Loop=CreateTrigger()
call DisableTrigger(gg_trg_Fire_Spiral_Loop)
call TriggerRegisterTimerEventPeriodic(gg_trg_Fire_Spiral_Loop,0.03)
call TriggerAddAction(gg_trg_Fire_Spiral_Loop,function Trig_Fire_Spiral_Loop_Actions)
endfunction
