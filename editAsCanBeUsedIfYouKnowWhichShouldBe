function Trig_RANDOM_ASS_Copy_2_Func004Func003Func001C takes nothing returns boolean
if(not(udg_Random_Neutral_Roll==udg_List[GetForLoopIndexA()]))then
return false
endif
return true
endfunction
function Trig_RANDOM_ASS_Copy_2_Func004Func004C takes nothing returns boolean
if(not(udg_Neutral_Hero_Is_Available==true))then
return false
endif
return true
endfunction
function Trig_RANDOM_ASS_Copy_2_Actions takes nothing returns nothing
set udg_Buying_Neutral_PLayer=GetOwningPlayer(GetBuyingUnit())
call UnitApplyTimedLifeBJ(2.00,'BTLF',GetSoldUnit())
set bj_forLoopAIndex=0
set bj_forLoopAIndexEnd=1000
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set udg_Neutral_Hero_Is_Available=true
set udg_Random_Neutral_Roll=GetRandomInt(1,10)
set bj_forLoopAIndex=0
set bj_forLoopAIndexEnd=(udg_List_Size-1)
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
if(Trig_RANDOM_ASS_Copy_2_Func004Func003Func001C())then
set udg_Neutral_Hero_Is_Available=false
else
endif
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
if(Trig_RANDOM_ASS_Copy_2_Func004Func004C())then
call IssueNeutralImmediateOrderById(udg_Buying_Neutral_PLayer,gg_unit_n002_0030,udg_Neutral_Random_Pool[udg_Random_Neutral_Roll])
return
else
endif
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endfunction
