function Trig_Creating_The_Stone_Actions takes nothing returns nothing
set udg_Shard=GetManipulatedItem()
set udg_Hero=GetManipulatingUnit()
call UnitRemoveItemSwapped(udg_Shard,udg_Hero)
if(Trig_Creating_The_Stone_Func004C())then
call SetItemCharges(GetItemOfTypeFromUnitBJ(udg_Hero,GetItemTypeId(udg_Shard)),(GetItemCharges(GetItemOfTypeFromUnitBJ(udg_Hero,GetItemTypeId(udg_Shard)))+GetItemCharges(udg_Shard)))
if(Trig_Creating_The_Stone_Func004Func002C())then
if(Trig_Creating_The_Stone_Func004Func002Func004C())then
set udg_First_Shard=gg_item_stpg_0232
else
if(Trig_Creating_The_Stone_Func004Func002Func004Func001C())then
set udg_Second_Shard=gg_item_stpg_0232
else
if(Trig_Creating_The_Stone_Func004Func002Func004Func001Func001C())then
set udg_Third_Shard=gg_item_stpg_0232
else
set udg_Fourth_Shard=gg_item_stpg_0232
endif
endif
endif
call RemoveItem(udg_Shard)
else
if(Trig_Creating_The_Stone_Func004Func002Func001C())then
if(Trig_Creating_The_Stone_Func004Func002Func001Func001C())then
set udg_First_Shard=gg_item_stpg_0232
else
if(Trig_Creating_The_Stone_Func004Func002Func001Func001Func001C())then
set udg_Second_Shard=gg_item_stpg_0232
else
if(Trig_Creating_The_Stone_Func004Func002Func001Func001Func001Func001C())then
set udg_Third_Shard=gg_item_stpg_0232
else
set udg_Fourth_Shard=gg_item_stpg_0232
endif
endif
endif
call RemoveItem(udg_Shard)
else
if(Trig_Creating_The_Stone_Func004Func002Func001Func002C())then
if(Trig_Creating_The_Stone_Func004Func002Func001Func002Func001C())then
set udg_First_Shard=gg_item_stpg_0232
else
if(Trig_Creating_The_Stone_Func004Func002Func001Func002Func001Func001C())then
set udg_Second_Shard=gg_item_stpg_0232
else
if(Trig_Creating_The_Stone_Func004Func002Func001Func002Func001Func001Func001C())then
set udg_Third_Shard=gg_item_stpg_0232
else
set udg_Fourth_Shard=gg_item_stpg_0232
endif
endif
endif
call DisableTrigger(GetTriggeringTrigger())
call UnitAddItemSwapped(udg_Shard,udg_Hero)
call EnableTrigger(GetTriggeringTrigger())
call RemoveItem(GetItemOfTypeFromUnitBJ(GetManipulatingUnit(),'I00F'))
else
endif
endif
endif
else
endif
call DisableTrigger(GetTriggeringTrigger())
call UnitAddItemSwapped(udg_Shard,udg_Hero)
call EnableTrigger(GetTriggeringTrigger())
endfunction
