function Trig_Envy_Reformed_Func001C takes nothing returns boolean
if(not(IsUnitType(udg_Envy_First_Form,UNIT_TYPE_DEAD)==true))then
return false
endif
if(not(udg_Envy_Is_In_Doll==true))then
return false
endif
return true
endfunction
function Trig_Envy_Reformed_Actions takes nothing returns nothing
if(Trig_Envy_Reformed_Func001C())then
set udg_Envy_Is_In_Doll=false
call ReviveHeroLoc(udg_Envy_First_Form,GetUnitLoc(udg_Envy_Doll_Body),false)
call SelectUnitForPlayerSingle(udg_Envy_First_Form,udg_Envy_Player)
call SetUnitOwner(udg_Envy_First_Form,udg_Envy_Player,true)
call RemoveUnit(udg_Envy_True_Form)
call RemoveUnit(udg_Envy_Doll_Body)
call UnitAddItemToSlotById(udg_Envy_First_Form,'I00G',0)
else
if(Trig_Envy_Reformed_Func001Func003C())then
call ReviveHeroLoc(udg_Envy_First_Form,GetUnitLoc(udg_Envy_True_Form),false)
call SetUnitOwner(udg_Envy_First_Form,udg_Envy_Player,true)
call SelectUnitForPlayerSingle(udg_Envy_First_Form,udg_Envy_Player)
call RemoveUnit(udg_Envy_True_Form)
else
call PauseUnitBJ(false,udg_Envy_First_Form)
call SetUnitInvulnerable(udg_Envy_First_Form,false)
call ShowUnitShow(udg_Envy_First_Form)
call SetUnitPositionLoc(udg_Envy_First_Form,GetUnitLoc(udg_Envy_True_Form))
call SelectUnitForPlayerSingle(udg_Envy_First_Form,udg_Envy_Player)
call RemoveUnit(udg_Envy_True_Form)
endif
endif
endfunction
function InitTrig_Envy_Reformed takes nothing returns nothing
set gg_trg_Envy_Reformed=CreateTrigger()
call TriggerRegisterTimerExpireEventBJ(gg_trg_Envy_Reformed,udg_Envy_Timer)
call TriggerAddAction(gg_trg_Envy_Reformed,function Trig_Envy_Reformed_Actions)
endfunction
