endfunction
function Trig_Fire_Spiral_Loop_Copy_Func001Func009Func015A takes nothing returns nothing
call UnitDamageTargetBJ(udg_Our_Father,GetEnumUnit(),udg_GS_Damage,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_NORMAL)
endfunction
function Trig_Fire_Spiral_Loop_Copy_Func001Func009Func023A takes nothing returns nothing
set udg_GS_TempPoint=GetUnitLoc(GetEnumUnit())
set udg_GS_TempPoint2=PolarProjectionBJ(udg_GS_TempPoint,300.00,(AngleBetweenPoints(udg_GS_TempPoint,udg_GS_CasterPoint)+5.00))
call SetUnitPositionLoc(GetEnumUnit(),udg_GS_TempPoint2)
call RemoveLocation(udg_GS_TempPoint)
call RemoveLocation(udg_GS_TempPoint2)
endfunction
function Trig_Fire_Spiral_Loop_Copy_Func001Func009C takes nothing returns boolean
if(not(udg_GS_Time<udg_GS_MaxTime))then
return false
endif
if(not(UnitHasBuffBJ(udg_Our_Father,'BPSE')!=true))then
return false
endif
if(not(UnitHasBuffBJ(udg_Our_Father,'BUsp')!=true))then
return false
endif
if(not(UnitHasBuffBJ(udg_Our_Father,'BUst')!=true))then
return false
endif
return true
endfunction
function Trig_Fire_Spiral_Loop_Copy_Func001A takes nothing returns nothing
set udg_Our_Father=LoadUnitHandleBJ(0,GetHandleIdBJ(GetEnumUnit()),udg_GS_Hashtable)
set udg_GS_CasterPoint=LoadLocationHandleBJ(1,GetHandleIdBJ(GetEnumUnit()),udg_GS_Hashtable)
set udg_GS_Time=LoadRealBJ(2,GetHandleIdBJ(GetEnumUnit()),udg_GS_Hashtable)
set udg_GS_Damage=LoadRealBJ(3,GetHandleIdBJ(GetEnumUnit()),udg_GS_Hashtable)
set udg_GS_Height=LoadRealBJ(4,GetHandleIdBJ(GetEnumUnit()),udg_GS_Hashtable)
set udg_GS_DummyGroup=LoadGroupHandleBJ(5,GetHandleIdBJ(GetEnumUnit()),udg_GS_Hashtable)
set udg_GS_MaxTime=LoadRealBJ(6,GetHandleIdBJ(GetEnumUnit()),udg_GS_Hashtable)
set udg_GS_Angle=LoadRealBJ(7,GetHandleIdBJ(GetEnumUnit()),udg_GS_Hashtable)
if(Trig_Fire_Spiral_Loop_Copy_Func001Func009C())then
set udg_GS_Height=(udg_GS_Height+2.50)
set udg_GS_TempPoint=PolarProjectionBJ(udg_GS_CasterPoint,300.00,udg_GS_Angle)
call CreateNUnitsAtLoc(1,'h04W',GetOwningPlayer(udg_Our_Father),udg_GS_TempPoint,(270.00+udg_GS_Angle))
call GroupAddUnitSimple(GetLastCreatedUnit(),udg_GS_DummyGroup)
call SetUnitFlyHeightBJ(GetLastCreatedUnit(),udg_GS_Height,9999999.00)
set udg_GS_Time=(udg_GS_Time+0.03)
set udg_GS_Angle=(udg_GS_Angle+5.00)
set bj_wantDestroyGroup=true
call ForGroupBJ(GetUnitsInRangeOfLocMatching(300.00,udg_GS_CasterPoint,Condition(function Trig_Fire_Spiral_Loop_Copy_Func001Func009Func015001003)),function Trig_Fire_Spiral_Loop_Copy_Func001Func009Func015A)
call RemoveLocation(udg_GS_TempPoint)
call SaveRealBJ(udg_GS_Time,2,GetHandleIdBJ(GetEnumUnit()),udg_GS_Hashtable)
call SaveRealBJ(udg_GS_Height,4,GetHandleIdBJ(GetEnumUnit()),udg_GS_Hashtable)
call SaveGroupHandleBJ(udg_GS_DummyGroup,5,GetHandleIdBJ(GetEnumUnit()),udg_GS_Hashtable)
call SaveRealBJ(udg_GS_Angle,7,GetHandleIdBJ(GetEnumUnit()),udg_GS_Hashtable)
call ForGroupBJ(udg_GS_DummyGroup,function Trig_Fire_Spiral_Loop_Copy_Func001Func009Func023A)
else
call ForGroupBJ(udg_GS_DummyGroup,function Trig_Fire_Spiral_Loop_Copy_Func001Func009Func001A)
call FlushChildHashtableBJ(GetHandleIdBJ(GetEnumUnit()),udg_GS_Hashtable)
call GroupRemoveUnitSimple(GetEnumUnit(),udg_GS_UnitGroup)
endif
endfunction
function Trig_Fire_Spiral_Loop_Copy_Func002C takes nothing returns boolean
if(not(IsUnitGroupEmptyBJ(udg_GS_UnitGroup)==true))then
return false
endif
return true
endfunction
function Trig_Fire_Spiral_Loop_Copy_Actions takes nothing returns nothing
call ForGroupBJ(udg_GS_UnitGroup,function Trig_Fire_Spiral_Loop_Copy_Func001A)
if(Trig_Fire_Spiral_Loop_Copy_Func002C())then
call DisableTrigger(GetTriggeringTrigger())
else
endif
endfunction
function InitTrig_Fire_Spiral_Loop_Copy takes nothing returns nothing
set gg_trg_Fire_Spiral_Loop_Copy=CreateTrigger()
call TriggerRegisterTimerEventPeriodic(gg_trg_Fire_Spiral_Loop_Copy,0.03)
call TriggerAddAction(gg_trg_Fire_Spiral_Loop_Copy,function Trig_Fire_Spiral_Loop_Copy_Actions)
endfunction
