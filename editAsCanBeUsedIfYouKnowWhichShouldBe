call SetUnitMoveSpeed(udg_He_Who_Sees_Truth,(udg_Truth_Movement-75.00))
call DisplayTextToForce(GetForceOfPlayer(udg_He_Who_Sees_Truth_Player),"|cff995500Truth: There little Al-che-mist. I have increased one of your attributes. But in exchange I have lowered another. That is the price of Truth.|r")
else
endif
endif
endif
endif
endif
endif
