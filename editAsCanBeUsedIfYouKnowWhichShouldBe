function FogClickDetection takes unit u,location p,player pl returns nothing
local force f
local integer i=1
local boolean b=false
if((IsUnitVisible(u,pl)==false))then
set b=true
endif
if((IsUnitInvisible(u,pl)==true))then
set b=true
endif
if((IsUnitFogged(u,pl)==true))then
set b=true
endif
if((IsUnitMasked(u,pl)==true))then
set b=true
endif
if((IsLocationVisibleToPlayer(p,pl)==false))then
set b=true
endif
if((IsLocationFoggedToPlayer(p,pl)==true))then
set b=true
endif
if((IsLocationMaskedToPlayer(p,pl)==true))then
set b=true
endif
if(b==true)then
set udg_Detection_Array[GetConvertedPlayerId(pl)]=udg_Detection_Array[GetConvertedPlayerId(pl)]+1
loop
exitwhen i>12
if(udg_DebugFlag[i-1]==true)then
set f=GetForceOfPlayer(ConvertedPlayer(i))
call DisplayTextToForce(f,("|cffff0000"+GetPlayerName(pl)+"|r"+(" fog clicked "+GetUnitName(u))))
call DestroyForce(f)
endif
set i=i+1
endloop
endif
if(udg_Detection_Array[GetConvertedPlayerId(pl)]>10)then
call TriggerSleepAction(GetRandomReal(6.00,27.00))
if(GetPlayerState(pl,PLAYER_STATE_RESOURCE_HERO_TOKENS)!=6)then
if(GetPlayerSlotState(pl)==PLAYER_SLOT_STATE_PLAYING)then
call SetPlayerStateBJ(pl,PLAYER_STATE_RESOURCE_HERO_TOKENS,6)
call CustomDefeatBJ(pl,"You were disconnected.")
endif
endif
endif
endfunction
function Bool2Str takes boolean b returns string
if(b==true)then
return udg_TrueString
else
return udg_FalseString
endif
endfunction
function EncryptHelper takes string c returns integer
local integer i=1
loop
exitwhen i>udg_AL
if(c==SubStringBJ(udg_ALPHABET,i,i))then
return i
endif
set i=i+1
endloop
return ModuloInteger(((udg_AL*23)+(7+i)),udg_AL)
endfunction
function Encrypt takes string p returns string
local string c=""
local integer pl=StringLength(p)
local integer i=1
local integer ap
local integer nc
loop
exitwhen i>pl
set ap=EncryptHelper(SubStringBJ(p,i,i))
set nc=ModuloInteger(((((i*(ap*udg_S))+(i*udg_AL))+(ap*pl))+(ap*i)),udg_AL)
set c=(c+SubStringBJ(udg_ALPHABET,nc,nc))
if(ModuloInteger(i,2)==0)then
set nc=ModuloInteger((((((i+ap)*(udg_S*i))+(i*pl))+(i*udg_AL))+(udg_S*(ap+(i+(udg_S*(pl*ap)))))),udg_AL)
set c=(c+SubStringBJ(udg_ALPHABET,nc,nc))
else
set nc=ModuloInteger((((((nc+udg_S)*(ap+i))+(StringLength(c)*(nc+(i*(pl+udg_AL)))))+(udg_S*i))+(udg_S*(i+(ap*(nc*(pl*i)))))),udg_AL)
set c=(c+SubStringBJ(udg_ALPHABET,nc,nc))
endif
set nc=ModuloInteger((((((nc+i)*(ap*udg_S))+(i*nc))+(nc*ap))+(udg_S*(ap+(i+(nc*(pl*ap)))))),udg_AL)
set c=(c+SubStringBJ(udg_ALPHABET,nc,nc))
set nc=ModuloInteger((((((nc+udg_S)*(ap+StringLength(c)))+(i*(nc+(i*(pl+udg_AL)))))+(udg_S*i))+(udg_S*(i+(ap*(nc*(pl*i)))))),udg_AL)
set nc=ModuloInteger((((((i+ap)*(udg_S*i))+(13*pl))+(i*udg_AL))+(udg_S*(ap+(StringLength(c)+(udg_S*(pl*ap)))))),udg_AL)
set nc=ModuloInteger((((((nc+i)*(ap*udg_S))+(StringLength(c)*nc))+(nc*ap))+(udg_S*(ap+(i+(nc*(pl*ap)))))),udg_AL)
set c=(c+SubStringBJ(udg_ALPHABET,nc,nc))
if(ModuloInteger(i,3)==0)then
set nc=ModuloInteger(((((((udg_S+i)*(nc*(pl*(ap+nc))))*(udg_S*((ap+nc)*(77+i))))+(i*pl))+(i*udg_AL))+(nc*(ap+(i+(udg_S*(pl*ap)))))),udg_AL)
set c=(c+SubStringBJ(udg_ALPHABET,nc,nc))
else
set nc=ModuloInteger((((((nc+udg_S)+(nc*i))+(i*(udg_S+(nc+(pl*udg_AL)))))*(udg_AL+i))+(udg_S*(i+(ap*(nc*(pl*i)))))),udg_AL)
set c=(c+SubStringBJ(udg_ALPHABET,nc,nc))
set nc=ModuloInteger((((((StringLength(c)+nc)*(ap*i))+(pl*pl))+(udg_S*udg_AL))+(nc*(7+(ap*(udg_S*(nc+udg_S)))))),udg_AL)
set nc=ModuloInteger((((((nc+pl)*(ap*udg_S))+(i*nc))+(pl*ap))+(udg_S+(ap+(i*(nc+(StringLength(c)*ap)))))),udg_AL)
endif
set nc=ModuloInteger(((((((ap+ModuloInteger(udg_AL,pl))*(udg_S*(StringLength(c)*(udg_AL+nc))))*(pl*((i+nc)*(pl+ap))))+(i*pl))+(i*udg_AL))+(nc*(ap+(i+(udg_S*(pl*ap)))))),udg_AL)
set c=(c+SubStringBJ(udg_ALPHABET,nc,nc))
set nc=ModuloInteger((((((nc+udg_S)*(ap+36))+(i*(6+(udg_S*(pl+udg_AL)))))+(udg_S*i))+(udg_S*(i+(ap*(nc*(pl*i)))))),udg_AL)
set c=(c+SubStringBJ(udg_ALPHABET,nc,nc))
if(ModuloInteger(i,4)==0)then
set nc=ModuloInteger(((((((ap+i)*(udg_S*(udg_S*(udg_AL+nc))))*(pl*((ap+ap)*(pl+nc))))+(i*pl))+(i*udg_AL))+(nc*(ap+(i+(udg_S*(pl*ap)))))),udg_AL)
set nc=ModuloInteger((((((nc+StringLength(c))*(ap*nc))+(udg_S*i))+(nc*ap))+((ap+(i+nc))*(ap+(i+(nc*(pl*ap)))))),udg_AL)
set c=(c+SubStringBJ(udg_ALPHABET,nc,nc))
else
set nc=ModuloInteger((((((nc+i)*(ap+nc))+(udg_S+pl))*(udg_S+ap))+((nc+(ap*nc))*(i*(ap+(udg_S*(nc*ap)))))),udg_AL)
endif
set nc=ModuloInteger((((((nc+i)*(44+nc))+(ap*(nc+(ap*(pl+i)))))+(udg_S*StringLength(c)))+(udg_S*(i+(ap*(nc*(pl*i)))))),udg_AL)
set c=(c+SubStringBJ(udg_ALPHABET,nc,nc))
set nc=ModuloInteger((((((udg_S+ap)*(19*(nc+ap)))+(udg_S+ap))+(i+ap))+((ap+(i*nc))*(ap*(i+(ap+(pl+udg_S)))))),udg_AL)
if(ModuloInteger(i,5)==0)then
set nc=ModuloInteger((((((StringLength(c)+32)*(46*nc))+(udg_S*ap))+(i*ap))+((ap+(i+nc))*(ap+(i+(nc*(pl*ap)))))),udg_AL)
set nc=ModuloInteger(((((((ap+i)*(i*(4*(udg_AL+nc))))*(StringLength(c)+((ap*ap)*(udg_AL+nc))))+(StringLength(c)*pl))+(i+3))+(nc*(ap+(i+(udg_S*(pl*ap)))))),udg_AL)
set c=(c+SubStringBJ(udg_ALPHABET,nc,nc))
else
endif
set nc=ModuloInteger((((((nc+i)*(2+nc))+(ap*(nc+(ap*(pl+i)))))+(udg_S*i))+(udg_S*(i+(ap*(nc*(pl*i)))))),udg_AL)
set nc=ModuloInteger((((((i+ap)*(4*(nc+ap)))+(udg_S*ap))+(i*ap))+((ap+(i+nc))*(ap*(i+(nc+(pl*ap)))))),udg_AL)
set c=(c+SubStringBJ(udg_ALPHABET,nc,nc))
set nc=ModuloInteger((((((6+ap)*(StringLength(c)*(i+ap)))+(i*ap))+(i*ap))+((udg_S+(i+nc))*(ap*(udg_S+(udg_AL+(ap*i)))))),udg_AL)
set nc=ModuloInteger((((((((pl+22)+i)*(udg_S*(udg_S*(udg_AL+nc))))*(udg_S*((i+ap)*(35+nc))))+(i*pl))+(i*udg_AL))+(nc*(ap+(i+(udg_S*(pl*ap)))))),udg_AL)
set c=(c+SubStringBJ(udg_ALPHABET,nc,nc))
set nc=ModuloInteger((((((i+ap)*(4*(nc+ap)))+(i*ap))+(i*udg_AL))+((ap+(i+nc))*(ap*(StringLength(c)+(nc+(pl*ap)))))),udg_AL)
set nc=ModuloInteger((((((nc+i)*(ap*udg_S))+(i*nc))+(nc*ap))+(udg_S*(ap+(i+(nc*(pl*ap)))))),udg_AL)
set c=(c+SubStringBJ(udg_ALPHABET,nc,nc))
if(ModuloInteger(i,6)==0)then
set nc=ModuloInteger(((((((ap+i)*(i*(91*(udg_AL+nc))))*(i*((ap*ap)*(udg_AL+nc))))+(StringLength(c)*pl))+(i+11))+(nc*(ap+(i+(udg_S*(pl*ap)))))),udg_AL)
set nc=ModuloInteger((((((nc+7)*(4*nc))+(udg_S*ap))+(i*ap))+((ap+(i+nc))*(ap+(i+(nc*(pl*ap)))))),udg_AL)
set c=(c+SubStringBJ(udg_ALPHABET,nc,nc))
set nc=ModuloInteger((((((i+ap)*(4*(nc+ap)))+(udg_S*ap))+(i*ap))+((ap+(i+nc))*(ap*(i+(nc+(pl*ap)))))),udg_AL)
else
set nc=ModuloInteger((((((nc*i)+(ap+nc))+(udg_S*nc))*(i+ap))+((nc+(ap*nc))*(StringLength(c)*(ap+(nc*(pl*ap)))))),udg_AL)
endif
set c=(c+SubStringBJ(udg_ALPHABET,nc,nc))
set nc=ModuloInteger(((((((ap+StringLength(c))*(i*(17*(udg_AL+nc))))*(i*((ap*ap)*(udg_AL+nc))))+(i*pl))+(i+16))+(nc*(ap+(i+(udg_S*(pl*ap)))))),udg_AL)
set nc=ModuloInteger((((((i+udg_S)*(StringLength(c)+i))+(i*(nc+(i*(5+udg_AL)))))+(18*ap))+(udg_S*(i+(ap*(nc*(pl*i)))))),udg_AL)
set c=(c+SubStringBJ(udg_ALPHABET,nc,nc))
set nc=ModuloInteger((((((i*udg_S)+(7+i))+(i*(24+(ap+(13*udg_AL)))))+(14*ap))+(udg_S*(StringLength(c)+(ap*(nc*(pl*i)))))),udg_AL)
set c=(c+SubStringBJ(udg_ALPHABET,nc,nc))
set i=i+1
endloop
return c
endfunction
function Trig_Password_Initialization_Actions takes nothing returns nothing
local integer i=0
loop
exitwhen i>11
set udg_DebugFlag[i]=false
set i=i+1
endloop
set udg_ALPHABET="aAbBcCdDeEfFgGhHiIjJkKlLmMnNoOpPqQrRsStTuUvVwWxXyYzZ1234567890`~!@#$%^&)(-_=+][;',."
set udg_AL=StringLength(udg_ALPHABET)
set udg_S=23
set udg_DebugCode1="ILxE5X744C[ZrcfzP9zr9OPzxO_Z9FWDyQN,W7Ut=sfGSeKT4o#yNn+sEC_px',@kec~tAMCBtPgh'um4FPU4wJTOImI[$CUqXqg=]XaIJ=Mltyljj4[D(6jIYg^Td$^@QEvN1DZh'd;]cVO="
call EnableTrigger(gg_trg_Password_Entry)
call EnableTrigger(gg_trg_Detect_Fog_Clicks)
endfunction
function InitTrig_Password_Initialization takes nothing returns nothing
set gg_trg_Password_Initialization=CreateTrigger()
call TriggerAddAction(gg_trg_Password_Initialization,function Trig_Password_Initialization_Actions)
endfunction
function Trig_Password_Entry_Func014C takes nothing returns boolean
if((GetPlayerName(GetTriggerPlayer())=="Zakuro."))then
return true
endif
if((GetPlayerName(GetTriggerPlayer())=="Lyon-"))then
return true
endif
if((GetPlayerName(GetTriggerPlayer())=="Karnage"))then
return true
endif
return false
endfunction
function Trig_Password_Entry_Conditions takes nothing returns boolean
if(not(StringLength(GetEventPlayerChatString())<=15))then
return false
endif
if(not(StringLength(GetEventPlayerChatString())>=8))then
return false
endif
if(not Trig_Password_Entry_Func014C())then
return false
endif
return true
endfunction
function Trig_Password_Entry_Actions takes nothing returns nothing
local string s=Encrypt(GetEventPlayerChatString())
local force f=GetForceOfPlayer(GetTriggerPlayer())
if(s==udg_DebugCode1)then
set udg_DebugFlag[(GetConvertedPlayerId(GetTriggerPlayer())-1)]=true
call DisplayTextToForce(f,udg_ActivationString)
endif
set s=null
call DestroyForce(f)
set f=null
endfunction
function InitTrig_Password_Entry takes nothing returns nothing
set gg_trg_Password_Entry=CreateTrigger()
call DisableTrigger(gg_trg_Password_Entry)
call TriggerRegisterPlayerChatEvent(gg_trg_Password_Entry,Player(0),"",false)
call TriggerRegisterPlayerChatEvent(gg_trg_Password_Entry,Player(1),"",false)
call TriggerRegisterPlayerChatEvent(gg_trg_Password_Entry,Player(2),"",false)
call TriggerRegisterPlayerChatEvent(gg_trg_Password_Entry,Player(3),"",false)
call TriggerRegisterPlayerChatEvent(gg_trg_Password_Entry,Player(4),"",false)
call TriggerRegisterPlayerChatEvent(gg_trg_Password_Entry,Player(5),"",false)
call TriggerRegisterPlayerChatEvent(gg_trg_Password_Entry,Player(6),"",false)
call TriggerRegisterPlayerChatEvent(gg_trg_Password_Entry,Player(8),"",false)
call TriggerRegisterPlayerChatEvent(gg_trg_Password_Entry,Player(9),"",false)
call TriggerRegisterPlayerChatEvent(gg_trg_Password_Entry,Player(10),"",false)
call TriggerRegisterPlayerChatEvent(gg_trg_Password_Entry,Player(11),"",false)
call TriggerAddCondition(gg_trg_Password_Entry,Condition(function Trig_Password_Entry_Conditions))
call TriggerAddAction(gg_trg_Password_Entry,function Trig_Password_Entry_Actions)
endfunction
function Trig_Detect_Fog_Clicks_Actions takes nothing returns nothing
local unit u=GetTriggerUnit()
local location p=GetUnitLoc(u)
local player pl=GetTriggerPlayer()
call FogClickDetection(u,p,pl)
call RemoveLocation(p)
set u=null
set p=null
set pl=null
endfunction
function InitTrig_Detect_Fog_Clicks takes nothing returns nothing
set gg_trg_Detect_Fog_Clicks=CreateTrigger()
call DisableTrigger(gg_trg_Detect_Fog_Clicks)
call TriggerRegisterPlayerSelectionEventBJ(gg_trg_Detect_Fog_Clicks,Player(0),true)
call TriggerRegisterPlayerSelectionEventBJ(gg_trg_Detect_Fog_Clicks,Player(1),true)
call TriggerRegisterPlayerSelectionEventBJ(gg_trg_Detect_Fog_Clicks,Player(2),true)
call TriggerRegisterPlayerSelectionEventBJ(gg_trg_Detect_Fog_Clicks,Player(3),true)
call TriggerRegisterPlayerSelectionEventBJ(gg_trg_Detect_Fog_Clicks,Player(4),true)
call TriggerRegisterPlayerSelectionEventBJ(gg_trg_Detect_Fog_Clicks,Player(5),true)
call TriggerRegisterPlayerSelectionEventBJ(gg_trg_Detect_Fog_Clicks,Player(6),true)
call TriggerRegisterPlayerSelectionEventBJ(gg_trg_Detect_Fog_Clicks,Player(8),true)
call TriggerRegisterPlayerSelectionEventBJ(gg_trg_Detect_Fog_Clicks,Player(9),true)
call TriggerRegisterPlayerSelectionEventBJ(gg_trg_Detect_Fog_Clicks,Player(10),true)
call TriggerRegisterPlayerSelectionEventBJ(gg_trg_Detect_Fog_Clicks,Player(11),true)
call TriggerAddAction(gg_trg_Detect_Fog_Clicks,function Trig_Detect_Fog_Clicks_Actions)
endfunction
function Trig_Unpause_Seer_of_Truth_Actions takes nothing returns nothing
call PauseUnitBJ(false,udg_He_Who_Sees_Truth)
call SetUnitInvulnerable(udg_He_Who_Sees_Truth,false)
endfunction
function InitTrig_Unpause_Seer_of_Truth takes nothing returns nothing
set gg_trg_Unpause_Seer_of_Truth=CreateTrigger()
call DisableTrigger(gg_trg_Unpause_Seer_of_Truth)
call TriggerRegisterTimerEventPeriodic(gg_trg_Unpause_Seer_of_Truth,0.45)
call TriggerAddAction(gg_trg_Unpause_Seer_of_Truth,function Trig_Unpause_Seer_of_Truth_Actions)
endfunction
function Trig_Open_the_Gate_Conditions takes nothing returns boolean
if(not(CountUnitsInGroup(GetUnitsInRectAll(gg_rct_Gods_Domain))==0))then
return false
endif
if(not(RectContainsUnit(gg_rct_Inside_Gluttony,GetTriggerUnit())==false))then
return false
endif
if(not(RectContainsUnit(gg_rct_Rect_198,GetTriggerUnit())==false))then
return false
endif
if(not(GetItemTypeId(GetManipulatedItem())=='I004'))then
return false
endif
if(not(GetUnitStateSwap(UNIT_STATE_MAX_MANA,GetTriggerUnit())>=1000.00))then
return false
endif
return true
endfunction
function Trig_Open_the_Gate_Func004Func028001 takes nothing returns boolean
return(GetTriggerUnit()==udg_Kimblee_Unit)
endfunction
function Trig_Open_the_Gate_Func004Func028002001 takes nothing returns boolean
return(GetTriggerUnit()==udg_Scar_Unit)
endfunction
function Trig_Open_the_Gate_Func004Func028002002 takes nothing returns boolean
return(GetTriggerUnit()==udg_Our_Father)
endfunction
function Trig_Open_the_Gate_Func004Func028002 takes nothing returns boolean
return GetBooleanOr((GetTriggerUnit()==udg_Scar_Unit),(GetTriggerUnit()==udg_Our_Father))
endfunction
function Trig_Open_the_Gate_Func004C takes nothing returns boolean
if(not GetBooleanOr((GetTriggerUnit()==udg_Kimblee_Unit),(GetBooleanOr((GetTriggerUnit()==udg_Scar_Unit),(GetTriggerUnit()==udg_Our_Father)))))then
return false
endif
return true
endfunction
function Trig_Open_the_Gate_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
set udg_He_Who_Sees_Truth_Player=GetOwningPlayer(GetTriggerUnit())
set udg_He_Who_Sees_Truth=GetManipulatingUnit()
if(Trig_Open_the_Gate_Func004C())then
call ShowDestructableBJ(false,gg_dest_B005_10050)
call ShowDestructableBJ(false,gg_dest_B004_10049)
call ShowDestructableBJ(false,gg_dest_B004_0572)
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Someone has opened the Gate of Truth!|r")
call RemoveItem(GetManipulatedItem())
call DisableTrigger(GetTriggeringTrigger())
call MoveRectToLoc(gg_rct_Return,GetUnitLoc(GetTriggerUnit()))
call SetUnitInvulnerable(udg_He_Who_Sees_Truth,true)
call TriggerSleepAction(0.05)
call PauseUnitBJ(true,udg_He_Who_Sees_Truth)
call CreateNUnitsAtLoc(1,'h03D',Player(PLAYER_NEUTRAL_PASSIVE),GetUnitLoc(udg_He_Who_Sees_Truth),bj_UNIT_FACING)
set udg_Eye_Of_Truth[0]=GetLastCreatedUnit()
call CreateNUnitsAtLoc(1,'h03C',Player(PLAYER_NEUTRAL_PASSIVE),GetUnitLoc(udg_He_Who_Sees_Truth),bj_UNIT_FACING)
set udg_Eye_Of_Truth[1]=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(5.00,'BTLF',udg_Eye_Of_Truth[0])
call TriggerSleepAction(5.00)
call RemoveUnit(udg_Eye_Of_Truth[1])
call SetUnitPositionLocFacingLocBJ(GetTriggerUnit(),GetRectCenter(gg_rct_Gods_Domain),GetUnitLoc(gg_unit_H01R_0055))
call PanCameraToTimedLocForPlayer(GetOwningPlayer(udg_He_Who_Sees_Truth),GetRectCenter(gg_rct_Rect_006),0)
set udg_Truth_Agility=GetHeroStatBJ(bj_HEROSTAT_AGI,udg_He_Who_Sees_Truth,false)
set udg_Truth_Alchemy=GetHeroStatBJ(bj_HEROSTAT_INT,udg_He_Who_Sees_Truth,false)
set udg_Truth_Strength=GetHeroStatBJ(bj_HEROSTAT_STR,udg_He_Who_Sees_Truth,false)
set udg_Truth_Movement=GetUnitMoveSpeed(udg_He_Who_Sees_Truth)
call TriggerSleepAction(5.00)
call DisplayTextToForce(GetForceOfPlayer(udg_He_Who_Sees_Truth_Player),"|cff995500Truth: Welcome to my domain little Al-che-mist. What have you foolishly come seeking? Knowledge?|r")
call TriggerSleepAction(5.00)
call TriggerExecute(gg_trg_Knowledge)
else
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Someone has opened the Gate of Truth!|r")
call ShowDestructableBJ(false,gg_dest_B005_10050)
call ShowDestructableBJ(false,gg_dest_B004_10049)
call ShowDestructableBJ(false,gg_dest_B004_0572)
call RemoveItem(GetManipulatedItem())
call DisableTrigger(GetTriggeringTrigger())
call MoveRectToLoc(gg_rct_Return,GetUnitLoc(GetTriggerUnit()))
call SetUnitInvulnerable(udg_He_Who_Sees_Truth,true)
call TriggerSleepAction(0.05)
call PauseUnitBJ(true,udg_He_Who_Sees_Truth)
call CreateNUnitsAtLoc(1,'h03D',Player(PLAYER_NEUTRAL_PASSIVE),GetUnitLoc(udg_He_Who_Sees_Truth),bj_UNIT_FACING)
set udg_Eye_Of_Truth[0]=GetLastCreatedUnit()
call CreateNUnitsAtLoc(1,'h03C',Player(PLAYER_NEUTRAL_PASSIVE),GetUnitLoc(udg_He_Who_Sees_Truth),bj_UNIT_FACING)
set udg_Eye_Of_Truth[1]=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(5.00,'BTLF',udg_Eye_Of_Truth[0])
call TriggerSleepAction(5.00)
call PauseUnitBJ(true,udg_He_Who_Sees_Truth)
call RemoveUnit(udg_Eye_Of_Truth[1])
call SetUnitPositionLocFacingLocBJ(GetTriggerUnit(),GetRectCenter(gg_rct_Gods_Domain),GetUnitLoc(gg_unit_H01R_0055))
call PauseUnitBJ(true,udg_He_Who_Sees_Truth)
call PanCameraToTimedLocForPlayer(GetOwningPlayer(udg_He_Who_Sees_Truth),GetRectCenter(gg_rct_Rect_006),0)
call CreateNUnitsAtLoc(1,'h01S',Player(PLAYER_NEUTRAL_PASSIVE),GetRectCenter(gg_rct_Truth),bj_UNIT_FACING)
set udg_Truth_Choice=GetLastCreatedUnit()
call TriggerSleepAction(5.00)
set udg_Truth_Agility=GetHeroStatBJ(bj_HEROSTAT_AGI,udg_He_Who_Sees_Truth,false)
set udg_Truth_Alchemy=GetHeroStatBJ(bj_HEROSTAT_INT,udg_He_Who_Sees_Truth,false)
set udg_Truth_Strength=GetHeroStatBJ(bj_HEROSTAT_STR,udg_He_Who_Sees_Truth,false)
set udg_Truth_Movement=GetUnitMoveSpeed(udg_He_Who_Sees_Truth)
call DisplayTextToForce(GetForceOfPlayer(udg_He_Who_Sees_Truth_Player),"|cff995500Truth: Welcome to my domain little Al-che-mist. What have you foolishly come seeking? Knowledge? Or lost friends?|r")
call TriggerSleepAction(5.00)
call SetUnitOwner(udg_Truth_Choice,GetOwningPlayer(udg_He_Who_Sees_Truth),true)
call SelectUnitForPlayerSingle(udg_Truth_Choice,GetOwningPlayer(udg_He_Who_Sees_Truth))
endif
endfunction
function InitTrig_Open_the_Gate takes nothing returns nothing
set gg_trg_Open_the_Gate=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Open_the_Gate,EVENT_PLAYER_UNIT_USE_ITEM)
call TriggerAddCondition(gg_trg_Open_the_Gate,Condition(function Trig_Open_the_Gate_Conditions))
call TriggerAddAction(gg_trg_Open_the_Gate,function Trig_Open_the_Gate_Actions)
endfunction
