return GetBooleanOr((GetUnitTypeId(GetDyingUnit())=='H02R'),(GetUnitTypeId(GetDyingUnit())=='H04O'))
endfunction
function Trig_Father_Dies_Conditions takes nothing returns boolean
if(not GetBooleanOr((GetUnitTypeId(GetDyingUnit())=='H04N'),(GetBooleanOr((GetUnitTypeId(GetDyingUnit())=='H02R'),(GetUnitTypeId(GetDyingUnit())=='H04O')))))then
return false
endif
return true
endfunction
