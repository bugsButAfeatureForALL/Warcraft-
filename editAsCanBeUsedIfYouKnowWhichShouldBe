function Trig_Cornello_Dies_Actions takes nothing returns nothing
call TriggerSleepAction(10.00)
if(Trig_Cornello_Dies_Func002C())then
call PauseTimerBJ(true,udg_Rebound_Duration)
set udg_Rebound_Duration=CreateTimer()
call ReviveHeroLoc(udg_Cornello_Unit,GetUnitLoc(gg_unit_H02L_0157),false)
call DisplayTextToForce(GetPlayersAll(),"|cffff0000The Gold Toothed Doctor|r has used his medical knowledge to revive Cornello!")
call SetHeroLevelBJ(GetTriggerUnit(),(GetHeroLevel(GetDyingUnit())-3),false)
call PanCameraToTimedLocForPlayer(GetOwningPlayer(GetDyingUnit()),GetUnitLoc(GetDyingUnit()),0)
call SelectUnitForPlayerSingle(GetDyingUnit(),udg_Cornello_Player)
call EnableTrigger(gg_trg_Rebound_Roll_5)
call EnableTrigger(gg_trg_Rebound_Limiter)
call DisableTrigger(gg_trg_Rebound_Limit)
else
call SetPlayerState(udg_Cornello_Player,PLAYER_STATE_OBSERVER,1)
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Cornello has been killed, the false priest will not be remembered.|r")
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Cornello_Player),bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Cornello_Player),udg_Evil_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Cornello_Player),bj_ALLIANCE_ALLIED_VISION)
call ForceRemovePlayerSimple(udg_Cornello_Player,udg_Homunculi)
if(Trig_Cornello_Dies_Func002Func007C())then
call DisplayTextToForce(GetPlayersAll(),"The |cffff0000Alchemists and their friends|r defeated all enemies as well as Father and put an end to his plans once and for all! A new era of peace falls over Amestris.")
call TriggerSleepAction(10.00)
call CustomVictoryBJ(Player(0),true,true)
call CustomVictoryBJ(Player(1),true,true)
call CustomVictoryBJ(Player(2),true,true)
call CustomDefeatBJ(Player(3),"You have lost!")
call CustomVictoryBJ(Player(4),true,true)
call CustomVictoryBJ(Player(5),true,true)
call CustomVictoryBJ(Player(6),true,true)
if(Trig_Cornello_Dies_Func002Func007Func010C())then
call CustomVictoryBJ(udg_Scar_Player,true,true)
else
call CustomDefeatBJ(udg_Scar_Player,"You have lost!")
endif
call CustomDefeatBJ(udg_Greed_Player,"You have lost!")
call CustomDefeatBJ(udg_Barry_Player,"You have lost!")
call CustomDefeatBJ(udg_Slicer_Player,"You have lost!")
call CustomDefeatBJ(Player(10),"You have lost!")
call CustomDefeatBJ(Player(11),"You have lost!")
else
endif
endif
endfunction
