call TriggerAddAction(gg_trg_Yok_Time,function Trig_Yok_Time_Actions)
endfunction
function Trig_Yok_Message_Actions takes nothing returns nothing
call CreateTextTagLocBJ("This is the boat to Yok Island, you must be level 10 and a hero to use it and it can only be used once per person!",GetRectCenter(gg_rct_Rect_190),0,10,100,100,100,0)
endfunction
