call TriggerSleepAction(1.00)
call RemoveUnit(udg_Jean_Unit)
call ForceRemovePlayerSimple(udg_Jean_Player,udg_Alchemists)
call ForceRemovePlayerSimple(udg_Jean_Player,udg_Dead_Good_Guys)
call UnitAddItemByIdSwapped('I00R',gg_unit_H01V_0080)
else
endif
if(Trig_Player_2_Leaves_Copy_Func014C())then
if(Trig_Player_2_Leaves_Copy_Func014Func001C())then
call TriggerExecute(gg_trg_Greed_Takes_Control_First_Time)
call ForceRemovePlayerSimple(udg_Ling_Player,udg_Alchemists)
call UnitRemoveAbilityBJ('A04J',udg_Greed)
else
set udg_Player_Left=udg_Greed
call EnableTrigger(gg_trg_Player_Leave_Items)
call TriggerSleepAction(1.00)
call RemoveUnit(udg_Greed)
call ForceRemovePlayerSimple(udg_Greed_Player,udg_Neutral)
endif
else
endif
endfunction
function InitTrig_Player_2_Leaves_Copy takes nothing returns nothing
set gg_trg_Player_2_Leaves_Copy=CreateTrigger()
call TriggerRegisterPlayerStateEvent(gg_trg_Player_2_Leaves_Copy,Player(1),PLAYER_STATE_RESOURCE_HERO_TOKENS,EQUAL,6.00)
call TriggerAddAction(gg_trg_Player_2_Leaves_Copy,function Trig_Player_2_Leaves_Copy_Actions)
endfunction
