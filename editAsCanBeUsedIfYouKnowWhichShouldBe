function Trig_Stun_Chance_Actions takes nothing returns nothing
set udg_Izumi_Stun_Chance=GetRandomInt(1,10)
if(Trig_Stun_Chance_Func002C())then
call IssueImmediateOrder(udg_Izumi_Unit,"stop")
call ResetUnitAnimation(udg_Izumi_Unit)
call TriggerSleepAction(0.10)
call IssueTargetOrder(gg_unit_hsor_0158,"thunderbolt",udg_Izumi_Unit)
call SetUnitAnimation(udg_Izumi_Unit,"Spell Two")
call AddSpecialEffectTargetUnitBJ("Sprite",udg_Izumi_Unit,"Objects\\Spawnmodels\\Critters\\Albatross\\CritterBloodAlbatross.mdl")
set udg_Zoomi_Blood=GetLastCreatedEffectBJ()
call TriggerSleepAction(1.00)
call DestroyEffect(udg_Zoomi_Blood)
else
endif
endfunction
