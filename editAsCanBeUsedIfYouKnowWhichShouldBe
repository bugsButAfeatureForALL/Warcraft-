function Trig_Canine_Cunning_Cast_Func001002003 takes nothing returns boolean
return(IsUnitEnemy(GetFilterUnit(),udg_Dolcetto_Player)==true)
endfunction
function Trig_Canine_Cunning_Cast_Func002A takes nothing returns nothing
call CreateNUnitsAtLoc(1,'h03R',udg_Dolcetto_Player,GetUnitLoc(GetEnumUnit()),bj_UNIT_FACING)
set udg_Cunning_Dummy=GetLastCreatedUnit()
call SetUnitAbilityLevelSwapped('A079',udg_Cunning_Dummy,GetUnitAbilityLevelSwapped('A07A',udg_Dolcetto_Unit))
call IssuePointOrderLoc(udg_Cunning_Dummy,"silence",GetUnitLoc(GetEnumUnit()))
call UnitApplyTimedLifeBJ(2.00,'BTLF',udg_Cunning_Dummy)
endfunction
function Trig_Canine_Cunning_Cast_Actions takes nothing returns nothing
set udg_Cunning_Group=GetUnitsInRangeOfLocMatching(400.00,GetUnitLoc(udg_Dolcetto_Unit),Condition(function Trig_Canine_Cunning_Cast_Func001002003))
call ForGroupBJ(udg_Cunning_Group,function Trig_Canine_Cunning_Cast_Func002A)
call DestroyGroup(udg_Cunning_Group)
endfunction
function InitTrig_Canine_Cunning_Cast takes nothing returns nothing
