function Trig_Zampano_Homunculi_LB_Actions takes nothing returns nothing
if(Trig_Zampano_Homunculi_LB_Func001C())then
call SetPlayerAllianceStateBJ(udg_Zampano_Player,udg_Heinkel_Player,bj_ALLIANCE_UNALLIED)
call SetPlayerAllianceStateBJ(udg_Heinkel_Player,udg_Zampano_Player,bj_ALLIANCE_UNALLIED)
call SetPlayerAllianceStateBJ(udg_Darius_Player,udg_Zampano_Player,bj_ALLIANCE_UNALLIED)
call SetPlayerAllianceStateBJ(udg_Zampano_Player,udg_Darius_Player,bj_ALLIANCE_UNALLIED)
call SetPlayerAllianceStateBJ(udg_Jerso_Player,udg_Zampano_Player,bj_ALLIANCE_UNALLIED)
call SetPlayerAllianceStateBJ(udg_Zampano_Player,udg_Jerso_Player,bj_ALLIANCE_UNALLIED)
set udg_Chimera_Limiter=1
call DisableTrigger(GetTriggeringTrigger())
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Zampano|r has decided to return to the military and side with the Homunculi! Father has used his Alchemy to enhance Zampano's strength!")
call ForceAddPlayerSimple(udg_Zampano_Player,udg_Homunculi)
call ForceAddPlayerSimple(udg_Zampano_Player,udg_Evil_Team)
call ForceRemovePlayerSimple(udg_Zampano_Player,udg_Neutral)
call SetForceAllianceStateBJ(udg_Homunculi,GetForceOfPlayer(udg_Zampano_Player),bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Zampano_Player),udg_Homunculi,bj_ALLIANCE_ALLIED_VISION)
call SetPlayerAllianceStateBJ(Player(bj_PLAYER_NEUTRAL_EXTRA),udg_Zampano_Player,bj_ALLIANCE_ALLIED_VISION)
call SetPlayerAllianceStateBJ(udg_Zampano_Player,Player(bj_PLAYER_NEUTRAL_EXTRA),bj_ALLIANCE_ALLIED_VISION)
call DisableTrigger(gg_trg_Zampano_Alchemists_LB)
call DisableTrigger(gg_trg_Zampano_Dies_S)
call EnableTrigger(gg_trg_Zampano_Dies_H)
call ModifyHeroStat(bj_HEROSTAT_STR,udg_Zampano_Unit,bj_MODIFYMETHOD_ADD,20)
call ModifyHeroStat(bj_HEROSTAT_AGI,udg_Zampano_Unit,bj_MODIFYMETHOD_ADD,20)
call RemoveUnit(udg_Chimera_Healer)
else
call DisplayTextToForce(GetForceOfPlayer(udg_Zampano_Player),"A Chimera has already joined the Homunculi! You can not join them.")
endif
call DestroyTrigger(GetTriggeringTrigger())
endfunction
