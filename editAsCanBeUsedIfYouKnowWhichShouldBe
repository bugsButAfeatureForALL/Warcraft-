set udg_DurationVariable_ShouUlt=10.00
set udg_DistancePentagon=0
set udg_FacingAnglePentagon=0
set udg_OffsetAmount=0
set udg_AnimationSpeed=0
set udg_ShouUltDamageGroup=CreateGroup()
set udg_Degrees_Trap=0
set udg_Trap_Duration=0
set udg_Rock_Spikes_Angle=0
set udg_Rock_Degrees=0
