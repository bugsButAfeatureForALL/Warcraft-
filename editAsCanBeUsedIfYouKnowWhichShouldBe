function Trig_Philosophers_Alchemy_Func002C takes nothing returns boolean
return true
endfunction
function Trig_Philosophers_Alchemy_Actions takes nothing returns nothing
set udg_Philosophers_Position[2]=GetUnitLoc(GetTriggerUnit())
if((true))then
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=20
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call CreateNUnitsAtLoc(1,'h045',udg_Hohenheim_Player,udg_Philosophers_Position[2],(I2R(GetForLoopIndexA())*18.00))
set udg_Philosophers_Alchemy_Dummy=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(2.00,'BTLF',udg_Philosophers_Alchemy_Dummy)
call SetUnitAbilityLevelSwapped('A08A',GetLastCreatedUnit(),GetUnitAbilityLevelSwapped('A08B',udg_Hohenheim_Unit))
set udg_Philosophers_Position[4]=PolarProjectionBJ(GetUnitLoc(GetLastCreatedUnit()),0.00,(I2R(GetForLoopIndexA())*18.00))
call IssuePointOrderLoc(GetLastCreatedUnit(),"impale",udg_Philosophers_Position[4])
call RemoveLocation(udg_Philosophers_Position[4])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
else
endif
call RemoveLocation(udg_Philosophers_Position[2])
call RemoveLocation(udg_Philosophers_Position[4])
endfunction
function InitTrig_Philosophers_Alchemy takes nothing returns nothing
set gg_trg_Philosophers_Alchemy=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Philosophers_Alchemy,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Philosophers_Alchemy,Condition(function Trig_Philosophers_Alchemy_Conditions))
call TriggerAddAction(gg_trg_Philosophers_Alchemy,function Trig_Philosophers_Alchemy_Actions)
endfunction
