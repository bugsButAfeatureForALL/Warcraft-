function InitTrig_Yok_Time takes nothing returns nothing
set gg_trg_Yok_Time=CreateTrigger()
call TriggerRegisterPlayerChatEvent(gg_trg_Yok_Time,Player(0),"-Time",true)
call TriggerRegisterPlayerChatEvent(gg_trg_Yok_Time,Player(1),"-Time",true)
call TriggerRegisterPlayerChatEvent(gg_trg_Yok_Time,Player(2),"-Time",true)
call TriggerRegisterPlayerChatEvent(gg_trg_Yok_Time,Player(4),"-Time",true)
call TriggerRegisterPlayerChatEvent(gg_trg_Yok_Time,Player(5),"-Time",true)
call TriggerRegisterPlayerChatEvent(gg_trg_Yok_Time,Player(6),"-Time",true)
