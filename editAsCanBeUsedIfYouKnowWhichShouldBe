function Trig_Smash_Cast_Actions takes nothing returns nothing
set udg_Smash_Point=GetSpellTargetLoc()
call CreateNUnitsAtLoc(1,'h025',GetOwningPlayer(GetTriggerUnit()),udg_Smash_Point,bj_UNIT_FACING)
set udg_Smash_Dummy=GetLastCreatedUnit()
call SetUnitAbilityLevelSwapped('A03S',udg_Smash_Dummy,GetUnitAbilityLevelSwapped('A03R',GetTriggerUnit()))
call IssueImmediateOrder(udg_Smash_Dummy,"stomp")
call TriggerSleepAction(0.40)
call RemoveUnit(udg_Smash_Dummy)
call RemoveLocation(udg_Smash_Point)
endfunction
function InitTrig_Smash_Cast takes nothing returns nothing
set gg_trg_Smash_Cast=CreateTrigger()
