if(not(udg_Winry_Is_Dead==false))then
return false
endif
return true
endfunction
function Trig_Darius_Dies_A_Func003C takes nothing returns boolean
if(not(CountPlayersInForceBJ(udg_Alchemists)==0))then
return false
endif
if(not(CountPlayersInForceBJ(udg_Neutral)==0))then
return false
endif
return true
endfunction
function Trig_Darius_Dies_A_Actions takes nothing returns nothing
call TriggerSleepAction(10.00)
if(Trig_Darius_Dies_A_Func002C())then
call ReviveHeroLoc(udg_Darius_Unit,GetUnitLoc(gg_unit_H01V_0080),false)
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Winry|r has used her medical knowledge to revive a member of the Alchemist team!")
call SetHeroLevelBJ(GetTriggerUnit(),(GetHeroLevel(GetDyingUnit())-3),false)
call PanCameraToTimedLocForPlayer(GetOwningPlayer(GetDyingUnit()),GetUnitLoc(GetDyingUnit()),0)
call SelectUnitForPlayerSingle(GetDyingUnit(),udg_Darius_Player)
else
call GroupAddUnitSimple(udg_Darius_Unit,udg_Dead_Heroes)
call SetPlayerState(udg_Darius_Player,PLAYER_STATE_OBSERVER,1)
call ForceRemovePlayerSimple(udg_Darius_Player,udg_Alchemists)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Darius_Player),bj_ALLIANCE_ALLIED_VISION)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Darius_Player),udg_Good_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Darius_Player),bj_ALLIANCE_ALLIED_VISION)
call ForceAddPlayerSimple(udg_Darius_Player,udg_Dead_Good_Guys)
endif
if(Trig_Darius_Dies_A_Func003C())then
call DisplayTextToForce(GetPlayersAll(),"|cffff0000Father|r has eradicated all those who stood before his plans. The Promised Day arrives and a dark cloud falls over Amestris...")
call TriggerSleepAction(10.00)
call CustomDefeatBJ(Player(0),"Father has won!")
call CustomDefeatBJ(Player(1),"Father has won!")
call CustomDefeatBJ(Player(2),"Father has won!")
call CustomVictoryBJ(Player(3),true,true)
call CustomDefeatBJ(Player(4),"Father has won!")
call CustomDefeatBJ(Player(5),"Father has won!")
call CustomDefeatBJ(Player(6),"Father has won!")
call CustomDefeatBJ(Player(8),"Father has won!")
call CustomDefeatBJ(Player(9),"Father has won!")
call CustomVictoryBJ(Player(10),true,true)
call CustomVictoryBJ(Player(11),true,true)
else
endif
endfunction
