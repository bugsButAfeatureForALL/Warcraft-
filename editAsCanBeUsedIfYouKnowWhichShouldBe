function Trig_Fire_Spiral_Init_Copy_Conditions takes nothing returns boolean
if(not(GetSpellAbilityId()=='A0A6'))then
return false
endif
return true
endfunction
function Trig_Fire_Spiral_Init_Copy_Func003C takes nothing returns boolean
if(not(IsUnitGroupEmptyBJ(udg_GS_UnitGroup)==true))then
return false
endif
return true
endfunction
function Trig_Fire_Spiral_Init_Copy_Actions takes nothing returns nothing
if(Trig_Fire_Spiral_Init_Copy_Func003C())then
call EnableTrigger(gg_trg_Fire_Spiral_Loop_Copy)
else
endif
call GroupAddUnitSimple(GetTriggerUnit(),udg_GS_UnitGroup)
call SaveUnitHandleBJ(GetTriggerUnit(),0,GetHandleIdBJ(GetTriggerUnit()),udg_GS_Hashtable)
call SaveLocationHandleBJ(GetUnitLoc(GetTriggerUnit()),1,GetHandleIdBJ(GetTriggerUnit()),udg_GS_Hashtable)
call SaveRealBJ(0.00,2,GetHandleIdBJ(GetTriggerUnit()),udg_GS_Hashtable)
call SaveRealBJ((100.00+(12.50*I2R(GetUnitAbilityLevelSwapped('A0A6',GetTriggerUnit())))),3,GetHandleIdBJ(GetTriggerUnit()),udg_GS_Hashtable)
call SaveRealBJ(75.00,4,GetHandleIdBJ(GetTriggerUnit()),udg_GS_Hashtable)
call SaveGroupHandleBJ(udg_GS_DummyGroup,5,GetHandleIdBJ(GetTriggerUnit()),udg_GS_Hashtable)
call SaveRealBJ((2.00+(I2R(GetUnitAbilityLevelSwapped('A0A6',GetTriggerUnit()))*0.00)),6,GetHandleIdBJ(GetTriggerUnit()),udg_GS_Hashtable)
call SaveRealBJ(0.00,7,GetHandleIdBJ(GetTriggerUnit()),udg_GS_Hashtable)
endfunction
function InitTrig_Fire_Spiral_Init_Copy takes nothing returns nothing
set gg_trg_Fire_Spiral_Init_Copy=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Fire_Spiral_Init_Copy,EVENT_PLAYER_UNIT_SPELL_EFFECT)
call TriggerAddCondition(gg_trg_Fire_Spiral_Init_Copy,Condition(function Trig_Fire_Spiral_Init_Copy_Conditions))
call TriggerAddAction(gg_trg_Fire_Spiral_Init_Copy,function Trig_Fire_Spiral_Init_Copy_Actions)
endfunction
function Trig_Fire_Spiral_Loop_Copy_Func001Func009Func001A takes nothing returns nothing
call SetUnitFlyHeightBJ(GetEnumUnit(),0.00,250.00)
call UnitApplyTimedLifeBJ(0.50,'BTLF',GetEnumUnit())
endfunction
