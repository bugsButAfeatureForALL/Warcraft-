set udg_Counter_AlchemicArray=0
loop
exitwhen udg_Counter_AlchemicArray>76
call SetUnitTimeScalePercent(udg_AlchemicShatter_RockArray[udg_Counter_AlchemicArray],100.00)
set udg_Counter_AlchemicArray=udg_Counter_AlchemicArray+1
endloop
call TriggerSleepAction(0.25)
set udg_Counter_AlchemicArray=0
loop
exitwhen udg_Counter_AlchemicArray>76
call RemoveUnit(udg_AlchemicShatter_RockArray[udg_Counter_AlchemicArray])
set udg_Counter_AlchemicArray=udg_Counter_AlchemicArray+1
endloop
set udg_Counter_AlchemicArray=0
endfunction
