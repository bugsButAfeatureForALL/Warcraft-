if(Trig_Alkahestry_Cast_Func003C())then
call UnitDamageTargetBJ(udg_Alkahestry_Caster,udg_Alkahestry_Target,500,ATTACK_TYPE_NORMAL,DAMAGE_TYPE_NORMAL)
call TriggerSleepAction(2.00)
call RemoveUnit(udg_Alkahestry_Caster)
else
if(Trig_Alkahestry_Cast_Func003Func004C())then
call UnitDamageTargetBJ(udg_Alkahestry_Caster,udg_Alkahestry_Target,750.00,ATTACK_TYPE_NORMAL,DAMAGE_TYPE_NORMAL)
call TriggerSleepAction(2.00)
call RemoveUnit(udg_Alkahestry_Caster)
else
call UnitDamageTargetBJ(udg_Alkahestry_Caster,udg_Alkahestry_Target,950.00,ATTACK_TYPE_NORMAL,DAMAGE_TYPE_NORMAL)
call TriggerSleepAction(2.00)
call RemoveUnit(udg_Alkahestry_Caster)
endif
endif
endfunction
