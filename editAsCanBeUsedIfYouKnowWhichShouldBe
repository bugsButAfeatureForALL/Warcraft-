function Trig_Pistol_Shot_Cast_Func004C takes nothing returns boolean
if(not(GetUnitAbilityLevelSwapped('A09L',udg_Jean_Unit)==3))then
return false
endif
return true
endfunction
function Trig_Pistol_Shot_Cast_Actions takes nothing returns nothing
set udg_Pistol_Target=GetSpellTargetUnit()
if(Trig_Pistol_Shot_Cast_Func002C())then
set udg_Jean_Spell_Point=GetUnitLoc(GetSpellTargetUnit())
call CreateNUnitsAtLoc(1,'h04V',udg_Jean_Player,udg_Jean_Spell_Point,bj_UNIT_FACING)
set udg_Jean_Spell_Dummy=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(3.00,'BTLF',udg_Jean_Spell_Dummy)
call UnitRemoveAbilityBJ('A09P',udg_Jean_Spell_Dummy)
call SetUnitAbilityLevelSwapped('A09O',udg_Jean_Spell_Dummy,GetUnitAbilityLevelSwapped('A09M',udg_Jean_Unit))
call IssueTargetOrder(udg_Jean_Spell_Dummy,"sleep",udg_Pistol_Target)
call RemoveLocation(udg_Jean_Spell_Point)
else
if(Trig_Pistol_Shot_Cast_Func002Func001C())then
set udg_Jean_Spell_Point=GetUnitLoc(GetSpellTargetUnit())
call CreateNUnitsAtLoc(1,'h04V',udg_Jean_Player,udg_Jean_Spell_Point,bj_UNIT_FACING)
set udg_Jean_Spell_Dummy=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(3.00,'BTLF',udg_Jean_Spell_Dummy)
call UnitRemoveAbilityBJ('A09O',udg_Jean_Spell_Dummy)
call SetUnitAbilityLevelSwapped('A09P',udg_Jean_Spell_Dummy,GetUnitAbilityLevelSwapped('A09M',udg_Jean_Unit))
call IssueTargetOrder(udg_Jean_Spell_Dummy,"sleep",udg_Pistol_Target)
call RemoveLocation(udg_Jean_Spell_Point)
else
endif
endif
if(Trig_Pistol_Shot_Cast_Func003C())then
set udg_Jean_Spell_Point=GetUnitLoc(GetSpellTargetUnit())
call CreateNUnitsAtLoc(1,'h04V',udg_Jean_Player,udg_Jean_Spell_Point,bj_UNIT_FACING)
set udg_Jean_Spell_Dummy=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(3.00,'BTLF',udg_Jean_Spell_Dummy)
call UnitAddAbilityBJ('A09R',udg_Pistol_Target)
call SetUnitAbilityLevelSwapped('A09R',udg_Pistol_Target,GetUnitAbilityLevelSwapped('A09M',udg_Jean_Unit))
call IssueTargetOrder(udg_Jean_Spell_Dummy,"slow",udg_Pistol_Target)
call RemoveLocation(udg_Jean_Spell_Point)
call TriggerSleepAction(6.00)
call UnitRemoveAbilityBJ('A09R',udg_Pistol_Target)
else
endif
if(Trig_Pistol_Shot_Cast_Func004C())then
set udg_Jean_Spell_Point=GetUnitLoc(GetSpellTargetUnit())
call CreateNUnitsAtLoc(1,'h04V',udg_Jean_Player,udg_Jean_Spell_Point,bj_UNIT_FACING)
set udg_Jean_Spell_Dummy=GetLastCreatedUnit()
call UnitApplyTimedLifeBJ(3.00,'BTLF',udg_Jean_Spell_Dummy)
call SetUnitAbilityLevelSwapped('A09Q',udg_Jean_Spell_Dummy,GetUnitAbilityLevelSwapped('A09M',udg_Jean_Unit))
call IssueTargetOrder(udg_Jean_Spell_Dummy,"chainlightning",udg_Pistol_Target)
call RemoveLocation(udg_Jean_Spell_Point)
else
endif
endfunction
function InitTrig_Pistol_Shot_Cast takes nothing returns nothing
set gg_trg_Pistol_Shot_Cast=CreateTrigger()
call TriggerRegisterAnyUnitEventBJ(gg_trg_Pistol_Shot_Cast,EVENT_PLAYER_UNIT_SPELL_EFFECT)
