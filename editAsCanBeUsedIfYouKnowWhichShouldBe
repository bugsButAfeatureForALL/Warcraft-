function Trig_Times_Up_Func001C takes nothing returns boolean
if(not(IsUnitAlly(udg_May_Chang,udg_Scar_Player)==true))then
return false
endif
return true
endfunction
function Trig_Times_Up_Func010Func001C takes nothing returns boolean
if(not(IsUnitAliveBJ(udg_Scar_Unit)==true))then
return false
endif
if(not(GetOwningPlayer(udg_Scar_Unit)==Player(9)))then
return false
endif
return true
endfunction
function Trig_Times_Up_Func010C takes nothing returns boolean
if(not(IsUnitAliveBJ(udg_Scar_Unit)==true))then
return false
endif
if(not(GetOwningPlayer(udg_Scar_Unit)==Player(8)))then
return false
endif
return true
endfunction
function Trig_Times_Up_Actions takes nothing returns nothing
if(Trig_Times_Up_Func001C())then
call DisplayTextToForce(GetForceOfPlayer(udg_May_Chang_Player),"You are about to unally Scar! In 10 seconds you will unally him and ally the Alchemist team instead, it is suggested you get away from him.")
else
endif
call TriggerSleepAction(10.00)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_May_Chang_Player),udg_Good_Team,bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_May_Chang_Player),bj_ALLIANCE_ALLIED)
call ForceAddPlayerSimple(udg_May_Chang_Player,udg_Alchemists)
call ForceAddPlayerSimple(udg_May_Chang_Player,udg_Good_Team)
call SetPlayerAllianceStateBJ(udg_May_Chang_Player,udg_Scar_Player,bj_ALLIANCE_UNALLIED)
call SetPlayerAllianceStateBJ(udg_Scar_Player,udg_May_Chang_Player,bj_ALLIANCE_UNALLIED)
call SetPlayerAllianceStateBJ(Player(7),udg_May_Chang_Player,bj_ALLIANCE_ALLIED_VISION)
if(Trig_Times_Up_Func010C())then
call DisplayTextToForce(bj_FORCE_PLAYER[8],"In the time you've spent on your journey of revenge you have realized the Homunculi were the ones behind the massacre of your people. Not the State Alchemists. What will you do with this information?\n\n-Solo (Continue your quest of revenge against everyone)\n\n-Alchemists (Side with the State Alchemists against the true enemy and avenge your people)\n\nYou have one minute to decide.")
call EnableTrigger(gg_trg_Alchemists)
call EnableTrigger(gg_trg_Solo)
else
if(Trig_Times_Up_Func010Func001C())then
call DisplayTextToForce(bj_FORCE_PLAYER[9],"In the time you've spent on your journey of revenge you have realized the Homunculi were the ones behind the massacre of your people. Not the State Alchemists. What will you do with this information?\n\n-Solo (Continue your quest of revenge against everyone)\n\n-Alchemists (Side with the State Alch\n\nYou have one minute to decide.")
call EnableTrigger(gg_trg_Alchemists_Copy)
call EnableTrigger(gg_trg_Solo_Copy)
else
endif
endif
call TriggerSleepAction(60.00)
call DisableTrigger(gg_trg_Alchemists_Copy)
call DisableTrigger(gg_trg_Solo_Copy)
call DisableTrigger(gg_trg_Alchemists)
call DisableTrigger(gg_trg_Solo)
call DestroyTrigger(GetTriggeringTrigger())
endfunction
function InitTrig_Times_Up takes nothing returns nothing
set gg_trg_Times_Up=CreateTrigger()
call TriggerRegisterTimerExpireEventBJ(gg_trg_Times_Up,udg_Scar_Timer)
call TriggerAddAction(gg_trg_Times_Up,function Trig_Times_Up_Actions)
endfunction
function Trig_Times_Up_Copy_Conditions takes nothing returns boolean
if(not(IsUnitAliveBJ(udg_May_Chang)==true))then
return false
endif
if(not(udg_May_Chang_Is_Not_Picked==false))then
return false
endif
return true
endfunction
function Trig_Times_Up_Copy_Func001C takes nothing returns boolean
if(not(IsUnitAlly(udg_May_Chang,udg_Scar_Player)==true))then
return false
endif
return true
endfunction
function Trig_Times_Up_Copy_Actions takes nothing returns nothing
if(Trig_Times_Up_Copy_Func001C())then
call DisplayTextToForce(GetForceOfPlayer(udg_May_Chang_Player),"You are about to unally Scar! In 10 seconds you will unally him and ally the Alchemist team instead, it is suggested you get away from him.")
else
endif
call TriggerSleepAction(10.00)
call EnableTrigger(gg_trg_Kills_a_Homunculus)
call EnableTrigger(gg_trg_Kills_Kimblee)
call EnableTrigger(gg_trg_Kills_Bradley)
call DisplayTextToForce(GetPlayersAll(),"Disgusted with Scar's killings of the State Alchemists, May Chang has joined the Alchemists in their fight against the Homunculi!")
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_May_Chang_Player),udg_Good_Team,bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_May_Chang_Player),bj_ALLIANCE_ALLIED)
call ForceAddPlayerSimple(udg_May_Chang_Player,udg_Alchemists)
call ForceAddPlayerSimple(udg_May_Chang_Player,udg_Good_Team)
call SetPlayerAllianceStateBJ(udg_May_Chang_Player,udg_Scar_Player,bj_ALLIANCE_UNALLIED)
call SetPlayerAllianceStateBJ(udg_Scar_Player,udg_May_Chang_Player,bj_ALLIANCE_UNALLIED)
call SetPlayerAllianceStateBJ(Player(7),udg_May_Chang_Player,bj_ALLIANCE_ALLIED_VISION)
call UnitRemoveAbilityBJ('A06H',udg_Scar_Unit)
call TriggerSleepAction(0.20)
call UnitAddAbilityBJ('A00G',udg_Scar_Unit)
call DestroyTrigger(GetTriggeringTrigger())
endfunction
function InitTrig_Times_Up_Copy takes nothing returns nothing
set gg_trg_Times_Up_Copy=CreateTrigger()
call DisableTrigger(gg_trg_Times_Up_Copy)
call TriggerRegisterTimerExpireEventBJ(gg_trg_Times_Up_Copy,udg_Scar_Timer)
call TriggerAddCondition(gg_trg_Times_Up_Copy,Condition(function Trig_Times_Up_Copy_Conditions))
call TriggerAddAction(gg_trg_Times_Up_Copy,function Trig_Times_Up_Copy_Actions)
endfunction
function Trig_Alchemists_Conditions takes nothing returns boolean
if(not(IsUnitAliveBJ(udg_Scar_Unit)==true))then
return false
endif
if(not(GetOwningPlayer(udg_Scar_Unit)==Player(8)))then
return false
endif
return true
endfunction
function Trig_Alchemists_Func003C takes nothing returns boolean
if(not(udg_May_Chang_Is_Not_Picked==true))then
return false
endif
if(not(IsUnitAliveBJ(udg_May_Chang)==true))then
return false
endif
return true
endfunction
function Trig_Alchemists_Actions takes nothing returns nothing
call ForceAddPlayerSimple(udg_Scar_Player,udg_Good_Team)
call ForceAddPlayerSimple(udg_May_Chang_Player,udg_Good_Team)
if(Trig_Alchemists_Func003C())then
call DisplayTextToForce(GetPlayersAll(),"Realizing that the Homunculi are the true enemies Scar has given up his quest for revenge and joined the Alchemists!")
else
call DisplayTextToForce(GetPlayersAll(),"Realizing that the Homunculi are the true enemies Scar has given up his quest for revenge and alongside May Chang has joined the Alchemists!")
endif
call DisableTrigger(GetTriggeringTrigger())
call DisableTrigger(gg_trg_Solo)
set udg_Scar_Is_With_Alchemists=true
set udg_Chimera_Limiter_A=1
call DisableTrigger(gg_trg_Kills_Basque_Grand)
call EnableTrigger(gg_trg_Kills_a_Homunculus)
call EnableTrigger(gg_trg_Kills_Bradley)
call EnableTrigger(gg_trg_Kills_Kimblee)
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[8],udg_Good_Team,bj_ALLIANCE_ALLIED)
call ForceAddPlayerSimple(udg_Scar_Player,udg_Good_Team)
call ForceAddPlayerSimple(udg_Scar_Player,udg_Alchemists)
call ForceAddPlayerSimple(udg_May_Chang_Player,udg_Good_Team)
call ForceAddPlayerSimple(udg_May_Chang_Player,udg_Alchemists)
call ForceRemovePlayerSimple(udg_Scar_Player,udg_Neutral)
call UnitRemoveAbilityBJ('A06H',udg_Scar_Unit)
call UnitRemoveAbilityBJ('A09N',udg_Scar_Unit)
call UnitAddAbilityBJ('A02S',udg_Scar_Unit)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_May_Chang_Player),udg_Good_Team,bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_May_Chang_Player),bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[bj_PLAYER_NEUTRAL_VICTIM],udg_Good_Team,bj_ALLIANCE_ALLIED)
call RemoveUnit(udg_Scar_Fountain)
call SetForceAllianceStateBJ(udg_Good_Team,bj_FORCE_PLAYER[8],bj_ALLIANCE_ALLIED)
call TriggerSleepAction(1.00)
call SetPlayerAllianceStateBJ(Player(7),udg_May_Chang_Player,bj_ALLIANCE_ALLIED_VISION)
call SetPlayerAllianceStateBJ(Player(7),udg_Scar_Player,bj_ALLIANCE_ALLIED_VISION)
call DestroyTrigger(GetTriggeringTrigger())
endfunction
function InitTrig_Alchemists takes nothing returns nothing
set gg_trg_Alchemists=CreateTrigger()
call DisableTrigger(gg_trg_Alchemists)
call TriggerRegisterPlayerChatEvent(gg_trg_Alchemists,Player(8),"-Alchemists",true)
call TriggerAddCondition(gg_trg_Alchemists,Condition(function Trig_Alchemists_Conditions))
call TriggerAddAction(gg_trg_Alchemists,function Trig_Alchemists_Actions)
endfunction
