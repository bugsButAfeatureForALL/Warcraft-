function Trig_Hohenheims_Cast_Func003C takes nothing returns boolean
if(not(DistanceBetweenPoints(GetUnitLoc(udg_Hohenheim_Unit),GetUnitLoc(udg_Our_Father))<=950.00))then
return false
endif
return true
endfunction
function Trig_Hohenheims_Cast_Actions takes nothing returns nothing
call CreateNUnitsAtLoc(1,'h047',GetOwningPlayer(GetSpellAbilityUnit()),GetUnitLoc(GetSpellAbilityUnit()),bj_UNIT_FACING)
set udg_Hohenheims_Power=GetLastCreatedUnit()
if(Trig_Hohenheims_Cast_Func003C())then
call IssueImmediateOrder(udg_Our_Father,"manashieldoff")
call UnitRemoveBuffBJ('BNms',udg_Our_Father)
call IssuePointOrderLoc(udg_Hohenheims_Power,"silence",GetUnitLoc(udg_Our_Father))
else
endif
call TriggerSleepAction(1.00)
call RemoveUnit(udg_Hohenheims_Power)
endfunction
