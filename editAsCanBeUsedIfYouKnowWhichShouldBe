endfunction
function Trig_Greed_is_Chosen_Conditions takes nothing returns boolean
if(not(GetUnitTypeId(GetTriggerUnit())=='H017'))then
return false
endif
return true
endfunction
function Trig_Greed_is_Chosen_Func018Func003C takes nothing returns boolean
if(not(IsUnitAliveBJ(udg_Roa_Unit)==true))then
return false
endif
return true
endfunction
function Trig_Greed_is_Chosen_Func018C takes nothing returns boolean
if(not(IsUnitAliveBJ(udg_Dolcetto_Unit)==true))then
return false
endif
return true
endfunction
function Trig_Greed_is_Chosen_Actions takes nothing returns nothing
set udg_Is_Greed_Chosen=true
set udg_Greed=GetTriggerUnit()
set udg_Greed_Player=GetOwningPlayer(GetTriggerUnit())
call SetPlayerName(udg_Greed_Player,"Greed")
call UnitAddItemByIdSwapped('I000',udg_Greed)
call SetUnitPositionLoc(GetTriggerUnit(),GetRectCenter(gg_rct_Greed_Spawn_1))
call PanCameraToTimedLocForPlayer(GetOwningPlayer(udg_Greed),GetRectCenter(gg_rct_Greed_Spawn_1),0)
call SelectUnitForPlayerSingle(GetTriggerUnit(),GetOwningPlayer(GetTriggerUnit()))
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Greed_Player),udg_Evil_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Greed_Player),udg_Good_Team,bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Greed_Player),bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(udg_Evil_Team,GetForceOfPlayer(udg_Greed_Player),bj_ALLIANCE_UNALLIED)
call SetForceAllianceStateBJ(bj_FORCE_PLAYER[bj_PLAYER_NEUTRAL_VICTIM],GetForceOfPlayer(udg_Greed_Player),bj_ALLIANCE_UNALLIED)
call ForceAddPlayerSimple(udg_Greed_Player,udg_Neutral)
call DisplayTextToForce(GetForceOfPlayer(udg_Greed_Player),"You are the Homunculus |cffff0000Greed|r! You are the most durable of all the neutral characters but if you need to heal return to the Devil's Nest bar. It is your goal to wipe out everyone and claim the world as your own! Should |cffff0000Dolcetto|r or |cffff0000Roa|r be selected they will join you!")
call CreateNUnitsAtLoc(1,'h01Z',udg_Greed_Player,GetRectCenter(gg_rct_Greed_Spawn_1),bj_UNIT_FACING)
set udg_Greed_Healer=GetLastCreatedUnit()
if(Trig_Greed_is_Chosen_Func018C())then
call ModifyHeroStat(bj_HEROSTAT_STR,udg_Dolcetto_Unit,bj_MODIFYMETHOD_ADD,15)
call ModifyHeroStat(bj_HEROSTAT_AGI,udg_Dolcetto_Unit,bj_MODIFYMETHOD_ADD,15)
call SetPlayerAllianceStateBJ(udg_Dolcetto_Player,udg_Greed_Player,bj_ALLIANCE_ALLIED_VISION)
call SetPlayerAllianceStateBJ(udg_Greed_Player,udg_Dolcetto_Player,bj_ALLIANCE_ALLIED_VISION)
else
if(Trig_Greed_is_Chosen_Func018Func003C())then
call ModifyHeroStat(bj_HEROSTAT_STR,udg_Roa_Unit,bj_MODIFYMETHOD_ADD,15)
call ModifyHeroStat(bj_HEROSTAT_AGI,udg_Roa_Unit,bj_MODIFYMETHOD_ADD,15)
call SetPlayerAllianceStateBJ(udg_Greed_Player,udg_Roa_Player,bj_ALLIANCE_ALLIED_VISION)
call SetPlayerAllianceStateBJ(udg_Roa_Player,udg_Greed_Player,bj_ALLIANCE_ALLIED_VISION)
else
endif
endif
call RemoveUnit(udg_Dolcetto_Healer)
call RemoveUnit(udg_Roa_Healer)
call DestroyTrigger(GetTriggeringTrigger())
endfunction
function InitTrig_Greed_is_Chosen takes nothing returns nothing
set gg_trg_Greed_is_Chosen=CreateTrigger()
call TriggerRegisterEnterRectSimple(gg_trg_Greed_is_Chosen,gg_rct_Neutral_Spawns)
call TriggerAddCondition(gg_trg_Greed_is_Chosen,Condition(function Trig_Greed_is_Chosen_Conditions))
call TriggerAddAction(gg_trg_Greed_is_Chosen,function Trig_Greed_is_Chosen_Actions)
endfunction
