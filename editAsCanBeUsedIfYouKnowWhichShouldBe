function Trig_Flame_Damage_Actions takes nothing returns nothing
if(Trig_Flame_Damage_Func001C())then
set udg_Flame_Interger_2=0
call DisableTrigger(GetTriggeringTrigger())
else
set udg_Flame_Interger_2=(udg_Flame_Interger_2+1)
endif
set udg_Flame_Integer=1
loop
exitwhen udg_Flame_Integer>10
set udg_Flame_Dmg_Grp=GetUnitsInRangeOfLocAll(250.00,udg_Flame_Group[udg_Flame_Integer])
call ForGroupBJ(udg_Flame_Dmg_Grp,function Trig_Flame_Damage_Func002Func002A)
call DestroyGroup(udg_Flame_Dmg_Grp)
set udg_Flame_Integer=udg_Flame_Integer+1
endloop
endfunction
