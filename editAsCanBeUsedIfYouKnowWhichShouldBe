endfunction
function main takes nothing returns nothing
call SetCameraBounds(-10240.0+GetCameraMargin(CAMERA_MARGIN_LEFT),-10240.0+GetCameraMargin(CAMERA_MARGIN_BOTTOM),9984.0-GetCameraMargin(CAMERA_MARGIN_RIGHT),10112.0-GetCameraMargin(CAMERA_MARGIN_TOP),-10240.0+GetCameraMargin(CAMERA_MARGIN_LEFT),10112.0-GetCameraMargin(CAMERA_MARGIN_TOP),9984.0-GetCameraMargin(CAMERA_MARGIN_RIGHT),-10240.0+GetCameraMargin(CAMERA_MARGIN_BOTTOM))
call SetDayNightModels("Environment\\DNC\\DNCLordaeron\\DNCLordaeronTerrain\\DNCLordaeronTerrain.mdl","Environment\\DNC\\DNCLordaeron\\DNCLordaeronUnit\\DNCLordaeronUnit.mdl")
call NewSoundEnvironment("Default")
call SetAmbientDaySound("LordaeronSummerDay")
call SetAmbientNightSound("LordaeronSummerNight")
call SetMapMusic("Music",true,0)
call InitSounds()
call CreateRegions()
call CreateAllDestructables()
call CreateAllItems()
call CreateAllUnits()
call InitBlizzard()
call InitGlobals()
call InitCustomTriggers()
call RunInitializationTriggers()
endfunction
function config takes nothing returns nothing
call SetMapName("Fullmetal Alchemist")
call SetMapDescription("")
call SetPlayers(11)
call SetTeams(11)
call SetGamePlacement(MAP_PLACEMENT_TEAMS_TOGETHER)
call DefineStartLocation(0,-4864.0,9792.0)
call DefineStartLocation(1,-4864.0,9792.0)
call DefineStartLocation(2,-4864.0,9792.0)
call DefineStartLocation(3,-3072.0,9792.0)
call DefineStartLocation(4,-4864.0,9792.0)
call DefineStartLocation(5,-4864.0,9792.0)
call DefineStartLocation(6,-4864.0,9792.0)
call DefineStartLocation(7,-4032.0,9792.0)
call DefineStartLocation(8,-4032.0,9792.0)
call DefineStartLocation(9,-3072.0,9792.0)
call DefineStartLocation(10,-3072.0,9792.0)
call InitCustomPlayerSlots()
call InitCustomTeams()
call InitAllyPriorities()
endfunction
