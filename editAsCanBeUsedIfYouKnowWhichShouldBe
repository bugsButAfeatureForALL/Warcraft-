call ForceAddPlayerSimple(udg_Jerso_Player,udg_Good_Team)
call ForceRemovePlayerSimple(udg_Jerso_Player,udg_Neutral)
call SetForceAllianceStateBJ(udg_Good_Team,GetForceOfPlayer(udg_Jerso_Player),bj_ALLIANCE_ALLIED)
call SetForceAllianceStateBJ(GetForceOfPlayer(udg_Jerso_Player),udg_Good_Team,bj_ALLIANCE_ALLIED)
call SetPlayerAllianceStateBJ(Player(7),udg_Jerso_Player,bj_ALLIANCE_ALLIED_VISION)
call SetPlayerAllianceStateBJ(udg_Jerso_Player,Player(7),bj_ALLIANCE_ALLIED_VISION)
call DisableTrigger(gg_trg_Jerso_Homunculi_G)
call DisableTrigger(gg_trg_Jerso_Dies_S)
call EnableTrigger(gg_trg_Jerso_Dies_A)
call RemoveUnit(udg_Chimera_Healer)
else
call DisplayTextToForce(GetForceOfPlayer(udg_Jerso_Player),"A Chimera has already joined the Alchemists! You can not join them.")
endif
call DestroyTrigger(GetTriggeringTrigger())
endfunction
