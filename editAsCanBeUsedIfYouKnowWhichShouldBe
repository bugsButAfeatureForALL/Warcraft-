return true
endfunction
function Trig_Canine_Reflexes_Actions takes nothing returns nothing
call DisableTrigger(GetTriggeringTrigger())
if(Trig_Canine_Reflexes_Func005C())then
call EnableTrigger(gg_trg_Canine_Reflexes_Removal)
call UnitAddAbilityBJ('A06O',udg_Dolcetto_Unit)
call UnitAddAbilityBJ('A075',udg_Dolcetto_Unit)
call SetUnitAbilityLevelSwapped('A06O',udg_Dolcetto_Unit,GetUnitAbilityLevelSwapped('A06N',udg_Dolcetto_Unit))
call SetUnitAbilityLevelSwapped('A075',udg_Dolcetto_Unit,GetUnitAbilityLevelSwapped('A06N',udg_Dolcetto_Unit))
else
if(Trig_Canine_Reflexes_Func005Func002C())then
call EnableTrigger(gg_trg_Canine_Reflexes_Removal)
call UnitAddAbilityBJ('A06O',udg_Dolcetto_Unit)
call UnitAddAbilityBJ('A075',udg_Dolcetto_Unit)
call SetUnitAbilityLevelSwapped('A06O',udg_Dolcetto_Unit,GetUnitAbilityLevelSwapped('A06N',udg_Dolcetto_Unit))
call SetUnitAbilityLevelSwapped('A075',udg_Dolcetto_Unit,GetUnitAbilityLevelSwapped('A06N',udg_Dolcetto_Unit))
else
if(Trig_Canine_Reflexes_Func005Func002Func004C())then
call EnableTrigger(gg_trg_Canine_Reflexes_Removal)
call UnitAddAbilityBJ('A06O',udg_Dolcetto_Unit)
call UnitAddAbilityBJ('A075',udg_Dolcetto_Unit)
call SetUnitAbilityLevelSwapped('A06O',udg_Dolcetto_Unit,GetUnitAbilityLevelSwapped('A06N',udg_Dolcetto_Unit))
call SetUnitAbilityLevelSwapped('A075',udg_Dolcetto_Unit,GetUnitAbilityLevelSwapped('A06N',udg_Dolcetto_Unit))
else
if(Trig_Canine_Reflexes_Func005Func002Func004Func004C())then
call EnableTrigger(gg_trg_Canine_Reflexes_Removal)
call UnitAddAbilityBJ('A06O',udg_Dolcetto_Unit)
call UnitAddAbilityBJ('A075',udg_Dolcetto_Unit)
call SetUnitAbilityLevelSwapped('A06O',udg_Dolcetto_Unit,GetUnitAbilityLevelSwapped('A06N',udg_Dolcetto_Unit))
call SetUnitAbilityLevelSwapped('A075',udg_Dolcetto_Unit,GetUnitAbilityLevelSwapped('A06N',udg_Dolcetto_Unit))
else
if(Trig_Canine_Reflexes_Func005Func002Func004Func004Func004C())then
call EnableTrigger(gg_trg_Canine_Reflexes_Removal)
call UnitAddAbilityBJ('A06O',udg_Dolcetto_Unit)
call UnitAddAbilityBJ('A075',udg_Dolcetto_Unit)
call SetUnitAbilityLevelSwapped('A06O',udg_Dolcetto_Unit,GetUnitAbilityLevelSwapped('A06N',udg_Dolcetto_Unit))
call SetUnitAbilityLevelSwapped('A075',udg_Dolcetto_Unit,GetUnitAbilityLevelSwapped('A06N',udg_Dolcetto_Unit))
else
endif
endif
endif
endif
endif
endfunction
